BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = EDI-param
   ACTOBJ            = EDI-param
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       EDI-param-ERR SECTION.
           use after error procedure on EDI-param.
           set RecLocked to false.
           evaluate status-EDI-param
           when "35"
                display message "File [EDI-param] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [EDI-param] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[EDI-param] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99"
           when "93"
                set RecLocked to true
                set errori    to true
                move 1 to mod-k
                move 0 to mod
                modify tool-modifica, value = mod
                display form1
                set StatusVisua to true
                move "00" to status-EDI-param
                if ReadSecca
                   read EDI-param       no lock 
                        invalid      continue
                        not invalid  perform FORM1-IUD-DISPLAY
                   end-read
                end-if
                display message box MSG-Record-gia-in-uso
                        title = tit-err
                        icon is mb-error-icon
           when "23"
                move 1 to mod-k
                move 0 to mod
                modify tool-modifica, value = mod
                set StatusVisua to true
                if nuovo              
                   initialize epa-dati replacing numeric data by zeroes
                                            alphanumeric data by spaces
                   perform FORM1-FLD-TO-BUF
                   display form1-tb-1
                   set vecchio to true
                end-if    
                perform ABILITAZIONI
                display form1
                |RISETTO IL VALORE A "23" PERCHE DENTRO AL PAR. CLEAR-SCREEN
                |CON L'OPERAZIONE UNLOCK VIENE RISETTATO A ZERO COSICCHÈ 
                |QUANDO TORNA DENTRO AI PAR. DI SCORRIMENTO DI TOTEM SI 
                |TROVA IN UNA SITUAZIONE CORRETTA E PROSEGUE CON LA READ
                |USCENDO CON UN MESSAGGIO DI ERRORE DI TIPO "46"
                move "23" to status-EDI-param
           end-evaluate.
 

END-TEXT
END-DATA
