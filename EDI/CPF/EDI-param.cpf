BEGIN-TEXT
           move epa-chiave to old-epa-chiave.

           initialize epa-dati replacing numeric data by zeroes
                                   alphanumeric data by spaces.
                         
           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.
                                          
           display form1. 
  
           unlock EDI-param all records.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-INSERIMENTO
BEGIN-TEXT
      * MODIFICA LA STATUS-BAR
      * CONTROLLO SE DA INSERIMENTO SCORRO. ALLORA DEVO SEGNALARE CHE
      * SONO TORNATO IN MODIFICA.
           inquire tool-modifica, value in mod.

           inquire tool-modifica, value in mod.

           evaluate mod
           when 1
                set StatusModifica to true
           when 0
                set StatusVisua to true   
           end-evaluate.
      
           perform STATUS-BAR-MSG.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.
      
           evaluate CONTROL-ID
           when 78-ID-ef-ID
                inquire ef-ID, value in ef-ID-buf
                if ef-ID-buf = space
                   set errori to true
                   display message "ID obbligatorio!"
                      title tit-err
                      icon  MB-WARNING-ICON
                   move 78-ID-ef-ID to CONTROL-ID
                end-if                       
           when 78-ID-ef-q
                inquire ef-q, value in ef-q-buf
                if ef-q-buf = space
                   set errori to true
                   display message "Qualificatore obbligatorio!"
                      title tit-err
                      icon  MB-WARNING-ICON
                   move 78-ID-ef-q to CONTROL-ID
                end-if                 
           when 78-ID-ef-piva
                inquire ef-piva, value in ef-piva-buf
                if ef-piva-buf = space
                   set errori to true
                   display message "PIVA obbligatoria!"
                      title tit-err
                      icon  MB-WARNING-ICON
                   move 78-ID-ef-piva to CONTROL-ID
                end-if                 
           when 78-ID-ef-ragsoc
                inquire ef-ragsoc, value in ef-ragsoc-buf
                if ef-ragsoc-buf = space
                   set errori to true
                   display message "Ragione Sociale obbligatoria!"
                      title tit-err
                      icon  MB-WARNING-ICON
                   move 78-ID-ef-ragsoc to CONTROL-ID
                end-if
           when 78-ID-ef-indirizzo
                inquire ef-indirizzo, value in ef-indirizzo-buf
                if ef-indirizzo-buf = space
                   set errori to true
                   display message "Indirizzo obbligatorio!"
                      title tit-err
                      icon  MB-WARNING-ICON
                   move 78-ID-ef-indirizzo to CONTROL-ID
                end-if                    
           when 78-ID-ef-citta
                inquire ef-citta, value in ef-citta-buf
                if ef-citta-buf = space
                   set errori to true
                   display message "Città obbligatoria!"
                      title tit-err
                      icon  MB-WARNING-ICON
                   move 78-ID-ef-citta to CONTROL-ID
                end-if                
           when 78-ID-ef-prov
                inquire ef-prov, value in ef-prov-buf
                if ef-prov-buf = space
                   set errori to true
                   display message "Provincia obbligatoria!"
                      title tit-err
                      icon  MB-WARNING-ICON
                   move 78-ID-ef-prov to CONTROL-ID
                end-if               
           when 78-ID-ef-cap
                inquire ef-cap, value in ef-cap-buf
                if ef-cap-buf = space
                   set errori to true
                   display message "CAP obbligatorio!"
                      title tit-err
                      icon  MB-WARNING-ICON
                   move 78-ID-ef-cap to CONTROL-ID
                end-if  
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move control-id to store-id
              move 4          to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if mod = 1
              read EDI-param    lock invalid 
                 set errori to true 
              end-read
           else
              read EDI-param no lock invalid 
                 set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0           to mod
                    move 1           to mod-k
                    move 78-ID-ef-id to CONTROL-ID
                    move 4           to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                    modify Form1-St-1-Handle, panel-index 3
                       panel-text "MODIFICA"
                 else                         
                    set StatusVisua    to true
                    modify Form1-St-1-Handle, panel-index 3
                       panel-text "MODIFICA"
                 end-if
                 perform VALORIZZA-OLD
              else
                 move ZERO to mod
                 move 1    to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-ID to CONTROL-ID
                       move 4           to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT
BEGIN-TEXT
           move 0 to StatusHelp.
           move 1 to mod-k.
           set StatusVisua to true.     
           set errore-des  to false.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-epa-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if CONTROL-ID < 78-ID-ef-ID
                       move 78-ID-ef-ID to CONTROL-ID
                    end-if
                    move ZERO to mod-k
                    move 1    to mod
                    set StatusModifica to true
                    modify Form1-St-1-Handle, panel-index 3
                           panel-text "MODIFICA"
                 end-if
              else
                 move 1    to mod
                 perform SALV-MOD            
                 move ZERO to mod
                 move 1    to mod-k
                 if errori
                    move 1    to mod
                    move ZERO to mod-k
                 else
                    move 78-ID-ef-ID to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    modify Form1-St-1-Handle, panel-index 3
                       panel-text "VISUALIZZAZIONE"
                    unlock EDI-param all records
                 end-if
              end-if
                        
              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           perform SALV-MOD.
      
           if tutto-ok  
              move 78-ID-ef-ID       to CONTROL-ID       
              move 4                 to ACCEPT-CONTROL   
              move 1                 to mod mod-k
              modify tool-modifica,  value = mod
              perform CANCELLA-COLORE                  
              perform VALORIZZA-NUOVO
              perform INIT-OLD-REC       
              set StatusIns to true
              perform STATUS-BAR-MSG 

              set nuovo to true       
              unlock EDI-param all records
           end-if.      
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ORDINAMENTO
BEGIN-TEXT
      *     inquire tool-ord, value in OrderBy.
      *
      *     if OrderBy = zero
      *        perform DataSet1-CHANGETO-KEY1
      *     else
      *        perform DataSet1-CHANGETO-KEY2
      *     end-if.
      *
      *     perform Form1-Curr.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           Move SPACES to epa-codice.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".
           copy "utydata.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok    
      
              perform CHECK-INSERIMENTO
              unlock EDI-param all records
      
              if mod = 1
                 move 0    to mod-k
                 set dataset1-EDI-param-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-EDI-param-lock-flag
              end-if
                                  
              perform CANCELLA-COLORE
              perform FORM1-FIRST

      * SPECIFICO DEL PGM "EDI-param"
              if Status-EDI-param not = ZERO
                 perform NUOVO
              end-if
      
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-EDI-param-lock-flag
                 move 1 to mod-k
                 perform FORM1-FIRST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.

           set tutto-ok to true.

           perform varying CONTROL-ID from 78-ID-ef-id by 1
                     until CONTROL-ID > 78-ID-ef-cap
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if errori
              perform ABILITAZIONI
              move store-id to CONTROL-ID       
              move 4        to ACCEPT-CONTROL   
           else                       
              perform FORM1-BUF-TO-FLD      
              perform CANCELLA-COLORE 

              write epa-rec invalid rewrite epa-rec end-write

              set vecchio to true
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform Form1-CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type MB-YES-NO-CANCEL 
                            giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori    to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA
BEGIN-TEXT
           continue.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STATUS-BAR-MSG
BEGIN-TEXT
           evaluate true
           when StatusModifica
                modify form1-st-1-handle, 
                       panel-index  3,
                       panel-text  "MODIFICA"
           when StatusVisua
                modify form1-st-1-handle, 
                       panel-index  3,
                       panel-text  "VISUALIZZAZIONE"
                move 0 to StatusHelp
                perform STATUS-HELP
           when StatusIns
                modify form1-st-1-handle, 
                       panel-index  3,
                       panel-text  "INSERIMENTO"
           when other
                modify form1-st-1-handle, 
                       panel-index  2,
                       panel-text   spaces
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STATUS-HELP
BEGIN-TEXT
           if StatusHelp = 1
              modify Form1-St-1-Handle, 
                     panel-index = 2, 
                     panel-text "F8 HELP record presenti"
              move BitmapZoomEnabled to BitmapNumZoom
           else
              modify Form1-St-1-Handle, 
                     panel-index = 2, 
                     panel-text spaces
              move BitmapZoomDisabled to BitmapNumZoom
           end-if.
           move StatusHelp to e-cerca.
           modify tool-cerca, enabled = e-cerca.
           modify tool-cerca, bitmap-number = BitmapNumZoom.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-ID to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock EDI-param all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move epa-rec to old-epa-rec.
           set vecchio to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-NUOVO
BEGIN-TEXT
      * ISTRUZIONE SPECIFICA DEL PGM tgeDPAG
           perform Form1-Clear
      * FINE                                                                                         

           initialize epa-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                                       
           perform FORM1-IUD-DISPLAY.
END-TEXT
END-DATA
