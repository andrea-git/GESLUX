      * USER DEFINE PARAGRAPH
       ABILITAZIONI.
      * <TOTEM:PARA. ABILITAZIONI>
           if mod = 1
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else
              |se sono in visua           
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move 0 to e-salva e-cancella
           end-if.     

      * ISACCO (CORREZIONE SU LIVELLO ABILITAZIONE 2) - 10/11/2003
      * (CANCELLAZIONE NON PERMESSA)
           if livello-abil = 2 
              move ZERO to e-cancella
              move BitmapDeleteDisabled to BitmapNumDelete
           end-if.
      * FINE

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-nuovo, tool-salva, tool-cancella, tool-modifica.
      * FINE.

           perform GRID-HELP 
           .
      * <TOTEM:END>

       ANTEPRIMA.
      * <TOTEM:PARA. ANTEPRIMA>
           set anteprima       to true.
      *     perform paragrafo-stampa 
           .
      * <TOTEM:END>

       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = 0 
              exit paragraph 
           end-if.

           if control-id = 78-id-gd-cve
              display message box MSG-Cancellare-la-riga-selezionata
                      title   titolo
                      type    mb-yes-no 
                      default mb-no
                      giving  scelta
              set DeleteRiga to true
           end-if.
      *
           if scelta = mb-yes      
              set tutto-ok to true      
              perform RIEMPI-CHIAVE   
              move aeg-articolo    to memo-art
              perform X-Y
              perform VALORE-RIGA
              ||move col-id-num   to aeg-articolo
              delete art-exp-get record 
                 invalid 
                    continue 
              end-delete
              modify gd-cve,  record-to-delete = riga
              inquire gd-cve, last-row in totale-righe
              move 0 to riga-nuova

              if totale-righe = 1 
                 move 1 to control-id 
              else
                 if riga > totale-righe
                    move totale-righe to riga
                 end-if
                 if riga = 2
                    unlock art-exp-get all records   
                    perform VALORE-RIGA
                    perform RIEMPI-CHIAVE   
                    perform LOCK-RECORD
                 end-if
                 move riga to MyEventData2
                 perform COLORE
                 modify gd-cve, cursor-y = riga, cursor-x = 1
              end-if

              set vecchio to true
              perform INIT-OLD-REC
      
              set StatusModifica to true
              perform STATUS-BAR-MSG
              display message MSG-cancellazione-avvenuta-con-successo
                      title = titolo
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate CONTROL-ID
           when 78-ID-EF-art
                inquire ef-art, VALUE art-codice
                move "articoli"  to Como-File
                call   "zoom-gt" using  como-file, art-rec
                                 giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-art, value art-codice
                   move art-descrizione  to des-art
                   display lbl-des-art
                   move 4 to accept-control
                end-if


           when 78-id-gd-cve
                if colonna = 1| and nuovo
                   perform VALORE-RIGA
                   move aeg-imballo to imq-codice
                   move "timbalqta"  to  como-file
                   call   "zoom-gt" using  como-file, imq-rec
                                    giving stato-zoom
               
                   if stato-zoom = 0  
                      move imq-codice to col-imballo
                      modify gd-cve(riga, 1), cell-data = col-imballo
                      perform DESCRIZIONE-IMBALLO
                      if aeg-imballo not = old-aeg-imballo
                         set NoSalvato to true                          
             
                      else 
                         set SiSalvato to true
                      end-if
                   end-if
                end-if
           end-evaluate  
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.
           set trovato  to true.
           perform VALORE-RIGA. 

           evaluate control-id
           when 78-ID-ef-art
                move ef-art-buf to como-articolo
                                   art-codice
                read articoli
                   invalid 
                      if vecchio
                         display message box MSG-record-inesistente
                                 title = tit-err
                                 icon mb-warning-icon
                          set errori to true
                          move 78-ID-ef-art to control-id
                      end-if
                      move spaces to des-art
                   not invalid      
                      move art-descrizione  to des-art
                end-read
                display lbl-des-art
           when 78-id-gd-cve             
                evaluate colonna
                when 1
                     move aeg-imballo to imq-codice
                     read timbalqta
                        invalid
                           move 2 to colonna
                           set errori to true
                           display message box "Imballo inesistente"
                                   title = tit-err
                                   icon mb-warning-icon
                     end-read
                     perform DESCRIZIONE-IMBALLO
                     if nuovo and tutto-ok
                        move ef-art-buf  to aeg-articolo
                        read art-exp-get
                           invalid
                              continue
                           not invalid
                               set errori to true
                               display message MGS-codice-gia-inserito|"Codice già inserito!"
                                       title = tit-err
                                       icon mb-warning-icon
                               move 1 to colonna
                        end-read
                     end-if
                end-evaluate
           end-evaluate.  

      ****************
           if errori
              if control-id = 78-id-gd-cve
                 modify gd-cve, cursor-y = riga, cursor-x = colonna
                 move riga    to event-data-2
                 move colonna to event-data-1
              else
                 move 78-ID-ef-art to control-id
                 move 4            to accept-control
              end-if
           end-if 
           .
      * <TOTEM:END>

       GRID-HELP.
      * <TOTEM:PARA. GRID-HELP>
      *    se non sono in modifica disabilito sempre lo zoom
           if mod = zero
              move BitmapZoomDisabled to BitmapNumZoom
              move 0 to e-cerca
              modify tool-cerca, bitmap-number = BitmapNumZoom,
                                 enabled       = e-cerca
           else
              evaluate colonna|event-data-1
              when 2
                   move BitmapZoomEnabled to BitmapNumZoom
                   move 1 to e-cerca
              when other
                   move BitmapZoomDisabled to BitmapNumZoom
                   move 0 to e-cerca
              end-evaluate
           
              modify tool-cerca, bitmap-number = BitmapNumZoom,
                                 enabled       = e-cerca
           end-if 
           .
      * <TOTEM:END>

       INIT.
      * <TOTEM:PARA. INIT>
           set vecchio  to true.
           set tutto-ok to true 
           .
