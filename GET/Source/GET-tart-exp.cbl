       MODIFICA.
      * <TOTEM:PARA. MODIFICA>
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else         
              set ReadSecca to true
      *  se l'utente e' abilitato puo' modificare un record
              if mod = 1
      * Adriano - Gestione lock sul file inizio
                 close art-exp-get
                 open i-o art-exp-get allowing readers
      * Adriano fine
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if not trovato 
                    set errori to true 
                 end-if
                 if RecLocked   
                    set errori to true 
                 end-if
                 
                 if errori 
                    move 0 to mod 
                    move 1 to mod-k
                    set StatusVisua to true
                 else
                    move 0 to mod-k
                    move 1 to mod
                    set StatusModifica to true
                    move 2 to riga MyEventData2
                    move 1 to colonna
                    modify gd-cve, cursor-y = riga, 
                                       cursor-x = colonna
                    move 1   to StatusHelp
                    perform STATUS-HELP

                    perform SPOSTAMENTO
                 end-if   
              else
                 move 1 to mod
                 perform SALV-MOD            
                 move 0 to mod
                 move 1 to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    if nuovo
                       if totale-righe not > 1
                          perform PRIMO
                       end-if
                    else
      * Adriano - Gestione lock sul file inizio
                       close art-exp-get
                       open input art-exp-get
      * Adriano - fine
                       move 0 to MyEventData2
                       set NoMessage to true
                       perform CURRENT-RECORD
                       set StatusVisua to true
                       unlock art-exp-get all records
                    end-if
                 end-if
              end-if                   

              perform DISPLAY-CAMPI-CHIAVE
                        
              display form1             
              modify control-handle, color = colore-or

              move 78-id-ef-art to control-id
              move 4 to accept-control

              set ReadSecca to false

           end-if.                      
           modify tool-modifica,  value mod.
           perform STATUS-BAR-MSG 
           .
      * <TOTEM:END>

       NUOVO.
      * <TOTEM:PARA. NUOVO>
           if control-id not = 78-id-gd-cve
              perform SALV-MOD
              if tutto-ok
                 move 78-id-ef-art to control-id
                 move 4 to accept-control
                 move 1 to mod mod-k
                 modify tool-modifica,  value = mod
                 modify control-handle, color = colore-or
                 perform CLEAR-SCREEN
                 perform INIT-OLD-REC
                 set StatusIns to true
                 perform STATUS-BAR-MSG  
                 set nuovo to true
                 move 0 to RowCounter
                 unlock art-exp-get all records
              end-if
           else   
              if riga-nuova = 0    
                 perform INSERT-RIGA
              else
                 perform VALORE-RIGA
                 if col-imballo not = space
                    perform INSERT-RIGA
                 end-if
              end-if
           end-if.

      ***---
       INSERT-RIGA.
           perform X-Y.
           if vecchio
              move 0 to mod-k       
           end-if.   
           set nuovo to true.
           move 1 to mod.
           modify tool-modifica,  value = mod.
           perform ABILITAZIONI.
           perform VALORE-RIGA.
      *     if aeg-imballo = spaces
      *        perform DEL-RIGA 
      *     end-if.
           add 1 to totale-righe giving riga.
           modify gd-cve, insert-rows = 1.
           move riga to MyEventData2. 
           move    1 to riga-nuova.
           move    1 to colonna.
           move    3 to control-id.
           move    4 to accept-control.
           modify gd-cve, cursor-y = riga, cursor-x = colonna.
           perform COLORE.
           set StatusModifica to true.
           perform STATUS-BAR-MSG.
           move 1   to StatusHelp.
           perform STATUS-HELP 
           .
      * <TOTEM:END>

