       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           perform CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel
                            giving scelta

              evaluate scelta
              when mb-yes
                   perform SALVA
              when mb-no
                   continue
              when other
                   modify control-handle, color = colore-or
                   set errori to true
                   move store-id to control-id
                   move 4 to accept-control
                   if control-id= 78-id-gd-cve
                      move store-riga to riga MyEventData2
                      modify gd-cve, cursor-y = riga, cursor-x = 2
                      perform COLORE
                   end-if
              end-evaluate

           end-if 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           if sisalvato or mod = 0 
              exit paragraph 
           end-if.

           inquire gd-cve, cursor-x in colonna, 
                               cursor-y in riga.


           if mod-k = 1
              perform varying control-id from 78-ID-ef-art by 1
                     until control-id > 78-id-gd-cve
                 perform CONTROLLO
                 if errori 
                    exit perform 
                 end-if
              end-perform
           end-if.

           if errori
              move store-id to control-id
              move 4 to accept-control
           else        
              move 78-id-gd-cve   to control-id
              perform X-Y
              if totale-righe = 1
                 set errori to true
              else
                 move riga to store-riga
                 perform RIEMPI-CHIAVE       
                 accept como-data from century-date
                 accept como-ora  from time
                 modify gd-cve, mass-update = 1
                 perform varying riga from 2 by 1 
                           until riga > totale-righe
   
                    modify gd-cve, cursor-y = riga
   
                    perform VALORE-RIGA
   
                    if mod = 1 
                       if nuovo or (riga - 1) > RowCounter
                          move 2 to colonna
                          modify gd-cve, cursor-x = colonna
                          perform CONTROLLO

                          move 3 to colonna
                          modify gd-cve, cursor-x = colonna
                          perform CONTROLLO

                          if not trovato 
                             set errori to true 
                          end-if
                       end-if
                    end-if
   
                    if tutto-ok
                       perform VALORIZZA-DATI-COMUNI
                       write aeg-rec 
                          invalid 
                             rewrite aeg-rec
                       end-write
                       set sisalvato to true
                       set vecchio   to true
                    else
                       move 3 to control-id
                       move 4 to accept-control
                       modify gd-cve, cursor-y = riga, cursor-x = 
           colonna
                       move riga to MyEventData2
                       perform COLORE
                       exit perform
                    end-if
                 end-perform                
                 modify gd-cve, mass-update = 0
                         
              end-if
           end-if.                    
           perform ABILITAZIONI              
           .
      * <TOTEM:END>

       SPOSTAMENTO.
      * <TOTEM:PARA. SPOSTAMENTO>
           inquire gd-cve, cursor-x = colonna, cursor-y = riga.

           if event-data-2 not = riga
              perform VALORE-RIGA
      *        if aeg-imballo = spaces or zero
      *           modify gd-cve, record-to-delete riga
      *           set vecchio to true
      *        else  
                 perform SALVA
                 if errori
                    move riga    to event-data-2
                    move colonna to event-data-1 | (isacco)
                    set event-action to event-action-fail
                 end-if
      *        end-if
           else
              if colonna not = event-data-1
                 perform CONTROLLO
                 if errori
                    set event-action to event-action-fail
                 end-if
              end-if
           end-if.

      *    valorizzo l'old della riga su cui sono andato
           move event-data-2 to riga.
           move event-data-1 to colonna.
           perform VALORE-OLD-RIGA.

      * colorazione riga in grid
           perform COLORE-RIGA.

           perform GRID-HELP 
           .
      * <TOTEM:END>

       STAMPA.
      * <TOTEM:PARA. STAMPA>
           set stampa       to true.
      *     perform paragrafo-stampa 
           .
      * <TOTEM:END>

       VALORE-RIGA.
      * <TOTEM:PARA. VALORE-RIGA>
           perform X-Y.
           inquire gd-cve(riga), RECORD-DATA rec-grid
           move col-imballo    to aeg-imballo
           .
      * <TOTEM:END>

       VALORE-OLD-RIGA.
      * <TOTEM:PARA. VALORE-OLD-RIGA>
           if vecchio
              inquire gd-cve(riga, 1) cell-data old-aeg-imballo
              move old-aeg-imballo  to aeg-imballo
              read art-exp-get no lock
              move aeg-rec         to old-aeg-rec
           else
              initialize old-aeg-rec replacing numeric data by zeroes
                                     alphanumeric data by spaces
           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-DATI-COMUNI.
      * <TOTEM:PARA. VALORIZZA-DATI-COMUNI>
      *    se non ho valorizzato i campi di creazione li valorizzo
           if aeg-data-creazione = zero
              accept aeg-data-creazione from century-date
           end-if.
           if aeg-ora-creazione = zero
              accept aeg-ora-creazione from time
           end-if.
           if aeg-utente-creazione = space
              move user-codi to aeg-utente-creazione
           end-if.

           accept aeg-data-ultima-modifica from century-date.
           accept aeg-ora-ultima-modifica from time.
           move user-codi to aeg-utente-ultima-modifica 
           .
      * <TOTEM:END>

       PRIMO.
      * <TOTEM:PARA. PRIMO>
           perform SALV-MOD.           
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock art-exp-get all records
              modify control-handle, color = colore-or 
              move low-value to aeg-chiave

              set ReadSecca to false
              move 0 to MyEventData2
              perform CURRENT-RECORD

              if errori
                 display message box "File [art-exp-get]"
                              x"0d0a""Archivio vuoto"
                         title = titolo
              end-if       

              perform DISPLAY-CAMPI-CHIAVE       
     
              move 1 to control-id
              move 4 to accept-control 
              perform VALORIZZA-OLD
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if                
           end-if 
           .
      * <TOTEM:END>

       PRECEDENTE.
      * <TOTEM:PARA. PRECEDENTE>
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock art-exp-get all records 
              modify control-handle, color = colore-or 
              perform RIEMPI-CHIAVE
              move low-value to aeg-imballo

              start art-exp-get key is < aeg-chiave
                 invalid 
                    set errori to true
                 not invalid 
                    read art-exp-get previous no lock
                       at end 
                          set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move low-value to aeg-imballo
                 set ReadSecca to false
                 move 0 to MyEventData2
                 perform CURRENT-RECORD
              end-if

              if errori or not trovato
                 display message box "File [art-exp-get]"
                              x"0d0a""Raggiunto inizio file"
                         title = titolo
              end-if                  

              perform DISPLAY-CAMPI-CHIAVE       
     
              move 78-id-ef-art to control-id
              move 4            to accept-control 
              perform VALORIZZA-OLD 
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if                 
           end-if 
           .
      * <TOTEM:END>

       SUCCESSIVO.
      * <TOTEM:PARA. SUCCESSIVO>
           perform SALV-MOD.           
           if tutto-ok            
              perform CHECK-INSERIMENTO
              unlock art-exp-get all records 
              modify control-handle, color = colore-or 
              perform RIEMPI-CHIAVE
              move high-value to aeg-imballo
                                         
              set ReadSecca to false
              move 0 to MyEventData2
              perform CURRENT-RECORD

              if errori or not trovato
                 display message box "File [art-exp-get]"
                              x"0d0a""Raggiunta fine file"
                         title = titolo
              end-if                  

              perform DISPLAY-CAMPI-CHIAVE  
     
              move 1 to control-id
              move 4 to accept-control 
              perform VALORIZZA-OLD
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if                
           end-if 
           .
      * <TOTEM:END>

       ULTIMO.
      * <TOTEM:PARA. ULTIMO>
           perform SALV-MOD.           
           if tutto-ok                 
              perform CHECK-INSERIMENTO
              unlock art-exp-get all records 
              modify control-handle, color = colore-or 
              move high-value to aeg-chiave
              start art-exp-get key is < aeg-chiave
                 invalid 
                    set errori to true
                 not invalid 
                    read art-exp-get previous no lock
                       at end 
                          set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move low-value to aeg-imballo
                 set ReadSecca to false
                 move 0 to MyEventData2
                 perform CURRENT-RECORD
              end-if

              if errori
                 display message box "File [tcauven]"
                              x"0d0a""Archivio vuoto"
                         title = titolo
              end-if         

              perform DISPLAY-CAMPI-CHIAVE
     
              move 78-id-ef-art to control-id
              move 4            to accept-control


              perform VALORIZZA-OLD
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if               

           end-if 
           .
      * <TOTEM:END>

       CERCA-TESTO.
      * <TOTEM:PARA. CERCA-TESTO>
           perform SCREEN-SEARCH-OPEN-ROUTINE.

           if ricerca
              move 0 to ResultSearch

              inquire gd-cve, cursor-y in riga

              inquire gd-cve, search-options in grid-search-options
              
              |direzione di ricerca
              set GRID-SEARCH-FORWARDS to true
              set GRID-SEARCH-WRAP     to true

              |Maiuscolo/minuscolo
              if MatchCase = 0
                 set GRID-SEARCH-IGNORE-CASE to true
              else                                                      
             
                 set GRID-SEARCH-IGNORE-CASE to false
              end-if

              |trova la parola nella cella
              set GRID-SEARCH-MATCH-ANY to true

              |ricerca nelle sole celle visible 
              |ignorando gli eventuali hidden-data
              set GRID-SEARCH-VISIBLE to true

              |non considerare quello su cui sono posizionato
              set GRID-SEARCH-SKIP-CURRENT   to true

              |si posiziona sulla cella trovata
              set GRID-SEARCH-MOVES-CURSOR   to true 

              |num colonna sulla quale fare la ricerca
              move tipo-ricerca to GRID-SEARCH-COLUMN 

              modify gd-cve, search-options GRID-SEARCH-OPTIONS

              modify gd-cve(riga, tipo-ricerca), 
                     search-text TextToSearch |testo da ricercare
                          giving ResultSearch |1, 2 = trovato 0 = non trovato

              if ResultSearch = GRDSRCH-NOT-FOUND
                 display message box MSG-Testo-non-trovato
                         title = titolo
              else
                 inquire gd-cve, cursor-y in riga
                 move riga to event-data-2
                 perform SPOSTAMENTO
              end-if
           end-if 
           .
      * <TOTEM:END>

       X-Y.
      * <TOTEM:PARA. X-Y>
           inquire gd-cve, cursor-y in riga, cursor-x in colonna
                            last-row in totale-righe
                            num-rows in NumRows 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE.
      * <TOTEM:PARA. RIEMPI-CHIAVE>
           inquire ef-art, value in aeg-articolo 
           .
      * <TOTEM:END>

       COLORE.
      * <TOTEM:PARA. COLORE>
           modify gd-cve, start-y = MyEventData2, y = MyEventData2,
                          start-x = 1,            x = 3,
                          region-color 257,
                          cursor-color 481 
           .
      * <TOTEM:END>

       INIT-OLD-REC.
      * <TOTEM:PARA. INIT-OLD-REC>
           initialize old-aeg-rec replacing numeric data by zeroes
                                      alphabetic data by spaces 
           .
      * <TOTEM:END>

       CLEAR-SCREEN.
      * <TOTEM:PARA. CLEAR-SCREEN>
           move aeg-articolo to old-aeg-articolo.

           initialize aeg-rec replacing numeric data by zeroes
                              alphabetic data by spaces.
                        
           move old-aeg-articolo to aeg-articolo.
           
           move spaces to des-art.
           display lbl-des-art.

           perform RESET-GRIGLIA.

           perform INIT-OLD-REC  
           .
      * <TOTEM:END>

       RESET-GRIGLIA.
      * <TOTEM:PARA. RESET-GRIGLIA>
           modify gd-cve, reset-grid = 1.
           perform INTESTAZIONE 
           .
      * <TOTEM:END>

       CURRENT-RECORD.
      * <TOTEM:PARA. CURRENT-RECORD>
           move -1 to Memo-art.
           if ReadSecca
              perform RIEMPI-CHIAVE
              move aeg-articolo    to Memo-art
              move low-value to aeg-imballo
           end-if.
           set tutto-ok  to true.
           set trovato   to false. 
           
           start art-exp-get key not less aeg-chiave
              invalid 
                 set errori to true 
           end-start.

           if nuovo
              move aeg-articolo  to Memo-art
           end-if.

           if tutto-ok
              modify gd-cve, mass-update = 1
              perform RESET-GRIGLIA
              move 0 to RowCounter
              move 2 to riga
              
              perform until 1 = 2
                 read art-exp-get next no lock 
                    at end 
                       exit perform 
                 end-read

                 if Memo-art not > -1
                    move aeg-articolo    to  Memo-art
                 end-if                  

                 if aeg-articolo    not = Memo-art
                    move Memo-art to aeg-articolo
                    exit perform
                 end-if

                 set trovato to true 

                 if nuovo
                    move 1 to mod
                    move 0 to mod-k
                    move 78-id-gd-cve   to control-id
                    move 4               to accept-control
                 end-if  

                 set vecchio to true

                 perform METTI-IN-GRIGLIA
                 add 1 to riga
                 add 1 to RowCounter
                 if RowCounter = 1
                    move spaces      to des-art
                    move Memo-art to ef-art-buf
                    display ef-art lbl-des-art
                 end-if
              end-perform          
             
              modify gd-cve, mass-update = 0

           end-if.

           if trovato   
              perform VALORIZZA-OLD
              if mod = 1
                 set  StatusModifica to true
                 perform LOCK-RECORD        
              else
                 set StatusVisua    to true
              end-if
              perform STATUS-BAR-MSG
           end-if.

           if not trovato and ReadSecca
              if vecchio
                 perform CLEAR-SCREEN
                 if YesMessage          
                    display message box "Record inesistente!"
                            title = tit-err
                            icon mb-warning-icon
                 end-if
                 move 0 to mod
                 move 78-id-ef-art   to control-id
                 move 4 to accept-control
              end-if
           end-if.             

           if MyEventData2 = 0
              move 2 to riga MyEventData2
           end-if.
           set ReadSecca to false.
           perform ABILITAZIONI.

      ***---
       METTI-IN-GRIGLIA.
           move aeg-imballo  to col-imballo
                                imq-codice

           read timbalqta
              invalid
                 initialize imq-codice
           end-read
           modify gd-cve(riga) record-data = rec-grid
           perform DESCRIZIONE-IMBALLO
           .
      * <TOTEM:END>

       DISPLAY-CAMPI-CHIAVE.
      * <TOTEM:PARA. DISPLAY-CAMPI-CHIAVE>
           move aeg-articolo  to ef-art-buf.
           display ef-art.
           
           move 78-id-ef-art   to control-id.
           perform CONTROLLO 
           .
      * <TOTEM:END>

       DEL-RIGA.
      * <TOTEM:PARA. DEL-RIGA>
           modify gd-cve, record-to-delete = riga.
           move 0 to riga-nuova 
           .
      * <TOTEM:END>

       CHECK-INSERIMENTO.
      * <TOTEM:PARA. CHECK-INSERIMENTO>
      * modifica la status-bar
      * controllo se da inserimento scorro. allora devo segnalare che
      * sono tornato in modifica.
           inquire tool-modifica, value in mod.

           if mod = 1 then 
              set StatusModifica to true
           end-if.

           if mod = 0 then
              set StatusVisua to true   
           end-if.                
      
           perform STATUS-BAR-MSG 
           .
      * <TOTEM:END>

       VALORIZZA-OLD.
      * <TOTEM:PARA. VALORIZZA-OLD>
           move aeg-rec to old-aeg-rec.
           set vecchio to true.
           move 0 to riga-nuova 
           .
      * <TOTEM:END>

       LOCK-RECORD.
      * <TOTEM:PARA. LOCK-RECORD>
           set RecLocked  to false.
           move Memo-art  to aeg-articolo.
           read art-exp-get with lock 
              invalid 
                 continue 
           end-read.
           if RecLocked      
              set StatusVisua to true
           end-if 
           .
      * <TOTEM:END>

       CONTROLLO-OLD.
      * <TOTEM:PARA. CONTROLLO-OLD>
           set SiSalvato to true.
      *     if mod = 0 exit paragraph end-if.
      *     perform X-Y.
      *     perform varying riga from 2 by 1
      *               until riga > totale-righe
      *        inquire gd-cve(riga, 1), cell-data in col-imballo
      *        perform VALORE-OLD-RIGA
      *        if col-imballo not = old-aeg-imballo
      *           move riga to store-riga
      *           set NoSalvato        to true
      *           move 78-id-gd-cve   to store-id
      *           exit perform
      *        end-if
      *     end-perform 
           .
      * <TOTEM:END>

       RIEMPI-GRIGLIA-cve.
      * <TOTEM:PARA. RIEMPI-GRIGLIA-cve>
      *     if old-cve-vet   not = cve-vet
              set ReadSecca to true
              perform CURRENT-RECORD
              |set YesZoom to false
              move zero to StatusHelp
              if not trovato and nuovo
                 perform X-Y
                 if totale-righe = 1
                    move 1               to mod             
                    modify gd-cve, enabled = mod
                    initialize rec-grid
                    modify gd-cve, insert-rows = 1
                    move 1               to riga-nuova
                    move 78-id-gd-cve   to control-id
                    move 2               to MyEventData2
                    perform COLORE      
                    move 2               to riga
                    move 1               to colonna
                    move 1               to StatusHelp
                 end-if
              end-if
              perform DISPLAY-CAMPI-CHIAVE
      *     end-if.
           perform STATUS-HELP 
           .
      * <TOTEM:END>

       COLORE-RIGA.
      * <TOTEM:PARA. COLORE-RIGA>
           if riga < 2 
              move 2 to riga 
           end-if.

           modify gd-cve, start-x = 1, x     = 3,
                                  start-y = riga,
                                        y = riga,
                                  region-color 257,
                                  cursor-color 481 
           .
      * <TOTEM:END>

       DESCRIZIONE-IMBALLO.
      * <TOTEM:PARA. DESCRIZIONE-IMBALLO>
           move imq-tipo  to imb-codice
           read timballi
              invalid 
                 initialize imb-dati
           end-read.

           initialize col-descr


           inspect imb-descrizione replacing trailing spaces 
                                   by low-value.
           move imq-qta-imb to qta-x.
           call "C$JUSTIFY" using qta-x, "L".
           inspect qta-x replacing trailing spaces by low-value.
           string imb-descrizione delimited by low-value
                  " da "          delimited by size
                  qta-x           delimited by low-value
                  into col-descr
           end-string.

           modify gd-cve(riga, 2) cell-data = col-descr
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       NUOVO-LinkTo.
      * <TOTEM:PARA. NUOVO-LinkTo>
           inquire tool-nuovo, enabled in e-nuovo.
           if e-nuovo = 1
              perform NUOVO
           end-if 

           .
      * <TOTEM:END>
       CANCELLA-LinkTo.
      * <TOTEM:PARA. CANCELLA-LinkTo>
           inquire tool-cancella, enabled in e-cancella.
           if e-cancella = 1
              perform CANCELLA
           end-if 
           .
      * <TOTEM:END>
       TOOL-SALVA-LinkTo.
      * <TOTEM:PARA. TOOL-SALVA-LinkTo>
           inquire tool-salva, enabled in e-salva.
           if e-salva = 1
              perform salva
           end-if 
           .
      * <TOTEM:END>
       ANTEPRIMA-LinkTo.
      * <TOTEM:PARA. ANTEPRIMA-LinkTo>
           inquire tool-anteprima, enabled in e-anteprima.
           if e-anteprima = 1
              perform ANTEPRIMA
           end-if 
           .
      * <TOTEM:END>
       TOOL-MODIFICA-LinkTo.
      * <TOTEM:PARA. TOOL-MODIFICA-LinkTo>
           inquire tool-modifica, enabled in e-modifica
           if e-modifica = 1
              perform MODIFICA
           end-if 
           .
      * <TOTEM:END>
       STAMPA-LinkTo.
      * <TOTEM:PARA. STAMPA-LinkTo>
           inquire tool-stampa, enabled in e-stampa
           if e-stampa = 1
              perform STAMPA
           end-if 
           .
      * <TOTEM:END>
       I-O-BLOCCO.
      * <TOTEM:PARA. I-O-BLOCCO>
           SET LK-BL-CANCELLAZIONE TO TRUE.
           MOVE COMO-PROG-ID       TO LK-BL-PROG-ID.
           CALL "BLOCKPGM"  USING LK-BLOCKPGM 
           .
      * <TOTEM:END>
       PAS004P-Ev-Before-Program.
      * <TOTEM:PARA. PAS004P-Ev-Before-Program>
           move LK-BL-PROG-ID    TO COMO-PROG-ID.
           move "GESLUX - Tabella Articoli inviati a GET" to titolo
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Finish-Entry.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Finish-Entry>
           inquire gd-cve, cursor-y in riga.
           perform VALORE-RIGA.

           perform  CONTROLLO.

           if errori 
              set event-action to event-action-fail 
              perform VALORE-RIGA
           end-if.

           if aeg-rec not = old-aeg-rec
              set NoSalvato to true                            
           else 
              set SiSalvato to true
           end-if 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Begin-Entry.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Begin-Entry>
           inquire gd-cve, cursor-x = colonna,
                                   cursor-y in riga.
                         
           perform VALORE-RIGA.

           evaluate colonna
           when 1
                |if aeg-imballo not = spaces
                if not nuovo
                   set event-action to event-action-fail
                end-if
           when 2
                if mod = zero              
                   set event-action to event-action-fail
                end-if
           when 3
      *          if mod = zero
                   set event-action to event-action-fail
      *          else
      *             if aeg-imballo = spaces
      *                set event-action to event-action-fail
      *                modify gd-cve, cursor-y = riga, cursor-x = 1
      *             end-if   
      *          end-if
           end-evaluate 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-End-Drag.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-End-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Goto-Cell.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Goto-Cell>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Goto-Cell-Drag.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Goto-Cell-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Goto-Cell-Mouse.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Goto-Cell-Mouse>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Begin-Drag.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Begin-Drag>
