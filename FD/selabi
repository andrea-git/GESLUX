    SELECT OPTIONAL ABI ASSIGN RANDOM SELABI
    COMPRESSION 90
    ORGANIZATION INDEXED ACCESS DYNAMIC RECORD ABI-CODICE
    ALTERNATE RECORD ABI-CODICE1 = ABI-BANCA1 ABI-CITTA ABI-BANCA2 ABI-CODICE
    ALTERNATE RECORD ABI-CODICE2 = ABI-CITTA ABI-BANCA1 ABI-BANCA2 ABI-CODICE
    FILE STATUS IS STATO.
