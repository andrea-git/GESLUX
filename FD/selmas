       SELECT OPTIONAL MAS ASSIGN RANDOM "MAS"
           COMPRESSION 90
           ORGANIZATION INDEXED ACCESS DYNAMIC RECORD MAS-CODICE
           ALTERNATE RECORD MAS-CODICE1 = MAS-CODICE-ALTERNATIVO MAS-CODICE
           ALTERNATE RECORD MAS-CODICE2 = MAS-DESCRIZIONE1 MAS-CODICE
           FILE STATUS IS STATO.
