BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = scr-ordfor
   ACTOBJ            = scr-ordfor
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           close coperfab.

           move 0 to primo-numero ultimo-numero.

           call   "crea-ordfor" using scr-ordfor-handle, 
                                      primo-numero, 
                                      ultimo-numero,
                                      user-codi.
           cancel "crea-ordfor".

           if primo-numero = 0
              display message "Nessun movimento generato!"
                        title titolo
                         icon 2
           else
              compute diff = ultimo-numero - primo-numero + 1
              move primo-numero  to primo-numero-ed
              move ultimo-numero to ultimo-numero-ed
              display message "GENERAZIONE TERMINATA!!!"
                       x"0d0a""Creati " diff, " ordini a fornitori"
                       x"0d0a""Dal n. " primo-numero-ed
                       x"0d0a""al n.  " ultimo-numero-ed
                        title titolo
                         icon 2
           end-if.
           open i-o coperfab.
           move 27 to key-status.
END-TEXT
