BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = AGGIORNA-DATA-RICALCOLO-TPARAMGE
BEGIN-TEXT
           close    tparamge.
           open i-o tparamge.
           move spaces to tge-chiave.
           perform until 1 = 2
              set RecLocked to false
              initialize geslock-linkage
              move "tparamge" to geslock-nome-file

              set tutto-ok  to true
              read tparamge lock invalid continue end-read

              if RecLocked
                 string "Tabella parametri generali"
                 x"0d0a""in uso su altro terminale."
                 x"0d0a""Aggiornamento data ricalcolo impossibile." 
                       delimited size
                       into geslock-messaggio
                 end-string
                 move 1 to geslock-v-riprova
                 move 1 to geslock-v-ignora
                 move 0 to geslock-v-termina
                 call   "geslock" using geslock-linkage
                 cancel "geslock"
                 evaluate true
                 when riprova continue
                 when ignora  set errori to true
                              exit perform
                 end-evaluate
              else
                 exit perform
              end-if
           end-perform.

           if tutto-ok
              accept tge-data-ricalcolo       from century-date
              accept tge-data-ultima-modifica from century-date
              accept tge-ora-ultima-modifica  from time
              move user-codi to tge-utente-ultima-modifica
              rewrite tge-rec
              unlock tparamge all records
              move tge-data-ricalcolo to como-data
              perform DATE-TO-SCREEN
              move como-data to lab-data-r-buf
              display lab-data-r
           end-if.
           close      tparamge.
           open input tparamge.
