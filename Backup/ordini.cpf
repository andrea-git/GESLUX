BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Goto-Cell-Mouse
BEGIN-TEXT
           if event-data-1 = 36
              inquire form1-gd-1(event-data-2, 1), hidden-data in gruppo-hidden
              move hid-chiave to ord2-chiave
              read ordfor2 no lock
                   invalid
                   display message "Chiave non trovata" ord2-chiave
                            x"0d0a""Contattare assistenza"
                             title tit-err
                              icon 3
               not invalid
                   if ord2-si-conferma
                      set ord2-no-conferma to true
                      rewrite ord2-rec
                      modify form1-gd-1(event-data-2, 36), 
                             bitmap-number = 1
                             bitmap spunta-ordfor-bmp
                             bitmap-width 30
                   else
                      inquire form1-gd-1(event-data-2, 37), 
                              cell-data in giacenza
                      if giacenza > 0
                         set ord2-si-conferma to true
                         rewrite ord2-rec
                         modify form1-gd-1(event-data-2, 36), 
                                bitmap-number = 2
                                bitmap spunta-ordfor-bmp
                                bitmap-width 30
                      end-if
                   end-if
              end-read
           end-if.
           perform SPOSTAMENTO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Finish-Entry
BEGIN-TEXT
           inquire form1-gd-1(event-data-2, 1), 
                   hidden-data in gruppo-hidden.

           move hid-chiave to ord2-chiave.
           read ordfor2 no lock 
                invalid 
                display message "Chiave non trovata" ord2-chiave
                         x"0d0a""Contattare assistenza"
                          title tit-err
                           icon 3
           end-read.

           inquire form1-gd-1(event-data-2, 78-col-imp), 
                   cell-data in col-impegnato.

           modify form1-gd-1(event-data-2, 78-col-imp), 
                   cell-data col-impegnato.

           move col-impegnato to ord2-impegnato.
           rewrite ord2-rec 
                   invalid
                   display message "Chiave non trovata" ord2-chiave
                            x"0d0a""Contattare assistenza"
                             title tit-err
                              icon 3
           end-rewrite.
           move event-data-2 to riga.
           perform CALCOLA-FABBISOGNO.
                 
           |Se vario l'impegnato non devo ricoprire il fabbisogno
           |ma dovr farlo dalla funzione di associazione fornitori
     
      *****     if path-tmp-forn = spaces
      *****        perform OPEN-OUTPUT-TMP-FORN
      *****     else
      *****        close       tmp-forn
      *****        open output tmp-forn
      *****        close       tmp-forn
      *****        open i-o    tmp-forn
      *****     end-if.
      *****     perform COPRI-FABBISOGNO.

           perform VALUTA-COPERTURA.

           move ord2-fabb-qta(1) to col-F1.
           compute giacenza = ord2-fabb-qta(1) / ord2-qta-imb.
           move giacenza         to col-F1-imb.
           move ord2-fabb-qta(2) to col-F2.
           move ord2-fabb-qta(3) to col-F3.
           move ord2-fabb-qta(4) to col-F4.
           move ord2-fabb-qta(5) to col-F5.
           move ord2-fabb-qta(6) to col-F6.
           rewrite ord2-rec 
                   invalid
                   display message "Chiave non trovata" ord2-chiave
                            x"0d0a""Contattare assistenza"
                             title tit-err
                              icon 3
           end-rewrite.
           modify form1-gd-1(event-data-2, 28), cell-data col-F1-imb.
           modify form1-gd-1(event-data-2, 29), cell-data col-F1.
           modify form1-gd-1(event-data-2, 30), cell-data col-F2.
           modify form1-gd-1(event-data-2, 31), cell-data col-F3.
           modify form1-gd-1(event-data-2, 32), cell-data col-F4.
           modify form1-gd-1(event-data-2, 33), cell-data col-F5.
           modify form1-gd-1(event-data-2, 34), cell-data col-F6.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-ricalcolo-LinkTo
BEGIN-TEXT
           display message "Ricalcolare il file partendo da ordfor?"
                     title titolo
                      icon 2
                      type mb-yes-no
                    giving scelta
           if scelta = mb-yes
              move 1 to chk-parziale-buf
              move 1 to chk-nessuna-buf
              move 1 to chk-forn-buf
              move 1 to chk-singolo-buf
              display chk-parziale
                      chk-nessuna
                      chk-forn
                      chk-singolo

              set tutto-ok  to true
              set ricalcolo to true
              close ordfor2
              perform CREA-ORDFOR2
              if errori
                 exit paragraph
              end-if
              close ordfor2
              open exclusive i-o ordfor2
              perform SCR-ELAB-OPEN-ROUTINE
              set ricalcolo to false
              modify pb-ricalcolo, bitmap-number 1
              move 0 to filtri
              modify chk-filtri, value filtri, bitmap-number = 2

              perform AGGIORNA-DATA-RICALCOLO-TPARAMGE
           end-if.
END-TEXT
END-DATA
