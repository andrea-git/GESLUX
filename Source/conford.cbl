       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok   to true
           evaluate control-id
           when 78-ID-ef-indirizzo
                if ef-indirizzo-buf = spaces
                   set errori to true
                   display message "Indirizzo E-mail obbligatorio"
                          title titolo
                   set errori   to true
                end-if  

           when 78-ID-ef-oggetto
                if ef-oggetto-buf = spaces
                   set errori to true
                   display message "Oggetto E-mail obbligatorio"
                          title titolo
                   set errori   to true
                end-if  

           when 78-ID-ef-mail
                if ef-mail-buf = spaces
                   set errori to true
                   display message "Testo E-mail obbligatorio"
                          title titolo
                   set errori   to true
                end-if

           end-evaluate

           if errori
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

       INVIO.
      * <TOTEM:PARA. INVIO>
           perform FORM3-OPEN-ROUTINE       
           .
      * <TOTEM:END>

       INVIO-MAIL.
      * <TOTEM:PARA. INVIO-MAIL>
           move ef-oggetto-buf   to LinkSubject.
           move ef-indirizzo-buf to LinkAddress.
           move ef-cc-buf        to LinkAddressCC.
           move ef-mail-buf      to LinkBody.    
           move link-path        to LinkAttach.
           
           set errori to true.
           move 0 to tentativi.
           move "conford" to NomeProgramma.
           perform 5 times
              add 1 to tentativi
              perform SEND-MAIL

      *        call "C$DELETE" using FileDest
              open input lineseq1
              read  lineseq1 next
              if line-riga of lineseq1 = "True"
                 set tutto-ok to true
                 close lineseq1
                 exit perform
              end-if
              close lineseq1

           end-perform 

           .
      * <TOTEM:END>

       PREPARA-FROM.
      * <TOTEM:PARA. PREPARA-FROM>
           move conf-user-codi to usr-codice
           read usr-tel
              invalid
                 initialize usr-ind-from
           end-read.      
           move usr-ind-from to LinkAddressFrom 
           .
      * <TOTEM:END>

       PREPARA-INDIRIZZO.
      * <TOTEM:PARA. PREPARA-INDIRIZZO>
           move conf-email to ef-indirizzo-buf LinkAddress 
           .
      * <TOTEM:END>

       PREPARA-CC.
      * <TOTEM:PARA. PREPARA-CC>
           if PgmChiamante = "ordinevar"
              accept ef-cc-buf  from environment "CONFERMA_CC"
           else
              accept ef-cc-buf  from environment "CONFERMA_EVASIONE_CC"
           end-if.

           inspect conf-email-agente replacing trailing space by 
           low-value.
           inspect ef-cc-buf replacing trailing space by low-value.
           string  ef-cc-buf          delimited low-value
                   conf-email-agente  delimited low-value
             into ef-cc-buf
           end-string.
           inspect ef-cc-buf replacing trailing low-value by space.
           move ef-cc-buf to LinkAddressCC 
           .
      * <TOTEM:END>

       PREPARA-SUBJECT.
      * <TOTEM:PARA. PREPARA-SUBJECT>
           move conf-numero to numero-x.
           inspect numero-x replacing leading x"30" by x"20".
           call "C$JUSTIFY" using numero-x, "L".
           inspect numero-x replacing trailing spaces by low-value.
           inspect conf-ragsoc replacing trailing spaces by low-value.
           if PgmChiamante = "ordinevar"
              accept  LinkSubject from environment "CONFERMA_SUBJECT"
              inspect LinkSubject replacing trailing spaces by low-value
              string conf-ragsoc     delimited low-value
                     " - "           delimited size
                     LinkSubject     delimited low-value     
                     " - N. MASTER " delimited size
                     numero-x        delimited size
                into  ef-oggetto-buf
             end-string
           else                                    
              accept  LinkSubject from environment "CONFERMA_EVASIONE_SU
      -    "BJECT"
              inspect LinkSubject replacing trailing spaces by low-value
              string conf-ragsoc       delimited low-value
                     " - "             delimited size
                     LinkSubject       delimited low-value
                     " - N. EVASIONE " delimited size
                     numero-x          delimited size
                into ef-oggetto-buf
              end-string
           end-if.

           move   ef-oggetto-buf to LinkSubject 
           .
      * <TOTEM:END>

