       Screen1-Event-Proc.
           .

      * USER DEFINE PARAGRAPH
       ABILITAZIONI.
      * <TOTEM:PARA. ABILITAZIONI>
           if mod = 1                                 
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella e-nuovo
           else
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move 0 to e-salva e-cancella
           end-if.

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva    
           .
      * <TOTEM:END>

       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = 0 exit paragraph end-if.

           display message box MSG-Cancellare-il-record-corrente
                   title   titolo
                   type    mb-yes-no 
                   default mb-no
                   giving  scelta
      
           if scelta = mb-yes
              move LinkCodice to notf-codice
              move 0          to notf-prog
              
              initialize usage-campi replacing numeric data by zeroes
                                          alphanumeric data by spaces

              perform FORM1-FLD-TO-BUF
              display form1
      
              set vecchio to true       
              perform CANCELLA-COLORE
                            
              display message MSG-Cancellazione-avvenuta-con-successo
                        title titolo
                                    
              move 78-ID-ef-codice to control-id
              move 4 to accept-control
              set Statusvisua to true
           end-if 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           evaluate control-id
           when 78-ID-ef-data
                inquire ef-note-1, value in ef-note-1-buf
                inquire ef-data,   value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-buf
                   display ef-data
                else
                   if ef-note-1-buf not = spaces
                      perform DATE-FORMAT
                      move como-data to ef-data-buf
                      display ef-data
                   end-if
                end-if
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO-OLD.
      * <TOTEM:PARA. CONTROLLO-OLD>
           set SiSalvato to true.
           if mod = 0 exit paragraph end-if.
           perform FORM1-BUF-TO-FLD.
           move 0 to scelta.

           if usage-note-1 not = LinkNote-1 and SiSalvato
              set NoSalvato to true
              move 4 to accept-control
           end-if.               
                        
           if usage-data not = LinkData and SiSalvato
              set NoSalvato to true
              move 4 to accept-control
           end-if.               

           if usage-note-2 not = LinkNote-2 and SiSalvato
              set NoSalvato to true
              move 4 to accept-control
           end-if.

           if usage-note-3 not = LinkNote-3 and SiSalvato
              set NoSalvato to true
              move 4 to accept-control
           end-if.

           if usage-note-4 not = LinkNote-4 and SiSalvato
              set NoSalvato to true
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

       NUOVO.
      * <TOTEM:PARA. NUOVO>
           perform FORM1-CLEAR  
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "color-custom.cpy".
           copy "utydata.cpy" 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           perform CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel 
                            giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other
                   set errori to true
                   perform CANCELLA-COLORE
                   move 4 to accept-control
              end-evaluate
           end-if 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           move 78-ID-ef-data to control-id.
           perform CONTROLLO.               
           perform FORM1-BUF-TO-FLD.
           move usage-campi to LinkCampi.
           perform CANCELLA-COLORE.
           move 78-ID-ef-note-1 to control-id.
           move 4 to accept-control 
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       CANCELLA-LinkTo.
      * <TOTEM:PARA. CANCELLA-LinkTo>
           inquire tool-cancella enabled e-cancella.
           if e-cancella = 1
              perform CANCELLA
           end-if 
           .
      * <TOTEM:END>
       TOOL-SALVA-LinkTo.
      * <TOTEM:PARA. TOOL-SALVA-LinkTo>
           inquire tool-salva enabled e-salva.
           if e-salva = 1
              perform SALVA
           end-if 
           .
      * <TOTEM:END>
