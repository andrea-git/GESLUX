       DataSet1-notef-START.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-notef-KEY1-Asc
                 START notef KEY >= notf-chiave OF notef
              ELSE
                 START notef KEY <= notf-chiave OF notef
              END-IF
           END-EVALUATE
           .

       DataSet1-notef-START-NOTGREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-notef-KEY1-Asc
                 START notef KEY <= notf-chiave OF notef
              ELSE
                 START notef KEY >= notf-chiave OF notef
              END-IF
           END-EVALUATE
           .

       DataSet1-notef-START-GREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-notef-KEY1-Asc
                 START notef KEY > notf-chiave OF notef
              ELSE
                 START notef KEY < notf-chiave OF notef
              END-IF
           END-EVALUATE
           .

       DataSet1-notef-START-LESS.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-notef-KEY1-Asc
                 START notef KEY < notf-chiave OF notef
              ELSE
                 START notef KEY > notf-chiave OF notef
              END-IF
           END-EVALUATE
           .

       DataSet1-notef-Read.
      * <TOTEM:EPT. FD:DataSet1, FD:notef, BeforeRead>
      * <TOTEM:END>
      * <TOTEM:EPT. FD:DataSet1, FD:notef, BeforeReadRecord>
      * <TOTEM:END>
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-notef-LOCK
                 READ notef WITH LOCK 
                 KEY notf-chiave OF notef
              ELSE
                 READ notef WITH NO LOCK 
                 KEY notf-chiave OF notef
              END-IF
           END-EVALUATE
           MOVE STATUS-notef TO TOTEM-ERR-STAT 
           MOVE "notef" TO TOTEM-ERR-FILE
           MOVE "READ" TO TOTEM-ERR-MODE
      * <TOTEM:EPT. FD:DataSet1, FD:notef, AfterRead>
      * <TOTEM:END>
      * <TOTEM:EPT. FD:DataSet1, FD:notef, AfterReadRecord>
      * <TOTEM:END>
