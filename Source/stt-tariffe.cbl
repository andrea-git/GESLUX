      * tvettori
       DataSet1-tvettori-START.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tvettori-KEY1-Asc
                 START tvettori KEY >= vet-chiave
              ELSE
                 START tvettori KEY <= vet-chiave
              END-IF
           WHEN 2
              IF DataSet1-tvettori-KEY2-Asc
                 START tvettori KEY >= k-des
              ELSE
                 START tvettori KEY <= k-des
              END-IF
           END-EVALUATE
           .

       DataSet1-tvettori-START-NOTGREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tvettori-KEY1-Asc
                 START tvettori KEY <= vet-chiave
              ELSE
                 START tvettori KEY >= vet-chiave
              END-IF
           WHEN 2
              IF DataSet1-tvettori-KEY2-Asc
                 START tvettori KEY <= k-des
              ELSE
                 START tvettori KEY >= k-des
              END-IF
           END-EVALUATE
           .

       DataSet1-tvettori-START-GREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tvettori-KEY1-Asc
                 START tvettori KEY > vet-chiave
              ELSE
                 START tvettori KEY < vet-chiave
              END-IF
           WHEN 2
              IF DataSet1-tvettori-KEY2-Asc
                 START tvettori KEY > k-des
