       Screen4-Event-Proc.
           .

      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-cli
                set cli-tipo-C to true
                move "clienti-alfa-all" to como-file
                call "zoom-gt" using como-file, cli-rec
                              giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move cli-codice   to ef-cli-buf
                   move cli-ragsoc-1 to lab-cli-buf
                   display ef-cli lab-cli
                end-if

           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-ID-ef-data
                if e-data = 1
                   inquire ef-data, value in ef-data-buf
                   move ef-data-buf to como-data
                   perform DATE-FORMAT
                   move como-data to ef-data-buf ef-data-to-buf
                   display ef-data ef-data-to

                   |CALCOLO IL PERIODO
                   perform DATE-TO-FILE

                   move como-data to como-data-from
                   compute como-data-from =
                           function integer-of-date(como-data-from)
                   subtract 365 from como-data-from                     
            
                   compute como-data-from =
                           function date-of-integer(como-data-from)
                   move como-data-from to como-data
                   perform DATE-TO-SCREEN
                   move como-data to ef-data-from-buf
                   display ef-data-from
                end-if
               
           when 78-ID-ef-data-from
                inquire ef-data-from, value in ef-data-from-buf
                move ef-data-from-buf to como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-from-buf
                end-if
                display ef-data-from
      *                       
           when 78-ID-ef-data-to                              
                inquire ef-data-from, value in ef-data-from-buf
                inquire ef-data-to,   value in ef-data-to-buf
                move ef-data-to-buf to como-data
                if como-data = 0
                   move 99999999  to como-data
                end-if
                if como-data not = 99999999
                   perform DATE-FORMAT
                   move como-data to ef-data-to-buf
                end-if

                move como-data to ef-data-to-buf
                display ef-data-to

                perform DATE-TO-FILE
                move como-data        to como-data-to

                move ef-data-from-buf to como-data
                perform DATE-TO-FILE
                move como-data    to como-data-from

                if como-data-to < como-data-from
                   set errori to true
                   display message box "Intervallo limiti data errato!"
                           title = tit-err
                           icon 2
                   move 78-ID-ef-data-from to control-id
                end-if

           when 78-ID-ef-cli
                inquire ef-cli, value in cli-codice
                set cli-tipo-C to true
                if cli-codice = 0
                   move "TUTTI I CLIENTI" to lab-cli-buf
                else
                   read clienti no lock
                        invalid 
                        move spaces to cli-ragsoc-1
                        display message "Cliente non valido"
                                  title tit-err
                                   icon 2
                        set errori to true
                   end-read
                   move cli-ragsoc-1 to lab-cli-buf
                end-if
                display lab-cli

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move control-id to store-id        
              move 4          to accept-control  
           end-if 
           .
      * <TOTEM:END>

