       Form1-Fld-To-Buf.
      * <TOTEM:EPT. FORM:Form1, FORM:Form1, BeforeFldToBuf>
      * <TOTEM:END>
      * DB_Entry-Field : ef-gdo
           MOVE asc-cod-gruppo-gdo TO ef-gdo-BUF
      * DB_Entry-Field : ef-cli
           MOVE asc-cod-cliente TO ef-cli-BUF
      * DB_Entry-Field : ef-dest
           MOVE asc-progressivo-destino TO ef-dest-BUF
      * DB_Entry-Field : ef-art
           MOVE asc-cod-articolo TO ef-art-BUF
      * DB_Entry-Field : ef-data
           MOVE asc-data-inizio-validita TO ef-data-BUF
      * DB_CHECK BOX : chk-da-articoli
              IF asc-da-articoli = 1
                 MOVE 1 TO chk-da-articoli-BUF
              ELSE
                 MOVE 0 TO chk-da-articoli-BUF
              END-IF
      * DB_Entry-Field : ef-prezzo
           MOVE asc-prezzo-finito TO ef-prezzo-BUF
      * DB_Entry-Field : ef-sconto
           MOVE asc-perc-sconto-listino TO ef-sconto-BUF
      * DB_Entry-Field : ef-cons
           MOVE asc-imposta-consumo TO ef-cons-BUF
      * DB_Entry-Field : ef-cou
           MOVE asc-imposta-ecologica-coubat TO ef-cou-BUF
      * DB_Entry-Field : ef-cod-art-cli
           MOVE asc-cod-articolo-per-cliente TO ef-cod-art-cli-BUF
      * DB_Entry-Field : ef-imp
           MOVE asc-imponibile-merce TO ef-imp-BUF
      * DB_LABEL : lab-art
              MOVE art-descrizione  TO lab-art-BUF
      * DB_LABEL : lab-gdo
              MOVE gdo-intestazione  TO lab-gdo-BUF
      * DB_LABEL : lab-cli
              MOVE cli-ragsoc-1  TO lab-cli-BUF
      * DB_LABEL : lab-dest
              MOVE des-localita  TO lab-dest-BUF
      * <TOTEM:EPT. FORM:Form1, FORM:Form1, AfterFldToBuf>
           move asc-stato        to stato.
           perform CARICA-COMBO-STATO.

           if asc-cod-gruppo-gdo not = spaces
              move asc-cod-gruppo-gdo to gdo-codice
              read tgrupgdo no lock invalid continue end-read
              move gdo-intestazione to lab-gdo-buf
           else
              move spaces to lab-gdo-buf
           end-if.

           if asc-cod-cliente not = 0
              move asc-cod-cliente                  to codice-ed 
                                                       cli-codice
                                                       des-codice
              move codice-ed                        to ef-cli-buf
              call "C$JUSTIFY" using ef-cli-buf, "L"
              set  cli-tipo-C to true
              read clienti no lock invalid continue end-read
              move cli-ragsoc-1 to lab-cli-buf

              move cli-tipo to tcl-codice
              read ttipocli invalid continue end-read

              if asc-progressivo-destino not = 0
                 move asc-progressivo-destino          to codice-ed 
                                                          des-prog
                 move codice-ed                        to ef-dest-buf
                 call "C$JUSTIFY" using ef-dest-buf, "L"
                 read destini no lock invalid continue end-read
                 move des-localita to lab-dest-buf
              else
                 move spaces to lab-dest-buf ef-dest-buf
              end-if

           else
              move spaces to ef-cli-buf  ef-dest-buf
              move spaces to lab-cli-buf lab-dest-buf
           end-if.

           move asc-cod-articolo                 to codice-ed 
                                                    art-codice.
           move codice-ed                        to ef-art-buf.
           call "C$JUSTIFY" using ef-art-buf, "L".
           read articoli no lock invalid continue end-read.
           move art-descrizione to lab-art-buf.

           move art-marca-prodotto to mar-codice.
           read tmarche no lock invalid continue end-read.

           move asc-data-inizio-validita to como-data.
           perform DATE-TO-SCREEN.
           move como-data               to ef-data-buf.

           perform VALORIZZA-OLD.
           perform ABILITAZIONI.

