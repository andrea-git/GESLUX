           inquire ef-prezzo, value in asc-prezzo-finito.
           inquire ef-cons,   value in asc-imposta-consumo.
           inquire ef-cou,    value in asc-imposta-ecologica-coubat.
           inquire ef-sconto, value in asc-perc-sconto-listino.


           if TrattamentoGDO
              compute asc-imponibile-merce =
                      asc-prezzo-finito    -
                      asc-imposta-consumo  -
                      asc-imposta-ecologica-coubat
           else
              compute asc-imponibile-merce =
                      ( asc-prezzo-finito  - 
            (( asc-prezzo-finito * asc-perc-sconto-listino ) / 100 )) +
                      asc-imposta-consumo  +
                      asc-imposta-ecologica-coubat
           end-if. 

           move asc-imponibile-merce to ef-imp-buf.
           display ef-imp 
           .
      * <TOTEM:END>

       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = ZERO  
              exit paragraph  
           end-if.
      *
           display message MSG-Cancellare-il-record-corrente
                     title titolo
                      type mb-yes-no 
                   default mb-no
                    giving scelta

           if scelta = mb-yes
              perform RIEMPI-CHIAVE
              delete assorcli record invalid continue end-delete
                                                            
              perform FORM1-CLEAR

              move 0 to mod
              move colore-non-protetto to c-chiave
              move colore-protetto     to c-campi   c-sconto 
                                          c-listino c-prezzo

              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC
                            
              display message MSG-Cancellazione-avvenuta-con-successo
                        title titolo                         

              unlock assorcli all records

              perform PRIMO
              move 1 to control-id
              move 4 to accept-control
              set Statusvisua to true
              perform STATUS-BAR-MSG
              modify TOOL-MODIFICA value = mod
           end-if 
           .
      * <TOTEM:END>

       CANCELLA-TMP.
      * <TOTEM:PARA. CANCELLA-TMP>
           delete file tmp-assorcli-zoom 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate CONTROL-ID
           when 78-ID-ef-gdo
                inquire  ef-gdo   value gdo-codice  
                move "tgrupgdo " to Como-File
                call   "zoom-gt" using como-file, gdo-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO  
                   modify ef-gdo, value gdo-codice
                   move 78-ID-ef-gdo     to store-id
                   move gdo-intestazione to lab-gdo-buf
                   display lab-gdo
                end-if

           when 78-ID-ef-cli
                inquire ef-cli   value cli-codice
                inquire ef-gdo   value cli-gdo
                move "clienti-gdo-all"  to Como-File
                call   "zoom-gt" using como-file, cli-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-cli,    value cli-codice
                   move cli-ragsoc-1    to lab-cli-buf
                   display lab-cli
                   move 78-ID-ef-cli to store-id
                end-if

           when 78-ID-ef-dest
                inquire ef-cli  value des-codice
                inquire ef-dest value des-prog
                move "clienti-des"   to Como-File
                call   "zoom-gt" using como-file, des-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO  
                   modify ef-dest, value des-prog
                   move des-localita  to lab-dest-buf
                   display lab-dest
                   move 78-ID-ef-dest to store-id
                end-if               

           when 78-ID-ef-art
                inquire ef-art  value art-codice
                move "articoli"  to Como-File
                call   "zoom-gt" using como-file, art-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO  
                   move  78-ID-ef-art   to store-id
                   modify ef-art, value art-codice
                   move art-descrizione to lab-art-buf
                   display lab-art
                end-if
            
           end-evaluate. 

           move store-id to CONTROL-ID.
           move 4        to ACCEPT-CONTROL.

           perform CANCELLA-COLORE 
           .
      * <TOTEM:END>

       CLEAR-SCREEN.
      * <TOTEM:PARA. CLEAR-SCREEN>
           move asc-chiave to old-asc-chiave.

           initialize asc-dati  replacing numeric data by zeroes
                                    alphanumeric data by spaces.

           if ef-cli-buf = spaces or 0
              initialize cli-dati  replacing numeric data by zeroes
                                        alphanumeric data by spaces
           end-if.
 
           if ef-dest-buf = spaces or 0
              initialize des-dati  replacing numeric data by zeroes
                                        alphanumeric data by spaces
           end-if.
                        
           perform FORM1-FLD-TO-BUF. 

      * SPECIFICO PGM "gassorcli"
           move "Attivo" to cbo-stato-buf
           Modify  cbo-stato,   value cbo-stato-buf
      * FINE

           perform INIT-OLD-REC.

           display form1.     
  
           unlock assorcli all records 
           .
      * <TOTEM:END>

       CHANGE-STATUS.
      * <TOTEM:PARA. CHANGE-STATUS>
           set disattivo to true
           perform CARICA-COMBO-STATO

           move 1    to mod
           move colore-non-protetto to c-stato
           move colore-protetto     to c-prezzo c-sconto
                                       c-campi  c-listino.
           
           display Form1 
           .
      * <TOTEM:END>

       CHECK-INSERIMENTO.
      * <TOTEM:PARA. CHECK-INSERIMENTO>
      * MODIFICA LA STATUS-BAR
      * CONTROLLO SE DA INSERIMENTO SCORRO. ALLORA DEVO SEGNALARE CHE
      * SONO TORNATO IN MODIFICA.
           inquire tool-modifica, value in mod.

           evaluate mod
           when 1   set StatusModifica to true
           when 0   set StatusVisua    to true   
           end-evaluate.
      
           perform STATUS-BAR-MSG 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.
      
           evaluate CONTROL-ID
           when 78-ID-ef-gdo
                inquire ef-gdo value in ef-gdo-buf
                move ef-gdo-buf to gdo-codice
                if gdo-codice = spaces
                   move spaces  to lab-gdo-buf
                else          
                   read tgrupgdo no lock
                        invalid 
                        set errori to true
                        display message "Codice GDO NON valido"
                                  title tit-err
                                   icon 2
                        move spaces  to lab-gdo-buf
                        move 78-ID-ef-gdo to store-id
                    not invalid
                        move gdo-intestazione to lab-gdo-buf
                        move 78-ID-ef-cli to control-id store-id
                        move 4            to accept-control
                   end-read
                end-if

                if tutto-ok
                   move 78-ID-ef-cli to control-id store-id
                   move 4            to accept-control
                end-if
      
                display lab-gdo
      
           when 78-ID-ef-cli
                set cli-tipo-c to true
                initialize CountChar
                inquire ef-cli,    value ef-cli-buf
                if ef-cli-buf not = spaces
                   inspect ef-cli-buf replacing trailing SPACES by 
           low-values
                   initialize CountChar
                   inspect ef-cli-buf 
                      tallying CountChar for characters before low-value
                   inspect ef-cli-buf replacing trailing low-values by 
           SPACES
      
                   | SE E' NUMERICO LEGGO IL RECORD NORMALMENTE
                   if ef-cli-buf(1:CountChar) is numeric
                      perform SELEZIONA-NUMERICO-CLIENTE
                   else
                      | HO DIGITATO UNA SIGLA ALFANUMERICA QUINDI 
                      | APRO LO ZOOM (CASE SENSITIVE)
                      perform SELEZIONA-ALFA-CLIENTE
                   end-if
                else
                   move spaces to lab-cli-buf  ef-dest-buf
                                  lab-dest-buf 
                   display lab-cli lab-dest ef-dest
                end-if
      
                if tutto-ok

                   if ef-cli-buf not = spaces
                      move cli-gdo to ef-gdo-buf gdo-codice
                      if cli-gdo not = spaces
                         read tgrupgdo no lock
                              invalid move "** NON TROVATO **" to 
           lab-gdo-buf
                          not invalid move gdo-intestazione    to 
           lab-gdo-buf
                         end-read
                      else
                         move spaces to lab-gdo-buf
                      end-if
                      display lab-gdo ef-gdo
                      move 78-ID-ef-dest to control-id store-id
                      move 4             to accept-control
                      move cli-tipo to tcl-codice
                      read ttipocli no lock
                           invalid  continue
                       not invalid
                           if ttipocli-gdo
                              set TrattamentoGDO to true
                              move colore-protetto to c-listino 
                                                      c-sconto
                           else
                              if mod = 1
                                 move colore-non-protetto 
                                   to c-listino c-sconto
                              end-if
                              set TrattamentoGDO to false
                           end-if
                      end-read
                      display chk-da-articoli ef-sconto

                   else
                      set TrattamentoGDO   to true
                      move colore-protetto to c-listino 
                                              c-sconto
                      if ef-gdo-buf = spaces
                         if mod = 1
                            set  TrattamentoGDO      to false
                            move colore-non-protetto to c-listino 
                                                        c-sconto
                         end-if
                      end-if
                      display chk-da-articoli ef-sconto
                   end-if
                end-if
      
                display lab-cli
      
           when 78-ID-ef-dest
                initialize CountChar
                inquire ef-dest,    value in ef-dest-buf
                if ef-dest-buf not = spaces
                   if ef-cli-buf not = spaces
                      inspect ef-dest-buf 
                              replacing trailing spaces by low-values
                      initialize CountChar
                      inspect ef-dest-buf tallying CountChar  
                                          for characters before 
           low-value
                      inspect ef-dest-buf 
                              replacing trailing low-values by spaces
      
                      | SE E' NUMERICO LEGGO IL RECORD NORMALMENTE
                      if ef-dest-buf(1:CountChar) is numeric
                         perform SELEZIONA-NUMERICO-DESTINO
                      else
                         | HO DIGITATO UNA SIGLA ALFANUMERICA QUINDI 
                         | APRO LO ZOOM (CASE SENSITIVE)
                         perform SELEZIONA-ALFA-DESTINO
                      end-if
                   else
                      move spaces to ef-dest-buf lab-dest-buf
                      display lab-dest ef-dest
                   end-if
                else
                   move spaces to lab-dest-buf
                end-if

                if tutto-ok
                   move 78-ID-ef-art to control-id store-id
                   move 4            to accept-control
                end-if 

                display lab-dest

           when 78-ID-ef-art
                initialize CountChar
                inquire ef-art,    value ef-art-buf
                if ef-art-buf not = spaces
                   inspect ef-art-buf replacing trailing spaces by 
           low-values
                   initialize CountChar
                   inspect ef-art-buf
                           tallying CountChar for characters before 
           low-value
                   inspect ef-art-buf
                           replacing trailing low-values by spaces
      
                   | SE E' NUMERICO LEGGO IL RECORD NORMALMENTE
                   if ef-art-buf(1:CountChar) is numeric
                      perform SELEZIONA-NUMERICO-ARTICOLO
                   else
                      | HO DIGITATO UNA SIGLA ALFANUMERICA QUINDI 
                      | APRO LO ZOOM (CASE SENSITIVE)
                      perform SELEZIONA-ALFA-ARTICOLO
                   end-if
                else
                   set errori to true
                   display message "Codice Articolo obbligatorio"
                             title tit-err
                              icon 2
                   move spaces to lab-art-buf
                end-if
                display lab-art
      *    
      * LETTURA DEL RECORD SE TUTTI I CONTROLLI SONO PASSATI OK
                if tutto-ok
                   perform RIEMPI-CHIAVE
                   read assorcli no lock
                        invalid
                        if vecchio
                           display message MSG-record-inesistente
                                     title tit-err
                                      icon 2
                           perform CLEAR-SCREEN
                           set errori to true
                           move 78-ID-ef-gdo to store-id
                        else
                           perform IMPOSTE
                           display ef-cons ef-cou
                        end-if
                    not invalid
                        if old-asc-chiave not = asc-chiave
                           perform CURRENT-RECORD 
                        end-if
                   end-read
                else
                   move 0 to ef-cons-buf ef-cou-buf
                   display ef-cons ef-cou
                   move 78-ID-ef-art to control-id store-id
                   move 4            to accept-control
                end-if
      
           when 78-ID-ef-prezzo
                 if c-prezzo = colore-non-protetto
                    inquire ef-prezzo value in asc-prezzo-finito
                    if asc-prezzo-finito = 0
                       set errori to true
                       display message "Prezzo Finito obbligatorio"
                                 title tit-err
                                  icon 2
                       if c-listino = colore-non-protetto
                          move 78-ID-chk-da-articoli to store-id
                       else
                          move 78-ID-ef-prezzo       to store-id
                       end-if
                    else
                       if asc-prezzo-finito not = old-asc-prezzo-finito
                          perform CALCOLA-IMPONIBILE
                       end-if
                    end-if
                 end-if

           when 78-ID-ef-sconto
                if c-sconto = colore-non-protetto
                   perform CALCOLA-IMPONIBILE
                 end-if
      
           when 78-ID-ef-data
                inquire ef-data  value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                else
                   accept como-data from century-date
                   perform DATE-TO-SCREEN
                end-if
                move como-data to ef-data-buf
                display ef-data
      
           when 78-ID-cbo-stato
                perform SCARICA-COMBO-STATO
                move stato   to  asc-stato
                if asc-stato not = old-asc-stato
                   move 1 to Passwd-password
                   call   "passwd" using Passwd-linkage
                   cancel "passwd"

                   if not Passwd-StatusOk
                      move old-asc-stato to stato
                      perform CARICA-COMBO-STATO
                   else
                      if disattivo
                         move control-id to store-id
                         perform CHANGE-STATUS
                         move store-id to control-id
                      end-if
                   end-if
                end-if
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move store-id to CONTROL-ID
              move 4        to accept-control
           end-if 
           .
      * <TOTEM:END>

       CURRENT-RECORD.
      * <TOTEM:PARA. CURRENT-RECORD>
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if nuovo move 0 to mod end-if.

           if mod = 1
              read assorcli    lock invalid set errori to true end-read
           else
              read assorcli no lock invalid set errori to true end-read
           end-if.
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok
                 if nuovo
                    move 0 to mod
                    move colore-protetto     to c-stato
                    move colore-non-protetto to c-chiave

                    move 1 to CONTROL-ID
                    move 4 to ACCEPT-CONTROL
                 end-if
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else                         
                    set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG
              else
                 if vecchio
                    move 0 to mod
                    move colore-protetto     to c-stato
                    move colore-non-protetto to c-chiave
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-gdo to control-id
                       move 4            to accept-control
                       display message MSG-Record-inesistente
                                 title tit-err
                                  icon 2
                    end-if
                 end-if
              end-if
           end-if 
           .
      * <TOTEM:END>

       IMPOSTE.
      * <TOTEM:PARA. IMPOSTE>
           if des-nazione = spaces
              move cli-nazione to naz-codice
           else
              move des-nazione to naz-codice
           end-if.
           read tnazioni no lock.

           if naz-imp-esenti-si
              move 0 to imposta-consumo imposta-cobat imposta-cou
              set lbl-cou-cobat to true
           else
              |Da Walter: fisso NO
              set TrattamentoPiombo to false
              move art-peso-utf     to prg-peso-utf
              move art-peso-non-utf to prg-peso-non-utf
              perform CALCOLA-IMPOSTE
           end-if.

           move imposta-consumo to ef-cons-buf.
           if ImpostaCobat
              move imposta-cobat to ef-cou-buf
              set  lbl-cobat     to true
           else
              move imposta-cou   to ef-cou-buf
              set  lbl-cou       to true
           end-if.

           display lbl-cou-bat 
           .
      * <TOTEM:END>

       INIT.
      * <TOTEM:PARA. INIT>
           Perform RIEMPI-COMBO-STATO.

           move "Attivo" to cbo-stato-buf.
           Modify  cbo-stato,   value cbo-stato-buf.

           set lbl-cou-cobat to true.
           display lbl-cou-bat 
           .
      * <TOTEM:END>

       INIT-OLD-REC.
      * <TOTEM:PARA. INIT-OLD-REC>
           initialize old-asc-rec  replacing numeric data by zeroes
                                        alphanumeric data by spaces.

           set Attivo to true
           move stato to old-asc-stato.
           set old-asc-no-da-articoli to true 
           .
      * <TOTEM:END>

       MODIFICA.
      * <TOTEM:PARA. MODIFICA>
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod
              move colore-non-protetto to c-stato
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1 to mod
                 perform SALV-MOD
                 if errori
                    move 1 to mod
                 else
                    move 0 to mod
                    move 78-ID-ef-gdo to control-id
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock assorcli all records
                    if KeyboardSaved
                       set KeyboardReleased to true
                       set environment "KEYSTROKE" to "DATA=44 44"
                       set environment "KEYSTROKE" to "DATA=46 46"
                    end-if
                 end-if
              end-if
                                     
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE
              move 4 to accept-control
              move 0 to StatusHelp
              perform STATUS-HELP
              display form1
           end-if 
           .
      * <TOTEM:END>

       NUOVO.
      * <TOTEM:PARA. NUOVO>
           perform SALV-MOD.

           if tutto-ok         
              move 78-ID-ef-gdo to control-id
              move 4            to accept-control 
              move 1 to mod          
              modify tool-modifica,  value = mod
                                     
              perform CANCELLA-COLORE
              
              perform FORM1-CLEAR

              move colore-non-protetto to c-stato 
                                          c-chiave 
                                          c-listino 
                                          c-campi 
                                          c-sconto 
                                          c-prezzo

              set lbl-cou-cobat to true

              perform VALORIZZA-NUOVO                                   
                                                
              perform INIT-OLD-REC

              move "Attivo"      to cbo-stato-buf
              modify  cbo-stato, value cbo-stato-buf
              display form1

      * IL FLAG SICCOME E' ANCORA SETTATO A VECCHIO DALLA PRECEDENTE
      * OPERAZIONE, VIENE RISETTATO A NUOVO
      * (STANDARD NORMALE)
              set nuovo to true
              set StatusIns to true
              perform STATUS-BAR-MSG  
              unlock assorcli all records
           end-if 
           .
      * <TOTEM:END>

       ORDINAMENTO.
      * <TOTEM:PARA. ORDINAMENTO>
      *     inquire tool-ord, value in OrderBy.
      *
      *     if OrderBy = zero
      *        perform DataSet1-CHANGETO-KEY1
      *     else
      *        perform DataSet1-CHANGETO-KEY2
      *     end-if.
      *     perform Form1-Curr 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".
           Copy "utydata.cpy".
           copy "status.cpy".
           copy "riempi-combo-stato.cpy".
           copy "imposte.cpy" 
           .
      * <TOTEM:END>

       PRECEDENTE.
      * <TOTEM:PARA. PRECEDENTE>
           perform SALV-MOD.           
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock assorcli all records

              if mod = 1
                 set dataset1-assorcli-lock to true
              else
                 initialize dataset1-assorcli-lock-flag
              end-if

              perform CANCELLA-COLORE

              perform FORM1-PREVIOUS

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-assorcli-lock-flag
                 perform FORM1-PREVIOUS
              end-if
           end-if 
           .
      * <TOTEM:END>

       PRIMO.
      * <TOTEM:PARA. PRIMO>
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock assorcli all records

              if mod = 1
                 set dataset1-assorcli-lock to true
              else
                 initialize dataset1-assorcli-lock-flag
              end-if

              perform CANCELLA-COLORE

              perform FORM1-FIRST

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-assorcli-lock-flag
                 perform FORM1-FIRST
              end-if
           end-if 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE.
      * <TOTEM:PARA. RIEMPI-CHIAVE>
           inquire ef-gdo  value in ef-gdo-buf.
           move    ef-gdo-buf    to asc-cod-gruppo-gdo.

           inquire ef-cli  value in ef-cli-buf.
           move    ef-cli-buf    to asc-cod-cliente convert.

           inquire ef-dest value in ef-dest-buf.
           move ef-dest-buf      to asc-progressivo-destino convert.
           
           inquire ef-art  value in ef-art-buf.
           move    ef-art-buf    to asc-cod-articolo convert 

           
           .
      * <TOTEM:END>

       RIEMPI-TMP.
      * <TOTEM:PARA. RIEMPI-TMP>
           set record-ok to true
      *     inquire ef-art, value in SaveArticolo.

           accept  path-tmp-assorcli-zoom from environment "PATH_ST".
           accept  como-data              from century-date.
           accept  como-ora               from time.
           inspect path-tmp-assorcli-zoom replacing trailing
                                          spaces by low-value.
           string path-tmp-assorcli-zoom  delimited by low-value
                  "tmp-assorcli-zoom"     delimited by size
                  "_"                     delimited by size
                  como-data               delimited by size
                  "_"                     delimited by size
                  como-ora                delimited by size
                  into path-tmp-assorcli-zoom
           end-string.

           set trovato to false.
           open output tmp-assorcli-zoom.
           initialize asc-rec.

           start assorcli key  is >= asc-chiave
                 invalid continue 
             not invalid
                 perform until 1 = 2
                    read assorcli next no lock at end exit perform 
           end-read
                    set record-ok to true
                    evaluate true
                    when filtro-cliente
                         if asc-cod-gruppo-gdo not = save-cod-gruppo-gdo
                            set record-ok to false
                         end-if
                    when filtro-destino
                         if asc-cod-gruppo-gdo not = 
           save-cod-gruppo-gdo or
                            asc-cod-cliente    not = save-cod-cliente
                            set record-ok to false
                         end-if
                    when filtro-articoli
                         if asc-cod-gruppo-gdo not = 
           save-cod-gruppo-gdo or
                            asc-cod-cliente    not = save-cod-cliente or
                            asc-progressivo-destino not = 
           save-progressivo-destino
                            set record-ok to false
                         end-if
                    end-evaluate

                    if record-ok
                       initialize tmp-asc-z-rec 
                                  replacing numeric data by zeroes
                                       alphanumeric data by spaces

                       move asc-cod-gruppo-gdo to gdo-codice
                                                  
           tmp-asc-z-cod-gruppo-gdo
                       read tgrupgdo no lock 
                            invalid initialize gdo-rec
                        not invalid move gdo-intestazione 
                                      to tmp-asc-z-gdo-intestazione
                        end-read

                        set cli-tipo-C          to true
                        move asc-cod-cliente    to cli-codice
                                                   des-codice
                                                   tmp-asc-z-cod-cliente
                        read clienti no lock
                             invalid initialize cli-rec
                         not invalid move cli-ragsoc-1 
                                       to tmp-asc-z-cli-ragsoc-1
                        end-read
                 

                        move asc-progressivo-destino to des-prog
                                           tmp-asc-z-progressivo-destino
                        read destini no lock
                             invalid initialize des-rec
                                     move cli-localita to des-localita
                        end-read
                        move des-localita      to tmp-asc-z-des-localita

                        move asc-cod-articolo  to art-codice
                                                  tmp-asc-z-cod-articolo
                        read articoli no lock
                             invalid initialize art-rec
                         not invalid move art-descrizione 
                                       to tmp-asc-z-art-descrizione
                        end-read

                        write tmp-asc-z-rec invalid continue end-write
                        set trovato to true
                     end-if

                  end-perform
           end-start.

           close tmp-assorcli-zoom.

           move "tmp-assorcli-zoom"   to como-file 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           If MOD = 0 exit paragraph end-if.

           set tutto-ok to true.
                                 
           move control-id to mem-id.
           perform  varying CONTROL-ID from 78-ID-ef-gdo by 1
                      until CONTROL-ID > 78-ID-cbo-stato
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if errori
              perform ABILITAZIONI
              move 4 to ACCEPT-CONTROL
           else            
              perform FORM1-BUF-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              perform CANCELLA-COLORE
              
              accept como-data from century-date
              accept como-ora  from time

              perform VALORIZZA-DATI-COMUNI

              write asc-rec invalid rewrite asc-rec end-write
              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   

           display form1 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD.

           if NoSalvato
              display message MSG-Salvare-le-modifiche
                        title titolo
                         type mb-yes-no-cancel 
                       giving scelta       
       
              evaluate scelta
              when mb-yes perform SALVA
              when mb-no  continue
              when other
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id   to CONTROL-ID    
                   move 4          to ACCEPT-CONTROL
              end-evaluate
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA.
      * <TOTEM:PARA. SELEZIONA>
           perform RIEMPI-CHIAVE.

           evaluate CONTROL-ID
           when 78-ID-ef-gdo
                move   "assorcli"       to como-file
                perform RIEMPI-TMP
           when 78-ID-ef-cli
      *          move   "asc-clienti"    to como-file
                set filtro-cliente      to true
                move asc-chiave         to save-asc-chiave
                perform RIEMPI-TMP
           when 78-ID-ef-dest      
                move   "asc-destini"    to como-file
                set filtro-destino      to true
                move asc-chiave         to save-asc-chiave
                perform RIEMPI-TMP
           when 78-ID-ef-art
                move   "asc-articoli"   to como-file
                set filtro-articoli     to true
                move asc-chiave         to save-asc-chiave
                perform RIEMPI-TMP
           when other
                move   "assorcli"       to como-file                
                perform RIEMPI-TMP
           end-evaluate.

           if trovato

              |Leggo i clienti per posizionarmi poi
              |giusto nello zoom su key alfabetica
              set  cli-tipo-C to true
              move ef-cli-buf to cli-codice convert
              read clienti no lock
                   invalid move spaces to cli-ragsoc-1 
              end-read
              move cli-ragsoc-1 to tmp-asc-z-cli-ragsoc-1

              move path-tmp-assorcli-zoom to ext-file
              move save-asc-chiave        to tmp-asc-z-chiave
              call   "zoom-gt"  using  como-file, tmp-asc-z-rec
                                giving stato-zoom
              cancel "zoom-gt"
           else
              move 1 to stato-zoom
           end-if.
           perform CANCELLA-TMP.

           if stato-zoom = 0
              if old-asc-CHIAVE not = tmp-asc-z-chiave
                 move tmp-asc-z-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key  to tmp-asc-z-chiave
                    modify ef-gdo  value tmp-asc-z-cod-gruppo-gdo
                    modify ef-cli  value tmp-asc-z-cod-cliente
                    modify ef-dest value tmp-asc-z-progressivo-destino
                    modify ef-art  value tmp-asc-z-cod-articolo

                    set ReadSecca to true
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-data to control-id
                    move 4             to accept-control
                 end-if
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-ALFA-ARTICOLO.
      * <TOTEM:PARA. SELEZIONA-ALFA-ARTICOLO>
      * LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      * ANCORA GLI ZERI DAVANTI CHE ELIMINO
           inspect ef-art-buf replacing leading ZERO by SPACES.
           move ef-art-buf to art-descrizione.
      
      * POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      * ALTERNATA
           call "C$JUSTIFY" using art-descrizione, "L".
      
           start articoli key >= art-descrizione
                 invalid  set errori to true
             not invalid  read articoli next at end continue end-read
           end-start.
      
           if tutto-ok
              move "articoli-alfa" to como-file
              call "zoom-gt" using como-file, art-rec
                             giving stato-zoom
              cancel "zoom-gt"
              if stato-zoom = 0
                 move art-codice    to codice-ed
                 move codice-ed     to ef-art-buf   
                 call "C$JUSTIFY" using ef-art-buf, "L"
                 display ef-art
                 read articoli no lock 
                      invalid continue 
                  not invalid
                      move art-descrizione to lab-art-buf
                      move art-marca-prodotto to mar-codice
                      read tmarche no lock invalid continue end-read
                 end-read
              else
                 set errori to true
                 move spaces to lab-art-buf
              end-if  
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-ALFA-CLIENTE.
      * <TOTEM:PARA. SELEZIONA-ALFA-CLIENTE>
      * LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      * ANCORA GLI ZERI DAVANTI CHE ELIMINO
           inspect ef-cli-buf replacing leading ZERO by SPACES.
           move ef-cli-buf to cli-ragsoc-1.
      
      * POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      * ALTERNATA
           set cli-tipo-C to true.
           call "C$JUSTIFY" using cli-ragsoc-1, "L".
      
           start clienti key >= cli-k1
                 invalid set errori to true
             not invalid 
                  read clienti next at end continue end-read 
                  if cli-tipo-F
                     initialize cli-rec
                  else
                     inquire ef-gdo   value cli-gdo
                  end-if
           end-start.
      
           if tutto-ok
              move "clienti-gdo-alf-all" to como-file
              call "zoom-gt" using como-file, cli-rec
                             giving stato-zoom
              cancel "zoom-gt"
              if stato-zoom = 0
                 move cli-codice    to codice-ed
                 move codice-ed     to ef-cli-buf   
                 call "C$JUSTIFY" using ef-cli-buf, "L"
                 display ef-cli
              
                 move spaces to cli-ragsoc-1
                 set cli-tipo-C  to true
                 read clienti no lock 
                      invalid continue 
                  not invalid
                      move cli-ragsoc-1 to lab-cli-buf
                 end-read
              else
                 set errori to true
                 move spaces to lab-cli-buf
              end-if  
           end-if.

           if errori
              move 78-ID-ef-cli to control-id store-id
              move 4            to accept-control
           end-if 
              
              
              
              
              
              
              
              
              
              
              
           .
      * <TOTEM:END>

       SELEZIONA-ALFA-DESTINO.
      * <TOTEM:PARA. SELEZIONA-ALFA-DESTINO>
      * LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      * ANCORA GLI ZERI DAVANTI CHE ELIMINO
           inspect ef-dest-buf replacing leading ZERO by SPACES.
           move ef-dest-buf to des-ragsoc-1.
      
      * POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      * ALTERNATA
           call "C$JUSTIFY" using des-ragsoc-1, "L".
           inquire ef-cli,  value in ef-cli-buf.
           move ef-cli-buf  to des-codice convert.
           move low-values  to des-prog.

           start destini key >= k1
                 invalid set errori to true
             not invalid read destini next at end continue end-read
           end-start.
      
           if tutto-ok
              move "clienti-des-alf" to como-file
              call "zoom-gt" using como-file, des-rec
                            giving stato-zoom
              cancel "zoom-gt"
              if stato-zoom = 0
                 move des-prog       to codice-ed
                 move codice-ed      to ef-dest-buf   
                 call "C$JUSTIFY" using ef-dest-buf, "L"
                 display ef-dest
                 read destini no lock 
                      invalid move des-localita to lab-dest-buf
                 end-read
              else
                 set errori to true
              end-if
           end-if.

           if errori
              move 78-ID-ef-dest to control-id store-id
              move 4             to accept-control
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-NUMERICO-ARTICOLO.
      * <TOTEM:PARA. SELEZIONA-NUMERICO-ARTICOLO>
      * READ SECCA [AFTER CAMPO ARTICOLO] 
           inquire ef-art, value in ef-art-buf
           move ef-art-buf to art-codice convert.     

           if art-codice = 0
              set errori to true
              display message "Codice Articolo obbligatorio!"
                      title tit-err
                      icon  mb-warning-icon
              move 78-ID-ef-art to store-id
           else
              read articoli no lock
                   invalid
                   set errori to true
                   display message "Codice Articolo NON valido!"
                             title tit-err
                              icon 2
                   move spaces to lab-art-buf
               not invalid
                   move art-descrizione to lab-art-buf
                   move art-marca-prodotto to mar-codice
                   read tmarche no lock invalid continue end-read
              end-read
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-NUMERICO-CLIENTE.
      * <TOTEM:PARA. SELEZIONA-NUMERICO-CLIENTE>
      * READ SECCA [AFTER DEL CAMPO CODICE CLIENTE]
           inquire ef-cli, value in ef-cli-buf.
           move ef-cli-buf to asc-cod-cliente convert.

           if asc-cod-cliente = 0
              move spaces to lab-cli-buf
           else
              set cli-tipo-c       to true
              move asc-cod-cliente to cli-codice
              read clienti |key cli-k3
                   invalid
                   set errori to true
                   display message "Codice Cliente NON valido!"
                             title tit-err
                              icon 2
                   move spaces to lab-cli-buf
               not invalid
                   move cli-ragsoc-1 to lab-cli-buf
              end-read
           end-if.

           if errori
              move 78-ID-ef-cli to control-id store-id
              move 4            to accept-control
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-NUMERICO-DESTINO.
      * <TOTEM:PARA. SELEZIONA-NUMERICO-DESTINO>
      * READ SECCA [AFTER DEL CAMPO PROGRESSIVO DESTINO]       
           inquire ef-cli,  value ef-cli-buf.
           inquire ef-dest, value ef-dest-buf.
           move ef-cli-buf,    to des-codice convert.
           move ef-dest-buf,   to des-prog   convert.
           
           if des-prog not = 0
              read destini no lock
                   invalid
                   set errori to true
                   display message "Codice Destino NON valido"
                             title tit-err
                              icon 2
                   move spaces to lab-dest-buf
               not invalid
                   move des-localita to lab-dest-buf
              end-read
           end-if.

           if errori
              move 78-ID-ef-dest to control-id store-id
              move 4             to accept-control
           end-if 
           .
      * <TOTEM:END>

       STAMPA.
      * <TOTEM:PARA. STAMPA>
           SET STAMPA     TO TRUE 
           .
      * <TOTEM:END>

       SUCCESSIVO.
      * <TOTEM:PARA. SUCCESSIVO>
           perform SALV-MOD.           
           if tutto-ok          
              perform CHECK-INSERIMENTO
              unlock assorcli all records

              if mod = 1
                 set dataset1-assorcli-lock to true
              else
                 initialize dataset1-assorcli-lock-flag
              end-if

              perform CANCELLA-COLORE

              perform FORM1-NEXT

             if RecLocked
                 set RecLocked to false             
                 initialize dataset1-assorcli-lock-flag
                 perform FORM1-NEXT
              end-if
           end-if 
           .
      * <TOTEM:END>

       TORNA-IN-VISUA.
      * <TOTEM:PARA. TORNA-IN-VISUA>
           move 0 to mod.
           move 78-ID-ef-gdo to control-id.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock assorcli all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if 
           .
      * <TOTEM:END>

       ULTIMO.
      * <TOTEM:PARA. ULTIMO>
           perform SALV-MOD.
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock assorcli all records 

              if mod = 1
                 set dataset1-assorcli-lock to true
              else
                 initialize dataset1-assorcli-lock-flag
              end-if

              perform CANCELLA-COLORE

              perform FORM1-LAST 

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-assorcli-lock-flag
                 perform FORM1-LAST
              end-if
           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-DATI-COMUNI.
      * <TOTEM:PARA. VALORIZZA-DATI-COMUNI>
      * SE NON HO VALORIZZATO I CAMPI DI CREAZIONE LI VALORIZZO
           if asc-data-creazione   = zero
              accept asc-data-creazione from century-date
           end-if.
           if asc-ora-creazione    = zero
              accept asc-ora-creazione  from time
           end-if.
           if asc-utente-creazione = space
              move user-codi to asc-utente-creazione
           end-if.

           accept asc-data-ultima-modifica from century-date.
           accept asc-ora-ultima-modifica  from time.
           move user-codi    to asc-utente-ultima-modifica 
           .
      * <TOTEM:END>

       VALORIZZA-NUOVO.
      * <TOTEM:PARA. VALORIZZA-NUOVO>
           move "00" to status-assorcli.
      
           initialize asc-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.

           set old-asc-no-da-articoli to true.
           set asc-attivo             to true 
           .
      * <TOTEM:END>

       VALORIZZA-OLD.
      * <TOTEM:PARA. VALORIZZA-OLD>
           move asc-rec to old-asc-rec.

           set vecchio to true.

           evaluate CONTROL-ID
           when 78-ID-ef-gdo
           when 78-ID-ef-cli
           when 78-ID-ef-dest
           when 78-ID-ef-art
                move 1 to StatusHelp
           when other
                move 0 to StatusHelp
           end-evaluate.

           perform STATUS-HELP 
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       anagr01-Ev-After-Program.
      * <TOTEM:PARA. anagr01-Ev-After-Program>
           SET LK-BL-CANCELLAZIONE TO TRUE.
           MOVE COMO-PROG-ID       TO LK-BL-PROG-ID.
           CALL "BLOCKPGM"  USING LK-BLOCKPGM 
           .
      * <TOTEM:END>
       anagr01-Ev-Before-Program.
      * <TOTEM:PARA. anagr01-Ev-Before-Program>
           move LK-BL-PROG-ID    TO COMO-PROG-ID 
           .
      * <TOTEM:END>
       NUOVO-LinkTo.
      * <TOTEM:PARA. NUOVO-LinkTo>
           INQUIRE TOOL-NUOVO, ENABLED IN E-NUOVO.

           IF E-NUOVO = 1
              PERFORM NUOVO
           END-IF                    
           .
      * <TOTEM:END>
       CANCELLA-LinkTo.
      * <TOTEM:PARA. CANCELLA-LinkTo>
           INQUIRE TOOL-MODIFICA, VALUE IN MOD.
                              
           INQUIRE TOOL-CANCELLA, ENABLED IN E-CANCELLA
           IF E-CANCELLA = 1
              PERFORM CANCELLA
           END-IF                                                
           .
      * <TOTEM:END>
       TOOL-SALVA-LinkTo.
      * <TOTEM:PARA. TOOL-SALVA-LinkTo>
           INQUIRE TOOL-MODIFICA, VALUE IN MOD           

           INQUIRE TOOL-SALVA, ENABLED IN E-SALVA
           IF E-SALVA = 1
              PERFORM SALVA
           END-IF           
           .
      * <TOTEM:END>
       ANTEPRIMA-LinkTo.
      * <TOTEM:PARA. ANTEPRIMA-LinkTo>
           INQUIRE TOOL-ANTEPRIMA, ENABLED IN E-ANTEPRIMA.
           IF E-ANTEPRIMA = 1
                PERFORM ANTEPRIMA
           END-IF 
           .
      * <TOTEM:END>
       TOOL-MODIFICA-LinkTo.
      * <TOTEM:PARA. TOOL-MODIFICA-LinkTo>
           INQUIRE TOOL-MODIFICA, ENABLED IN E-MODIFICA.

           IF E-MODIFICA = 1
              PERFORM MODIFICA
           END-IF 
           .
      * <TOTEM:END>
       STAMPA-LinkTo.
      * <TOTEM:PARA. STAMPA-LinkTo>
           INQUIRE TOOL-STAMPA, ENABLED IN E-STAMPA

           IF E-STAMPA = 1
                PERFORM STAMPA
           END-IF 
           .
      * <TOTEM:END>
       TOOL-PRIMO.
      * <TOTEM:PARA. TOOL-PRIMO>
           PERFORM PRIMO 
           .
      * <TOTEM:END>
       TOOL-PRECEDENTE.
      * <TOTEM:PARA. TOOL-PRECEDENTE>
           if nuovo             
              perform PRIMO
           else
              perform PRECEDENTE
           end-if 
           .
      * <TOTEM:END>
       TOOL-SUCCESSIVO.
      * <TOTEM:PARA. TOOL-SUCCESSIVO>
           if nuovo             
              perform ULTIMO
           else
              perform SUCCESSIVO
           end-if 
           .
      * <TOTEM:END>
       TOOL-ULTIMO.
      * <TOTEM:PARA. TOOL-ULTIMO>
           PERFORM ULTIMO 
           .
      * <TOTEM:END>
       TOOL-SELEZIONA-LinkTo.
      * <TOTEM:PARA. TOOL-SELEZIONA-LinkTo>
           inquire tool-seleziona, enabled in e-seleziona

           if e-seleziona = 1
              perform SELEZIONA
           end-if 
           .
      * <TOTEM:END>
       TOOL-MODIFICA-BeforeProcedure.
      * <TOTEM:PARA. TOOL-MODIFICA-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       TOOL-MODIFICA-AfterProcedure.
      * <TOTEM:PARA. TOOL-MODIFICA-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       TOOL-CERCA-LinkTo.
      * <TOTEM:PARA. TOOL-CERCA-LinkTo>
           inquire tool-cerca, enabled in e-cerca

           if e-cerca = 1
              perform CERCA
           end-if 
           .
      * <TOTEM:END>
       ef-stabile-BeforeProcedure.
      * <TOTEM:PARA. ef-stabile-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-stabile-AfterProcedure.
      * <TOTEM:PARA. ef-stabile-AfterProcedure>
              INQUIRE ef-gdo, VALUE IN asc-cod-gruppo-gdo
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-gdo-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
  
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

       ef-cod-con-BeforeProcedure.
      * <TOTEM:PARA. ef-cod-con-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-cod-con-AfterProcedure.
      * <TOTEM:PARA. ef-cod-con-AfterProcedure>
              INQUIRE ef-cli, VALUE IN asc-cod-cliente
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-cli-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
  
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

       ef-periodo-BeforeProcedure.
      * <TOTEM:PARA. ef-periodo-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-periodo-AfterProcedure.
      * <TOTEM:PARA. ef-periodo-AfterProcedure>
              INQUIRE ef-dest, VALUE IN asc-progressivo-destino
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-dest-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

       ef-voce-BeforeProcedure.
      * <TOTEM:PARA. ef-voce-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-voce-AfterProcedure.
      * <TOTEM:PARA. ef-voce-AfterProcedure>
              INQUIRE ef-data, VALUE IN asc-data-inizio-validita
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-data-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-sconto, VALUE IN asc-perc-sconto-listino
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-sconto-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-cons, VALUE IN asc-imposta-consumo
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-cons-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-cou, VALUE IN asc-imposta-ecologica-coubat
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-cou-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-cod-art-cli, VALUE IN 
           asc-cod-articolo-per-cliente
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-cod-art-cli-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-imp, VALUE IN asc-imponibile-merce
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-imp-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
  
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

       EF-COD-ARTICOLO-AfterProcedure.
      * <TOTEM:PARA. EF-COD-ARTICOLO-AfterProcedure>
              INQUIRE ef-art, VALUE IN asc-cod-articolo
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-art-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

       EF-PREZZO-FINITO-AfterProcedure.
      * <TOTEM:PARA. EF-PREZZO-FINITO-AfterProcedure>
              INQUIRE ef-prezzo, VALUE IN asc-prezzo-finito
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-prezzo-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
