      * USER DEFINE PARAGRAPH
       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true. 
      
           evaluate CONTROL-ID
           when 78-ID-ef-mese-da
                inquire ef-mese-da, value in ef-mese-da-buf
                evaluate ef-mese-da-buf
                when 1 thru 12 continue
                when other
                     set errori to true
                     display message "Mese NON valido"
                               title tit-err
                                icon 2
                end-evaluate

           when 78-ID-ef-anno-da
                inquire ef-anno-da, value in ef-anno-da-buf
                if ef-anno-da-buf = 0
                   set errori to true
                   display message "Inserimento anno mancante"
                             title tit-err
                              icon 2
                end-if

           when 78-ID-ef-mese-a
                inquire ef-mese-da, value in ef-mese-da-buf
                inquire ef-mese-a,  value in ef-mese-a-buf
                evaluate ef-mese-a-buf
                when 1 thru 12
                     if ef-mese-a-buf < ef-mese-da-buf
                        inquire ef-anno-da, value in ef-anno-da-buf
                        inquire ef-anno-a,  value in ef-anno-a-buf
                        if ef-anno-a-buf <= ef-anno-da-buf
                           move 78-ID-ef-mese-da to control-id
                           set errori to true
                           display message "Intervallo mese errato"
                                     title tit-err
                                      icon 2
                        end-if
                     end-if
                when other
                     set errori to true
                     display message "Mese NON valido"
                               title tit-err
                                icon 2
                end-evaluate

           when 78-ID-ef-anno-a
                inquire ef-anno-da, value in ef-anno-a-buf
                if ef-anno-a-buf = 0
                   set errori to true
                   display message "Inserimento anno mancante"
                             title tit-err
                              icon 2
                else
                   inquire ef-anno-da, value in ef-anno-da-buf
                   inquire ef-anno-a,  value in ef-anno-a-buf
                   if ef-anno-a-buf < ef-anno-da-buf
                      move 78-ID-ef-anno-da to control-id
                      set errori to true
                      display message "Intervallo anno errato"
                                title tit-err
                                 icon 2
                   end-if
                end-if
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if 
           .
      * <TOTEM:END>

