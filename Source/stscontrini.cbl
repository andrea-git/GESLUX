       Screen2-AFTER-SCREEN.

      * Generazione risettaggio keyboard "." ---> "."

      * Generazione stringa perform CONTROLLO
           evaluate control-id
           |78-ID-ef-anno è l'ID del campo ef-anno
           when 78-ID-ef-anno
                perform CONTROLLO
           |78-ID-ef-num-to è l'ID del campo ef-num-to
           when 78-ID-ef-num-to
                perform CONTROLLO
           |78-ID-ef-data-to è l'ID del campo ef-data-to
           when 78-ID-ef-data-to
                perform CONTROLLO
           |78-ID-ef-cli è l'ID del campo ef-cli
           when 78-ID-ef-cli
                perform CONTROLLO
           |99999 è un valore fittizio, che non sarà MAI usato,
           |ma mi serve per non riscontrare errori di compilazione
           |in caso non avessi generato nulla nella AFTER CONTROLLO della screen
           when 99999 continue
           when other continue
           end-evaluate.



       Screen4-Event-Proc.
           .

      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-cli
                move "clienti-all"   to como-file
                set cli-tipo-C   to true
                inquire ef-cli,  value in cli-codice
      
                call "zoom-gt"   using como-file, cli-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move cli-codice to ef-cli-buf
                   display ef-cli
                end-if

           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID

           when 78-ID-ef-anno
      * ANNO DI FINE SELEZIONE
                inquire ef-anno, value in anno-to
                if anno-to = ZERO
                   set errori  to true
                   display message "Inserimento anno mancante"
                           title tit-err
                           icon  mb-warning-icon
                   move 78-ID-ef-anno   to store-id
                end-if

           when 78-ID-ef-num-to
      * ORDINE DI FINE SELEZIONE
                if num-to = ZERO
                   move all "9" to num-to
                   display ef-num-to
                end-if

                if num-to < num-from
                   set errori to true
                   display message MSG-intervallo-num-ord-err
                       title tit-err
                       icon  mb-warning-icon
                       type  mb-ok
                   move 3 to STORE-ID
                end-if
           when 78-ID-ef-data-from
                if data-from not = zero
                   move data-from to como-data
                   perform DATE-FORMAT
                   move como-data to data-from
                   display ef-data-from
                end-if
           when 78-ID-ef-data-to
                if data-to = zero
                   move 99999999 to data-to
                end-if
                if data-to not = 99999999
                   move data-to to como-data
                   perform DATE-FORMAT
                   move como-data to data-to
                end-if
                display ef-data-to

                move data-from(1:2) to data-from-AAAAMMGG(7:2)
                move data-from(3:2) to data-from-AAAAMMGG(5:2)
                move data-from(5:4) to data-from-AAAAMMGG(1:4)
                move data-to(1:2)   to data-to-AAAAMMGG(7:2)
                move data-to(3:2)   to data-to-AAAAMMGG(5:2)
                move data-to(5:4)   to data-to-AAAAMMGG(1:4)

                if data-from-AAAAMMGG = 0
                   move anno-to to data-from-AAAAMMGG(1:4)
                   move "0101"    to data-from-AAAAMMGG(5:4)
                end-if

                if data-to-AAAAMMGG < data-from-AAAAMMGG
                   set errori to true
                   move 78-ID-ef-data-to to CONTROL-ID
                   display message MSG-intervallo-date-err
                      title tit-err
                      icon  MB-WARNING-ICON
                end-if

           when 78-ID-ef-cli
                inquire ef-cli, value in ef-cli-buf
                move ef-cli-buf to cli-codice
                if cli-codice = 0
                   move "TUTTI I CLIENTI" to lab-cli-buf
                else
                   set cli-tipo-C to true
                   read clienti no lock
                        invalid 
                        display message "Cliente non valido"
                                  title tit-err
                                   icon 2
                        set errori to true              
                        move spaces to cli-ragsoc-1
                   end-read
                   move cli-ragsoc-1 to lab-cli-buf
                end-if
                display lab-cli
           end-evaluate.
                        
           if errori                  
              move STORE-ID to CONTROL-ID
              move 4        to ACCEPT-CONTROL
           end-if.

           perform CANCELLA-COLORE 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "utydata.cpy".
           copy "color-custom.cpy".
           copy "calcola-colore-trasparente.cpy" 
           .
      * <TOTEM:END>

       STAMPA-EFFETTIVA.
      * <TOTEM:PARA. STAMPA-EFFETTIVA>
           move spaces to selprint-stampante.
           move anno-to  to mto-anno.
           move num-from to mto-numero.

           start mtordini key >= mto-chiave
                 invalid continue
             not invalid 
                 perform until 1 = 2
                    read mtordini next at end exit perform end-read

                    if mto-anno > anno-to
                       exit perform
                    end-if

                    if mto-anno = anno-to and 
                       mto-numero > num-to
                       exit perform
                    end-if

                    perform VALIDA-ORDINE

                    if tutto-ok
                       if selprint-stampante = spaces
                          call   "selprint" using selprint-linkage
                          cancel "selprint"
                       end-if
                       if selprint-stampante = spaces
                          exit perform
                       end-if

                       call   "st-ordine-m" using mto-chiave,
                                                  selprint-stampante, 
                                                  "E"
                       cancel "st-ordine-m"

                    end-if
                 end-perform

           end-start.    

      ***---
       VALIDA-ORDINE.
           set tutto-ok to true.  

           move ef-cli-buf to cli-codice.
           if cli-codice not = 0 and
              cli-codice not = mto-cod-cli
              set errori to true
           end-if.

           if mto-numero < num-from or
              mto-numero > num-to
              set errori to true
           end-if.

           if mto-data-creazione < data-from-AAAAMMGG or 
              mto-data-creazione > data-to-AAAAMMGG
              set errori   to true
           end-if 
           .
      * <TOTEM:END>

