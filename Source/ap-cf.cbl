      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 1003
                move "CO"             to tblco-codice1
                inquire ef-cau, value in tblco-codice2
                move "tblco"        to como-File
                call   "zoom-gt" using como-file, record-tblco
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tblco-codice2      to ef-cau-buf
                   move tblco-descrizione1 to lab-cau-buf
                   display ef-cau lab-cau
                   move 4 to accept-control
                end-if
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate  control-id
           when 1001
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-buf
                display ef-data

           when 1002
                inquire ef-anno, value in ef-anno-buf
                if ef-anno-buf = spaces
                   display message "Inserire l'anno d'esercizio"
                             title tit-err
                              icon 2
                   set errori to true
                end-if

           when 1003
                inquire ef-cau, value in ef-cau-buf
                move "CO"       to tblco-codice1
                move ef-cau-buf to tblco-codice2
                read tblco no lock
                     invalid move spaces to lab-cau-buf
                             display message "Causale contabile NON vali
      -    "da!"
                                       title tit-err
                                        icon 2
                             set errori to true
                 not invalid move tblco-descrizione1 to lab-cau-buf
                end-read
                display lab-cau

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move 4  to accept-control
           end-if 
           .
      * <TOTEM:END>

