       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = 0 exit paragraph end-if.

           display message box MSG-cancellare-il-record-corrente
                   title = titolo
                   type mb-yes-no
                   default mb-no
                   giving scelta.

           if scelta = mb-yes

              inquire form1-gd-1, cursor-y  in riga
              perform valore-riga

              delete impforn record 
                 invalid continue 
              end-delete

              modify  form1-gd-1, record-to-delete = riga 
              inquire form1-gd-1, last-row        in tot-righe
              
              if riga > tot-righe 
                 move tot-righe to riga 
              end-if
              
              modify form1-gd-1, cursor-y = riga, cursor-x = 1
              move riga to event-data-2
              perform COLORE-RIGA
              display message MSG-cancellazione-avvenuta-con-successo
                      title = titolo
              set vecchio to true
           end-if 

           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
      *     evaluate colonna
      *     when 8   |n. registro
      *     when 9   |n. registro
      *     when 10  |n. registro
      *          inquire form1-gd-1, cursor-y in riga, 
      *                              cursor-x in colonna
      *          inquire form1-gd-1(riga, colonna), 
      *                  cell-data in utf-num-reg
      *          move tge-anno     to utf-anno
      *
      *          move "anautf-anno"  to como-file
      *          call "zoom-gt"   using como-file, utf-rec
      *                          giving stato-zoom
      *          cancel "zoom-gt"
      *
      *          if stato-zoom = 0
      *             evaluate colonna
      *             when  8 
      *                  move utf-num-reg to col-num-reg-c
      *                  modify form1-gd-1(riga, colonna), 
      *                         cell-data col-num-reg-c
      *             when  9
      *                  move utf-num-reg to col-num-reg-s1
      *                  modify form1-gd-1(riga, colonna), 
      *                         cell-data col-num-reg-s1
      *             when 10
      *                  move utf-num-reg to col-num-reg-s2
      *                  modify form1-gd-1(riga, colonna), 
      *                         cell-data col-num-reg-s2
      *             end-evaluate
      *             perform VALORE-RIGA
      *          end-if
      *     end-evaluate
           .
      * <TOTEM:END>

       COLORE-RIGA.
      * <TOTEM:PARA. COLORE-RIGA>
           if riga < 2 
              move 2 to riga 
           end-if.

           modify form1-gd-1, start-x = 1, x = 78-max-colonne,
                                  start-y = riga,
                                        y = riga,
                                  region-color 257,
                                  cursor-color 481 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           inquire form1-gd-1, cursor-y in riga.

           perform VALORE-RIGA.

           evaluate colonna
           when 78-col-id
                if nuovo
                   if imf-codice = 0
                      set errori  to true
                      move 1      to colonna
                      display message box MSG-Codice-obbligatorio
                              title = tit-err
                              icon mb-warning-icon
                   else
      *                inspect col-id-x replacing 
      *                                    trailing low-values by x"30"
      *                modify form1-gd-1(riga, 1), cell-data = col-id-x
      *                move col-id-x to imf-codice  
                      read impforn
                         not invalid
                            set errori to true
                            display message MGS-codice-gia-inserito
                                    title = tit-err
                                    icon mb-warning-icon
                      end-read
                   end-if
                end-if
           when 78-col-des
                if imf-descrizione = SPACES
                   set errori to true
                   move 2 to colonna
                   display message msg-descrizione-mancante
                           title = tit-err
                           icon mb-warning-icon
                end-if

           when 78-col-prez-real-utf
                move imf-prz-reale-utf   to como-colonna
                perform CONTROLLO-COLONNA

           when 78-col-prz-reale-cou
                move imf-prz-reale-cou   to como-colonna
                perform CONTROLLO-COLONNA
           when 78-col-prz-reale-cobat
                move imf-prz-reale-cobat to como-colonna
                perform CONTROLLO-COLONNA
           when 78-col-prz-reale-pb
                move imf-prz-reale-pb    to como-colonna
                perform CONTROLLO-COLONNA
           when 78-col-conf-utf
                move imf-conf-utf        to como-colonna
                perform CONTROLLO-COLONNA
           when 78-col-conf-cou
                move imf-conf-cou        to como-colonna
                perform CONTROLLO-COLONNA
           when 78-col-conf-cobat
                move imf-conf-cobat      to como-colonna
                perform CONTROLLO-COLONNA
           when 78-col-conf-pb
                move imf-conf-pb         to como-colonna
                perform CONTROLLO-COLONNA
           end-evaluate.

           if errori
              modify form1-gd-1, cursor-y = riga, cursor-x = colonna
              move riga    to event-data-2
              move colonna to event-data-1
           end-if 
           .
      * <TOTEM:END>

       CONTROLLO-COLONNA.
      * <TOTEM:PARA. CONTROLLO-COLONNA>
      *     move 7      to colonna

           evaluate colonna
           when 78-col-prez-real-utf
           when 78-col-prz-reale-cou
           when 78-col-prz-reale-cobat
           when 78-col-prz-reale-pb
                if como-colonna = "+" or "-" or "0"
                   continue
                else
                   set errori  to true
                   display message box "Valori possibili: +, -, 0"
                           title = tit-err
                           icon mb-warning-icon
                end-if
           when 78-col-conf-utf
           when 78-col-conf-cou
           when 78-col-conf-cobat
           when 78-col-conf-pb
                if como-colonna = "S" or "N"
                   continue
                else
                   set errori  to true
                   display message box "Valori possibili: S, N"
                           title = tit-err
                           icon mb-warning-icon
                end-if
           end-evaluate
           .
      * <TOTEM:END>

       GRID-HELP.
      * <TOTEM:PARA. GRID-HELP>
      * NON CI SONO ZOOM SULLE COLONNE 
      * QUINDI NON SERVE L'EVALUATE DELLA COLONNA. MA IL BOTTONE CERCA
      * SARA' SEMPRE DISABILITATO
           move BitmapZoomDisabled to BitmapNumZoom
           move ZERO to e-cerca

      *     evaluate colonna
      *     when 8   |numero registro 
      *     when 9
      *     when 10
      *        move BitmapZoomEnabled to BitmapNumZoom
      *        move 1 to e-cerca
      *     end-evaluate
           modify tool-cerca, bitmap-number = BitmapNumZoom,
                              enabled       = e-cerca 
            
           .
      * <TOTEM:END>

       INIT.
      * <TOTEM:PARA. INIT>
           set vecchio  to true.
           set tutto-ok to true 
           .
      * <TOTEM:END>

       LOAD-RECORD.
      * <TOTEM:PARA. LOAD-RECORD>
           modify form1-gd-1, mass-update = 1.

           move low-value to imf-codice.
           start impforn key is >= imf-chiave
              invalid 
                 continue
              not invalid
                 perform varying riga from 2 by 1 until 1 = 2
                    read impforn next
                       at end 
                          exit perform
                       not at end
                          perform METTI-IN-GRIGLIA
                     end-read                                           
                  end-perform
           end-start.

           modify form1-gd-1, mass-update = 0 
           .
      * <TOTEM:END>

       MODIFICA.
      * <TOTEM:PARA. MODIFICA>
           inquire tool-modifica, value in mod.

      *--->MODIFICA (ISACCO)
           if nuovo 
              move 1 to mod 
           else
              |SPECIFICO PER I PGM. CON GRID-CONTROL
              if mod = 1
                 close    impforn
                 open i-o impforn allowing readers
              else
                 set tutto-ok to true
                 if NoSalvato
                    move 1 to mod
                    perform SALV-MOD
                 end-if   
                 if tutto-ok
                    move ZERO  to mod
                    close      impforn
                    open input impforn

      *             | SPECIFICO X LE TABELLE.
                    | FA IN MODO CHE RESTO SULLA CELLA IN CUI ERO
                    if ricarica
                       set ricarica to false
                       modify form1-gd-1, reset-grid = 1
                       perform FORM1-GD-1-CONTENT
                       perform LOAD-RECORD
                    end-if

                 else
                    move 1 to mod
                 end-if
              end-if
           end-if.

           perform ABILITAZIONI.

           modify tool-modifica, value mod 
           .
      * <TOTEM:END>

       NUOVO.
      * <TOTEM:PARA. NUOVO>
           set tutto-ok to true.

           if nuovo
              perform VALORE-RIGA
              if imf-codice not = ZERO
                 perform SALVA
              else
                 set errori to true
              end-if
           end-if.

           if tutto-ok
              set nuovo to true
              inquire form1-gd-1,  last-row   in tot-righe
              modify  form1-gd-1,  insert-rows = 1
              modify  form1-gd-1,  cursor-x    = 1 
                                   cursor-y    = tot-righe + 1

              move 1 to mod
              modify  tool-modifica,   value = mod

              add 1 to tot-righe giving riga


              initialize imf-rec replacing numeric data by zeroes
                                          alphanumeric data by spaces
              set imf-prz-reale-utf-zero    to true 
              set imf-conf-utf-no           to true 
              set imf-prz-reale-cou-zero    to true 
              set imf-conf-cou-no           to true 
              set imf-prz-reale-cobat-zero  to true 
              set imf-conf-cobat-no         to true
              set imf-prz-reale-pb-zero     to true
              set imf-conf-pb-no            to true

              move imf-codice               to col-id                
              move imf-descrizione          to col-des               
              move imf-prz-reale-utf        to col-prez-real-utf     
              move imf-conf-utf             to col-conf-utf          
              move imf-prz-reale-cou        to col-prz-reale-cou     
              move imf-conf-cou             to col-conf-cou          
              move imf-prz-reale-cobat      to col-prz-reale-cobat   
              move imf-conf-cobat           to col-conf-cobat        
              move imf-prz-reale-pb         to col-prz-reale-pb      
              move imf-conf-pb              to col-conf-pb            

              modify form1-gd-1(riga), record-data = rec-grid

              perform ABILITAZIONI

              move imf-rec   to old-imf-rec 

              add 1 to tot-righe giving event-data-2
              perform COLORE-RIGA
           end-if 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           if nuovo
              perform VALORE-RIGA
              if imf-rec not = old-imf-rec
                 set NoSalvato to true
              else
                 set SiSalvato to true
              end-if
           end-if.

           set tutto-ok to true.
           if NoSalvato
              display message box MSG-Salvare-le-modifiche, 
                             title = titolo
                             type = mb-yes-no-cancel
                             giving scelta

              evaluate scelta
              when mb-yes    
                   perform SALVA
              when mb-cancel 
                   set errori    to true
              when mb-no     
                   set tutto-ok  to true
                   set SiSalvato to true
                   set ricarica  to true
              end-evaluate
           end-if 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           if SiSalvato or mod = ZERO 
              exit paragraph 
           end-if.

           inquire form1-gd-1, cursor-x in colonna, 
                               cursor-y in riga.


           perform varying colonna from 1 by 1 
                     until colonna > 78-max-colonne
              perform CONTROLLO
              
              if errori 
                 exit perform 
              end-if
           end-perform.

           perform CONTR-CONGRUENZA-IMPOSTE

           if tutto-ok
              perform VALORIZZA-DATI-COMUNI
              write imf-rec 
                 invalid 
                    rewrite imf-rec 
              end-write
              set sisalvato to true
              set vecchio   to true
           else                   
              perform ABILITAZIONI
           end-if                  
           .
      * <TOTEM:END>

       SPOSTAMENTO.
      * <TOTEM:PARA. SPOSTAMENTO>
           inquire form1-gd-1, cursor-x = colonna, cursor-y = riga.

           if event-data-2 not = riga
              perform VALORE-RIGA
              if imf-codice = ZERO
                 modify form1-gd-1, record-to-delete riga
                 set vecchio to true
              else  
                 perform SALVA
                 if errori
                    move riga    to event-data-2
                    move colonna to event-data-1 | (ISACCO)
                    set event-action to event-action-fail
                 end-if
              end-if
           else
              if colonna not = event-data-1
                 perform CONTROLLO
                 if errori
                    set event-action to event-action-fail
                 end-if
              end-if
           end-if.

      * VALORIZZO L'OLD DELLA RIGA SU CUI SONO ANDATO
           move event-data-2 to riga.
           move event-data-1 to colonna.
           perform VALORE-OLD-RIGA.

      * COLORAZIONE RIGA IN GRID
           perform COLORE-RIGA.

           perform GRID-HELP 
           .
      * <TOTEM:END>

       STAMPA.
      * <TOTEM:PARA. STAMPA>
           set stampa       to true.
      *     perform paragrafo-stampa 
           .
      * <TOTEM:END>

       VALORE-RIGA.
      * <TOTEM:PARA. VALORE-RIGA>
      *     inquire form1-gd-1(riga, 1), cell-data imf-codice.
      *     inquire form1-gd-1(riga, 2), cell-data imf-descrizione.
      *     inquire form1-gd-1(riga, 3), cell-data imf-olio.
      *     inquire form1-gd-1(riga, 4), cell-data imf-utf.
      *     inquire form1-gd-1(riga, 5), cell-data imf-ic-cou.
      *     inquire form1-gd-1(riga, 6), cell-data imf-cobat.
      *     inquire form1-gd-1(riga, 7), cell-data imf-colonna.
      *     inquire form1-gd-1(riga, 8), cell-data imf-num-reg-c.
      *     inquire form1-gd-1(riga, 9), cell-data imf-num-reg-s1.
      *     inquire form1-gd-1(riga, 10), cell-data imf-num-reg-s2.
      *     inquire form1-gd-1(riga, 11), cell-data col-oper.

           inquire form1-gd-1(riga), RECORD-DATA rec-grid

           move col-id                to imf-codice
           move col-des               to imf-descrizione
           move col-prez-real-utf     to imf-prz-reale-utf   
           move col-conf-utf          to imf-conf-utf        
           move col-prz-reale-cou     to imf-prz-reale-cou   
           move col-conf-cou          to imf-conf-cou        
           move col-prz-reale-cobat   to imf-prz-reale-cobat 
           move col-conf-cobat        to imf-conf-cobat
           move col-prz-reale-pb      to imf-prz-reale-pb
           move col-conf-pb           to imf-conf-pb 
           .
      * <TOTEM:END>

       VALORE-OLD-RIGA.
      * <TOTEM:PARA. VALORE-OLD-RIGA>
           if vecchio
              inquire form1-gd-1(riga, 1) cell-data old-imf-codice
              move old-imf-codice         to imf-codice
              read impforn                no lock
              move imf-rec                to old-imf-rec
           else
              initialize old-imf-rec replacing numeric data by zeroes
                                          alphanumeric data by spaces
              set old-imf-prz-reale-utf-zero   to true 
              set old-imf-conf-utf-no          to true 
              set old-imf-prz-reale-cou-zero   to true 
              set old-imf-conf-cou-no          to true 
              set old-imf-prz-reale-cobat-zero to true 
              set old-imf-conf-cobat-no        to true
              set old-imf-prz-reale-pb-zero    to true
              set old-imf-conf-pb-no           to true

           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-DATI-COMUNI.
      * <TOTEM:PARA. VALORIZZA-DATI-COMUNI>
      *    se non ho valorizzato i campi di creazione li valorizzo
           if imf-data-creazione   = zero
              accept imf-data-creazione from century-date
           end-if.
           if imf-ora-creazione    = zero
              accept imf-ora-creazione  from time
           end-if.
           if imf-utente-creazione = space
              move user-codi to imf-utente-creazione
           end-if.

           accept imf-data-ultima-modifica from century-date.
           accept imf-ora-ultima-modifica  from time.
           move user-codi    to imf-utente-ultima-modifica 
           .
      * <TOTEM:END>

       METTI-IN-GRIGLIA.
      * <TOTEM:PARA. METTI-IN-GRIGLIA>
           move imf-codice            to col-id
           move imf-descrizione       to col-des
           move imf-prz-reale-utf     to col-prez-real-utf  
           move imf-conf-utf          to col-conf-utf     
           move imf-prz-reale-cou     to col-prz-reale-cou  
           move imf-conf-cou          to col-conf-cou     
           move imf-prz-reale-cobat   to col-prz-reale-cobat
           move imf-conf-cobat        to col-conf-cobat   
           move imf-prz-reale-pb      to col-prz-reale-pb 
           move imf-conf-pb           to col-conf-pb      


           modify form1-gd-1, record-to-add = rec-grid

      *         10 imf-dati-comuni 
           .
      * <TOTEM:END>

       CONTR-CONGRUENZA-IMPOSTE.
      * <TOTEM:PARA. CONTR-CONGRUENZA-IMPOSTE>
      
           evaluate true
           when imf-prz-reale-utf-piu
                set imp-piu     to true
           when imf-prz-reale-utf-meno
                set imp-meno    to true
           when imf-prz-reale-utf-zero
                set imp-neutre  to true
           end-evaluate


           evaluate true
           when imp-piu
                if imf-prz-reale-cou-meno   
                   set errori   to true
                end-if
           when imp-meno
                if imf-prz-reale-cou-piu   
                   set errori   to true
                end-if
           when imp-neutre
                evaluate true
                when imf-prz-reale-cou-piu
                     set imp-piu     to true
                when imf-prz-reale-cou-meno
                     set imp-meno    to true
                when imf-prz-reale-cou-zero
                     set imp-neutre  to true
                end-evaluate
           end-evaluate

           if tutto-ok
              evaluate true
              when imp-piu
                   if imf-prz-reale-cobat-meno   
                      set errori   to true
                   end-if
              when imp-meno
                   if imf-prz-reale-cobat-piu   
                      set errori   to true
                   end-if
              when imp-neutre
                   evaluate true
                   when imf-prz-reale-cobat-piu
                        set imp-piu     to true
                   when imf-prz-reale-cobat-meno
                        set imp-meno    to true
                   when imf-prz-reale-cobat-zero
                        set imp-neutre  to true
                   end-evaluate
              end-evaluate
           end-if

           if tutto-ok
              evaluate true
              when imp-piu
                   if imf-prz-reale-pb-meno   
                      set errori   to true
                   end-if
              when imp-meno
                   if imf-prz-reale-pb-piu   
                      set errori   to true
                   end-if
              end-evaluate
           end-if

           if errori
              display message box "Tipi imposte non compatibili"
                      title = tit-err
                      icon mb-warning-icon
           end-if
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       NUOVO-LinkTo.
      * <TOTEM:PARA. NUOVO-LinkTo>
           inquire tool-nuovo, enabled in e-nuovo.
           if e-nuovo = 1
              perform NUOVO
           end-if 

           .
      * <TOTEM:END>
       CANCELLA-LinkTo.
      * <TOTEM:PARA. CANCELLA-LinkTo>
           inquire tool-cancella, enabled in e-cancella.
           if e-cancella = 1
              perform CANCELLA
           end-if 
           .
      * <TOTEM:END>
       TOOL-SALVA-LinkTo.
      * <TOTEM:PARA. TOOL-SALVA-LinkTo>
           inquire tool-salva, enabled in e-salva.
           if e-salva = 1
              perform salva
           end-if 
           .
      * <TOTEM:END>
       ANTEPRIMA-LinkTo.
      * <TOTEM:PARA. ANTEPRIMA-LinkTo>
           inquire tool-anteprima, enabled in e-anteprima.
           if e-anteprima = 1
              perform ANTEPRIMA
           end-if 
           .
      * <TOTEM:END>
       TOOL-MODIFICA-LinkTo.
      * <TOTEM:PARA. TOOL-MODIFICA-LinkTo>
           inquire tool-modifica, enabled in e-modifica
           if e-modifica = 1
              perform MODIFICA
           end-if 
           .
      * <TOTEM:END>
       STAMPA-LinkTo.
      * <TOTEM:PARA. STAMPA-LinkTo>
           inquire tool-stampa, enabled in e-stampa
           if e-stampa = 1
              perform STAMPA
           end-if 
           .
      * <TOTEM:END>
       I-O-BLOCCO.
      * <TOTEM:PARA. I-O-BLOCCO>
           SET LK-BL-CANCELLAZIONE TO TRUE.
           MOVE COMO-PROG-ID       TO LK-BL-PROG-ID.
           CALL "BLOCKPGM"  USING LK-BLOCKPGM 
           .
      * <TOTEM:END>
       PAS004P-Ev-Before-Program.
      * <TOTEM:PARA. PAS004P-Ev-Before-Program>
           move LK-BL-PROG-ID    TO COMO-PROG-ID 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Finish-Entry.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Finish-Entry>
           inquire form1-gd-1, cursor-y in riga.
           perform VALORE-RIGA.

           perform  CONTROLLO.

           if errori 
              set event-action to event-action-fail 
              perform VALORE-RIGA
           end-if.

           if imf-rec not = old-imf-rec
              set NoSalvato to true                            
           else 
              set SiSalvato to true
           end-if 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Begin-Entry.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Begin-Entry>
           inquire form1-gd-1,     cursor-x = colonna,
                                   cursor-y = riga.
                         
           perform VALORE-RIGA.

           evaluate colonna
           when 78-col-id
                if imf-codice not = ZERO
                   set event-action to event-action-fail
                end-if
           when 78-col-des
           when 78-col-prez-real-utf
           when 78-col-conf-utf
           when 78-col-prz-reale-cou
           when 78-col-conf-cou
           when 78-col-prz-reale-cobat
           when 78-col-conf-cobat
           when 78-col-prz-reale-pb
           when 78-col-conf-pb
                if mod = 0
                   set event-action to event-action-fail
                else
                   if imf-codice = ZERO
                      set event-action to event-action-fail
                      modify form1-gd-1, cursor-y = riga, cursor-x = 1
                   end-if   
                end-if
           when other
                set event-action to event-action-fail
           end-evaluate 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-End-Drag.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-End-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Goto-Cell.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Goto-Cell>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Goto-Cell-Drag.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Goto-Cell-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Goto-Cell-Mouse.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Goto-Cell-Mouse>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Begin-Drag.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Begin-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       TOOL-CERCA-LinkTo.
      * <TOTEM:PARA. TOOL-CERCA-LinkTo>
           inquire tool-cerca, enabled in e-cerca.
           if e-cerca = 1
              perform CERCA
           end-if 
           .
      * <TOTEM:END>
       ef-cod-BeforeProcedure.
      * <TOTEM:PARA. ef-cod-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-des-BeforeProcedure.
      * <TOTEM:PARA. ef-des-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-cod-AfterProcedure.
      * <TOTEM:PARA. ef-cod-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       ef-des-AfterProcedure.
      * <TOTEM:PARA. ef-des-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       rb-cod-BeforeProcedure.
      * <TOTEM:PARA. rb-cod-BeforeProcedure>
           modify control-handle, color = colore-nu
           .
      * <TOTEM:END>
       rb-des-BeforeProcedure.
      * <TOTEM:PARA. rb-des-BeforeProcedure>
           modify control-handle, color = colore-nu
           .
      * <TOTEM:END>
       rb-cod-AfterProcedure.
      * <TOTEM:PARA. rb-cod-AfterProcedure>
           modify control-handle, color = colore-or
           .
      * <TOTEM:END>
       rb-des-AfterProcedure.
      * <TOTEM:PARA. rb-des-AfterProcedure>
           modify control-handle, color = colore-or
           .
      * <TOTEM:END>
       pb-esegui-LinkTo.
      * <TOTEM:PARA. pb-esegui-LinkTo>
           set tutto-ok to true.

           if tipo-ricerca = 1
              inquire ef-cod, value in cod
              if cod not > spaces 
                 set errori  to true 
              else
                 move cod    to TextToSearch
                 call "c$justify" using TextToSearch, "L"
                 move spaces to des
                 display ef-des
              end-if 
           else                           
              inquire ef-des, value in des
              if des = spaces 
                 set errori to true 
              else
                 move des to TextToSearch   
                 move ZERO to cod
                 display ef-cod
              end-if
           end-if.

           if tutto-ok 
              move 27 to key-status 
              set Ricerca to true
           end-if.

