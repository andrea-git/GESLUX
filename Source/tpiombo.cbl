              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-sca4-toa, VALUE IN tpb-pb-sca-9-a
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-sca4-toa-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-euro4a, VALUE IN tpb-pb-sca-9-euro
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-euro4a-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-sca5-toa, VALUE IN tpb-pb-sca-10-a
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-sca5-toa-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-euro5a, VALUE IN tpb-pb-sca-10-euro
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-euro5a-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

