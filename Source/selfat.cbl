       PB-ESEGUI-LinkTo.
      * <TOTEM:PARA. PB-ESEGUI-LinkTo>
           move 0 to LinkStatus.
           set tutto-ok     to true.

           inquire ef-anno,  value in anno.
           move       anno         to tor-anno btno-anno.
           inquire ef-num,   value in numero.
           move       numero       to tor-numero.
           inquire ef-num-b, value in numero-bozza.
           move       numero-bozza to btno-numero.
                                
           if tor-numero  not = 0 and
              btno-numero not = 0
              set errori to true
              display message "Inserire n. documento o n. bozza"
                        title tit-err
                         icon 2
           end-if.

           set LinkFattMan to true.

           if tor-numero not = 0 and tutto-ok
              read tordini no lock
                   invalid 
                   set errori to true
                   display message "Ordine NON valido"
                             title tit-err
                              icon 2
               not invalid
                   if tor-fattura-manuale
                      set errori to true
                      display message "Impossibile associare fattura man
      -    "uale"
                                title tit-err
                                 icon 2
                   end-if
              end-read
           end-if.

           if btno-numero not = 0 and tutto-ok
              read btnotacr no lock
                   invalid 
                   set errori to true
                   display message "Bozza non caricata"
                             title tit-err
                              icon 2
               not invalid
                   |dato che le righe addebito non sono obbligatorie
                   |controllo che la bozza di partenza ne abbia 
                   move low-value   to brno-rec
                   move btno-chiave to brno-chiave
                   start brnotacr key >= brno-chiave
                         invalid
                         set errori to true
                         display message "BOZZA SENZA RIGHE!!!"
                                   title tit-err
                                    icon 2
                     not invalid
                         read brnotacr next
                         if brno-anno   not = btno-anno or
                            brno-numero not = btno-numero
                            set errori to true
                            display message "BOZZA SENZA RIGHE!!!"
                                      title tit-err
                                       icon 2
                         else
                            if brno-nota
                               set errori to true
                               display message 
                               "Bozza senza righe per addebito!!!"
                                         title tit-err
                                          icon 2
                            end-if
                         end-if
                   end-start
                   if btno-num-fm  not = 0 or
                      btno-data-fm not = 0 and tutto-ok
                      move btno-num-fm to num-edit
                      display message 
                      "ATTENZIONE!!!"
               x"0d0a""Già assegnata fattura manuale n. " num-edit"."
               x"0d0a""Prseguire comunque?"
                                title tit-err
                                 type mb-yes-no
                                 icon 2
                               giving scelta
                      if scelta = mb-no
                         set errori to true
                      end-if
                   end-if
              end-read
              set  LinkBozzaMan to true
              move btno-anno    to tor-anno-fattura
              move btno-numero  to tor-num-fattura
           end-if.

           perform CANCELLA-COLORE.

           if tutto-ok
              modify Form-doc-handle, visible false
              if LinkFattMan
                 if tor-numero not = 0
                    move 0 to LinkStatus
                    move livello-abil     to sav-livello-abil
                    move 1                to livello-abil
                    move "selfat"         to LinkPgm
                    move Link-TipoFat     to SaveTipo
                    if tor-ordine
                       move tor-anno         to LinkAnno   FatAnno
                       move tor-numero       to LinkNumero FatNumero
                       call   "gordcvar"  using  lk-blockpgm,
                                                 user-codi,
                                                 livello-abil,
                                                 gordcvar-linkage
                       cancel "gordcvar"
                    else
                       move tor-anno          to FatAnno
                       move tor-numero        to FatNumero
                       call   "fatmanvar"  using lk-blockpgm,
                                                 user-codi,
                                                 livello-abil,
                                                 fatman-linkage
                       cancel "fatmanvar"
                    end-if
                    move SaveTipo         to Link-TipoFat
                    move sav-livello-abil to livello-abil
                    if LinkStatus = 1
                       move mb-yes to scelta
                    else
                       display message "La fattura è quella desiderata?"
                                 title titolo
                                  type mb-yes-no
                                giving scelta
                    end-if
                 else
                    move mb-yes to scelta
                 end-if
              else
                 if btno-numero not = 0
                    move 0                to LinkStatusNota
                    move livello-abil     to sav-livello-abil
                    move 1                to livello-abil
                    move btno-anno        to NotaAnno
                    move btno-numero      to NotaNumero
                    move "selnota"        to LinkPgm
                    call   "bnotavar"  using lk-blockpgm,
                                             user-codi,
                                             livello-abil,
                                             notacr-linkage
                    cancel "bnotavar"
                    move sav-livello-abil to livello-abil
                    if LinkStatusNota = 1
                       move mb-yes      to scelta
                       move btno-anno   to FatAnno
                       move btno-numero to FatNumero
                    else
                       move mb-no to scelta
                    end-if
                 else
                    move mb-yes to scelta
                 end-if
              end-if

              if scelta = mb-yes
                 if tipo-prezzo = 1
                    set LinkMedesimo   to true
                 else
                    set LinkCostoMedio to true
                 end-if
                 move  2 to LinkStatus
                 move 27 to key-status
              else
                 modify form-doc-handle, visible true
              end-if

           else
              move 1002 to control-id
           end-if.

           move    4    to accept-control.
           modify pb-esegui, bitmap-number = 1.
           modify pb-cancel, bitmap-number = 1 
           .
      * <TOTEM:END>
       Form1-Pb-2b-BeforeProcedure.
      * <TOTEM:PARA. Form1-Pb-2b-BeforeProcedure>
           modify pb-cancel, bitmap-number = 2 
           .
      * <TOTEM:END>
       Form1-Pb-2b-AfterProcedure.
      * <TOTEM:PARA. Form1-Pb-2b-AfterProcedure>
           modify pb-cancel, bitmap-number = 1 
           .
      * <TOTEM:END>
       TOOL-CERCA-LinkTo.
      * <TOTEM:PARA. TOOL-CERCA-LinkTo>
           inquire tool-cerca, enabled in e-cerca.
           if e-cerca = 1 perform CERCA end-if 
           .
      * <TOTEM:END>
       Screen1-Rb-1-BeforeProcedure.
      * <TOTEM:PARA. Screen1-Rb-1-BeforeProcedure>
           modify control-handle, color = colore-nu
           .
      * <TOTEM:END>
       Screen1-Rb-1-AfterProcedure.
      * <TOTEM:PARA. Screen1-Rb-1-AfterProcedure>
           modify control-handle, color = colore-or
           .
      * <TOTEM:END>
       rb-1-LinkTo.
      * <TOTEM:PARA. rb-1-LinkTo>
           move 1 to tipo-prezzo 
           .
      * <TOTEM:END>
       rb-2-LinkTo.
      * <TOTEM:PARA. rb-2-LinkTo>
           move 2 to tipo-prezzo 
           .
      * <TOTEM:END>

      *{TOTEM}END

      *{TOTEM}SHOW-MSG
       MESSAGE-FOR-FILEERROR.
           CALL "C$RERRNAME" USING TOTEM-MSG-ERR-FILE
           CALL "C$RERR" USING EXTEND-STAT, TEXT-MESSAGE
              MOVE PRIMARY-ERROR TO TOTEM-MSG-ID
           .

       SHOW-TRANSACTION-ROUTINE.
           IF TOTEM-TRANSACTION-FLAG = SPACES
              IF TRANSACTION-STATUS NOT = 0
                 STRING "TRANSACTION ERROR ", TRANSACTION-STATUS
                    DELIMITED BY SIZE INTO TOTEM-MSG-1
              END-IF
           ELSE
              PERFORM SHOW-MSG-ROUTINE
              IF TRANSACTION-STATUS = 0
                 MOVE "Transazione terminata con Rollback." TO 
           TOTEM-MSG-3
              ELSE
                 STRING "TRANSACTION ERROR ", TRANSACTION-STATUS
                    DELIMITED BY SIZE INTO TOTEM-MSG-3
              END-IF
           END-IF
           .

       SHOW-MSG-ROUTINE.
           MOVE SPACE TO TOTEM-MSG-1 TOTEM-MSG-2 TOTEM-MSG-3
           EVALUATE TOTEM-MSG-ID
               WHEN "10"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Nessun altro dato." TO TOTEM-MSG-2
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "22"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Chiave Duplicata." TO TOTEM-MSG-2
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "23"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Raggiunta fine file." TO TOTEM-MSG-2
                    MOVE MB-WARNING-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "101"
                    MOVE "Terminare l'applicazione?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "201"
                    MOVE "Aggiungere un nuovo record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "202"
                    MOVE "Aggiornare il Record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "203"
                    MOVE "Cancellare il Record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "204"
                    MOVE "Chiave duplicata." TO TOTEM-MSG-1
                    MOVE MB-WARNING-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "301"
                    MOVE "Inserimento avvenuto con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "302"
                    MOVE "Aggiornamento avvenuto con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "303"
                    MOVE "Cancellazione avvenuta con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "401"
                    MOVE "Shell non trovata." TO TOTEM-MSG-1
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN OTHER
                    MOVE TEXT-MESSAGE TO TOTEM-MSG-1
                    STRING "FILE:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-3
                    MOVE 0 TO TOTEM-IDX1
                    INSPECT TOTEM-MSG-3 TALLYING TOTEM-IDX1
                       FOR TRAILING SPACE
                    STRING TOTEM-MSG-3(1:TOTEM-MSG-LENGTH - 
           TOTEM-IDX1), 
                       ", FILE STATUS ", PRIMARY-ERROR "," 
                       SECONDARY-ERROR
                    DELIMITED BY SIZE INTO TOTEM-MSG-2
                    MOVE SPACES TO TOTEM-MSG-3
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
           END-EVALUATE
           PERFORM MESSAGE-BOX-ROUTINE
           .

       MESSAGE-BOX-ROUTINE.
           MOVE 1 TO TOTEM-MSG-TEXT-POINTER
           IF TOTEM-MSG-1 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-1 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              STRING TOTEM-MSG-1( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                 DELIMITED BY SIZE
                 INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-2 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-2 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              IF TOTEM-MSG-TEXT-POINTER > 1
                 STRING X"0A" DELIMITED BY SIZE
                     INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
              END-IF
              STRING TOTEM-MSG-2( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                  DELIMITED BY SIZE
                  INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-3 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-3 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              IF TOTEM-MSG-TEXT-POINTER > 1
                 STRING X"0A" DELIMITED BY SIZE
                     INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
              END-IF
              STRING TOTEM-MSG-3( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                  DELIMITED BY SIZE
                  INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-TEXT-POINTER = 1
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-TEXT TALLYING TOTEM-MSG-SIZE FOR 
           TRAILING SPACE
                  COMPUTE TOTEM-MSG-TEXT-POINTER = 
           TOTEM-MSG-FULL-LENGTH - TOTEM-MSG-SIZE + 1
           END-IF
           MOVE LOW-VALUES TO TOTEM-MSG-TEXT(TOTEM-MSG-TEXT-POINTER : 1 
           )
           .

       MESSAGE-BOX-DISPLAY.
           PERFORM SHOW-MSG-ROUTINE
           PERFORM MESSAGE-BOX-ROUTINE
           DISPLAY MESSAGE BOX TOTEM-MSG-TEXT
               TITLE IS TOTEM-MSG-TITLE
               TYPE  IS TOTEM-MSG-BUTTON-TYPE
               ICON  IS TOTEM-MSG-DEFAULT-BUTTON
               RETURNING TOTEM-MSG-RETURN-VALUE
           .

      *{TOTEM}END

