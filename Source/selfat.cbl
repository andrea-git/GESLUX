      * USER DEFINE PARAGRAPH
       CALL-PGM.
      * <TOTEM:PARA. CALL-PGM>
           modify form1-handle, visible false.

           evaluate true
           when LinkFTTR
           when LinkFTND
                perform FORM-DOC-OPEN-ROUTINE
           when LinkFTMA
                move 0 to LinkStatus
                move 0 to FatAnno
                move 0 to FatNumero
           end-evaluate.

           if LinkStatus >= 0
              call   "fatman" using lk-blockpgm
                                    user-codi
                                    livello-abil
                                    fatman-linkage
              cancel "fatman"
              move 27 to key-status
           else
              modify pb-fttr, bitmap-number = 1
              modify pb-ftnd, bitmap-number = 3
              modify pb-ftma, bitmap-number = 5
              evaluate true
              when LinkFTTR   move 5001 to control-id
              when LinkFTND   move 5002 to control-id
              when LinkFTMA   move 5003 to control-id
              end-evaluate
              move 4 to accept-control
              modify form1-handle, visible true
           end-if 
           .
      * <TOTEM:END>

       CANCELLA-TMP.
      * <TOTEM:PARA. CANCELLA-TMP>
           delete file zoom-tordini 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 1000
                perform RIEMPI-TMP

                move "zoom-tordini+fat"   to como-file

                inquire ef-anno, value in zoom-tor-anno
                inquire ef-num,  value in zoom-tor-numero

                if trovato
                   move path-zoom-tordini to ext-file
                   call   "zoom-gt"   using como-file, zoom-tor-rec
                                     giving stato-zoom
                   cancel "zoom-gt"
                else
                   move 1 to stato-zoom
                end-if
                perform CANCELLA-TMP

                if stato-zoom = 0
                   move zoom-tor-numero to numero
                   display ef-num
                end-if
           when 2000
                inquire ef-anno,  value in btno-anno
                inquire ef-num-b, value in btno-numero

                move "btnotacr"    to como-file

                call   "zoom-gt"    using como-file, btno-rec
                                   giving stato-zoom
                cancel "zoom-gt"

                if stato-zoom = 0
                   move btno-numero to numero-bozza
                   display ef-num-b
                end-if
           end-evaluate 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "color-custom.cpy" 
           .
      * <TOTEM:END>

       RIEMPI-TMP.
      * <TOTEM:PARA. RIEMPI-TMP>
           set record-ok to true.

           accept  path-zoom-tordini  from environment "PATH_ST".
           accept  como-data          from century-date.
           accept  como-ora           from time.
           inspect path-zoom-tordini  replacing trailing
                                      spaces by low-value.
           string path-zoom-tordini   delimited by low-value
                  "zoom-tordini"      delimited by size
                  "_"                 delimited by size
                  como-data           delimited by size
                  "_"                 delimited by size
                  como-ora            delimited by size
                  into path-zoom-tordini
           end-string.

           set trovato to false.
           open output zoom-tordini.
           initialize tor-rec.
           move esercizio to tor-anno.

           start tordini key  is >= tor-chiave
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read tordini next no lock at end exit perform 
           end-read
                    if tor-anno not = esercizio exit perform end-if

                    set record-ok to true

                    if tor-fattura-manuale
                       set record-ok to false
                    end-if

                    if record-ok
                       move tor-anno           to zoom-tor-anno
                       move tor-numero         to zoom-tor-numero
                       move tor-data-ordine    to zoom-tor-data-ordine
                       move tor-causale        to tca-codice
                       read tcaumag 
                            invalid initialize tca-rec 
                       end-read
                       move tca-caus-trasporto to zoom-tor-causale
                       move tor-cod-cli        to zoom-tor-cod-cli
                                                  cli-codice
                                                  des-codice
                       set cli-tipo-C          to true
                       move tor-prg-destino    to zoom-tor-prg-destino
                                                  des-prog
                       read clienti no lock
                            invalid initialize cli-rec
                       end-read
                       move cli-ragsoc-1       to zoom-tor-cli-ragsoc-1

                       read destini no lock
                            invalid initialize des-rec
                                    move cli-localita to des-localita
                       end-read
                       move des-localita     to zoom-tor-des-localita

                       move tor-num-fattura  to zoom-tor-num-fat
                       move tor-data-fattura to zoom-tor-data-fat

                       write zoom-tor-rec invalid continue end-write
                       set trovato to true
                    end-if

                 end-perform
           end-start.

           close zoom-tordini 
           .
      * <TOTEM:END>

