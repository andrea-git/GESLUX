      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate CONTROL-ID   
      *
           when 78-ID-ef-tipo
                inquire ef-tipo, value in tcl-codice
                move "ttipocli" to como-file
                call   "zoom-gt" using como-file, tcl-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-tipo,   value tcl-codice
                   modify lab-tipo,  value tcl-descrizione
                end-if          
      *
           when 78-ID-ef-gdo
                inquire ef-gdo    value gdo-codice
                move "tgrupgdo"    to Como-File
                call   "zoom-gt" using como-file, gdo-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO  
                   modify ef-gdo,   value gdo-codice
                   modify lab-gdo,  value gdo-intestazione
                end-if            
      *
           when 78-ID-ef-cod
                set cli-tipo-C to true
                inquire ef-cod    value cli-codice
                move "clienti-CF-all"    to Como-File
                call   "zoom-gt" using como-file, cli-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO  
                   modify ef-cod,   value cli-codice
                   modify lab-cod,  value cli-ragsoc-1
                end-if           
      *
           when 78-ID-ef-des
                inquire ef-cod    value des-codice
                inquire ef-des    value des-prog
                move "clienti-des"   to Como-File
                call   "zoom-gt" using como-file, des-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-des,   value des-prog
                   modify lab-des,  value des-ragsoc-1
                end-if
      *
           when 78-ID-ef-marca
                inquire ef-marca value mar-codice
                move "tmarche"    to Como-File
                call   "zoom-gt" using como-file, mar-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-marca,   value mar-codice
                end-if           
      *
           when 78-ID-ef-art                      
                inquire ef-marca  value art-marca-prodotto
                inquire ef-art    value art-codice
                if art-marca-prodotto = 0
                   move "articoli"  to Como-File
                   call   "zoom-gt" using como-file, art-rec
                                   giving stato-zoom
                else
                   move "articoli-marche"  to Como-File
                   call   "zoom-gt" using como-file, art-rec
                                   giving stato-zoom
                end-if                              
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-art,   value art-codice
                end-if
      *
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-ID-ef-data-from                          
                inquire ef-data-from, value in ef-data-from-buf
                move ef-data-from-buf to como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-from-buf
                   display ef-data-from
                end-if
      *                       
           when 78-ID-ef-data-to                              
                inquire ef-data-from, value in ef-data-from-buf
                inquire ef-data-to,   value in ef-data-to-buf
                move ef-data-to-buf to como-data
                if como-data = 0
                   move all "9" to como-data
                else
                   if como-data not = 99999999
                      perform DATE-FORMAT
                   end-if
                end-if
                move como-data to ef-data-to-buf
                display ef-data-to
                perform DATE-TO-FILE
                move como-data        to como-data-to
                move ef-data-from-buf to como-data
                perform DATE-TO-FILE
                move como-data    to como-data-from
                if como-data-to < como-data-from
                   set errori to true
                   display message box "Intervallo limiti data errato!"
                           title = tit-err
                           icon 2
                   move 78-ID-ef-data-from to control-id
                end-if       
      *         
           when 78-ID-ef-tipo
                inquire ef-tipo, value in tcl-codice
                if tcl-codice not = spaces
                   read ttipocli no lock
                        invalid
                        move spaces to tcl-descrizione
                        set errori to true
                        display message "Inserimento tipologia NON valid
      -    "a"
                                  title tit-err
                                   icon 2
                        move 78-ID-ef-tipo to control-id
                   end-read
                   move tcl-descrizione   to lab-tipo-buf
                   move 0                 to ef-cod-buf cli-codice
                   move "TUTTI I CLIENTI" to lab-cod-buf 
                   move 0                 to ef-des-buf des-prog 
           desf-prog
                   move "TUTTI I DESTINI" to lab-des-buf
                   display ef-tipo lab-tipo ef-cod 
                           lab-cod ef-des   lab-des
                else
                   move "TUTTE LE TIPOLOGIE" to lab-tipo-buf
                   display lab-tipo
                end-if  
                         
                move 0 to e-gdo
                if ef-tipo-buf = spaces
                   move 1 to e-gdo
                else
                   if tcl-gdo-si
                      move 1 to e-gdo
                   end-if
                end-if

                if e-gdo = 0
                   move spaces to ef-gdo-buf
                   move "TUTTI I GRUPPI" to lab-gdo-buf
                end-if
                display ef-gdo lab-gdo
      *
           when 78-ID-ef-gdo 
                if e-gdo = 1
                   inquire ef-gdo, value in gdo-codice
                   if gdo-codice not = spaces
                      read tgrupgdo no lock
                           invalid
                           set errori to true
                           display message "Inserimento codice NON valid
      -    "o"
                                     title tit-err
                                      icon 2
                           move 78-ID-ef-gdo to control-id 
                      end-read
                   else
                      move "TUTTI I GRUPPI" to gdo-intestazione
                   end-if
                   move gdo-intestazione    to lab-gdo-buf
                   display lab-gdo
                else
                   move spaces           to ef-gdo-buf
                   move "TUTTI I GRUPPI" to lab-gdo-buf
                   display ef-gdo lab-gdo
                end-if
                           
      *
           when 78-ID-ef-cod
                set cli-tipo-C to true
                inquire ef-tipo, value in ef-tipo-buf
                if ef-tipo-buf = spaces
                   move cli-tipo-CF to Save-CF

                   inquire ef-cod,   value in cli-codice
                   move spaces to cli-ragsoc-1
                   if cli-codice not = 0
                      read clienti no lock
                           invalid
                           set errori to true
                           display message "Inserimento codice NON valid
      -    "o"
                                     title tit-err
                                      icon 2
                           move 78-ID-ef-cod to control-id 
                      end-read
                   else
                      move "TUTTI I CLIENTI" to cli-ragsoc-1
                   end-if     
                   move cli-ragsoc-1 to lab-cod-buf
                   display lab-cod
                else
                   move 0                 to ef-cod-buf
                   move "TUTTI I CLIENTI" to lab-cod-buf
                   display ef-cod lab-cod
                end-if
      *
           when 78-ID-ef-des
                inquire ef-des,   value in des-prog
                move spaces to des-ragsoc-1 desf-ragsoc-1
                inquire ef-tipo, value in ef-tipo-buf
                if ef-tipo-buf = spaces
                   if des-prog not = 0
                      inquire ef-cod,   value in des-codice
                      read destini no lock
                           invalid
                              set errori to true
                      end-read  
                      move des-localita to lab-des-buf
                      if errori
                         display message "Inserimento destino NON valido
      -    ""
                                   title tit-err
                                    icon 2
                         move 78-ID-ef-des to control-id 
                      end-if
                   else
                      move 0 to des-prog desf-prog
                      move "TUTTI I DESTINI" to lab-des-buf
                   end-if   
                   display lab-des
                else
                   move 0      to ef-des-buf
                   move "TUTTI I DESTINI" to lab-des-buf
                   display ef-des lab-des
                end-if
      *
           when 78-ID-ef-marca
                inquire ef-marca,   value in mar-codice
                move spaces to mar-descrizione
                if mar-codice not = 0
                   read tmarche no lock
                        invalid
                           set errori to true
                           display message"Inserimento marca NON valido"
                                   title = tit-err
                                   icon 2
                           move 78-ID-ef-marca to control-id 
                   end-read
                end-if     
                move mar-descrizione to lab-marca-buf
                display lab-marca
      *                        
           when 78-ID-ef-art
                inquire ef-art,   value in art-codice
                move spaces to art-descrizione
                if art-codice not = 0   
                   read articoli no lock
                        invalid
                           set errori to true
                           display message box "Inserimento articolo NON
      -    " valido"
                                   title = tit-err
                                   icon 2
                           move 78-ID-ef-art to control-id 
                    not invalid
                        inquire ef-marca,   value in mar-codice
                        if mar-codice not = art-marca-prodotto
                           move art-marca-prodotto to ef-marca-buf 
                                                      mar-codice
                           read tmarche no lock
                                invalid move spaces          to 
           lab-marca-buf
                            not invalid move mar-descrizione to 
           lab-marca-buf
                           end-read
                           display ef-marca lab-marca
                        end-if
                   end-read    
                end-if     
                move art-descrizione to lab-art-buf
                display lab-art
                move art-codice to SaveArticolo
      *               
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to STORE-ID        
              move 4          to ACCEPT-CONTROL  
           end-if.

           display form1 
           .
      * <TOTEM:END>

