                 MOVE 1 TO lis-calcolo-imposte-old of lisagente
              ELSE
                 MOVE 0 TO lis-calcolo-imposte-old of lisagente
              END-IF
      * DB_Entry-Field : ef-ini-new
           MOVE ef-ini-new-BUF TO lis-data-inizio-new of lisagente
      * DB_Entry-Field : ef-fine-new
           MOVE ef-fine-new-BUF TO lis-data-fine-new of lisagente
      * DB_Entry-Field : ef-prezzo-new
           MOVE ef-prezzo-new-BUF TO lis-prezzo-new of lisagente
      * DB_CHECK BOX : chk-imposte-new
              IF chk-imposte-new-BUF = 1
                 MOVE 1 TO lis-calcolo-imposte-new of lisagente
              ELSE
                 MOVE 0 TO lis-calcolo-imposte-new of lisagente
              END-IF
      * <TOTEM:EPT. FORM:Form1, FORM:Form1, AfterBufToFld>
          
           move ef-ini-old-buf  to como-data.
           perform DATE-TO-FILE.
           move como-data   to lis-data-inizio-old of lisagente.

           move ef-fine-old-buf to como-data.
           perform DATE-TO-FILE.
           move como-data   to lis-data-fine-old of lisagente.

          
           move ef-ini-new-buf  to como-data.
           perform DATE-TO-FILE.
           move como-data   to lis-data-inizio-new of lisagente.

           move ef-fine-new-buf to como-data.
           perform DATE-TO-FILE.
           move como-data   to lis-data-fine-new of lisagente.

           .
      * <TOTEM:END>
           .

