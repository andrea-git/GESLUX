           When 5009 PERFORM ef-num-da-BeforeProcedure
           When 5010 PERFORM ef-num-a-BeforeProcedure
           END-EVALUATE
           .

       Screen1-Event-Proc.
           .

      * USER DEFINE PARAGRAPH
       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate control-id

           when 78-ID-ef-anno
                inquire ef-anno, value in con-anno
                if con-anno not = esercizio
                   display message "L'anno d'assegnazione non coincide"
                                   " con l'anno d'esercizio"
                             title tit-err
                              icon 2
                   set errori to true
                else
                   read tcontat no lock
                        invalid
                        set errori   to true
                        display message "Anno errato"
                                  title tit-err
                                   icon 2
                   end-read
                end-if                 

           when 78-ID-ef-num-da
                inquire ef-num-da, value in numero-da
                if numero-da = 0
                   move 1 to numero-da
                   move numero-da to ef-num-da-buf
                   display ef-num-da
                end-if

           when 78-ID-ef-num-a
                move 0 to lab-tot-doc-buf
                inquire ef-num-a, value in numero
                if numero = 0
                   move 99999999 to numero
                   move numero to ef-num-a-buf
                   display ef-num-a
                end-if
                inquire ef-num-da, value in numero-da
                if numero < numero-da
                   move 78-ID-ef-num-da to control-id
                   set errori   to true
                   display message "Range numero documento POSTEL errato
      -    ""
                             title tit-err
                              icon 2
                else
                   compute como-numero = ( numero - numero-da ) + 1
                   move como-numero to lab-tot-doc-buf
                end-if
                display lab-tot-doc

           when 78-ID-ef-num-da-e
                inquire ef-num-da-e, value in numero-da
                if numero-da = 0
                   move 1 to numero-da
                   move numero-da to ef-num-da-e-buf
                   display ef-num-da-e
                end-if

           when 78-ID-ef-num-a-e
                move 0 to lab-tot-doc-e-buf
                inquire ef-num-a-e, value in numero
                if numero = 0
                   move 99999999 to numero
                   move numero to ef-num-a-e-buf
                   display ef-num-a-e
                end-if
                inquire ef-num-da-e, value in numero-da
                if numero < numero-da
                   move 78-ID-ef-num-da-e to control-id
                   set errori   to true
                   display message "Range numero documento EDI errato"
                             title tit-err
                              icon 2
                else
                   compute como-numero = ( numero - numero-da ) + 1
                   move como-numero to lab-tot-doc-e-buf
                end-if
                display lab-tot-doc-e

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if            
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "color-custom.cpy".
           copy "calcola-colore-trasparente.cpy".
           copy "contatore-anno.cpy" 
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       pb-sib-AfterProcedure.
      * <TOTEM:PARA. pb-sib-AfterProcedure>
           modify pb-sib, bitmap-number = 1 
           .
      * <TOTEM:END>
       pb-sib-LinkTo.
      * <TOTEM:PARA. pb-sib-LinkTo>
           perform varying control-id from 78-ID-ef-anno by 1
                     until control-id   >  78-ID-ef-num-a-e
              perform CONTROLLO
           end-perform.

           display message "Procedere con l'elaborazione?"
                     title titolo
                      type mb-yes-no
                   default mb-no
                    giving scelta

           if scelta = mb-yes

              modify form1-handle, visible false

              evaluate tipo-invio
              when 1 perform SCR-ELAB-POSTEL-OPEN-ROUTINE
              when 2 perform SCR-ELAB-EDI-OPEN-ROUTINE
              when 3 perform SCR-ELAB-POSTEL-OPEN-ROUTINE
                     perform SCR-ELAB-EDI-OPEN-ROUTINE
              end-evaluate

              if postel-tot-doc > 0 or link-edi-tot-doc > 0
                 move postel-doc-da  to FirstNumberEdit
                 move postel-doc-a   to LastNumberEdit
                 move postel-tot-doc to TotMovEdit

                 move link-edi-doc-da     to FirstNumberEdit-e
                 move link-edi-doc-a      to LastNumberEdit-e
                 move link-edi-tot-doc    to TotMovEdit-e
                 move link-edi-tot-pdf    to TotMovEdit-p
                 perform SCR-FINE-OPEN-ROUTINE
              end-if

              move 27 to key-status

           end-if.

           modify pb-sib, bitmap-number = 1 
           .
      * <TOTEM:END>
       pb-noa-BeforeProcedure.
      * <TOTEM:PARA. pb-noa-BeforeProcedure>
           modify pb-noa, bitmap-number = 2 
           .
      * <TOTEM:END>
       pb-noa-AfterProcedure.
      * <TOTEM:PARA. pb-noa-AfterProcedure>
           modify pb-noa, bitmap-number = 1 
           .
      * <TOTEM:END>
       pb-sib-BeforeProcedure.
      * <TOTEM:PARA. pb-sib-BeforeProcedure>
           modify pb-sib, bitmap-number = 2 
           .
      * <TOTEM:END>
       pb-sia-BeforeProcedure.
      * <TOTEM:PARA. pb-sia-BeforeProcedure>
           .
      * <TOTEM:END>
       pb-sia-AfterProcedure.
      * <TOTEM:PARA. pb-sia-AfterProcedure>
           .
      * <TOTEM:END>
       pb-sia-LinkTo.
      * <TOTEM:PARA. pb-sia-LinkTo>
           move 27 to key-status 
           .
      * <TOTEM:END>
       postel-Ev-Before-Program.
      * <TOTEM:PARA. postel-Ev-Before-Program>
           move LK-BL-PROG-ID    TO COMO-PROG-ID.
           perform CALCOLA-COLORE-TRASPARENTE.
           perform TROVA-CONTATORE-ANNO-ESERCIZIO.
           if errori
              perform POSTEL-EV-AFTER-PROGRAM
              goback
           end-if 
           .
      * <TOTEM:END>
       postel-Ev-After-Program.
      * <TOTEM:PARA. postel-Ev-After-Program>
           SET LK-BL-CANCELLAZIONE TO TRUE.
           MOVE COMO-PROG-ID       TO LK-BL-PROG-ID.
           CALL "BLOCKPGM"  USING LK-BLOCKPGM 
           .
      * <TOTEM:END>
       ef-anno-BeforeProcedure.
      * <TOTEM:PARA. ef-anno-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-anno-AfterProcedure.
      * <TOTEM:PARA. ef-anno-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           perform CONTROLLO 
           .
      * <TOTEM:END>
       scr-elab-Rb-1-BeforeProcedure.
      * <TOTEM:PARA. scr-elab-Rb-1-BeforeProcedure>
           modify control-handle, color = colore-nu
           .
      * <TOTEM:END>
       scr-elab-Rb-1a-BeforeProcedure.
      * <TOTEM:PARA. scr-elab-Rb-1a-BeforeProcedure>
           modify control-handle, color = colore-nu
           .
      * <TOTEM:END>
       scr-elab-Rb-1-AfterProcedure.
      * <TOTEM:PARA. scr-elab-Rb-1-AfterProcedure>
           modify control-handle, color = colore-or
           .
      * <TOTEM:END>
       scr-elab-Rb-1a-AfterProcedure.
      * <TOTEM:PARA. scr-elab-Rb-1a-AfterProcedure>
           modify control-handle, color = colore-or
           .
      * <TOTEM:END>
       rb-fatt-LinkTo.
      * <TOTEM:PARA. rb-fatt-LinkTo>
           add 1 to con-ult-num-postel giving numero.
           move numero to ef-num-da-buf.
           display ef-num-da.  
           
           add 1 to con-ult-num-edi giving numero.
           move numero to ef-num-da-e-buf.
           display ef-num-da-e 
           .
      * <TOTEM:END>
       rb-nc-LinkTo.
      * <TOTEM:PARA. rb-nc-LinkTo>
           add 1 to con-ult-num-nc-postel giving numero.
           move numero to ef-num-da-buf.
           display ef-num-da. 

           add 1 to con-ult-num-nc-edi giving numero.
           move numero to ef-num-da-e-buf.
           display ef-num-da-e  
           .
      * <TOTEM:END>
       ef-num-da-BeforeProcedure.
      * <TOTEM:PARA. ef-num-da-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-num-a-BeforeProcedure.
      * <TOTEM:PARA. ef-num-a-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-num-da-AfterProcedure.
      * <TOTEM:PARA. ef-num-da-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           perform CONTROLLO 
           .
      * <TOTEM:END>
       ef-num-a-AfterProcedure.
      * <TOTEM:PARA. ef-num-a-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           perform CONTROLLO 
           .
      * <TOTEM:END>
       rb-entrambi-LinkTo.
      * <TOTEM:PARA. rb-entrambi-LinkTo>
           move 3 to tipo-invio 
           .
      * <TOTEM:END>
       rb-edi-LinkTo.
