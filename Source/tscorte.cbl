                 IF DataSet1-tscorte-LOCK
                    READ tscorte PREVIOUS WITH LOCK
                 ELSE
                    READ tscorte PREVIOUS WITH NO LOCK
                 END-IF
              ELSE
                 IF DataSet1-tscorte-LOCK
                    READ tscorte NEXT WITH LOCK
