                            else
                               move splcrt2graf-rigo(3:2) to 
           spl-tipo-colonna
                            end-if
                            if spl-tipo-colonna = 99
                               perform SALTO-PAGINA 
                               set spl-stringa   to true
                            end-if
                       end-evaluate            
                    end-if                          
                    exit perform cycle
                 end-if     
                    
                 inspect splcrt2graf-rigo tallying cont for all x"09"

      *****           if cont = 0                                     
                    add spl-passo to spl-riga
                    if extra-passo not = 0
