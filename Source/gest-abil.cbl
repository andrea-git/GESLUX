      * USER DEFINE PARAGRAPH
       ABILITA-DIRECTORY.
      * <TOTEM:PARA. ABILITA-DIRECTORY>
           move h-prog-menu-level to prog-menu-level.
           start prog key is >= prog-key
                 invalid continue
             not invalid
                 move prog-level-1 to save-livello
                 perform until 1 = 2
                    read prog previous at end exit perform end-read
                    if prog-level-1 not = save-livello
                       exit perform
                    end-if
                    if prog-type = 0
                       move link-user to tmp-up-user-id
                       move prog-id   to tmp-up-prog-id
                       move 3         to tmp-up-livello-abil
                       write tmp-up-record
                             invalid rewrite tmp-up-record
                       end-write
                    end-if
                 end-perform
           end-start 
           .
      * <TOTEM:END>

       ABIL-DA-TEMP-A-USER.
      * <TOTEM:PARA. ABIL-DA-TEMP-A-USER>
      *    CANCELLO TUTTI I RECORD RELATIVI ALL'UTENTE
           move low-value to up-prog-id.
           move link-user to up-user-id.

           start user-pgm key >= up-chiave
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read user-pgm next at end exit perform end-read
                    if up-user-id = link-user
                       delete user-pgm invalid continue end-delete
                    else
                       exit perform
                    end-if
                 end-perform
           end-start.

      *    porto le nuove abilitzioni dal temporaneo al file effettivo
           move low-value to tmp-up-prog-id.
           move user-cod  to tmp-up-user-id.

           start tmp-user-pgm key >= tmp-up-chiave
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read tmp-user-pgm next at end exit perform end-read
                    move tmp-up-record   to up-record
                    write up-record invalid continue end-write
                 end-perform
           end-start 
           .
      * <TOTEM:END>

       CANCELLA-USER-PGM.
      * <TOTEM:PARA. CANCELLA-USER-PGM>
      *    CANCELLO TUTTI I RECORD RELATIVI ALL'UTENTE
           move low-value to up-prog-id.
           move user-cod  to up-user-id.

           start user-pgm key not < up-chiave
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read user-pgm next at end exit perform end-read
                    if up-user-id = user-cod
                       delete user-pgm invalid continue end-delete
                    else
                       exit perform
                    end-if
                 end-perform
           end-start 
           .
      * <TOTEM:END>

       CARICA-TREE-VIEW.
      * <TOTEM:PARA. CARICA-TREE-VIEW>
      *    creo il temporaneo delle abilitazioni.
           initialize path-tmp-userpgm.
           accept  path-tmp-userpgm from environment "TEMP".
           inspect path-tmp-userpgm replacing trailing space by 
           low-value
           string  path-tmp-userpgm delimited low-value
                   "/tmp-userprgm-" delimited size
                   link-user        delimited size
              into path-tmp-userpgm
           end-string.
           inspect path-tmp-userpgm replacing trailing low-value by 
           space

           open output tmp-user-pgm.
           close       tmp-user-pgm.
           open i-o    tmp-user-pgm.

           modify Form1-Menu-Tree, reset-list = 1.
           move  "0000000000" to prog-menu-level.

           start prog key >= prog-menu-level
             not invalid perform CICLA-PROG
           end-start.

      ***---
       CICLA-PROG.
           perform until 1 = 2
              read prog next at end exit perform end-read
              perform TRATTA-PROG
           end-perform.

      ***---
       TRATTA-PROG.
           if prog-type = 0
              move 1         to up-livello-abil
              move 1         to num-bitmap
           end-if.

           move prog-id   to up-prog-id.
           move link-user to up-user-id.
           read user-pgm
                invalid
                move 0 to up-livello-abil
                write up-record invalid continue end-write
           end-read.
           move up-record to tmp-up-record.
           write tmp-up-record invalid continue end-write.
           perform VAL-NUM-BITMAP.

           move up-livello-abil to h-livello-abil.
           move prog-menu-level to h-prog-menu-level.

           perform DECIDI-LIVELLO-MENU.

           evaluate true
           when livello-0
                modify form1-menu-tree, parent zero
                modify form1-menu-tree, 
                           item-to-add prog-s-desc
                                giving como-pointer-level-0
                        ensure-visible como-pointer-level-0
                         bitmap-number num-bitmap
                           hidden-data hidden-tree

           when livello-1
                modify Form1-Menu-Tree, parent como-pointer-level-0
                modify Form1-Menu-Tree, 
                           item-to-add prog-s-desc
                                giving como-pointer-level-1
                        ensure-visible como-pointer-level-1
                         bitmap-number num-bitmap
                           hidden-data hidden-tree

           when livello-2
                modify Form1-Menu-Tree, parent como-pointer-level-1
                modify Form1-Menu-Tree,
                           item-to-add prog-s-desc
                                giving como-pointer-level-2
      *                  ensure-visible como-pointer-level-2
                         bitmap-number num-bitmap
                           hidden-data hidden-tree

           when livello-3
                modify Form1-Menu-Tree, parent como-pointer-level-2
                modify Form1-Menu-Tree,
                           item-to-add prog-s-desc
                                giving como-pointer-level-3
      *                  ensure-visible como-pointer-level-3
                         bitmap-number num-bitmap
                           hidden-data hidden-tree

           when livello-4
                modify Form1-Menu-Tree, parent como-pointer-level-3
                modify Form1-Menu-Tree, 
                           item-to-add prog-s-desc
                                giving como-pointer-level-4
      *                  ensure-visible como-pointer-level-4
                         bitmap-number num-bitmap
                           hidden-data hidden-tree


           when livello-5
                modify Form1-Menu-Tree,  parent como-pointer-level-4
                modify Form1-Menu-Tree, 
                           item-to-add prog-s-desc
                                giving como-pointer-level-5
      *                  ensure-visible como-pointer-level-5
                         bitmap-number num-bitmap
                           hidden-data hidden-tree

           end-evaluate.

      ***---
       DECIDI-LIVELLO-MENU.
           set livello-5 to true.
           if prog-level-5 = 0
              set livello-4 to true
           end-if.

           if prog-level-4 = 0
              set livello-3 to true
           end-if.

           if prog-level-3 = 0
              set livello-2 to true
           end-if.

           if prog-level-2 = 0
              set livello-1 to true
           end-if.

           if prog-level-1 = 0
              set livello-0 to true
           end-if 
           .
      * <TOTEM:END>

       DISABILITA-FIGLI.
      * <TOTEM:PARA. DISABILITA-FIGLI>
           set livello-5  to true.
           if prog-level-5 = 0
              set livello-4  to true
           end-if.

           if prog-level-4 = 0
              set livello-3  to true
           end-if.

           if prog-level-3 = 0
              set livello-2  to true
           end-if.

           if prog-level-2 = 0
              set livello-1  to true
           end-if.

           evaluate true
           when livello-1
                move 2    to como-offset
           when livello-2
                move 4    to como-offset
           when livello-3
                move 6    to como-offset
           when livello-4
                move 8    to como-offset
           when livello-5
                move 10   to como-offset
           end-evaluate.

           move low-value to prog-menu-level.
           move h-prog-menu-level(1:como-offset)
             to prog-menu-level(1:como-offset).

           start prog key >= prog-key
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read prog next at end exit perform end-read
                    if h-prog-menu-level(1:como-offset) not = 
                         prog-menu-level(1:como-offset)
                       exit perform
                    end-if
                    perform DISABILITA-FIGLIO
                 end-perform
           end-start.

      ***---
       DISABILITA-FIGLIO.
           read prog
                invalid continue
            not invalid
                move link-user   to tmp-up-user-id
                move prog-id     to tmp-up-prog-id
                read tmp-user-pgm
                     invalid continue
                 not invalid
                     move 0 to tmp-UP-LIVELLO-ABIL
                     rewrite tmp-up-record invalid continue end-rewrite
                end-read
           end-read 
           .
      * <TOTEM:END>

       VAL-NUM-BITMAP.
      * <TOTEM:PARA. VAL-NUM-BITMAP>
           evaluate true
           when prog-t-elaborazione
                move 3 to num-bitmap
           when prog-t-archivio
                move 6 to num-bitmap
           when prog-t-stampa
                move 5 to num-bitmap
           end-evaluate 
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       Form1-AfterInitData.
      * <TOTEM:PARA. Form1-AfterInitData>
           perform CARICA-TREE-VIEW 
           .
      * <TOTEM:END>
       Form1-Menu-Tree-Ev-Msg-Tv-Selchanging.
      * <TOTEM:PARA. Form1-Menu-Tree-Ev-Msg-Tv-Selchanging>
           move event-data-2 to indice-tre-view

           inquire form1-menu-tree(event-data-2), 
                   hidden-data hidden-tree.

           move h-prog-menu-level to prog-menu-level.

           read prog no lock invalid continue end-read.
           move prog-l-desc to ef-descr-buf
           display ef-descr.

           if prog-menu-level = "0000000000" |Menu Geslux
              move 0 to v-altro
           else
              move 1 to v-altro
           end-if.

           if prog-type-2 = spaces
              move "Directory" to prog-type-2
           end-if.

           modify  form1-st-1-handle,
                   panel-index = 1,
                   panel-text  = prog-s-desc,
                   panel-index = 2,
                   panel-text  = prog-id,
                   panel-index = 3,
                   panel-text  = prog-type-2.

           move link-user   to tmp-up-user-id.
           move prog-id     to tmp-up-prog-id.
           read tmp-user-pgm
                invalid
                move h-livello-abil      to como-abilitazione
            not invalid
                move tmp-up-livello-abil to como-abilitazione
           end-read.

           if como-abilitazione = 0
              move 5 to como-abilitazione
           end-if.

           if prog-type = 1
              move "&Disabilitato" to tit-rb-disabilitato
              evaluate true
              when prog-t-archivio
                   move 1                      to vis-livelli
                   modify rb-completa,         line 26,00
              when prog-t-elaborazione
              when prog-t-stampa
                   move 0                      to vis-livelli
                   modify rb-completa,         line 22,00
              end-evaluate
           else
              move "&Disabilitato (invisibile)" to tit-rb-disabilitato
              move 0                            to vis-livelli
              modify rb-completa,               line 22,00
           end-if.

           display  rb-disabilitato
                    rb-visua
                    rb-viaria
                    rb-completa
                    lab 
           .
      * <TOTEM:END>
       pb-ok-BeforeProcedure.
      * <TOTEM:PARA. pb-ok-BeforeProcedure>
           modify pb-ok, bitmap-number = 2 
           .
      * <TOTEM:END>
       pb-ok-AfterProcedure.
      * <TOTEM:PARA. pb-ok-AfterProcedure>
           modify pb-ok, bitmap-number = 1 
           .
      * <TOTEM:END>
       pb-esci-BeforeProcedure.
      * <TOTEM:PARA. pb-esci-BeforeProcedure>
           modify pb-esci, bitmap-number = 2 
           .
      * <TOTEM:END>
       pb-esci-AfterProcedure.
      * <TOTEM:PARA. pb-esci-AfterProcedure>
           modify pb-esci, bitmap-number = 1 
           .
      * <TOTEM:END>
       rb-viaria-LinkTo.
      * <TOTEM:PARA. rb-viaria-LinkTo>
           move 2    to h-livello-abil
                   tmp-up-livello-abil.
           rewrite tmp-up-record invalid continue end-rewrite.
           modify form1-menu-tree(indice-tre-view), 
                  hidden-data hidden-tree.

           if prog-type = 1
              perform ABILITA-DIRECTORY
           end-if 
           .
      * <TOTEM:END>
       rb-disabilitato-LinkTo.
      * <TOTEM:PARA. rb-disabilitato-LinkTo>
           move 5 to h-livello-abil
           move 0 to tmp-up-livello-abil.
           rewrite tmp-up-record invalid continue end-rewrite.
           if prog-type = 0
              perform DISABILITA-FIGLI
           end-if.

           modify form1-menu-tree(indice-tre-view),
                  hidden-data hidden-tree 
           .
      * <TOTEM:END>
       rb-visua-LinkTo.
      * <TOTEM:PARA. rb-visua-LinkTo>
           move 1    to h-livello-abil
                   tmp-up-livello-abil.
           rewrite tmp-up-record invalid continue end-rewrite.
           modify form1-menu-tree(indice-tre-view),
                  hidden-data hidden-tree.

           if prog-type = 1
              perform ABILITA-DIRECTORY
           end-if 
           .
      * <TOTEM:END>
       rb-completa-LinkTo.
      * <TOTEM:PARA. rb-completa-LinkTo>
           move 3    to h-livello-abil
                   tmp-up-livello-abil.
           rewrite tmp-up-record invalid continue end-rewrite.
           modify form1-menu-tree(indice-tre-view), 
                  hidden-data hidden-tree.

           if prog-type = 1
              perform ABILITA-DIRECTORY
           end-if 
           .
      * <TOTEM:END>
       pb-ok-LinkTo.
      * <TOTEM:PARA. pb-ok-LinkTo>
           perform ABIL-DA-TEMP-A-USER.
           perform FORM-ABIL-EXIT 
           .
      * <TOTEM:END>

      *{TOTEM}END

      *{TOTEM}SHOW-MSG
       MESSAGE-FOR-FILEERROR.
           CALL "C$RERRNAME" USING TOTEM-MSG-ERR-FILE
           CALL "C$RERR" USING EXTEND-STAT, TEXT-MESSAGE
              MOVE PRIMARY-ERROR TO TOTEM-MSG-ID
           .

       SHOW-TRANSACTION-ROUTINE.
           IF TOTEM-TRANSACTION-FLAG = SPACES
              IF TRANSACTION-STATUS NOT = 0
                 STRING "TRANSACTION ERROR ", TRANSACTION-STATUS
                    DELIMITED BY SIZE INTO TOTEM-MSG-1
              END-IF
           ELSE
              PERFORM SHOW-MSG-ROUTINE
              IF TRANSACTION-STATUS = 0
                 MOVE "Transazione terminata con Rollback." TO 
           TOTEM-MSG-3
              ELSE
                 STRING "TRANSACTION ERROR ", TRANSACTION-STATUS
                    DELIMITED BY SIZE INTO TOTEM-MSG-3
              END-IF
           END-IF
           .

       SHOW-MSG-ROUTINE.
           MOVE SPACE TO TOTEM-MSG-1 TOTEM-MSG-2 TOTEM-MSG-3
           EVALUATE TOTEM-MSG-ID
               WHEN "10"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Nessun altro dato." TO TOTEM-MSG-2
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "22"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Chiave Duplicata." TO TOTEM-MSG-2
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "23"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Raggiunta fine file." TO TOTEM-MSG-2
                    MOVE MB-WARNING-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "101"
                    MOVE "Terminare l'applicazione?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "201"
                    MOVE "Aggiungere un nuovo record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "202"
                    MOVE "Aggiornare il Record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "203"
                    MOVE "Cancellare il Record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "204"
                    MOVE "Chiave duplicata." TO TOTEM-MSG-1
                    MOVE MB-WARNING-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "301"
                    MOVE "Inserimento avvenuto con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "302"
                    MOVE "Aggiornamento avvenuto con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "303"
                    MOVE "Cancellazione avvenuta con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "401"
                    MOVE "Shell non trovata." TO TOTEM-MSG-1
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN OTHER
                    MOVE TEXT-MESSAGE TO TOTEM-MSG-1
                    STRING "FILE:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-3
                    MOVE 0 TO TOTEM-IDX1
                    INSPECT TOTEM-MSG-3 TALLYING TOTEM-IDX1
                       FOR TRAILING SPACE
                    STRING TOTEM-MSG-3(1:TOTEM-MSG-LENGTH - 
           TOTEM-IDX1), 
                       ", FILE STATUS ", PRIMARY-ERROR "," 
                       SECONDARY-ERROR
                    DELIMITED BY SIZE INTO TOTEM-MSG-2
                    MOVE SPACES TO TOTEM-MSG-3
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
           END-EVALUATE
           PERFORM MESSAGE-BOX-ROUTINE
           .

       MESSAGE-BOX-ROUTINE.
           MOVE 1 TO TOTEM-MSG-TEXT-POINTER
           IF TOTEM-MSG-1 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-1 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              STRING TOTEM-MSG-1( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                 DELIMITED BY SIZE
                 INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-2 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-2 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              IF TOTEM-MSG-TEXT-POINTER > 1
                 STRING X"0A" DELIMITED BY SIZE
                     INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
              END-IF
              STRING TOTEM-MSG-2( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                  DELIMITED BY SIZE
                  INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-3 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-3 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              IF TOTEM-MSG-TEXT-POINTER > 1
                 STRING X"0A" DELIMITED BY SIZE
                     INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
              END-IF
              STRING TOTEM-MSG-3( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                  DELIMITED BY SIZE
                  INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-TEXT-POINTER = 1
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-TEXT TALLYING TOTEM-MSG-SIZE FOR 
           TRAILING SPACE
                  COMPUTE TOTEM-MSG-TEXT-POINTER = 
           TOTEM-MSG-FULL-LENGTH - TOTEM-MSG-SIZE + 1
           END-IF
           MOVE LOW-VALUES TO TOTEM-MSG-TEXT(TOTEM-MSG-TEXT-POINTER : 1 
           )
           .

       MESSAGE-BOX-DISPLAY.
           PERFORM SHOW-MSG-ROUTINE
           PERFORM MESSAGE-BOX-ROUTINE
           DISPLAY MESSAGE BOX TOTEM-MSG-TEXT
               TITLE IS TOTEM-MSG-TITLE
               TYPE  IS TOTEM-MSG-BUTTON-TYPE
               ICON  IS TOTEM-MSG-DEFAULT-BUTTON
               RETURNING TOTEM-MSG-RETURN-VALUE
           .

      *{TOTEM}END

