       pb-grid-elimina-LinkTo.
      * <TOTEM:PARA. pb-grid-elimina-LinkTo>
           if mod-campi   = 0
              exit paragraph
           end-if.

           inquire form1-gd-1, last-row in tot-righe, cursor-y in riga.
           if tot-righe > 1
              if riga <= tot-righe or riga > 2
                 inquire form1-gd-1(riga, 5), cell-data in como-qta
                 if como-qta not = 0
                    display message "Impossibile cancellare riga con eva
      -    "sioni"
                              title titolo
                               icon 2
                    move mb-no to scelta
                 else
                    display message "Cancellare la riga selezionta?"
                              title titolo
                               type mb-yes-no
                               icon 2
                             giving scelta
                 end-if
                 if scelta = mb-yes
                            
                    move col-qta to como-qta
                    set RigaCambiata   to true
                    set PrezzoCambiato to false

                    modify  form1-gd-1, record-to-delete = riga
                    inquire form1-gd-1, last-row in tot-righe
                    if tot-righe = 1                         
                       perform CANCELLA-COLORE
                       perform PULISCI-CAMPI-LABELS
                    else
LUBEXX                 move riga to store-riga
LUBEXX                 move store-riga to riga
                       if riga > tot-righe
                          move tot-righe to riga
                       end-if
                       modify form1-gd-1, cursor-y = riga
                       move riga to event-data-2
                       perform SPOSTAMENTO   
                       set ControllaCampi to true
                    end-if
                    set YesDeleted to true
                 end-if
              else
                 display message "Occorre selezionare una riga"
                           title titolo
              end-if
           end-if 
           .
      * <TOTEM:END>
