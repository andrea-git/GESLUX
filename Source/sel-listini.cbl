       Form1-PROC.
      * <TOTEM:EPT. FORM:Form1, FORM:Form1, BeforeAccept>
           set listino-auto  to false
           if link-fornitore not = zero
              move link-fornitore  to ef-forn-buf
                                      cli-codice
              modify ef-forn, VALUE ef-forn-buf

              set cli-tipo-f  to true
              read clienti no lock
                 invalid
                    move "Fornitore non valido"  to cli-ragsoc-1
              end-read
              move cli-ragsoc-1 to lab-forn-buf
              display lab-forn
              perform CONTROLLA-LISTINO-AUTO
           end-if

           if listino-auto
              set scelto-promo   to true
              perform Form1-Exit
           else
              move "<<HELP>> 0 = TUTTI GLI ARTICOLI" to lab-art-buf
              display lab-art

              move "<<HELP>> 0 = TUTTI I DESTINI" to lab-dest-buf
              display lab-dest

              accept start-data from century-date
              move start-data(7:2) to como-data(1:2)
              move start-data(5:2) to como-data(3:2)
              move start-data(1:4) to como-data(5:4)
              move como-data to ef-ini-buf
              display ef-ini

              move 99999999  to como-data
              move como-data to ef-fine-buf
              display ef-fine

              perform CARICA-GRID
              modify form1-Handle, VISIBLE 1
           end-if.

           .
