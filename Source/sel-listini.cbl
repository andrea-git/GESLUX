       SPOSTAMENTO.
      * <TOTEM:PARA. SPOSTAMENTO>
           inquire gd1, last-row in tot-righe.
           if event-data-2 < 2
              move 2 to event-data-2
           end-if.

           if event-data-2 > tot-righe
              move tot-righe to event-data-2
           end-if.

           modify gd1, start-y = event-data-2, y = event-data-2,
                       start-x = 1,            x = 4,
                       region-color = 481 
           .
      * <TOTEM:END>

       CONTROLLA-LISTINO-AUTO.
      * <TOTEM:PARA. CONTROLLA-LISTINO-AUTO>
           set listino-auto  to true
      *    per prima cosa controllo che abbia un solo destino
           move zero         to cont
           move cli-codice   to desf-codice
           move low-value    to desf-prog

           start destinif key not < desf-chiave
              invalid
                 set listino-auto  to false
              not invalid
                 perform until 1 = 2
                    read destinif next no lock
                       at end
                          exit perform
                    end-read
                    if cli-codice not = desf-codice
                       exit perform
                    end-if
                    add 1 to cont
                    if cont = 2
                       exit perform
                    end-if
                 end-perform
           end-start.
           
           if cont not = 1
              set listino-auto  to false
           end-if

      *    controllo di avere un solo listino
           if listino-auto
              move zero         to cont
              move cli-codice   to tlis-fornitore
              move low-value    to tlis-destino
                                   tlis-ini-val
                                   tlis-fine-val
              start tlistini key >= tlis-chiave-ricerca
                 invalid 
                    continue
                 not invalid
                    perform until 1 = 2
                       read tlistini next 
                          at end 
                             exit perform 
                       end-read
                       if cli-codice not = tlis-fornitore
                          exit perform 
                       end-if
                       add 1 to cont
                       evaluate cont 
                       when 1
                            move tlis-codice   to link-codice
                       when 2
                            exit perform
                       end-evaluate
                    end-perform
              end-start
              if cont not = 1
                 set listino-auto  to false
                 initialize link-codice
              end-if
           end-if.

      *    controllo che l'unico listino sia giusto come data
           if listino-auto
              set rec-ok  to true
              move link-codice  to tlis-codice
              read tlistini no lock
                 invalid
                    continue
              end-read
              accept start-data from century-date
              move 99999999     to como-data-to
              perform CONTROLLO-DATE
              if not rec-ok
                 set listino-auto  to false
              end-if
           end-if 
           .
      * <TOTEM:END>

