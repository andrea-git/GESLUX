                            icon 3
                set errori to true
           when "39"
                display message "File [TREGIONI] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[TREGIONI] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           end-evaluate.

