      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-iva-old
                move "tivaese" to como-file
                move "IV"      to tbliv-codice2
                inquire ef-iva-old, value in tbliv-codice2
                call "zoom-gt"   using como-file, record-tbliv
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tbliv-codice2      to ef-iva-old-buf
                   move tbliv-descrizione1 to lab-iva-old-buf
                   display ef-iva-old lab-iva-old
                end-if

           when 78-ID-ef-iva-new
                move "tivaese" to como-file
                move "IV"      to tbliv-codice2
                inquire ef-iva-new, value in tbliv-codice2
                call "zoom-gt"   using como-file, record-tbliv
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tbliv-codice2      to ef-iva-new-buf
                   move tbliv-descrizione1 to lab-iva-new-buf
                   display ef-iva-new lab-iva-new
                end-if
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.
           evaluate control-id

           when 78-ID-ef-iva-old
                inquire ef-iva-old, value in ef-iva-old-buf
                move "IV"           to tbliv-codice1
                move ef-iva-old-buf to tbliv-codice2
                read tivaese no lock
                     invalid
                     set errori to true
                     move  spaces to tbliv-descrizione1
                     display message "Codice IVA non valido"
                               title tit-err
                                icon 2
                end-read
                move tbliv-descrizione1 to lab-iva-old-buf
                display lab-iva-old

           when 78-ID-ef-iva-new
                inquire ef-iva-new, value in ef-iva-new-buf
                move "IV"           to tbliv-codice1
                move ef-iva-new-buf to tbliv-codice2
                read tivaese no lock
                     invalid
                     set errori to true
                     move  spaces to tbliv-descrizione1
                     display message "Codice IVA non valido"
                               title tit-err
                                icon 2
                end-read
                move tbliv-descrizione1 to lab-iva-new-buf
                display lab-iva-new

                inquire ef-iva-old, value in ef-iva-old-buf
                if ef-iva-new-buf = ef-iva-old-buf
                   display message "Codici IVA uguali"
                             title tit-err
                              icon 2
                   set errori to true
                   move 78-ID-ef-iva-old to control-id
                end-if


           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

