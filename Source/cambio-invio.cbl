      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-cli
                set cli-tipo-c  to true
                inquire ef-cli, value in cli-codice
                move "clienti" to como-file
                call "zoom-gt"  using como-file, cli-rec
                               giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move cli-codice     to ef-cli-buf
                   move cli-ragsoc-1   to lab-cli-buf

                   display ef-cli lab-cli 
                end-if

           when 78-ID-ef-des
                inquire ef-cli, value in des-codice
                set cli-tipo-c to true
                move "clienti-des"    to como-file
                inquire ef-des, value in des-prog
                call "zoom-gt"  using como-file, des-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move des-prog     to ef-des-buf
                   move des-ragsoc-1   to lab-des-buf
                   display ef-des lab-des 
                end-if 
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID            

           when 78-ID-ef-data-from                          
                inquire ef-data-from, value in ef-data-from-buf
                move ef-data-from-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-from-buf
                display ef-data-from
      *                       
           when 78-ID-ef-data-to
                inquire ef-data-from, value in ef-data-from-buf
                move ef-data-from-buf to como-data                      
               
                perform DATE-TO-FILE
                move como-data to como-data-from

                inquire ef-data-to,   value in ef-data-to-buf
                move ef-data-to-buf to como-data
                perform DATE-FORMAT 
                move como-data to ef-data-to-buf
                display ef-data-to                             

                perform DATE-TO-FILE
                move como-data to como-data-to


                if como-data-to < como-data-from
                   set errori to true
                   display message box "Intervallo date errato!"
                           title = tit-err
                           icon 2
                   move 78-ID-ef-data-from to control-id
                end-if
      *               
      *                       
           when 78-ID-ef-cli
                inquire ef-cli, value in ef-cli-buf
                set cli-tipo-C to true
                move ef-cli-buf to cli-codice
                read clienti no lock
                     invalid
                     move spaces to cli-ragsoc-1
                     set errori to true
                     display message "Cliente non valido"
                               title tit-err
                                icon 2
                end-read
                move cli-ragsoc-1 to lab-cli-buf
                display lab-cli
      *                        
           when 78-ID-ef-des
                inquire ef-cli, value in des-codice
                move ef-des-buf to des-prog
                if des-prog = 0
                   move "TUTTI I DESTINI" to des-ragsoc-1
                else
                   read destini no lock
                        invalid
                        move spaces to des-ragsoc-1
                        set errori to true
                        display message "Destino non valido"
                                  title tit-err
                                   icon 2
                   end-read                         
                end-if   
                move des-ragsoc-1 to lab-des-buf 
                display lab-des
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to STORE-ID        
              move 4          to ACCEPT-CONTROL  
           end-if.

           display form1 
           .
      * <TOTEM:END>

