                 when "N" perform ELABORA-TNOTACR
                 when "T" perform ELABORA-TORDINI
                          perform ELABORA-TNOTACR
                 end-evaluate       
                 perform RESET-CONTATORE
                 display message "ELABORAZIONE TERMINATA"
                          x"0d0a""DOCUMENTI: " tot-doc                  
                  
                          x"0d0a"" - ORDINI: " tot-ord "  ELABORATI: " 
           tot-ord-ok
                          x"0d0a"" - NOTE: " tot-nc "  ELABORATE: " 
           tot-nc-ok
                           title titolo
                           icon 2
              end-if
           end-if.
    
      ***---
       ELABORA-TORDINI.
           move low-value to tor-rec.      
           move como-data-from to tor-data-creazione.
           start tordini key >= k-data
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read tordini next at end exit perform end-read
                    if tor-data-creazione > como-data-to
                       exit perform
                    end-if     
                    perform CONTATORE-SCREEN
                    add 1 to tot-doc
                    add 1 to tot-ord
                    if como-cli = 0
                       set record-ok to true
                    else                     
                       if como-cli not = tor-cod-cli
                          set record-ok to false
                       else
                          if como-des = 0
                             set record-ok to true
                          else
                             if como-des = tor-prg-destino
                                set record-ok to true
                             else
                                set record-ok to false
                             end-if
                          end-if
                       end-if
                    end-if
                    if record-ok    
                       perform READ-TORDINI-LOCK
                       if not RecLocked 
                          if tor-fattura-manuale
                             if tor-fattura-from-numero = spaces or
                                tor-fattura-from-data   = 0
                                display message "Fattura manuale n. " 
           tor-numero
                                         x"0d0a""senza riferimenti alla 
      -    "fattura"
                                          title titolo
                             end-if
                          end-if
                          add 1 to tot-ord-ok
                          move rec-invio to tor-invio
                          rewrite tor-rec
                          unlock tordini all records
                       end-if
                       add 1 to tor-numero
                       start tordini key >= k-data
                             invalid exit perform
                       end-start
                    end-if
                 end-perform
           end-start.
   
      ***---
       READ-TORDINI-LOCK.
           move tor-numero to num-edit.
           string   "Ordine " num-edit " risulta in uso su altro termina
      -    "le."
                 delimited size
                 into geslock-messaggio
           end-string.
           move "tordini" to geslock-nome-file.

           set tutto-ok   to true.
           perform until 1 = 2
              set RecLocked to false
              read tordini lock
              if not RecLocked
                 exit perform
              end-if

              move 1 to geslock-v-riprova
              move 1 to geslock-v-ignora
              move 0 to geslock-v-termina
              call   "geslock" using geslock-linkage
              cancel "geslock"
              evaluate true
              when riprova continue
              when ignora
                   exit perform
              end-evaluate
           end-perform.      
    
      ***---
       ELABORA-TNOTACR.
           move low-value to tno-rec.      
           move como-data-from to tno-data.
           start tnotacr key >= k-data
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read tnotacr next at end exit perform end-read
                    if tno-data > como-data-to
                       exit perform
                    end-if       
                    perform CONTATORE-SCREEN
                    add 1 to tot-doc
                    add 1 to tot-nc
                    if como-cli = 0
                       set record-ok to true
                    else                     
                       if como-cli not = tno-cod-cli
                          set record-ok to false
                       else
                          if como-des = 0
                             set record-ok to true
                          else
                             if como-des = tno-prg-destino
                                set record-ok to true
                             else
                                set record-ok to false
                             end-if
                          end-if
                       end-if
                    end-if
                    if record-ok    
                       perform READ-TNOTACR-LOCK
                       if not RecLocked
                          set EsisteNiente to true          
                          if tno-fattura-from-numero not = 0 and
                             tno-fattura-from-data   not = 0
                             set EsisteFattura to true
                          end-if
                          if tno-bolla-from-data     not = 0 and      
                             tno-bolla-from-numero   not = spaces
                             set EsisteBolla to true
                          end-if                            
                          if tno-fattura-from-numero not = 0 and
                             tno-fattura-from-data   not = 0 and   
                             tno-bolla-from-data     not = 0 and      
                             tno-bolla-from-numero   not = spaces
                             set EsisteTutto to true
                          end-if                        
                             
                          evaluate true
                          when EsisteTutto continue
                          when EsisteFattura
                               move tno-fattura-from-data   to 
           tno-bolla-from-data
                               move tno-fattura-from-numero to numero-z
                               move numero-z to tno-bolla-from-numero
                          when EsisteBolla                              
                       
                               move tno-bolla-from-data   to 
           tno-fattura-from-data
                               move tno-bolla-from-numero to 
           tno-fattura-from-numero convert
                          when EsisteNiente                             
                   
                               move tno-data   to tno-fattura-from-data
                               move tno-numero to 
           tno-fattura-from-numero
                               move tno-data   to tno-bolla-from-data   
                      
                               move tno-numero to numero-z
                               move numero-z   to tno-bolla-from-numero
                          end-evaluate
                          add 1 to tot-nc-ok
                          move rec-invio to tno-invio
                          rewrite tno-rec
                          unlock tnotacr all records
                       end-if
                       add 1 to tno-numero
                       start tnotacr key >= k-data
                             invalid exit perform
                       end-start
                    end-if
                 end-perform
           end-start.
   
      ***---
       READ-TNOTACR-LOCK.
           move tno-numero to num-edit.
           string   "Nota " num-edit " risulta in uso su altro terminale
      -    "."
                 delimited size
                 into geslock-messaggio
           end-string.
           move "tnotacr" to geslock-nome-file.

           set tutto-ok   to true.
           perform until 1 = 2
              set RecLocked to false
              read tnotacr lock
              if not RecLocked
                 exit perform
              end-if

              move 1 to geslock-v-riprova
              move 1 to geslock-v-ignora
              move 0 to geslock-v-termina
              call   "geslock" using geslock-linkage
              cancel "geslock"
              evaluate true
              when riprova continue
              when ignora
                   exit perform
              end-evaluate
           end-perform.


      ***---
       CONTATORE-SCREEN.
           add 1 to counter
           add 1 to counter2
           if counter2 = 10
              move counter to counter-edit
              display counter-edit
                 upon form1-handle at column 02,00
                                        line 19,00
              move 0 to counter2
           end-if.

      ***---
       RESET-CONTATORE.
           move 0 to counter.
           move 0 to counter2.
           move counter to counter-edit
           display counter-edit
             upon form1-handle at column 02,00
                                    line 19,00 
           .
      * <TOTEM:END>
