                  move "V"            to orientamento
                  call "spooler-A" using operazione
                                         WSTAMPA
                                         orientamento
                  cancel "spooler-A"
              end-if

              call "C$DELETE" using stbrogcm-path

              modify pb-esegui,  bitmap-number = 1
              modify Form1-Pb-2, bitmap-number = 1

           end-if 
           .
      * <TOTEM:END>
