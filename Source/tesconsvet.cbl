       Form1-Event-Proc.
           .

       Form1-Gd-1-Event-Proc.
           EVALUATE Event-Type ALSO Event-Control-Id ALSO
                                    Event-Window-Handle
           WHEN Msg-Begin-Drag ALSO 5001 ALSO
                    Form1-Handle 
              PERFORM form1-gd-1-Ev-Msg-Begin-Drag
           WHEN Msg-Begin-Entry ALSO 5001 ALSO
                    Form1-Handle 
              PERFORM form1-gd-1-Ev-Msg-Begin-Entry
           WHEN Msg-End-Drag ALSO 5001 ALSO
                    Form1-Handle 
              PERFORM form1-gd-1-Ev-Msg-End-Drag
           WHEN Msg-Finish-Entry ALSO 5001 ALSO
                    Form1-Handle 
              PERFORM form1-gd-1-Ev-Msg-Finish-Entry
           WHEN Msg-Goto-Cell ALSO 5001 ALSO
                    Form1-Handle 
              PERFORM form1-gd-1-Ev-Msg-Goto-Cell
           WHEN Msg-Goto-Cell-Drag ALSO 5001 ALSO
                    Form1-Handle 
              PERFORM form1-gd-1-Ev-Msg-Goto-Cell-Drag
           WHEN Msg-Goto-Cell-Mouse ALSO 5001 ALSO
                    Form1-Handle 
              PERFORM form1-gd-1-Ev-Msg-Goto-Cell-Mouse
           END-EVALUATE
           .

       Screen1-Event-Proc.
           .

      * USER DEFINE PARAGRAPH
       ABILITAZIONI.
      * <TOTEM:PARA. ABILITAZIONI>
           if mod = 1
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else
              |se sono in visua           
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move 0 to e-salva e-cancella
           end-if.     

      * ISACCO (CORREZIONE SU LIVELLO ABILITAZIONE 2) - 10/11/2003
      * (CANCELLAZIONE NON PERMESSA)
           if livello-abil = 2 
              move ZERO to e-cancella
              move BitmapDeleteDisabled to BitmapNumDelete
           end-if.
      * FINE

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-nuovo, tool-salva, tool-cancella, tool-modifica.
      * FINE.

           perform GRID-HELP 
           .
      * <TOTEM:END>

       ANTEPRIMA.
      * <TOTEM:PARA. ANTEPRIMA>
           set anteprima       to true.
      *     perform paragrafo-stampa 
           .
      * <TOTEM:END>

       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = 0 
              exit paragraph 
           end-if.

           if control-id = 78-id-gd-cve
              display message box MSG-Cancellare-la-riga-selezionata
                      title   titolo
                      type    mb-yes-no 
                      default mb-no
                      giving  scelta
              set DeleteRiga to true
           end-if.
      *
           if scelta = mb-yes      
              set tutto-ok to true      
              perform RIEMPI-CHIAVE   
              move tev-vet    to memo-vet
              perform X-Y
              perform VALORE-RIGA
              ||move col-id-num   to tev-vet
              delete tesconsvet record 
                 invalid 
                    continue 
              end-delete
              modify gd-cve,  record-to-delete = riga
              inquire gd-cve, last-row in totale-righe
              move 0 to riga-nuova

              if totale-righe = 1 
                 move 1 to control-id 
              else
                 if riga > totale-righe
                    move totale-righe to riga
                 end-if
                 if riga = 2
                    unlock tesconsvet all records   
                    perform VALORE-RIGA
                    perform RIEMPI-CHIAVE   
                    perform LOCK-RECORD
                 end-if
                 move riga to MyEventData2
                 perform COLORE
                 modify gd-cve, cursor-y = riga, cursor-x = 1
              end-if

              set vecchio to true
              perform INIT-OLD-REC
      
              set StatusModifica to true
              perform STATUS-BAR-MSG
              display message MSG-cancellazione-avvenuta-con-successo
                      title = titolo
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate CONTROL-ID
           when 78-ID-EF-vet
                inquire ef-vet, VALUE vet-codice
                move "tvettori"  to Como-File
                call   "zoom-gt" using  como-file, vet-rec
                                 giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-vet, value vet-codice
                   move vet-descrizione  to des-vet
                   display lbl-des-vet
                   move 4 to accept-control
                end-if


           when 78-id-gd-cve
                if colonna = 2| and nuovo
                   perform VALORE-RIGA
                   move tev-esito  to tec-codice
                   move "tescons"  to  como-file
                   call   "zoom-gt" using  como-file, tec-rec
                                    giving stato-zoom
               
                   if stato-zoom = 0  
                      move tec-codice to col-escons
                      modify gd-cve(riga, 2), cell-data = col-escons
                      move tec-descrizione to col-descr
                      modify gd-cve(riga, 3) cell-data = col-descr
                      if tev-esito not = old-tev-esito
                         set NoSalvato to true                          
             
                      else 
                         set SiSalvato to true
                      end-if
                   end-if
                end-if



           end-evaluate  
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.
           set trovato  to true.
           perform VALORE-RIGA. 

           evaluate control-id
           when 78-ID-ef-vet
                move ef-vet-buf to como-vettore
                                   vet-codice
                read tvettori
                   invalid 
                      if vecchio
                         display message box MSG-record-inesistente
                                 title = tit-err
                                 icon mb-warning-icon
                          set errori to true
                          move 78-ID-ef-vet to control-id
                      end-if
                      move spaces to des-vet
                   not invalid      
                      move vet-descrizione  to des-vet
                end-read
                display lbl-des-vet
           when 78-id-gd-cve             
                evaluate colonna
                when 1
                     if nuovo
                        move ef-vet-buf  to tev-vet
                        read tesconsvet
                           invalid
                              continue
                           not invalid
                               set errori to true
                               display message MGS-codice-gia-inserito|"Codice già inserito!"
                                       title = tit-err
                                       icon mb-warning-icon
                               move 1 to colonna
                        end-read
                     end-if
                when 2
                     move tev-esito  to tec-codice
                     read tescons
                        invalid
                           move 2 to colonna
                           set errori to true
                           display message box "Esito inesistente"
                                   title = tit-err
                                   icon mb-warning-icon
                           move space to tec-descrizione
                     end-read
                     move tec-descrizione   to col-descr
                     modify gd-cve(riga, 3), CELL-DATA col-descr
                end-evaluate
           end-evaluate.  

      ****************
           if errori
              if control-id = 78-id-gd-cve
                 modify gd-cve, cursor-y = riga, cursor-x = colonna
                 move riga    to event-data-2
                 move colonna to event-data-1
              else
                 move 78-ID-ef-vet   to control-id
                 move 4               to accept-control
              end-if
           end-if 
           .
      * <TOTEM:END>

       GRID-HELP.
      * <TOTEM:PARA. GRID-HELP>
      *    se non sono in modifica disabilito sempre lo zoom
           if mod = zero
              move BitmapZoomDisabled to BitmapNumZoom
              move 0 to e-cerca
              modify tool-cerca, bitmap-number = BitmapNumZoom,
                                 enabled       = e-cerca
           else
              evaluate colonna|event-data-1
              when 2
                   move BitmapZoomEnabled to BitmapNumZoom
                   move 1 to e-cerca
              when other
                   move BitmapZoomDisabled to BitmapNumZoom
                   move 0 to e-cerca
              end-evaluate
           
              modify tool-cerca, bitmap-number = BitmapNumZoom,
                                 enabled       = e-cerca
           end-if 
           .
      * <TOTEM:END>

       INIT.
      * <TOTEM:PARA. INIT>
           set vecchio  to true.
           set tutto-ok to true 
           .
      * <TOTEM:END>

       INTESTAZIONE.
      * <TOTEM:PARA. INTESTAZIONE>
           perform GD-CVE-CONTENT 
           .
      * <TOTEM:END>

       MODIFICA.
      * <TOTEM:PARA. MODIFICA>
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else         
              set ReadSecca to true
      *  se l'utente e' abilitato puo' modificare un record
              if mod = 1
      * Adriano - Gestione lock sul file inizio
                 close tesconsvet
                 open i-o tesconsvet allowing readers
      * Adriano fine
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if not trovato 
                    set errori to true 
                 end-if
                 if RecLocked   
                    set errori to true 
                 end-if
                 
                 if errori 
                    move 0 to mod 
                    move 1 to mod-k
                    set StatusVisua to true
                 else
                    move 0 to mod-k
                    move 1 to mod
                    set StatusModifica to true
                    move 2 to riga MyEventData2
                    move 1 to colonna
                    modify gd-cve, cursor-y = riga, 
                                       cursor-x = colonna
                    move 1   to StatusHelp
                    perform STATUS-HELP

                    perform SPOSTAMENTO
                 end-if   
              else
                 move 1 to mod
                 perform SALV-MOD            
                 move 0 to mod
                 move 1 to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    if nuovo
                       if totale-righe not > 1
                          perform PRIMO
                       end-if
                    else
      * Adriano - Gestione lock sul file inizio
                       close tesconsvet
                       open input tesconsvet
      * Adriano - fine
                       move 0 to MyEventData2
                       set NoMessage to true
                       perform CURRENT-RECORD
                       set StatusVisua to true
                       unlock tesconsvet all records
                    end-if
                 end-if
              end-if                   

              perform DISPLAY-CAMPI-CHIAVE
                        
              display form1             
              modify control-handle, color = colore-or

              move 78-id-ef-vet to control-id
              move 4 to accept-control

              set ReadSecca to false

           end-if.                      
           modify tool-modifica,  value mod.
           perform STATUS-BAR-MSG 
           .
      * <TOTEM:END>

       NUOVO.
      * <TOTEM:PARA. NUOVO>
           if control-id not = 78-id-gd-cve
              perform SALV-MOD
              if tutto-ok
                 move 78-id-ef-vet   to control-id
                 move 4 to accept-control
                 move 1 to mod mod-k
                 modify tool-modifica,  value = mod
                 modify control-handle, color = colore-or
                 perform CLEAR-SCREEN
                 perform INIT-OLD-REC
                 set StatusIns to true
                 perform STATUS-BAR-MSG  
                 set nuovo to true
                 move 0 to RowCounter
                 unlock tesconsvet all records
              end-if
           else   
              if riga-nuova = 0    
                 perform INSERT-RIGA
              else
                 perform VALORE-RIGA
                 if col-esito not = space
                    perform INSERT-RIGA
                 end-if
              end-if
           end-if.

      ***---
       INSERT-RIGA.
           perform X-Y.
           if vecchio
              move 0 to mod-k       
           end-if.   
           set nuovo to true.
           move 1 to mod.
           modify tool-modifica,  value = mod.
           perform ABILITAZIONI.
           perform VALORE-RIGA.
      *     if tev-codice = spaces
      *        perform DEL-RIGA 
      *     end-if.
           add 1 to totale-righe giving riga.
           modify gd-cve, insert-rows = 1.
           move riga to MyEventData2. 
           move    1 to riga-nuova.
           move    1 to colonna.
           move    3 to control-id.
           move    4 to accept-control.
           modify gd-cve, cursor-y = riga, cursor-x = colonna.
           perform COLORE.
           set StatusModifica to true.
           perform STATUS-BAR-MSG.
           move 1   to StatusHelp.
           perform STATUS-HELP 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".   
           copy "status.cpy" 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           perform CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel
                            giving scelta

              evaluate scelta
              when mb-yes
                   perform SALVA
              when mb-no
                   continue
              when other
                   modify control-handle, color = colore-or
                   set errori to true
                   move store-id to control-id
                   move 4 to accept-control
                   if control-id= 78-id-gd-cve
                      move store-riga to riga MyEventData2
                      modify gd-cve, cursor-y = riga, cursor-x = 2
                      perform COLORE
                   end-if
              end-evaluate

           end-if 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           if sisalvato or mod = 0 
              exit paragraph 
           end-if.

           inquire gd-cve, cursor-x in colonna, 
                               cursor-y in riga.


           if mod-k = 1
              perform varying control-id from 78-ID-ef-vet by 1
                     until control-id > 78-id-gd-cve
                 perform CONTROLLO
                 if errori 
                    exit perform 
                 end-if
              end-perform
           end-if.

           if errori
              move store-id to control-id
              move 4 to accept-control
           else        
              move 78-id-gd-cve   to control-id
              perform X-Y
              if totale-righe = 1
                 set errori to true
              else
                 move riga to store-riga
                 perform RIEMPI-CHIAVE       
                 accept como-data from century-date
                 accept como-ora  from time
                 modify gd-cve, mass-update = 1
                 perform varying riga from 2 by 1 
                           until riga > totale-righe
   
                    modify gd-cve, cursor-y = riga
   
                    perform VALORE-RIGA
   
                    if mod = 1 
                       if nuovo or (riga - 1) > RowCounter
                          move 2 to colonna
                          modify gd-cve, cursor-x = colonna
                          perform CONTROLLO

                          move 3 to colonna
                          modify gd-cve, cursor-x = colonna
                          perform CONTROLLO

                          if not trovato 
                             set errori to true 
                          end-if
                       end-if
                    end-if
   
                    if tutto-ok
                       perform VALORIZZA-DATI-COMUNI
                       write tev-rec 
                          invalid 
                             rewrite tev-rec
                       end-write
                       set sisalvato to true
                       set vecchio   to true
                    else
                       move 3 to control-id
                       move 4 to accept-control
                       modify gd-cve, cursor-y = riga, cursor-x = 
           colonna
                       move riga to MyEventData2
                       perform COLORE
                       exit perform
                    end-if
                 end-perform                
                 modify gd-cve, mass-update = 0
                         
              end-if
           end-if.                    
           perform ABILITAZIONI              
           .
      * <TOTEM:END>

       SPOSTAMENTO.
      * <TOTEM:PARA. SPOSTAMENTO>
           inquire gd-cve, cursor-x = colonna, cursor-y = riga.

           if event-data-2 not = riga
              perform VALORE-RIGA
      *        if tev-codice = spaces or zero
      *           modify gd-cve, record-to-delete riga
      *           set vecchio to true
      *        else  
                 perform SALVA
                 if errori
                    move riga    to event-data-2
                    move colonna to event-data-1 | (isacco)
                    set event-action to event-action-fail
                 end-if
      *        end-if
           else
              if colonna not = event-data-1
                 perform CONTROLLO
                 if errori
                    set event-action to event-action-fail
                 end-if
              end-if
           end-if.

      *    valorizzo l'old della riga su cui sono andato
           move event-data-2 to riga.
           move event-data-1 to colonna.
           perform VALORE-OLD-RIGA.

      * colorazione riga in grid
           perform COLORE-RIGA.

           perform GRID-HELP 
           .
      * <TOTEM:END>

       STAMPA.
      * <TOTEM:PARA. STAMPA>
           set stampa       to true.
      *     perform paragrafo-stampa 
           .
      * <TOTEM:END>

       VALORE-RIGA.
      * <TOTEM:PARA. VALORE-RIGA>
           perform X-Y.
           inquire gd-cve(riga), RECORD-DATA rec-grid
           move col-esito    to tev-codice
           move col-escons   to tev-esito
           .
      * <TOTEM:END>

       VALORE-OLD-RIGA.
      * <TOTEM:PARA. VALORE-OLD-RIGA>
           if vecchio
              inquire gd-cve(riga, 1) cell-data old-tev-codice
              move old-tev-codice  to tev-codice
              read tesconsvet no lock
              move tev-rec         to old-tev-rec
           else
              initialize old-tev-rec replacing numeric data by zeroes
                                     alphanumeric data by spaces
           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-DATI-COMUNI.
      * <TOTEM:PARA. VALORIZZA-DATI-COMUNI>
      *    se non ho valorizzato i campi di creazione li valorizzo
           if tev-data-creazione = zero
              accept tev-data-creazione from century-date
           end-if.
           if tev-ora-creazione = zero
              accept tev-ora-creazione from time
           end-if.
           if tev-utente-creazione = space
              move user-codi to tev-utente-creazione
           end-if.

           accept tev-data-ultima-modifica from century-date.
           accept tev-ora-ultima-modifica from time.
           move user-codi to tev-utente-ultima-modifica 
           .
      * <TOTEM:END>

       PRIMO.
      * <TOTEM:PARA. PRIMO>
           perform SALV-MOD.           
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock tesconsvet all records
              modify control-handle, color = colore-or 
              move low-value to tev-chiave

              set ReadSecca to false
              move 0 to MyEventData2
              perform CURRENT-RECORD

              if errori
                 display message box "File [tesconsvet]"
                              x"0d0a""Archivio vuoto"
                         title = titolo
              end-if       

              perform DISPLAY-CAMPI-CHIAVE       
     
              move 1 to control-id
              move 4 to accept-control 
              perform VALORIZZA-OLD
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if                
           end-if 
           .
      * <TOTEM:END>

       PRECEDENTE.
      * <TOTEM:PARA. PRECEDENTE>
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock tesconsvet all records 
              modify control-handle, color = colore-or 
              perform RIEMPI-CHIAVE
              move low-value to tev-codice

              start tesconsvet key is < tev-chiave
                 invalid 
                    set errori to true
                 not invalid 
                    read tesconsvet previous no lock
                       at end 
                          set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move low-value to tev-codice
                 set ReadSecca to false
                 move 0 to MyEventData2
                 perform CURRENT-RECORD
              end-if

              if errori or not trovato
                 display message box "File [tesconsvet]"
                              x"0d0a""Raggiunto inizio file"
                         title = titolo
              end-if                  

              perform DISPLAY-CAMPI-CHIAVE       
     
              move 78-id-ef-vet   to control-id
              move 4            to accept-control 
              perform VALORIZZA-OLD 
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if                 
           end-if 
           .
      * <TOTEM:END>

       SUCCESSIVO.
      * <TOTEM:PARA. SUCCESSIVO>
           perform SALV-MOD.           
           if tutto-ok            
              perform CHECK-INSERIMENTO
              unlock tesconsvet all records 
              modify control-handle, color = colore-or 
              perform RIEMPI-CHIAVE
              move high-value to tev-codice
                                         
              set ReadSecca to false
              move 0 to MyEventData2
              perform CURRENT-RECORD

              if errori or not trovato
                 display message box "File [tesconsvet]"
                              x"0d0a""Raggiunta fine file"
                         title = titolo
              end-if                  

              perform DISPLAY-CAMPI-CHIAVE  
     
              move 1 to control-id
              move 4 to accept-control 
              perform VALORIZZA-OLD
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if                
           end-if 
           .
      * <TOTEM:END>

       ULTIMO.
      * <TOTEM:PARA. ULTIMO>
           perform SALV-MOD.           
           if tutto-ok                 
              perform CHECK-INSERIMENTO
              unlock tesconsvet all records 
              modify control-handle, color = colore-or 
              move high-value to tev-chiave
              start tesconsvet key is < tev-chiave
                 invalid 
                    set errori to true
                 not invalid 
                    read tesconsvet previous no lock
                       at end 
                          set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move low-value to tev-codice
                 set ReadSecca to false
                 move 0 to MyEventData2
                 perform CURRENT-RECORD
              end-if

              if errori
                 display message box "File [tcauven]"
                              x"0d0a""Archivio vuoto"
                         title = titolo
              end-if         

              perform DISPLAY-CAMPI-CHIAVE
     
              move 78-id-ef-vet   to control-id
              move 4            to accept-control


              perform VALORIZZA-OLD
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if               

           end-if 
           .
      * <TOTEM:END>

       CERCA-TESTO.
      * <TOTEM:PARA. CERCA-TESTO>
           perform SCREEN-SEARCH-OPEN-ROUTINE.

           if ricerca
              move 0 to ResultSearch

              inquire gd-cve, cursor-y in riga

              inquire gd-cve, search-options in grid-search-options
              
              |direzione di ricerca
              set GRID-SEARCH-FORWARDS to true
              set GRID-SEARCH-WRAP     to true

              |Maiuscolo/minuscolo
              if MatchCase = 0
                 set GRID-SEARCH-IGNORE-CASE to true
              else                                                      
             
                 set GRID-SEARCH-IGNORE-CASE to false
              end-if

              |trova la parola nella cella
              set GRID-SEARCH-MATCH-ANY to true

              |ricerca nelle sole celle visible 
              |ignorando gli eventuali hidden-data
              set GRID-SEARCH-VISIBLE to true

              |non considerare quello su cui sono posizionato
              set GRID-SEARCH-SKIP-CURRENT   to true

              |si posiziona sulla cella trovata
              set GRID-SEARCH-MOVES-CURSOR   to true 

              |num colonna sulla quale fare la ricerca
              move tipo-ricerca to GRID-SEARCH-COLUMN 

              modify gd-cve, search-options GRID-SEARCH-OPTIONS

              modify gd-cve(riga, tipo-ricerca), 
                     search-text TextToSearch |testo da ricercare
                          giving ResultSearch |1, 2 = trovato 0 = non trovato

              if ResultSearch = GRDSRCH-NOT-FOUND
                 display message box MSG-Testo-non-trovato
                         title = titolo
              else
                 inquire gd-cve, cursor-y in riga
                 move riga to event-data-2
                 perform SPOSTAMENTO
              end-if
           end-if 
           .
      * <TOTEM:END>

       X-Y.
      * <TOTEM:PARA. X-Y>
           inquire gd-cve, cursor-y in riga, cursor-x in colonna
                            last-row in totale-righe
                            num-rows in NumRows 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE.
      * <TOTEM:PARA. RIEMPI-CHIAVE>
           inquire ef-vet, value in tev-vet 
           .
      * <TOTEM:END>

       COLORE.
      * <TOTEM:PARA. COLORE>
           modify gd-cve, start-y = MyEventData2, y = MyEventData2,
                          start-x = 1,            x = 3,
                          region-color 257,
                          cursor-color 481 
           .
      * <TOTEM:END>

       INIT-OLD-REC.
      * <TOTEM:PARA. INIT-OLD-REC>
           initialize old-tev-rec replacing numeric data by zeroes
                                      alphabetic data by spaces 
           .
      * <TOTEM:END>

       CLEAR-SCREEN.
      * <TOTEM:PARA. CLEAR-SCREEN>
           move tev-vet to old-tev-vet.

           initialize tev-rec replacing numeric data by zeroes
                              alphabetic data by spaces.
                        
           move old-tev-vet to tev-vet.
           
           move spaces to des-vet.
           display lbl-des-vet.

           perform RESET-GRIGLIA.

           perform INIT-OLD-REC  
           .
      * <TOTEM:END>

       RESET-GRIGLIA.
      * <TOTEM:PARA. RESET-GRIGLIA>
           modify gd-cve, reset-grid = 1.
           perform INTESTAZIONE 
           .
      * <TOTEM:END>

       CURRENT-RECORD.
      * <TOTEM:PARA. CURRENT-RECORD>
           move -1 to Memo-vet.
           if ReadSecca
              perform RIEMPI-CHIAVE
              move tev-vet    to Memo-vet
              move low-value to tev-codice
           end-if.
           set tutto-ok  to true.
           set trovato   to false. 
           
           start tesconsvet key not less tev-chiave
              invalid 
                 set errori to true 
           end-start.

           if nuovo
              move tev-vet  to Memo-vet
           end-if.

           if tutto-ok
              modify gd-cve, mass-update = 1
              perform RESET-GRIGLIA
              move 0 to RowCounter
              move 2 to riga
              
              perform until 1 = 2
                 read tesconsvet next no lock 
                    at end 
                       exit perform 
                 end-read

                 if Memo-vet not > -1
                    move tev-vet    to  Memo-vet
                 end-if                  

                 if tev-vet    not = Memo-vet
                    move Memo-vet to tev-vet
                    exit perform
                 end-if

                 set trovato to true 

                 if nuovo
                    move 1 to mod
                    move 0 to mod-k
                    move 78-id-gd-cve   to control-id
                    move 4               to accept-control
                 end-if  

                 set vecchio to true

                 perform METTI-IN-GRIGLIA
                 add 1 to riga
                 add 1 to RowCounter
                 if RowCounter = 1
                    move spaces      to des-vet
                    move Memo-vet to ef-vet-buf
                    display ef-vet lbl-des-vet 
                 end-if
              end-perform          
             
              modify gd-cve, mass-update = 0

           end-if.

           if trovato   
              perform VALORIZZA-OLD
              if mod = 1
                 set  StatusModifica to true
                 perform LOCK-RECORD        
              else
                 set StatusVisua    to true
              end-if
              perform STATUS-BAR-MSG
           end-if.

           if not trovato and ReadSecca
              if vecchio
                 perform CLEAR-SCREEN
                 if YesMessage          
                    display message box "Record inesistente!"
                            title = tit-err
                            icon mb-warning-icon
                 end-if
                 move 0 to mod
                 move 78-id-ef-vet   to control-id
                 move 4 to accept-control
              end-if
           end-if.             

           if MyEventData2 = 0
              move 2 to riga MyEventData2
           end-if.
           set ReadSecca to false.
           perform ABILITAZIONI.

