                            add 1 to counter2
                            if counter2 = 20
                               move counter to counter-edit
                               display counter-edit
                                  upon form3-handle at column 42,00
                                                         line  2,00
                               move 0 to counter2
                            end-if
                            read redi next at end exit perform end-read
                            if redi-tedi-chiave not = tedi-chiave
                               exit perform
                            end-if
                            delete redi record invalid continue 
           end-delete
                            add 1 to num-rec

                         end-perform
                         delete tedi record invalid continue end-delete
                   end-start
              end-read

              call "W$MOUSE" using set-mouse-shape, arrow-pointer
              close tedi redi
           end-if.

