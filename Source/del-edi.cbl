       CANCELLAZIONE.
      * <TOTEM:PARA. CANCELLAZIONE>
           set tutto-ok  to true.

           perform until 1 = 2
              set RecLocked to false
              open i-o tedi allowing readers
              if RecLocked
                 initialize geslock-messaggio
                 move "TEDI" to geslock-nome-file
                 move 1 to geslock-v-riprova
                 move 0 to geslock-v-ignora
                 move 1 to geslock-v-termina
                 call   "geslock" using geslock-linkage
                 cancel "geslock"
                 evaluate true
                 when riprova 
                      continue
                 when termina 
                      set errori to true
                      exit perform
                 end-evaluate                 
              else
                 exit perform
              end-if
           end-perform.

           if tutto-ok
              perform until 1 = 2
                 set RecLocked to false
                 open i-o redi allowing readers
                 if RecLocked
                    initialize geslock-messaggio
                    move "REDI" to geslock-nome-file
                    move 1 to geslock-v-riprova
                    move 0 to geslock-v-ignora
                    move 1 to geslock-v-termina
                    call   "geslock" using geslock-linkage
                    cancel "geslock"
                    evaluate true
                    when riprova 
                         continue
                    when termina 
                         set errori to true
                         exit perform
                    end-evaluate
                 else
                    exit perform
                 end-if
              end-perform
           end-if.

           if errori
              close tedi
           else
              move 0 to counter counter2 num-rec
              call "W$MOUSE" using set-mouse-shape, wait-pointer
              move ef-anno-buf to tedi-anno
              move ef-mese-buf to tedi-mese
              read tedi 
                   invalid display message "NO TEDI"
               not invalid
                   move low-value   to redi-rec
                   move tedi-chiave to redi-tedi-chiave
                   start redi key >= redi-chiave
                         invalid display message "NO REDI"
                     not invalid
                         perform until 1 = 2

