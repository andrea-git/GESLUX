       CERCA.
      * <TOTEM:PARA. CERCA>
           set tutto-ok to true.
           call   "sel-listini" using link-codice link-articolo.
           cancel "sel-listini".

           if link-codice not = 0
              move link-codice  to tlis-codice
                                   ef-forn-buf
              read tlistini no lock
                 invalid
                    continue
              end-read
              move tlis-descrizione to lbl-forn-buf
              modify ef-forn, VALUE ef-forn-buf
              modify lbl-forn, TITLE lbl-forn-buf
           end-if 
                 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID
      *    

           when 78-ID-ef-forn
                if controllo-finale
                   inquire ef-forn, VALUE tlis-codice
                   read tlistini
                      invalid
                         set errori to true
                         display message "Listino Inesisitente"
                                  title tit-err
                         move space to lbl-forn-buf
                      not invalid
                         move tlis-descrizione  to lbl-forn-buf
                   end-read
                   modify lbl-forn, TITLE lbl-forn-buf
                end-if

      *     when 78-id-ef-dt-val
      *          inquire ef-dt-val value ef-dt-val-buf
      *          move ef-dt-val-buf to como-data
      *          perform DATE-FORMAT
      *          move como-data  to ef-dt-val-buf
      *          modify ef-dt-val value ef-dt-val-buf
      *          if controllo-finale
      *             inquire ef-forn, VALUE tlis-fornitore
      *             move como-data(1:2)   to tlis-dt-val(7:2)
      *             move como-data(3:2)   to tlis-dt-val(5:2)
      *             move como-data(5:4)   to tlis-dt-val(1:4)
      *             read tlistini no lock
      *                invalid
      *                   continue
      *                not invalid
      *                   Display message box "Listino già inserito."
      *                                      x"0D0A"
      *                                      "procedere comunque?"
      *                                      x"0D0A"
      *                                      "Tutti i vecchi dati andranno persi."
      *                          title titolo
      *                          type mb-yes-no
      *                          default mb-no
      *                          giving scelta
      *                   if scelta = mb-no
      *                      set errori   to true
      *                   end-if
      *             end-read
      *          end-if
           when 78-id-ef-path
                if controllo-finale
                   inquire ef-path, VALUE ef-path-buf
                   if ef-path-buf = space
                      set errori to true
                      display message "File CSV obbligatorio"
