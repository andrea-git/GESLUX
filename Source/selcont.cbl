       Screen1-Event-Proc.
           .

      * USER DEFINE PARAGRAPH
       CANCELLA-TMP.
      * <TOTEM:PARA. CANCELLA-TMP>
           delete file zoom-tordini 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 1002
                perform RIEMPI-TMP

                inquire ef-anno, value in zoom-tor-anno
                inquire ef-num,  value in zoom-tor-numero

                move "zoom-fatture"    to como-file

                if trovato
                   move path-zoom-tordini to ext-file
                   call   "zoom-gt"   using como-file, zoom-tor-rec
                                     giving stato-zoom

                   cancel "zoom-gt"
                else
                   move 1 to stato-zoom
                end-if
                perform CANCELLA-TMP

                if stato-zoom = 0
                   move zoom-tor-numero to numero
                   display ef-num
                end-if

           when 1003
                perform RIEMPI-TMP-NOTE

                inquire ef-anno,  value in zoom-tor-anno
                inquire ef-nota,  value in zoom-tor-numero

                move "zoom-note-fatt" to como-file

                if trovato
                   move path-zoom-tordini to ext-file
                   call   "zoom-gt"   using como-file, zoom-tor-rec
                                     giving stato-zoom

                   cancel "zoom-gt"
                else
                   move 1 to stato-zoom
                end-if
                perform CANCELLA-TMP

                if stato-zoom = 0
                   move zoom-tor-numero to numero-nota
                   display ef-nota
                end-if
           when 1004
                perform RIEMPI-TMP-BOLLE

                inquire ef-anno, value in zoom-bol-anno
                inquire ef-bolla,  value in zoom-bol-numero

                move "zoom-bolle"    to como-file

                if trovato
                   move path-zoom-bolle to ext-file
                   call   "zoom-gt"   using como-file, zoom-bol-rec
                                     giving stato-zoom

                   cancel "zoom-gt"
                else
                   move 1 to stato-zoom
                end-if
                delete file zoom-bolle

                if stato-zoom = 0
                   move zoom-bol-numero to numero-bolla
                   display ef-bolla
                end-if
           end-evaluate 
           .
      * <TOTEM:END>

       CREA-CONTESTAZIONE.
      * <TOTEM:PARA. CREA-CONTESTAZIONE>
           if tor-num-fattura not = 0
              move tor-anno-fattura to NotaAnno
              move tor-num-fattura  to NotaNumero
              set LinkFatt to true
           end-if.

           if tno-num-fattura not = 0
              move tno-anno-fattura to NotaAnno
              move tno-num-fattura  to NotaNumero
              set LinkNota to true
           end-if.

           call   "gcontest" using lk-blockpgm,
                                   user-codi,
                                   livello-abil,
                                   notacr-linkage
           cancel "gcontest" 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "utydata.cpy".
           copy "color-custom.cpy" 
           .
      * <TOTEM:END>

       RIEMPI-TMP.
      * <TOTEM:PARA. RIEMPI-TMP>
           call "W$MOUSE" using set-mouse-shape, wait-pointer.
           set record-ok to true

           accept  path-zoom-tordini      from environment "PATH_ST".
           accept  como-data              from century-date.
           accept  como-ora               from time.
           inspect path-zoom-tordini      replacing trailing
                                          spaces by low-value.
           string path-zoom-tordini       delimited by low-value
                  "zoom-tordini"          delimited by size
                  "_"                     delimited by size
                  como-data               delimited by size
                  "_"                     delimited by size
                  como-ora                delimited by size
                  into path-zoom-tordini
           end-string.

           set trovato to false.
           open output zoom-tordini.
           initialize tor-rec.
           move esercizio to tor-anno-fattura.

           start tordini key  is >= k4
                 invalid continue 
             not invalid
                 perform until 1 = 2
                    read tordini next no lock at end exit perform 
           end-read
                    
                    if tor-anno-fattura not = esercizio or
                       tor-data-fattura     = 0         or
                       tor-num-prenot       = 0
                       exit perform
                    end-if

                    set record-ok to true

                    if tor-fattura-manuale
                       set record-ok to false
                    end-if

                    if record-ok
                       move tor-anno-fattura  to zoom-tor-anno
                       move tor-num-fattura   to zoom-tor-numero
                       move tor-data-fattura  to zoom-tor-data-ordine
                       move tor-causale       to tca-codice
                       read tcaumag 
                            invalid initialize tca-caus-trasporto 
                       end-read
                       move tca-caus-trasporto to zoom-tor-causale
                       move tor-cod-cli        to zoom-tor-cod-cli
                                                  cli-codice
                                                  des-codice
                       set cli-tipo-C          to true
                       move tor-prg-destino    to zoom-tor-prg-destino
                                                  des-prog

                       read clienti no lock
                            invalid initialize cli-rec
                       end-read
                       move cli-ragsoc-1       to zoom-tor-cli-ragsoc-1

                       read destini no lock
                            invalid
                            initialize des-rec
                            move cli-localita to des-localita
                       end-read
                       move des-localita       to zoom-tor-des-localita

                       write zoom-tor-rec invalid continue end-write
                       set trovato to true
                    end-if

                 end-perform
           end-start.

           close zoom-tordini.

           call "W$MOUSE" using set-mouse-shape, arrow-pointer 
           .
      * <TOTEM:END>

       RIEMPI-TMP-NOTE.
      * <TOTEM:PARA. RIEMPI-TMP-NOTE>
           call "W$MOUSE" using set-mouse-shape, wait-pointer.
           set record-ok to true

           accept  path-zoom-tordini      from environment "PATH_ST".
           accept  como-data              from century-date.
           accept  como-ora               from time.
           inspect path-zoom-tordini      replacing trailing
                                          spaces by low-value.
           string path-zoom-tordini       delimited by low-value
                  "zoom-tordini"          delimited by size
                  "_"                     delimited by size
                  como-data               delimited by size
                  "_"                     delimited by size
                  como-ora                delimited by size
                  into path-zoom-tordini
           end-string.

           set trovato to false.
           open output zoom-tordini.
           initialize tno-rec.
           move esercizio to tno-anno-fattura.

           start tnotacr key  is >= k4
                 invalid continue 
             not invalid
                 perform until 1 = 2
                    read tnotacr next no lock at end exit perform 
           end-read
                    
                    if tno-anno-fattura not = esercizio or
                       tno-data-fattura     = 0         or
                       tno-num-prenot       = 0
                       exit perform
                    end-if

                    set record-ok to true

                    if record-ok
                       move tno-anno-fattura  to zoom-tor-anno
                       move tno-num-fattura   to zoom-tor-numero
                       move tno-data-fattura  to zoom-tor-data-ordine
                       move tno-causale       to tca-codice
                       read tcaumag 
                            invalid initialize tca-caus-trasporto 
                       end-read
                       move tca-caus-trasporto to zoom-tor-causale
                       move tno-cod-cli        to zoom-tor-cod-cli
                                                  cli-codice
                                                  des-codice
                       set cli-tipo-C          to true
                       move tno-prg-destino    to zoom-tor-prg-destino
                                                  des-prog

                       read clienti no lock
                            invalid initialize cli-rec
                       end-read
                       move cli-ragsoc-1       to zoom-tor-cli-ragsoc-1

                       read destini no lock
                            invalid
                            initialize des-rec
                            move cli-localita to des-localita
                       end-read
                       move des-localita       to zoom-tor-des-localita

                       write zoom-tor-rec invalid continue end-write
                       set trovato to true
                    end-if

                 end-perform
           end-start.

           close zoom-tordini.

           call "W$MOUSE" using set-mouse-shape, arrow-pointer 
           .
      * <TOTEM:END>

       TRATTA-BOLLA.
      * <TOTEM:PARA. TRATTA-BOLLA>
           move zero   to cont
           start tordini key not < k-bolla
              invalid
                 continue
              not invalid
                 perform until 1 = 2
                    read tordini next no lock
                       at end
                          exit perform
                    end-read
                    if numero-bolla not = tor-num-bolla or 
                       anno         not = tor-anno-bolla
                       exit perform
                    end-if
                    add 1 to cont
                    if cont > 1
                       exit perform
                    end-if
                 end-perform
           end-start

           evaluate cont 
           when zero
                set errori to true
                display message "Bolla non caricata"
                        title = tit-err
                        icon mb-warning-icon
           when 1 
                move numero-bolla  to tor-num-bolla
                move anno          to tor-anno-bolla
                read tordini no lock key is k-bolla
                   invalid 
                      set errori to true
                      display message "Bolla non caricata"
                              title = tit-err
                              icon mb-warning-icon
                end-read
           when other
                perform SCEGLI-ORDINE-BOLLA
           end-evaluate

           if tutto-ok
              if tor-data-fattura = 0 or
                 tor-num-fattura  = 0 or
                 tor-anno-fattura = 0 or
                 tor-num-prenot   = 0
                 set errori to true
                 display message "Documento non fatturato"
                         title tit-err
                         icon mb-warning-icon
              else
                 if tor-fattura-manuale
                    set errori to true
                    display message "Documento non valido"
                            title tit-err
                            icon mb-warning-icon
                 else
                    move tor-anno-fattura to cnt-anno-fatt
                    move tor-num-fattura  to cnt-num-fatt
                    read contestazioni key k-fattura
                       invalid 
                          continue
                       not invalid
                          move cnt-numero to numero-edit
                          display message   "ATTENZIONE!!"
                                   x"0d0a"
                                   "Esiste già la contestazione n. " 
                                      numero-edit 
                                      " anno " 
                                      cnt-anno
                                   x"0d0a"
                          "che fa riferimento alla bolla indicata."
                                   x"0d0a"
                                   "Procedere comunque?"
                                  title titolo
                                   type mb-yes-no
                                   icon 2
                                 giving scelta
                          if scelta = mb-no
                           set errori to true
                        end-if
                   end-read
                end-if
              end-if
           end-if.

      ***---
       SCEGLI-ORDINE-BOLLA.
           call "W$MOUSE" using set-mouse-shape, wait-pointer.

           accept  path-zoom-bolle        from environment "PATH_ST".
           accept  como-data              from century-date.
           accept  como-ora               from time.
           inspect path-zoom-bolle        replacing trailing
                                          spaces by low-value.
           string path-zoom-bolle         delimited by low-value
                  "zoom-bolle"            delimited by size
                  "_"                     delimited by size
                  como-data               delimited by size
                  "_"                     delimited by size
                  como-ora                delimited by size
                  into path-zoom-bolle
           end-string.

           set trovato to false.
           open output zoom-bolle.
           initialize tor-rec.
           move numero-bolla to tor-num-bolla
           move anno         to tor-anno-bolla

           start tordini key  not < k-bolla
              invalid 
                 continue 
              not invalid
                 perform until 1 = 2
                    read tordini next no lock 
                       at end 
                          exit perform 
                    end-read
                    
                    if numero-bolla not = tor-num-bolla or 
                       anno         not = tor-anno-bolla
                       exit perform
                    end-if

                    move tor-anno           to zoom-bol-anno
                    move tor-numero         to zoom-bol-numero
                    move tor-data-ordine    to zoom-bol-data-ordine
                    move tor-causale        to tca-codice
                    read tcaumag 
                       invalid 
                          initialize tca-caus-trasporto 
                    end-read
                    move tca-caus-trasporto to zoom-bol-causale
                    move tor-cod-cli        to zoom-bol-cod-cli
                                               cli-codice
                                               des-codice
                    set cli-tipo-C          to true
                    move tor-prg-destino    to zoom-bol-prg-destino
                                               des-prog

                    read clienti no lock
                       invalid 
                          initialize cli-rec
                    end-read
                    move cli-ragsoc-1       to zoom-bol-cli-ragsoc-1

                    read destini no lock
                       invalid
                          initialize des-rec
                          move cli-localita to des-localita
                    end-read
                    move des-localita       to zoom-bol-des-localita

                    move tor-num-fattura    to zoom-bol-num-fat
                    move tor-data-fattura   to zoom-bol-data-fat

                    write zoom-bol-rec
                       invalid 
                          continue 
                    end-write
                    set trovato to true
                 end-perform
           end-start.
           close zoom-bolle.

           call "W$MOUSE" using set-mouse-shape, arrow-pointer.

           if trovato
              move low-value to zoom-bol-anno
                                zoom-bol-numero

              move "tordini-bolla" to como-file
              move path-zoom-bolle to ext-file
              call   "zoom-gt" using como-file, zoom-bol-rec
                               giving stato-zoom
              cancel "zoom-gt"
           else
               move 1 to stato-zoom
           end-if
           delete file zoom-bolle

           if stato-zoom = 0
              move zoom-bol-chiave to tor-chiave
              read tordini no lock
                 invalid
                    continue
              end-read
           else
              set errori  to true
           end-if 
           .
      * <TOTEM:END>

       RIEMPI-TMP-BOLLE.
      * <TOTEM:PARA. RIEMPI-TMP-BOLLE>
           call "W$MOUSE" using set-mouse-shape, wait-pointer.
           set record-ok to true

           accept  path-zoom-bolle        from environment "PATH_ST".
           accept  como-data              from century-date.
           accept  como-ora               from time.
           inspect path-zoom-bolle        replacing trailing
                                          spaces by low-value.
           string path-zoom-bolle         delimited by low-value
                  "zoom-bolle"            delimited by size
                  "_"                     delimited by size
                  como-data               delimited by size
                  "_"                     delimited by size
                  como-ora                delimited by size
                  into path-zoom-bolle
           end-string.

           set trovato to false.
           open output zoom-bolle.
           initialize tor-rec.
           move esercizio to tor-anno-bolla.

           move 1         to tor-num-bolla

           start tordini key  not < k-bolla
              invalid 
                 continue 
              not invalid
                 perform until 1 = 2
                    read tordini next no lock 
                       at end 
                          exit perform 
                    end-read
                    
                    if tor-anno-bolla not = esercizio
                       exit perform
                    end-if

                    set record-ok to true

                    if tor-fattura-manuale
                       set record-ok to false
                    end-if

                    if tor-data-fattura = 0 or
                       tor-num-fattura  = 0 or
                       tor-anno-fattura = 0 or
                       tor-num-prenot   = 0
                       set record-ok to false
                    end-if

                    if record-ok

                       move tor-anno-bolla  to zoom-bol-anno
                       move tor-num-bolla   to zoom-bol-numero

                       move tor-chiave      to zoom-bol-tor-chiave

                       move tor-anno-fattura  to zoom-bol-anno
                       move tor-num-fattura   to zoom-bol-numero
                       move tor-data-fattura  to zoom-bol-data-ordine
                       move tor-causale       to tca-codice
                       read tcaumag 
                          invalid 
                             initialize tca-caus-trasporto 
                       end-read
                       move tca-caus-trasporto to zoom-bol-causale
                       move tor-cod-cli        to zoom-bol-cod-cli
                                                  cli-codice
                                                  des-codice
                       set cli-tipo-C          to true
                       move tor-prg-destino    to zoom-bol-prg-destino
                                                  des-prog

                       read clienti no lock
                          invalid 
                             initialize cli-rec
                       end-read
                       move cli-ragsoc-1       to zoom-bol-cli-ragsoc-1

                       read destini no lock
                          invalid
                             initialize des-rec
                             move cli-localita to des-localita
                       end-read
                       move des-localita       to zoom-bol-des-localita

                       write zoom-bol-rec
                          invalid 
                             continue 
                       end-write
                       set trovato to true
                    end-if

                 end-perform
           end-start.

           close zoom-bolle.

           call "W$MOUSE" using set-mouse-shape, arrow-pointer 
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       ef-anno-BeforeProcedure.
      * <TOTEM:PARA. ef-anno-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-anno-AfterProcedure.
      * <TOTEM:PARA. ef-anno-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       ef-num-BeforeProcedure.
      * <TOTEM:PARA. ef-num-BeforeProcedure>
           move 1 to e-cerca.
           modify tool-cerca, enabled = e-cerca.
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-num-AfterProcedure.
      * <TOTEM:PARA. ef-num-AfterProcedure>
           move 0 to e-cerca.
           modify tool-cerca, enabled = e-cerca.
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       TOOL-CERCA-LinkTo.
      * <TOTEM:PARA. TOOL-CERCA-LinkTo>
           inquire tool-cerca, enabled in e-cerca.

           if e-cerca = 1 
              perform CERCA 
           end-if 
           .
      * <TOTEM:END>
       PB-ESEGUI-BeforeProcedure.
      * <TOTEM:PARA. PB-ESEGUI-BeforeProcedure>
           modify PB-ESEGUI, bitmap-number 2
           .
      * <TOTEM:END>
       PB-ESEGUI-AfterProcedure.
      * <TOTEM:PARA. PB-ESEGUI-AfterProcedure>
           modify PB-ESEGUI, bitmap-number 1
           .
      * <TOTEM:END>
       PB-ESEGUI-LinkTo.
      * <TOTEM:PARA. PB-ESEGUI-LinkTo>
           set tutto-ok     to true.
           move 0 to tor-anno-fattura tno-num-fattura.

           inquire ef-anno,  value in anno.
           move       anno         to tor-anno-fattura 
                                      tno-anno-fattura
                                      tor-anno-bolla.
           inquire ef-num,   value in numero.
           move       numero       to tor-num-fattura. 
           inquire ef-nota,  value in numero-nota.
           move       numero-nota  to tno-num-fattura.
           inquire ef-bolla, value in numero-bolla.
           move       numero-bolla to tor-num-bolla.

           if tor-num-fattura not = 0 and
              tno-num-fattura not = 0 and
              tor-num-bolla   not = 0
              set errori to true
              display message 
                          "Selezionare fattura, nota credito o bolla"
                        title tit-err
                         icon 2
           end-if

           if tutto-ok
              if tor-num-fattura not = 0
                 read tordini no lock key is k-fattura
                      invalid 
                      set errori to true
                      display message "Fattura non caricata"
                              title = tit-err
                              icon mb-warning-icon
                  not invalid
                      if tor-data-fattura = 0 or
                         tor-num-fattura  = 0 or
                         tor-anno-fattura = 0 or
                         tor-num-prenot   = 0
                         set errori to true
                         display message "Documento non fatturato"
                                 title tit-err
                                 icon mb-warning-icon
                      else
                         if tor-fattura-manuale
                            set errori to true
                            display message "Documento non valido"
                                    title tit-err
                                     icon mb-warning-icon
                         else
                            move tor-anno-fattura to cnt-anno-fatt
                            move tor-num-fattura  to cnt-num-fatt
                            read contestazioni key k-fattura
                                 invalid continue
                             not invalid
                                 move cnt-numero to numero-edit
                                 display message
                                "ATTENZIONE!!"
                         x"0d0a""Esiste già la contestazione n. " 
           numero-edit " anno " cnt-anno
                         x"0d0a""che fa riferimento alla fattura indicat
      -    "a."
                         x"0d0a""Procedere comunque?"
                                           title titolo
                                            type mb-yes-no
                                            icon 2
                                          giving scelta
                                 if scelta = mb-no
                                    set errori to true
                                 end-if
                            end-read
                         end-if
                      end-if
                 end-read
              end-if
              if tno-num-fattura not = 0
                 read tnotacr no lock key k-fattura
                      invalid 
                      set errori to true
                      display message "Nota cr. non caricata"
                                title tit-err
                                 icon 2
                  not invalid
                      if tno-data-fattura = 0 or
                         tno-num-fattura  = 0 or
                         tno-anno-fattura = 0 or
                         tno-num-prenot   = 0
                         set errori to true
                         display message "Documento non fatturato"
                                 title tit-err
                                 icon mb-warning-icon
                      else
                         move tno-anno-fattura to cnt-anno-nota
                         move tno-num-fattura  to cnt-num-nota
                         read contestazioni key k-nota
                              invalid
                              move tno-anno   to noco-anno
                              move tno-numero to noco-numero
                              read note-cont  no lock 
                                   invalid continue
                               not invalid
                                   if noco-anno-cont(1)   not = 0 or
                                      noco-numero-cont(1) not = 0 or
                                      noco-anno-cont(2)   not = 0 or
                                      noco-numero-cont(2) not = 0 or
                                      noco-anno-cont(3)   not = 0 or
                                      noco-numero-cont(3) not = 0 or
                                      noco-anno-cont(4)   not = 0 or
                                      noco-numero-cont(4) not = 0 or
                                      noco-anno-cont(5)   not = 0 or
                                      noco-numero-cont(5) not = 0
                                      display message
                                      "ATTENZIONE!!"
                         x"0d0a""Esistono già contestazioni collegate "
                         x"0d0a""alla nota credito indicata."
                         x"0d0a""Procedere comunque?"
                                           title titolo
                                            type mb-yes-no
                                            icon 2
                                          giving scelta
                                      if scelta = mb-no
                                         set errori to true
                                      end-if
                                   end-if
                              end-read
                          not invalid
                              move cnt-numero to numero-edit
                              display message
                                "ATTENZIONE!!"
                         x"0d0a""Esiste già la bozza n. " numero-edit " 
      -    "anno " cnt-anno
                         x"0d0a""che fa riferimento alla nota cr. indica
      -    "ta."
                         x"0d0a""Procedere comunque?"
                                        title titolo
                                         type mb-yes-no
                                         icon 2
                                       giving scelta
                              if scelta = mb-no
                                 set errori to true
                              end-if
                         end-read
                      end-if
                 end-read
              end-if

              if tor-num-bolla not = 0 and tor-num-fattura = 0 |Altrimenti vuol dire che ho scelto la fattura
                 perform TRATTA-BOLLA
              end-if
           end-if.

           perform CANCELLA-COLORE.

           if tutto-ok
              modify Form2-Handle, visible = 0
              evaluate true
              when tor-num-fattura not = 0
                   move livello-abil     to sav-livello-abil
                   move 1                to livello-abil
                   move tor-anno         to LinkAnno
                   move tor-numero       to LinkNumero
                   move "selcont"        to LinkPgm
                   call "gordcvar"    using lk-blockpgm,
                                            user-codi,
                                            livello-abil,
                                            gordcvar-linkage
                   cancel "gordcvar"
                   move sav-livello-abil to livello-abil
                   if LinkStatus = 1
                      move mb-yes to scelta
                   else
                      move mb-no  to scelta
                   end-if
              when tno-num-fattura not = 0
                   move livello-abil     to sav-livello-abil
                   move 1                to livello-abil
                   move tno-anno         to NotaAnno
                   move tno-numero       to NotaNumero
                   move "NCPZ"           to Link-TipoNota |FITTIZIA!!!
                   move "selcont"        to LinkPgm
                   call "notavar"     using lk-blockpgm,
                                            user-codi,
                                            livello-abil,
                                            notacr-linkage
                   cancel "notavar"
                   move sav-livello-abil to livello-abil
                   if LinkStatusNota = 1
                      move mb-yes to scelta
                   else
                      move mb-no  to scelta
                   end-if
              when other
                   move mb-yes to scelta
              end-evaluate
              if scelta = mb-yes
                 perform CREA-CONTESTAZIONE
              end-if
              modify Form2-Handle, visible = 1
              move 27 to key-status
           else
              move 1002 to control-id
           end-if.

           move    4    to accept-control.
           modify pb-esegui, bitmap-number = 1.
           modify pb-cancel, bitmap-number = 1 
           .
      * <TOTEM:END>
       Form1-Pb-2-BeforeProcedure.
      * <TOTEM:PARA. Form1-Pb-2-BeforeProcedure>
           modify pb-cancel, bitmap-number 2
           .
      * <TOTEM:END>
       Form1-Pb-2-AfterProcedure.
      * <TOTEM:PARA. Form1-Pb-2-AfterProcedure>
           modify pb-cancel, bitmap-number 1
           .
      * <TOTEM:END>
       selnota-Ev-Before-Program.
      * <TOTEM:PARA. selnota-Ev-Before-Program>
           accept esercizio-x  from environment "ESERCIZIO".
           move   esercizio-x  to esercizio 
           move LK-BL-PROG-ID    TO COMO-PROG-ID 
           .
      * <TOTEM:END>
       selnota-Ev-After-Program.
      * <TOTEM:PARA. selnota-Ev-After-Program>
           SET LK-BL-CANCELLAZIONE TO TRUE.
           MOVE COMO-PROG-ID       TO LK-BL-PROG-ID.
           CALL "BLOCKPGM"  USING LK-BLOCKPGM 
           .
      * <TOTEM:END>
       ef-bolla-BeforeProcedure.
      * <TOTEM:PARA. ef-bolla-BeforeProcedure>
           move 1 to e-cerca.
           modify tool-cerca, enabled = e-cerca.
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-bolla-AfterProcedure.
      * <TOTEM:PARA. ef-bolla-AfterProcedure>
           move 0 to e-cerca.
           modify tool-cerca, enabled = e-cerca.
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>

      *{TOTEM}END

      *{TOTEM}SHOW-MSG
       MESSAGE-FOR-FILEERROR.
           CALL "C$RERRNAME" USING TOTEM-MSG-ERR-FILE
           CALL "C$RERR" USING EXTEND-STAT, TEXT-MESSAGE
              MOVE PRIMARY-ERROR TO TOTEM-MSG-ID
           .

       SHOW-TRANSACTION-ROUTINE.
           IF TOTEM-TRANSACTION-FLAG = SPACES
              IF TRANSACTION-STATUS NOT = 0
                 STRING "TRANSACTION ERROR ", TRANSACTION-STATUS
                    DELIMITED BY SIZE INTO TOTEM-MSG-1
              END-IF
           ELSE
              PERFORM SHOW-MSG-ROUTINE
              IF TRANSACTION-STATUS = 0
                 MOVE "Transazione terminata con Rollback." TO 
           TOTEM-MSG-3
              ELSE
                 STRING "TRANSACTION ERROR ", TRANSACTION-STATUS
                    DELIMITED BY SIZE INTO TOTEM-MSG-3
              END-IF
           END-IF
           .

       SHOW-MSG-ROUTINE.
           MOVE SPACE TO TOTEM-MSG-1 TOTEM-MSG-2 TOTEM-MSG-3
           EVALUATE TOTEM-MSG-ID
               WHEN "10"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Nessun altro dato." TO TOTEM-MSG-2
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "22"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Chiave Duplicata." TO TOTEM-MSG-2
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "23"
                    STRING "File:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-1
                    MOVE "Raggiunta fine file." TO TOTEM-MSG-2
                    MOVE MB-WARNING-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "101"
                    MOVE "Terminare l'applicazione?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "201"
                    MOVE "Aggiungere un nuovo record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "202"
                    MOVE "Aggiornare il Record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "203"
                    MOVE "Cancellare il Record?" TO TOTEM-MSG-1
                    MOVE 4 TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-YES-NO TO TOTEM-MSG-BUTTON-TYPE
               WHEN "204"
                    MOVE "Chiave duplicata." TO TOTEM-MSG-1
                    MOVE MB-WARNING-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "301"
                    MOVE "Inserimento avvenuto con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "302"
                    MOVE "Aggiornamento avvenuto con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "303"
                    MOVE "Cancellazione avvenuta con Successo." TO 
           TOTEM-MSG-1
                    MOVE MB-DEFAULT-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN "401"
                    MOVE "Shell non trovata." TO TOTEM-MSG-1
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
               WHEN OTHER
                    MOVE TEXT-MESSAGE TO TOTEM-MSG-1
                    STRING "FILE:" TOTEM-MSG-ERR-FILE DELIMITED BY SPACE
                       INTO TOTEM-MSG-3
                    MOVE 0 TO TOTEM-IDX1
                    INSPECT TOTEM-MSG-3 TALLYING TOTEM-IDX1
                       FOR TRAILING SPACE
                    STRING TOTEM-MSG-3(1:TOTEM-MSG-LENGTH - 
           TOTEM-IDX1), 
                       ", FILE STATUS ", PRIMARY-ERROR "," 
                       SECONDARY-ERROR
                    DELIMITED BY SIZE INTO TOTEM-MSG-2
                    MOVE SPACES TO TOTEM-MSG-3
                    MOVE MB-ERROR-ICON TO TOTEM-MSG-ICON-TYPE
                    MOVE MB-OK TO TOTEM-MSG-BUTTON-TYPE
           END-EVALUATE
           PERFORM MESSAGE-BOX-ROUTINE
           .

       MESSAGE-BOX-ROUTINE.
           MOVE 1 TO TOTEM-MSG-TEXT-POINTER
           IF TOTEM-MSG-1 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-1 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              STRING TOTEM-MSG-1( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                 DELIMITED BY SIZE
                 INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-2 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-2 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              IF TOTEM-MSG-TEXT-POINTER > 1
                 STRING X"0A" DELIMITED BY SIZE
                     INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
              END-IF
              STRING TOTEM-MSG-2( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                  DELIMITED BY SIZE
                  INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-3 NOT = SPACE
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-3 TALLYING TOTEM-MSG-SIZE FOR TRAILING 
           SPACE
              IF TOTEM-MSG-TEXT-POINTER > 1
                 STRING X"0A" DELIMITED BY SIZE
                     INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
              END-IF
              STRING TOTEM-MSG-3( 1 : TOTEM-MSG-LENGTH - TOTEM-MSG-SIZE 
           )
                  DELIMITED BY SIZE
                  INTO TOTEM-MSG-TEXT, POINTER TOTEM-MSG-TEXT-POINTER
           END-IF

           IF TOTEM-MSG-TEXT-POINTER = 1
              MOVE 0 TO TOTEM-MSG-SIZE
              INSPECT TOTEM-MSG-TEXT TALLYING TOTEM-MSG-SIZE FOR 
           TRAILING SPACE
                  COMPUTE TOTEM-MSG-TEXT-POINTER = 
           TOTEM-MSG-FULL-LENGTH - TOTEM-MSG-SIZE + 1
           END-IF
           MOVE LOW-VALUES TO TOTEM-MSG-TEXT(TOTEM-MSG-TEXT-POINTER : 1 
           )
           .

       MESSAGE-BOX-DISPLAY.
           PERFORM SHOW-MSG-ROUTINE
           PERFORM MESSAGE-BOX-ROUTINE
           DISPLAY MESSAGE BOX TOTEM-MSG-TEXT
               TITLE IS TOTEM-MSG-TITLE
               TYPE  IS TOTEM-MSG-BUTTON-TYPE
               ICON  IS TOTEM-MSG-DEFAULT-BUTTON
               RETURNING TOTEM-MSG-RETURN-VALUE
           .

      *{TOTEM}END

