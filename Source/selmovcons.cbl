      * USER DEFINE PARAGRAPH
       CAMBIA-FONT-RIGA.
      * <TOTEM:PARA. CAMBIA-FONT-RIGA>
           modify gd-mov(riga), ROW-FONT = small-font.

           modify gd-mov(riga, 1),
                  bitmap        = elemento-bmp,
                  bitmap-number = 1,
                  bitmap-width  = 16.

           move event-data-2 to riga.
           modify gd-mov(riga), ROW-FONT = font-evidenzia-griglia.


           modify gd-mov(riga, 1),
                  bitmap        = elemento-bmp,
                  bitmap-number = 2,
                  bitmap-width  = 16.

      *    cambio la label legenda
      *     display form2 
           .
      * <TOTEM:END>

       CARICA-MOVIMENTI.
      * <TOTEM:PARA. CARICA-MOVIMENTI>
           move anno-to      to tmo-anno.
           move data-from    to tmo-data-movim.
           move ordine-from  to tmo-numero.

           start tmovmag key not < k-data
                 invalid continue
             not invalid perform SCORRI-ORDINI
           end-start 

           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-causale-to
                move "tcaumag"  to como-file         
                inquire ef-causale-to, value in tca-codice
                call "zoom-gt"   using como-file, tca-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tca-codice      to causale-to
                   display ef-causale-to
                end-if
           when 78-ID-ef-causale-from
                move "tcaumag"  to como-file         
                inquire ef-causale-from, value in tca-codice
                call "zoom-gt"   using como-file, tca-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tca-codice      to causale-from
                   display ef-causale-from
                end-if
           end-evaluate 
           .
      * <TOTEM:END>

       CHIAMA-MANUTENZIONE.
      * <TOTEM:PARA. CHIAMA-MANUTENZIONE>
           move como-anno          to tmo-anno.
           move como-numero-ordine to tmo-numero.

           modify form2-Handle, visible false.
           call   "gmovcvar" using lk-blockpgm
                                   user-codi
                                   livello-abil
                                   tmo-chiave
                                   LinkRicaricaGrid.
           cancel "gmovcvar".                 
           modify form2-Handle, visible true.

           inquire gd-mov, cursor-y in riga.
           
           set CallManutenzione to false.

LUBEXX     if SceltaDaOrdineSingolo
LUBEXX        set RicaricaGrid to false
LUBEXX     end-if.

           if RicaricaGrid
              move riga to store-riga
              perform FORM3-OPEN-ROUTINE
              move store-riga to riga
           end-if.
           
           modify form2-Handle, visible = 1 .
           move riga to event-data-2.
           modify gd-mov, CURSOR-Y = riga.
           perform CAMBIA-FONT-RIGA 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID
      *
           when 78-ID-ef-anno
                inquire ef-anno, value in anno-to
                if anno-to = 0
                   set errori  to true
                   display message "Inserimento anno mancante"
                           title tit-err
                           icon  mb-warning-icon
                   move 78-ID-ef-anno   to store-id
                else
                   if anno-to > DataConsolidamento(1:4)
                      set errori  to true
                      display message 
                      "Anno superiore a quello di consolidamento"
                                title tit-err
                                 icon 2
                      move 78-ID-ef-anno   to store-id
                   end-if
                end-if
           when 78-ID-ef-ordine-from
                if tipo-elab = 1
                   if ordine-from = zero
                      if controllo-finale
                         set errori to true                             
                              

                         move 78-ID-ef-ordine-from to CONTROL-ID
                         display message "Il numero di movimento è obbli
      -    "gatorio!"
                                 title tit-err
                                 icon  MB-WARNING-ICON
                      end-if
                   else
      *                move anno-from     to tmo-anno
                      move anno-to       to tmo-anno
                      move ordine-from   to tmo-numero
                      read tmovmag
                         invalid 
                             set errori to true
                             move 78-ID-ef-ordine-from to CONTROL-ID
                             display message "Movimento inesistente!"
                                      title tit-err
                                      icon  MB-WARNING-ICON
                      end-read
                   end-if
                end-if
      *
           when 78-ID-ef-ordine-to
                if ordine-to = ZERO
                   move all "9" to ordine-to
                   modify EF-ORDINE-TO, VALUE ordine-to
                end-if
                if ordine-to < ordine-from
                   set errori to true
                   move 78-ID-ef-ordine-to to CONTROL-ID
                   display message MSG-intervallo-num-ord-err
                      title tit-err
                      icon  MB-WARNING-ICON
                end-if
      *
           when 78-ID-ef-data-from
                if data-from not = zero
                   move data-from to como-data
                   perform DATE-FORMAT
                   move como-data to data-from
                end-if

           when 78-ID-ef-data-to
                if data-to = zero
                   move 99999999 to data-to
                end-if
                if data-to not = 99999999
                   move data-to to como-data
                   perform DATE-FORMAT
                   move como-data to data-to
                end-if

                move data-from to como-data
                perform DATE-TO-FILE
                move como-data to data-from-AAAAMMGG

                move data-to   to como-data
                perform DATE-TO-FILE
                move como-data to data-to-AAAAMMGG

                if data-to-AAAAMMGG < data-from-AAAAMMGG
                   set errori to true
                   move 78-ID-ef-data-to to CONTROL-ID
                   display message MSG-intervallo-date-err
                      title tit-err
                      icon  MB-WARNING-ICON
                end-if
      *
           when 78-ID-ef-causale-to
                if causale-to = ZERO
                   move all "9" to causale-to
                   modify ef-causale-to, value causale-to
                end-if
                if causale-to < causale-from
                   set errori to true
                   move 78-ID-ef-causale-to to control-id
                   display message MSG-intervallo-causale-err
                      title tit-err
                      icon  MB-WARNING-ICON
                end-if

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if.

           display form1 
           .
      * <TOTEM:END>

       METTI-IN-GRIGLIA.
      * <TOTEM:PARA. METTI-IN-GRIGLIA>
           move tmo-anno       to Col-anno.

           move tmo-numero     to Col-numero

           move tmo-data-movim to como-data.
           perform DATE-TO-SCREEN.
           move como-data      to col-data.

           move tmo-causale          to tca-codice.
           move "** NON TROVATA **"  to tca-descrizione.
           move "*"                  to tca-tipo.
           read tcaumag no lock invalid continue end-read.
           move tca-descrizione      to col-descau.
           
           move tca-tipo to col-clifor cli-tipo-CF.

           move tmo-cod-clifor       to cli-codice.
           move "** NON TROVATA **"  to cli-ragsoc-1.
           read clienti no lock invalid continue end-read.
           move cli-ragsoc-1         to col-rag-soc-clifor.

           move tmo-numdoc-clifor    to col-numdoc.

           move tmo-data-doc         to como-data.
           perform DATE-TO-SCREEN.
           move como-data            to col-datadoc.

           modify gd-mov, record-to-add rec-grid

           inquire gd-mov, last-row = riga.

           modify gd-mov(riga), row-color = como-colore. 
           set trovato to true 
           .
      * <TOTEM:END>

       ORDINE-DA-GRIGLIA.
      * <TOTEM:PARA. ORDINE-DA-GRIGLIA>
           inquire gd-mov(riga), record-data = rec-grid.

           move col-anno            to como-anno.
           move Col-numero          to como-numero-ordine.
           move riga                to como-riga.

           perform CHIAMA-MANUTENZIONE.

           move como-riga to riga, event-data-2.
           modify gd-mov, cursor-y riga.
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "utydata.cpy".
           copy "color-custom.cpy".
           copy "calcola-colore-trasparente.cpy" 
           .
      * <TOTEM:END>

       SPOSTAMENTO.
      * <TOTEM:PARA. SPOSTAMENTO>
           inquire gd-mov, last-row in tot-righe.
           if event-data-2 < 2 move 2 to event-data-2 end-if.
           if event-data-2 > tot-righe move tot-righe to event-data-2 
           end-if.
           if riga not = event-data-2 
              perform CAMBIA-FONT-RIGA
           end-if 
           .
      * <TOTEM:END>

       SCORRI-ORDINI.
      * <TOTEM:PARA. SCORRI-ORDINI>
           set trovato to false.

           modify gd-mov, mass-update = 1.
           perform until 1 = 2
              read tmovmag next at end exit perform end-read

              if tmo-data-movim > DataConsolidamento
                 exit perform 
              end-if
              
              perform VALIDA-MOVIMENTO
              
              if record-ok perform METTI-IN-GRIGLIA end-if

           end-perform.
           modify gd-mov, mass-update = 0 
           .
      * <TOTEM:END>

       VALIDA-MOVIMENTO.
      * <TOTEM:PARA. VALIDA-MOVIMENTO>
           set record-ok to true.

           if tmo-numero < ordine-from or 
              tmo-numero > ordine-to
              set record-ok to false
           end-if.

           if tmo-data-movim < data-from-AAAAMMGG or 
              tmo-data-movim > data-to-AAAAMMGG
              set record-ok to false
           end-if.

           if tmo-causale < causale-from or 
              tmo-causale > causale-to
              set record-ok to false
           end-if 
           .
      * <TOTEM:END>

