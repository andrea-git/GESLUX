      * <TOTEM:EPT. FD:DataSet1, FD:tmovmag, BeforeReadNext>
      * <TOTEM:END>
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tmovmag-KEY1-Asc
                 IF DataSet1-tmovmag-LOCK
                    READ tmovmag NEXT WITH LOCK
                 ELSE
                    READ tmovmag NEXT WITH NO LOCK
                 END-IF
              ELSE
