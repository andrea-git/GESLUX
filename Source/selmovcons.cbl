       DataSet1-tmovmag-START-LESS.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tmovmag-KEY1-Asc
                 START tmovmag KEY < tmo-chiave
              ELSE
                 START tmovmag KEY > tmo-chiave
              END-IF
           END-EVALUATE
