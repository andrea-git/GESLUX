      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-num-reg
                inquire ef-anno,    value in utf-anno
                inquire ef-num-reg, value in utf-num-reg

                move "anautf-anno"  to como-file
                call "zoom-gt"   using como-file, utf-rec
                                giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move utf-num-reg to ef-num-reg-buf
                   display ef-num-reg
                   set tutto-ok to true
                   perform FILD-TU-BAF
                   display scr-stampa
                end-if
           end-evaluate
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate control-id

           when 78-ID-ef-num-reg
                inquire ef-anno,      value in utf-anno
                inquire ef-num-reg,   value in utf-num-reg
                inquire ef-data-da,   value in ef-data-da-buf
                read anautf no lock
                     invalid
                     initialize utf-dati replacing numeric data by zero
                                              alphanumeric data by space
                     set errori to true
                     display message "Numero registro NON valido"
                               title tit-err
                                icon 2
                end-read
                perform FILD-TU-BAF
                display scr-stampa

           when 78-ID-ef-data-da
                inquire ef-data-da, value in ef-data-da-buf
                move ef-data-da-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-da-buf
                perform DATE-TO-FILE
                if como-data <= utf-data-ult-stampa
                   set errori to true
                   display message 
                           "La data di partenza dev'essere superiore"
                    x"0d0a""alla data di ultima stampa da registro"
                             title tit-err
                              icon 2
                end-if
                display ef-data-da
      *                       
           when 78-ID-ef-data-a                              
                inquire ef-data-da,  value in ef-data-da-buf
                inquire ef-data-a,   value in ef-data-a-buf
                move ef-data-a-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-a-buf

                move como-data to ef-data-a-buf
                display ef-data-a

                perform DATE-TO-FILE
                move como-data        to como-data-to

                move ef-data-da-buf to como-data
                perform DATE-TO-FILE
                move como-data    to como-data-from

                if como-data-to < como-data-from
                   set errori to true
                   display message box "Intervallo limiti data errato!"
                           title = tit-err
                           icon 2
                   move 78-ID-ef-data-da to control-id
                else
                   if como-data-to(1:4) not = como-data-from(1:4)
                      set errori to true
                      display message "L'anno delle date NON coincide!"
                                title tit-err
                                 icon 2
                      move 78-ID-ef-data-da to control-id
                   else
                      if como-data-to(5:2) not = como-data-from(5:2)
                         set errori to true
                         display message "Il mese delle date NON coincid
      -    "e!"
                                   title tit-err
                                    icon 2
                         move 78-ID-ef-data-da to control-id
                      end-if
                   end-if
                end-if

           end-evaluate.

           if errori
              perform  CANCELLA-COLORE
              move 4 to accept-control
              modify pb-sib, bitmap-number = 1
              modify pb-noa, bitmap-number = 1
           end-if 
           .
      * <TOTEM:END>

       FILD-TU-BAF.
      * <TOTEM:PARA. FILD-TU-BAF>
           initialize lab-descr-buf.
           inspect utf-desc1 replacing trailing spaces by low-value
           string  utf-desc1 delimited low-value
                   " "       delimited size
                   utf-desc2 delimited size
                   into lab-descr-buf
           end-string.
           move utf-data-ult-stampa    to  como-data.
           perform DATE-TO-SCREEN.
           move como-data              to  lab-data-buf.
           move utf-giac-bianchi       to  lab-giac-bianchi-buf.
           move utf-giac-lubrif        to  lab-giac-lubrif-buf.
           move utf-prog-e-bianchi     to  lab-carico-bianchi-buf.
           move utf-prog-e-lubrif      to  lab-carico-lubrif-buf.
           move utf-prog-u-bianchi     to  lab-scarico-bianchi-buf.
           move utf-prog-u-lubrif      to  lab-scarico-lubrif-buf.
           move utf-num-prog-riga-reg  to  lab-prog-buf.
           move utf-ragsoc             to  lab-ragsoc-buf.
           move utf-modello            to  lab-modello-buf.

           if tutto-ok
              if utf-data-ult-stampa not = 0 
                 compute como-data   =
                         function INTEGER-OF-DATE(utf-data-ult-stampa)
                 add 1 to como-data
                 compute como-data =
                         function DATE-OF-INTEGER(como-data)
                 perform DATE-TO-SCREEN
              end-if
           else
              move 0 to como-data
           end-if.
           move como-data to ef-data-da-buf ef-data-a-buf 
           .
      * <TOTEM:END>

