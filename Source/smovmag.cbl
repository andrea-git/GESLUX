      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate CONTROL-ID
           when 78-ID-EF-caus
                inquire ef-caus, value tca-codice
                move "tcaumag"     to Como-File
                call   "zoom-gt" using como-file, tca-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-caus, value tca-codice
                   move 4 to ACCEPT-CONTROL
                end-if

           when 78-ID-EF-num-from
                inquire ef-num-from value tmo-numero
                inquire ef-anno,    value tmo-anno
                inquire ef-caus,    value tmo-causale
                move "tmovmag-cau"  to como-File
                call   "zoom-gt" using como-file, tmo-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-num-from, value tmo-numero
                   move 4 to ACCEPT-CONTROL
                end-if

           when 78-ID-EF-num-to
                inquire ef-num-to,   value tmo-numero
                inquire ef-anno,     value tmo-anno
                inquire ef-caus,     value tmo-causale
                move "tmovmag-cau"     to Como-File
                call   "zoom-gt" using como-file, tmo-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-num-to, value tmo-numero
                   move 4 to ACCEPT-CONTROL
                end-if
           end-evaluate  
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID

      * ANNO DI FINE SELEZIONE
           when 78-ID-ef-anno
                inquire ef-anno, value in anno-to
                if anno-to = ZERO
                   set errori  to true
                   display message "Inserimento anno mancante"
                           title tit-err
                           icon  mb-warning-icon
                   move 78-ID-ef-anno   to store-id
                end-if

      * CAUSALE
           when 78-ID-ef-caus
                inquire ef-caus, value tca-codice
                move spaces to lbl-des-caus-buf

                if tca-codice not = spaces
                   read tcaumag no lock
                        invalid
                           set errori  to true
                           move spaces to lbl-des-caus-buf
                           display message "Codice Causale NON valido!"
                                   title tit-err
                                   icon  mb-warning-icon
                           move 78-ID-ef-caus   to store-id
                    not invalid
                           move tca-descrizione to lbl-des-caus-buf
                   end-read
                end-if

                display lbl-des-caus

      * NUMERO DI FINE SELEZIONE
           when 78-ID-ef-num-to
                inquire ef-num-to,    value num-to
                inquire ef-num-from,  value num-from

                if num-to = 0
                   move all "9" to num-to ef-num-to-buf
                   display ef-num-to
                end-if

                if num-to < num-from
                   set errori to true
                   display message "Intervallo movimenti errato"
                           title tit-err
                           icon  mb-warning-icon
                   move 78-ID-ef-num-from to store-id
                end-if

           end-evaluate.
                        
           if errori                  
              move store-id to CONTROL-ID
              move 4        to ACCEPT-CONTROL
           end-if.

           perform CANCELLA-COLORE 
           .
      * <TOTEM:END>

