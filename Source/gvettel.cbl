      *{TOTEM}COPY-PROCEDURE
       EXIT-STOP-ROUTINE.
           PERFORM CLOSE-FILE-RTN
      * <TOTEM:EPT. INIT:gvettel, INIT:gvettel, BeforeDestroyResource>
      * <TOTEM:END>
           CALL "w$bitmap" USING WBITMAP-DESTROY, SETTODEF-BMP
           CALL "w$bitmap" USING WBITMAP-DESTROY, MAKEDIR-BMP
           CALL "w$bitmap" USING WBITMAP-DESTROY, toolbar-bmp
      *    After-Program
           EXIT PROGRAM TOTEM-PgmStatus
           STOP RUN.

       INITIALIZE-ROUTINE.
      *    Before Init
      * initialize program status variable
           Initialize TOTEM-PgmStatus.
      * get system information
           ACCEPT SYSTEM-INFORMATION FROM SYSTEM-INFO.
      * get terminal information
           ACCEPT TERMINAL-ABILITIES FROM TERMINAL-INFO.
      * set font
           PERFORM INIT-FONT.
      * load bitmap
           PERFORM INIT-BMP.
      * load resource
           PERFORM INIT-RES.
      * create pop-up menu
           PERFORM INIT-POPUP.
      * open files
           PERFORM OPEN-FILE-RTN.
           PERFORM gcli-tel-Ev-After-Init
           .
    
       INIT-FONT.
           .

       INIT-BMP.
      * pb-settodef
           COPY RESOURCE "SETTODEF.BMP".
           CALL "w$bitmap" USING WBITMAP-LOAD "SETTODEF.BMP", 
                   GIVING SETTODEF-BMP.
      * pb-makedir
           COPY RESOURCE "MAKEDIR.BMP".
           CALL "w$bitmap" USING WBITMAP-LOAD "MAKEDIR.BMP", 
                   GIVING MAKEDIR-BMP.
      * TOOL-ESCI
           COPY RESOURCE "toolbar.bmp".
           CALL "w$bitmap" USING WBITMAP-LOAD "toolbar.bmp", 
                   GIVING toolbar-bmp.
           .

       INIT-RES.
           .

       INIT-POPUP.
           .

       OPEN-FILE-RTN.
      *    Before Open
           PERFORM OPEN-vettel
           PERFORM OPEN-tvettori
      *    After Open
           .

       OPEN-vettel.
      * <TOTEM:EPT. INIT:gvettel, FD:vettel, BeforeOpen>
      * <TOTEM:END>
           OPEN  I-O vettel
           IF STATUS-vettel = "35"
              OPEN OUTPUT vettel
                IF Valid-STATUS-vettel
                   CLOSE vettel
                   OPEN I-O vettel
                END-IF
           END-IF
           IF NOT Valid-STATUS-vettel
              PERFORM  Form1-EXTENDED-FILE-STATUS
              GO TO EXIT-STOP-ROUTINE
           END-IF
      * <TOTEM:EPT. INIT:gvettel, FD:vettel, AfterOpen>
      * <TOTEM:END>
           .

       OPEN-tvettori.
      * <TOTEM:EPT. INIT:gvettel, FD:tvettori, BeforeOpen>
      * <TOTEM:END>
           OPEN  INPUT tvettori
           IF NOT Valid-STATUS-tvettori
              PERFORM  Form1-EXTENDED-FILE-STATUS
              GO TO EXIT-STOP-ROUTINE
           END-IF
      * <TOTEM:EPT. INIT:gvettel, FD:tvettori, AfterOpen>
      * <TOTEM:END>
           .

       CLOSE-FILE-RTN.
      *    Before Close
           PERFORM CLOSE-vettel
           PERFORM CLOSE-tvettori
      *    After Close
           .

       CLOSE-vettel.
      * <TOTEM:EPT. INIT:gvettel, FD:vettel, BeforeClose>
      * <TOTEM:END>
           CLOSE vettel
           .

       CLOSE-tvettori.
      * <TOTEM:EPT. INIT:gvettel, FD:tvettori, BeforeClose>
      * <TOTEM:END>
           CLOSE tvettori
           .

       DataSet1-vettel-INITSTART.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-vettel-KEY1-Asc
                 MOVE Low-Value TO vtt-chiave OF vettel
              ELSE
                 MOVE High-Value TO vtt-chiave OF vettel
              END-IF
           END-EVALUATE
           .

       DataSet1-vettel-INITEND.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-vettel-KEY1-Asc
                 MOVE High-Value TO vtt-chiave OF vettel
              ELSE
                 MOVE Low-Value TO vtt-chiave OF vettel
              END-IF
           END-EVALUATE
           .

       DataSet1-CHANGETO-KEY1.
           MOVE 1 TO DataSet1-KEYIS
           .   

       DataSet1-Change-CurrentKey-Asc.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              MOVE "A" TO DataSet1-vettel-KEY1-ORDER
           END-EVALUATE
           .

       DataSet1-Change-CurrentKey-Desc.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              MOVE "D" TO DataSet1-vettel-KEY1-ORDER
           END-EVALUATE
           .

      * vettel
       DataSet1-vettel-START.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-vettel-KEY1-Asc
                 START vettel KEY >= vtt-chiave OF vettel
              ELSE
                 START vettel KEY <= vtt-chiave OF vettel
              END-IF
           END-EVALUATE
           .

       DataSet1-vettel-START-NOTGREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-vettel-KEY1-Asc
                 START vettel KEY <= vtt-chiave OF vettel
              ELSE
                 START vettel KEY >= vtt-chiave OF vettel
              END-IF
           END-EVALUATE
           .

       DataSet1-vettel-START-GREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-vettel-KEY1-Asc
                 START vettel KEY > vtt-chiave OF vettel
              ELSE
                 START vettel KEY < vtt-chiave OF vettel
              END-IF
           END-EVALUATE
           .

       DataSet1-vettel-START-LESS.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-vettel-KEY1-Asc
                 START vettel KEY < vtt-chiave OF vettel
              ELSE
                 START vettel KEY > vtt-chiave OF vettel
              END-IF
           END-EVALUATE
           .

       DataSet1-vettel-Read.
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, BeforeRead>
      * <TOTEM:END>
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, BeforeReadRecord>
      * <TOTEM:END>
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-vettel-LOCK
                 READ vettel WITH LOCK 
                 KEY vtt-chiave OF vettel
              ELSE
                 READ vettel WITH NO LOCK 
                 KEY vtt-chiave OF vettel
              END-IF
           END-EVALUATE
           MOVE STATUS-vettel TO TOTEM-ERR-STAT 
           MOVE "vettel" TO TOTEM-ERR-FILE
           MOVE "READ" TO TOTEM-ERR-MODE
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, AfterRead>
      * <TOTEM:END>
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, AfterReadRecord>
      * <TOTEM:END>
           .

       DataSet1-vettel-Read-Next.
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, BeforeRead>
      * <TOTEM:END>
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, BeforeReadNext>
      * <TOTEM:END>
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-vettel-KEY1-Asc
                 IF DataSet1-vettel-LOCK
                    READ vettel NEXT WITH LOCK
                 ELSE
                    READ vettel NEXT WITH NO LOCK
                 END-IF
              ELSE
                 IF DataSet1-vettel-LOCK
                    READ vettel PREVIOUS WITH LOCK
                 ELSE
                    READ vettel PREVIOUS WITH NO LOCK
                 END-IF
              END-IF
           END-EVALUATE
           MOVE STATUS-vettel TO TOTEM-ERR-STAT
           MOVE "vettel" TO TOTEM-ERR-FILE
           MOVE "READ NEXT" TO TOTEM-ERR-MODE
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, AfterRead>
      * <TOTEM:END>
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, AfterReadNext>
      * <TOTEM:END>
           .

       DataSet1-vettel-Read-Prev.
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, BeforeRead>
      * <TOTEM:END>
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, BeforeReadPrev>
      * <TOTEM:END>
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-vettel-KEY1-Asc
                 IF DataSet1-vettel-LOCK
                    READ vettel PREVIOUS WITH LOCK
                 ELSE
                    READ vettel PREVIOUS WITH NO LOCK
                 END-IF
              ELSE
                 IF DataSet1-vettel-LOCK
                    READ vettel NEXT WITH LOCK
                 ELSE
                    READ vettel NEXT WITH NO LOCK
                 END-IF
              END-IF
           END-EVALUATE
           MOVE STATUS-vettel TO TOTEM-ERR-STAT
           MOVE "vettel" TO TOTEM-ERR-FILE
           MOVE "READ PREVIOUS" TO TOTEM-ERR-MODE
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, AfterRead>
      * <TOTEM:END>
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, AfterReadPrev>
      * <TOTEM:END>
           .

       DataSet1-vettel-Rec-Write.
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, BeforeWrite>
      * <TOTEM:END>
           WRITE vtt-rec OF vettel.
           MOVE STATUS-vettel TO TOTEM-ERR-STAT
           MOVE "vettel" TO TOTEM-ERR-FILE
           MOVE "WRITE" TO TOTEM-ERR-MODE
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, AfterWrite>
      * <TOTEM:END>
           .

       DataSet1-vettel-Rec-Rewrite.
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, BeforeRewrite>
      * <TOTEM:END>
           REWRITE vtt-rec OF vettel.
           MOVE STATUS-vettel TO TOTEM-ERR-STAT
           MOVE "vettel" TO TOTEM-ERR-FILE
           MOVE "REWRITE" TO TOTEM-ERR-MODE
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, AfterRewrite>
      * <TOTEM:END>
           .

       DataSet1-vettel-Rec-Delete.
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, BeforeDelete>
      * <TOTEM:END>
           DELETE vettel.
           MOVE STATUS-vettel TO TOTEM-ERR-STAT
           MOVE "vettel" TO TOTEM-ERR-FILE
           MOVE "DELETE" TO TOTEM-ERR-MODE
      * <TOTEM:EPT. FD:DataSet1, FD:vettel, AfterDelete>
      * <TOTEM:END>
           .

       tvettori-k-des-MERGE-SPLITBUF.
