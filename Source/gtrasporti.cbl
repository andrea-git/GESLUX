              PERFORM ef-prog-bolla-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-data, VALUE IN trs-data-bolla
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-data-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-data-fatt, VALUE IN trs-data-fattura
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-data-fatt-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

       Form1-DaEf-2-BeforeProcedure.
      * <TOTEM:PARA. Form1-DaEf-2-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       Form1-DaEf-2-AfterProcedure.
      * <TOTEM:PARA. Form1-DaEf-2-AfterProcedure>
              INQUIRE ef-vet, VALUE IN trs-vettore
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-vet-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-prov, VALUE IN trs-provincia
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-prov-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

       Form1-DaEf-3-BeforeProcedure.
      * <TOTEM:PARA. Form1-DaEf-3-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       Form1-DaEf-3-AfterProcedure.
      * <TOTEM:PARA. Form1-DaEf-3-AfterProcedure>
              INQUIRE ef-cli, VALUE IN trs-cliente
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-cli-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-reg, VALUE IN trs-regione
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-reg-VALIDATION
              IF NOT TOTEM-CHECK-OK
