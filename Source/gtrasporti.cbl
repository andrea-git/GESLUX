       ABILITAZIONI.
      * <TOTEM:PARA. ABILITAZIONI>
           if mod = 1                  
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else         
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move 0 to e-salva e-cancella
           end-if.            

      * ISACCO (CORREZIONE SU LIVELLO ABILITAZIONE 2) - 10/11/2003
      * (CANCELLAZIONE NON PERMESSA)
           if livello-abil = 2 
              move ZERO to e-cancella
              move BitmapDeleteDisabled to BitmapNumDelete
           end-if.
      * FINE

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-nuovo, tool-salva, tool-cancella, tool-modifica.
      * FINE 
           .
      * <TOTEM:END>

       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = 0  exit paragraph  end-if.
      *
           display message  MSG-Cancellare-il-record-corrente
                   title   titolo
                   type    mb-yes-no 
                   default mb-no
                   giving  scelta

           if scelta = mb-yes

              perform RIEMPI-CHIAVE
              delete trasporti record invalid continue end-delete
   
              move 0  to mod mod-campi
              move 1  to mod-k
              perform FORM1-CLEAR

              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC
                            
              display message  MSG-Cancellazione-avvenuta-con-successo
                      title titolo                         

              unlock trasporti all records

              perform PRIMO
              move 1 to control-id
              move 4 to accept-control
              set Statusvisua to true
              perform STATUS-BAR-MSG
              modify TOOL-MODIFICA value = mod
           end-if 
           .
      * <TOTEM:END>

       CANCELLA-TMP.
      * <TOTEM:PARA. CANCELLA-TMP>
           delete file zoom-trasporti 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id

           when 78-ID-ef-vet
                inquire ef-vet, value vet-codice
                move "tvettori"      to como-file
                call   "zoom-gt"  using como-file, vet-rec
                                 giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move vet-codice       to ef-vet-buf
                   move vet-descrizione  to lab-vet-buf
                   move 4 to accept-control
                end-if
                display ef-vet lab-vet

           when 78-ID-ef-cli
                set cli-tipo-C to true
                move "clienti-CF-all"     to como-file         
                inquire ef-cli, value in cli-codice
                call "zoom-gt"  using como-file, cli-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move cli-codice   to ef-cli-buf
                   move cli-ragsoc-1 to lab-cli-buf
                   display ef-cli lab-cli
                end-if

           when 78-ID-ef-dest
                move "clienti-des"     to como-file         
                inquire ef-cli,  value in des-codice
                inquire ef-dest, value in des-prog
                call "zoom-gt"  using como-file, des-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move des-prog      to ef-dest-buf
                   move des-ragsoc-1  to lab-dest-buf
                   move des-localita  to lab-citta-buf
                   display ef-dest lab-citta lab-dest
                end-if

           when 78-ID-ef-reg
                move "tregioni"     to como-file         
                inquire ef-reg, value in reg-codice
                call "zoom-gt"  using como-file, reg-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move reg-codice      to ef-reg-buf
                   move reg-descrizione to lab-reg-buf
                   display ef-reg lab-reg
                end-if

           when 78-ID-ef-prov
                move "tprov"     to como-file         
                inquire ef-prov, value in prv-codice
                call "zoom-gt"  using como-file, prv-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move prv-codice      to ef-prov-buf
                   move prv-descrizione to lab-prov-buf
                   display ef-prov lab-prov
                end-if
           end-evaluate
           .
      * <TOTEM:END>

       CLEAR-SCREEN.
      * <TOTEM:PARA. CLEAR-SCREEN>
           move trs-anno       to old-trs-anno.
           move trs-num-bolla  to old-trs-num-bolla.
           move trs-prog-bolla to old-trs-prog-bolla.

           initialize trs-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                        
           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.

           move spaces to lab-vet-buf.
           move spaces to lab-cli-buf.
           move spaces to lab-dest-buf.
           move spaces to lab-citta-buf.

           display form1.
  
           unlock trasporti all records 
           .
      * <TOTEM:END>

       CHECK-INSERIMENTO.
      * <TOTEM:PARA. CHECK-INSERIMENTO>
      * modifica la status-bar
      * controllo se da inserimento scorro. allora devo segnalare che
      * sono tornato in modifica.
           inquire tool-modifica, value in mod.

           evaluate mod
           when 1   set StatusModifica to true
           when 0   set StatusVisua    to true   
           end-evaluate.
      
           perform STATUS-BAR-MSG 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.
      
      * Paragrafo per la struttura dei controlli sulla screen Form1
           evaluate control-id
           |78-ID-ef-anno è l'ID del control ef-anno
           when 78-ID-ef-anno
                inquire ef-anno, value in trs-anno
                if trs-anno = 0
                   set errori to true
                   display message "Anno obbligatorio"
                             title tit-err
                              icon 2
                end-if
      
           |78-ID-ef-bolla è l'ID del control ef-bolla
           when 78-ID-ef-bolla
                inquire ef-bolla, value in trs-num-bolla
                if trs-num-bolla = 0
                   set errori to true
                   display message "Inserimento num. bolla mancante"
                             title tit-err
                              icon 2
                end-if
      
           |78-ID-ef-prog-bolla è l'ID del control ef-prog-bolla
           when 78-ID-ef-prog-bolla
                inquire ef-prog-bolla, value in trs-prog-bolla
                if trs-prog-bolla = 0
                   set errori to true
                   display message "Inserimento progressivo bolla mancan
      -    "te"
                             title tit-err
                              icon 2
                else
                   perform RIEMPI-CHIAVE
                   read trasporti no lock
                        invalid 
                        if vecchio
                           display message MSG-record-inesistente
                                     title tit-err
                                      icon mb-warning-icon
                           perform CLEAR-SCREEN
                           set errori to true
                           move 78-ID-ef-anno to control-id
                        end-if
                    not invalid      
                        if old-trs-chiave not = trs-chiave
                           perform CURRENT-RECORD 
                           set errori to true  
                           move 78-ID-ef-anno to control-id
                        end-if
                   end-read
                end-if
      
           |78-ID-ef-data è l'ID del control ef-data
           when 78-ID-ef-data
                inquire ef-data, value in como-data
                perform DATE-FORMAT
                move como-data to ef-data-buf
                display ef-data
      
           |78-ID-ef-data-fatt è l'ID del control ef-data-fatt
           when 78-ID-ef-data-fatt
