       VAL-SCREEN.
      * <TOTEM:PARA. VAL-SCREEN>
      *    VERIFICA 1
           move sf-lince           to lab-lince-buf.
           move sf-scoperto-tot    to lab-scad-tot-buf.
           move sf-saldo-scheda    to lab-scaduto-buf.
           move sf-saldo-scaduto   to lab-scaduti-buf.
           move sf-saldo-scadenza  to lab-scadenza-buf.

           if sf-verifica-1-ok
              set verifica-1-ok to true
              move 2   to num-bmp-1
           else
              set verifica-1-ok to false
              move 3   to num-bmp-1
           end-if.

      *    VERIFICA 2
           move sf-scaduto-al   to lab-scaduto-al-buf
           move sf-scadenze     to lab-scadenze-buf
           move sf-perce-max    to lab-perce-max-buf
           move sf-num          to lab-num-buf
           move sf-den          to lab-den-buf
      *     move sf-perce        to lab-perce-buf

           move cli-fido-extra  to lab-fido-extra-buf
           compute como-dif-den-fido-extra = sf-den - cli-fido-extra
           if como-dif-den-fido-extra < 0
              move 0 to como-dif-den-fido-extra
           end-if.
           move como-dif-den-fido-extra  to lab-dif-den-fido-extra-buf

      *    ricalcolo la verifica in base al fido extra
           if sf-den = 0 or cli-fido-extra > sf-den
              move 0 to sf-perce
           else
              compute sf-perce = (sf-den - cli-fido-extra)
                                                        / sf-num * 100
           end-if
           move sf-perce        to lab-perce-buf.

           if sf-verifica-2-ok
              set verifica-2-ok to true
              move 2   to num-bmp-2
              move 3   to colore-perce
           else
              set verifica-2-ok to false
              move 3   to num-bmp-2
              move 5   to colore-perce
           end-if.

      *    VERIFICA 3
           move sf-fat-per          to lab-fatt-per-buf.
           move sf-fatturato        to lab-fatturato-buf.
           move sf-fido-max         to lab-fido-buf.
           move sf-effetti-rischio  to lab-rb-buf.
           move sf-ordini-in-essere to lab-ordini-buf.
           move sf-scoperto-tot     to lab-tot-scad-buf.
           move sf-gg-dilazione     to lab-gg-buf.

           if sf-verifica-3-ok
              set verifica-3-ok to true
              move 2   to num-bmp-3
      *****     else
      *****        set verifica-3-ok to false
      *****        modify bmp-3, bitmap-number = 3
      *****        move 3   to num-bmp-3
           end-if.

           move sf-piva to lab-piva-buf.

           display Screen1.

           rewrite sf-rec
              invalid 
                 continue
           end-rewrite 
           .
      * <TOTEM:END>

