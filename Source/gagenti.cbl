      * USER DEFINE PARAGRAPH
       ABILITAZIONI.
      * <TOTEM:PARA. ABILITAZIONI>
           if mod = 1              
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else           
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move ZERO to e-salva e-cancella
           end-if.      

      * ISACCO (CORREZIONE SU LIVELLO ABILITAZIONE 2) - 10/11/2003
      * (CANCELLAZIONE NON PERMESSA)
           if livello-abil = 2 
              move ZERO to e-cancella
              move BitmapDeleteDisabled to BitmapNumDelete
           end-if.
      * FINE

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-nuovo, tool-salva, tool-cancella, tool-modifica.
      * FINE 
           .
      * <TOTEM:END>

       ANTEPRIMA.
      * <TOTEM:PARA. ANTEPRIMA>
           continue 
           .
      * <TOTEM:END>

       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = ZERO
              exit paragraph 
           end-if.
      *
           display message box MSg-Cancellare-il-record-corrente
                   title   titolo
                   type    mb-yes-no 
                   default mb-no
                   giving  scelta

           if scelta = mb-yes
              perform RIEMPI-CHIAVE
              delete agenti record invalid continue end-delete

              initialize G2Agg-linkage
              move age-codice   to G2Agg-agente
              set  G2Agg-age    to true
              set  G2Agg-delete to true
              call   "G2Agg" using G2Agg-linkage
              cancel "G2Agg"

              move zero to mod
              move 1    to mod-k
              perform FORM1-CLEAR
              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC

              
              display message box 
           MSG-Cancellazione-avvenuta-con-successo
                      title titolo                         

              unlock agenti all records

              perform PRIMO 
              move 78-ID-ef-codice to  CONTROL-ID     
              move 4         to  ACCEPT-CONTROL 

              set Statusvisua to true
              perform STATUS-BAR-MSG
              modify TOOL-MODIFICA value = mod
           end-if 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate CONTROL-ID
           when 78-ID-EF-provincia
                perform RIEMPI-CHIAVE-PROVINCIA
                move "tprov"     to Como-File
                call   "zoom-gt" using como-file, prv-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-provincia, value prv-codice
                   move 4 to accept-control
                end-if
      *
           when 78-ID-ef-nazione        
                perform RIEMPI-CHIAVE-NAZIONE
                move "tnazioni" to Como-File
                call   "zoom-gt" using como-file, naz-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO  
                   modify ef-nazione, value naz-codice
                   move 4 to accept-control
                end-if
      *
           when 78-ID-ef-listino
                inquire ef-listino, value in lis-codice
                move "lisagente" to Como-File
                call   "zoom-gt" using como-file, lis-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-listino, value lis-codice
                   move lis-descrizione  to lab-listino-buf
                   display lab-listino
                   move 4 to accept-control
                end-if
      *
           when 78-ID-gd-marche
                perform X-Y
                if colonna = 1
                   inquire gd-marche(riga, 1), cell-data in mar-codice
                   move "tmarche"      to Como-File
                   call   "zoom-gt" using como-file, mar-rec
                                   giving stato-zoom
                   cancel "zoom-gt"
                   if stato-zoom = 0
                      perform CONTROLLA-MARCA-GIA-INSERITA
                      if not trovato
                         move mar-codice      to col-marca
                         move mar-descrizione to col-des-marca
                         modify gd-marche(riga, 1), cell-data = 
           col-marca
                         modify gd-marche(riga, 2), cell-data = 
           col-des-marca
                      end-if
                   end-if
                end-if
           end-evaluate  
           .
      * <TOTEM:END>

       CLEAR-SCREEN.
      * <TOTEM:PARA. CLEAR-SCREEN>
           move age-chiave to old-age-chiave.

           initialize age-dati replacing numeric data by zeroes
                                   alphanumeric data by spaces.
                         
           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.
                                          
           display form1. 

           perform RESETTA-GRID.
  
           unlock agenti all records 
           .
      * <TOTEM:END>

       CHANGE-TAB.
      * <TOTEM:PARA. CHANGE-TAB>
           set NonCambiareTab   to false.

           evaluate EVENT-DATA-1
           when 1
                perform CHECK-PAGE-2
                if tutto-ok
                   perform CANCELLA-COLORE
                   set ControllaCampi to true
                   move 78-ID-ef-ragsoc-1 to control-id
                   move 4 to accept-control
                else
                   move 2 to pagina
                   set ControllaCampi to false    
                   move 4             to ACCEPT-CONTROL
                   set NonCambiareTab to true
                end-if
           when 2
                perform CHECK-PAGE-1
                if tutto-ok
                   perform CANCELLA-COLORE
                   set ControllaCampi to true
                   move 78-ID-ef-marg to control-id
                   move 4 to accept-control
                else
                   move 1 to pagina
                   set ControllaCampi to false    
                   move 4             to ACCEPT-CONTROL
                   set NonCambiareTab to true
                end-if
           end-evaluate.

           display Screen1-Ta-1 
           .
      * <TOTEM:END>

       CHECK-INSERIMENTO.
      * <TOTEM:PARA. CHECK-INSERIMENTO>
      * MODIFICA LA STATUS-BAR
      * CONTROLLO SE DA INSERIMENTO SCORRO. ALLORA DEVO SEGNALARE CHE
      * SONO TORNATO IN MODIFICA.
           inquire tool-modifica, value in mod.

           evaluate mod
           when 1   set StatusModifica to true
           when 0   set StatusVisua    to true   
           end-evaluate.
      
           perform STATUS-BAR-MSG 
           .
      * <TOTEM:END>

       CHECK-CODFIS-PIVA.
      * <TOTEM:PARA. CHECK-CODFIS-PIVA>
           inquire ef-codfis   value cf-codice-fiscale.
           inquire ef-piva     value cf-piva.

           call   "codfis"     using link-codfis.
           cancel "codfis".

      * FLAG CHE INDICA SE IL CF O PIVA E' CORRETTO
           if cf-risultato not = ZERO
              set errori to true
           end-if 
           .
      * <TOTEM:END>

       CHECK-PAGE-1.
      * <TOTEM:PARA. CHECK-PAGE-1>
           if mod = 0 exit paragraph end-if.
           set tutto-ok to true.
           perform  varying control-id from 78-ID-ef-codice by 1
                      until control-id    > 78-ID-rb-pluri
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform 
           .
      * <TOTEM:END>

       CHECK-PAGE-2.
      * <TOTEM:PARA. CHECK-PAGE-2>
           if mod = 0 exit paragraph end-if.
           set tutto-ok to true.
           perform  varying control-id from 78-ID-ef-marg by 1
                      until control-id    > 78-ID-ef-listino
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform 
           .
      * <TOTEM:END>

       CONTROLLA-MARCA-GIA-INSERITA.
      * <TOTEM:PARA. CONTROLLA-MARCA-GIA-INSERITA>
           set trovato  to false.

           perform varying store-riga from 2 by 1 until
                           store-riga > tot-righe
              inquire gd-marche(store-riga, 1), cell-data in store-marca
              if store-marca     = mar-codice and 
                 store-riga  not = riga
                 set trovato to true
                 subtract 1 from store-riga
                 move store-riga to riga-ed
                 display message "Marca già inserita a riga: " riga-ed
                           title tit-err
                            icon 2
                 set errori to true
                 move spaces to col-des-marca
                 modify gd-marche(riga, 2), cell-data = col-des-marca
                 exit perform
              end-if
           end-perform 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true. 
           if not ControllaCampi
              set ControllaCampi to true
              exit paragraph
           end-if.

           evaluate CONTROL-ID
           when 78-ID-ef-codice
                perform RIEMPI-CHIAVE

      *          if ef-codice-buf not = spaces
                   inspect ef-codice-buf replacing trailing SPACES by 
           low-values
                   initialize CountChar
                   inspect ef-codice-buf 
                      tallying CountChar for characters before low-value
                   inspect ef-codice-buf replacing trailing low-values 
           by SPACES

                   | SE E' NUMERICO LEGGO IL RECORD NORMALMENTE
                   if ef-codice-buf(1:CountChar) is numeric
                      perform SELEZIONA-NUMERICO
                   else
                      | HO DIGITATO UNA SIGLA ALFANUMERICA QUINDI 
                      | APRO LO ZOOM (CASE SENSITIVE)
                      perform SELEZIONA-ALFA
                   end-if
      *          else
      *             set errori to true
      *             display message box MSG-Codice-obbligatorio
      *                     title tit-err
      *                     icon  mb-warning-icon
      *          end-if

           when 78-ID-ef-ragsoc-1
                inquire ef-ragsoc-1 value in age-ragsoc-1
                if age-ragsoc-1 = spaces
                   set errori to true
                   display message box "Ragione Sociale 1 obbligatoria"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                end-if
                move age-ragsoc-1 to lab-des-cli-buf
                display lab-des-cli

           when 78-ID-ef-indirizzo
                inquire ef-indirizzo, value in age-indirizzo
                if age-indirizzo = SPACES
                   set errori to true
                   display message box "Indirizzo obbligatorio"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                end-if     
      
           when 78-ID-ef-cap
                inquire ef-cap, value in age-cap
                if age-cap = SPACES
                   set errori to true
                   display message box "C.A.P. obbligatorio"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                end-if     

           when 78-ID-ef-localita     
                inquire ef-localita, value in age-localita
                if age-localita = SPACES
                   set errori to true
                   display message box "Località obbligatoria"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                end-if     

           when 78-ID-ef-provincia                                      
               
                inquire ef-provincia, value in age-provincia
                if age-provincia = SPACES
                   set errori to true
                   display message box "Provincia obbligatoria"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                   move SPACES to lbl-des-prov-buf
                else
                   perform RIEMPI-CHIAVE-PROVINCIA
                   read tprov key prv-chiave
                      invalid
                         set errori to true
                         display message "Provincia NON valida"
                            title tit-err
                            type  mb-ok
                            icon  mb-warning-icon
                         move SPACES to lbl-des-prov-buf
                      not invalid
                         move prv-descrizione to lbl-des-prov-buf
                   end-read
               end-if

               display lbl-des-prov

           when 78-ID-ef-nazione                                        
             
                inquire ef-nazione, value in age-nazione
                if age-nazione = SPACES
                   set errori to true
                   display message box "Nazione obbligatoria"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                   move SPACES to lbl-des-naz-buf
                else
                   perform RIEMPI-CHIAVE-NAZIONE
                   read tnazioni key naz-chiave
                      invalid
                         set errori to true
                         display message "Nazione NON valida"
                            title tit-err
                            type  mb-ok
                            icon  mb-warning-icon
                         move SPACES to lbl-des-naz-buf
                      not invalid
                          move naz-descrizione to lbl-des-naz-buf
                   end-read
               end-if

               display lbl-des-naz

           when 78-ID-ef-codfis
                inquire ef-piva,   value in age-partita-iva
                inquire ef-codfis, value in age-codice-fiscale
                if ef-codfis-buf not = spaces
                   set cf-contr-gen to true
                   perform CHECK-CODFIS-PIVA
                   if errori
                      move 78-ID-ef-codfis to control-id 
                      display message box MSG-Codifce-Fiscale-errato
                              title = tit-err
                              icon mb-warning-icon
                   end-if
                end-if
      
           when 78-ID-ef-piva
                inquire ef-piva,   value in age-partita-iva
                inquire ef-codfis, value in age-codice-fiscale
                if ef-piva-buf   not = spaces
                   set cf-partita-iva to true
                   perform CHECK-CODFIS-PIVA
                   if errori
                      move 78-ID-ef-piva to control-id 
                      display message box MSG-Partita-IVA-errata
                              title = tit-err
                              icon mb-warning-icon
                   end-if
                else
                   if ef-codfis-buf = spaces
                      set errori to true
                      move 78-ID-ef-codfis to control-id
                      display message box MSG-codfis-piva-mancanti
                              title = tit-err
                              icon mb-warning-icon
                   end-if
                end-if

           |78-ID-ef-marg è l'ID del control ef-marg
           when 78-ID-ef-marg
                inquire ef-marg, value in ef-marg-buf
                move ef-marg-buf to age-marg
                evaluate true
                when age-marg = 0
                     set errori to true
                     display message "Marginalità obbligatoria"
                               title tit-err
                                icon 2
                when age-marg > 100
                     set errori to true
                     display message "Massimo valore = 100"
                               title tit-err
                                icon 2
                end-evaluate

           |78-ID-ef-listino è l'ID del control ef-listino
           when 78-ID-ef-listino
                inquire ef-listino, value in ef-listino-buf
                move ef-listino-buf to lis-codice
                if lis-codice = 0
                   move spaces to lab-listino-buf
                else
                   if lis-codice = codice-listino-promo
                      move spaces to lab-listino-buf
                      set errori to true
                      display message 
                "Impossibile inserire listino promo come personalizzato"
                                title tit-err
                                 icon 2
                      display lab-listino
                   else
                      perform X-Y
                      if tot-righe > 1
                         display message 
                         "Verrano cancellate TUTTE le righe delle marche
      -    "."
                  x"0d0a""Confermi l'inserimento del codice listino?"
                                   title titolo
                                    icon 2
                                    type mb-yes-no
                                 default mb-no
                                  giving scelta
                         if scelta = mb-no
                            move 0      to ef-listino-buf
                            move spaces to lab-listino-buf
                            display ef-listino lab-listino
                         else
                            perform RESETTA-GRID
                         end-if
                      else
                         move mb-yes to scelta
                      end-if
                      if scelta = mb-yes
                         read lisagente no lock key is k-codice
                              invalid
                              set errori to true
                              display message"Codice listino NON valido"
                                        title tit-err
                                         icon 2
                              move spaces to lab-listino-buf
                          not invalid
                              move lis-descrizione to lab-listino-buf
                         end-read
                      end-if
                      display lab-listino
                   end-if
                end-if
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if 
           .
      * <TOTEM:END>

       CONTROLLO-RIGA.
      * <TOTEM:PARA. CONTROLLO-RIGA>
           set tutto-ok to true.
           move ef-listino-buf to lis-codice.
           if lis-codice not = 0 or mod = 0
              exit paragraph
           end-if.

           evaluate colonna
           when 1
                read tmarche no lock
                     invalid
                     set errori to true
                     display message "Marca NON valida"
                               title tit-err
                                icon 2
                     move spaces to col-des-marca
                 not invalid
                     move mar-descrizione to col-des-marca
                end-read
                modify gd-marche(riga, 2), cell-data col-des-marca
           when 3
                if como-numero = 0
                   set errori to true
                   display message "Percentuale obbligatoria"
                             title tit-err
                              icon 2
                   move spaces to col-des-marca
                end-if
           end-evaluate 
                   
           .
      * <TOTEM:END>

       CURRENT-RECORD.
      * <TOTEM:PARA. CURRENT-RECORD>
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if nuovo move 0 to mod end-if.

           if mod = 1
              read agenti    lock invalid 
                 set errori to true 
              end-read
           else
              read agenti no lock invalid 
                 set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0      to mod
                    move 1      to mod-k
                    move 78-ID-ef-codice to CONTROL-ID
                    move 4         to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else                         
                    set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG
                 perform VALORIZZA-OLD
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-codice to CONTROL-ID
                       move 4         to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                    end-if
                 end-if
              end-if
           end-if 
           .
      * <TOTEM:END>

       HELP-GRID.
      * <TOTEM:PARA. HELP-GRID>
           move 0 to StatusHelp.

           evaluate colonna
           when 1   move "Digitare la marca" 
                      to totem-hint-text
                    if mod = 1 
                       inquire ef-listino, value in lis-codice
                       if lis-codice = 0
                          move 1 to StatusHelp 
                       end-if
                    end-if
           when 2   move "Descrizione della marca"
                      to totem-hint-text
           when 3   move "Digitare la percentuale"
                      to totem-hint-text
           end-evaluate.

           modify Form1-St-1-Handle, panel-index = 1,
                                     panel-text  = totem-hint-text.
           perform STATUS-HELP 
           .
      * <TOTEM:END>

       INIT.
      * <TOTEM:PARA. INIT>
           move ZERO to StatusHelp.
           move 1    to mod-k.
           set StatusVisua    to true. 
           set ControllaCampi to true.
           set errore-des     to false.

           move 1 to Form1-RADIO-1-BUF.
           move 1 to Form1-RADIO-2-BUF.
           move 1 to Form1-RADIO-3-BUF.
           display   rb-mono.
           display   rb-age-normale.
           display   rb-pr-normale 
           .
      * <TOTEM:END>

       INIT-OLD-REC.
      * <TOTEM:PARA. INIT-OLD-REC>
           initialize old-age-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.

      * SPECIFICO PGM "gagenti"
           Set old-age-mono           to true.
           set old-age-normale        to true.
           set old-age-prezzo-normale to true.
           move 50 to old-age-marg.
      * FINE
           .
      * <TOTEM:END>

       MODIFICA.
      * <TOTEM:PARA. MODIFICA>
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if CONTROL-ID < 78-ID-ef-ragsoc-1
                       move 78-ID-ef-ragsoc-1 to CONTROL-ID
                    end-if
                    move ZERO to mod-k
                    move 1    to mod
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1    to mod
                 perform SALV-MOD            
                 move ZERO to mod
                 move 1    to mod-k
                 if errori
                    move 1    to mod
                    move ZERO to mod-k
                 else
                    move 78-ID-ef-codice to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock agenti all records
                 end-if
              end-if
                        
              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if 
           .
      * <TOTEM:END>

       NUOVO.
      * <TOTEM:PARA. NUOVO>
           perform SALV-MOD.

           if tutto-ok  
              move 78-ID-ef-codice to CONTROL-ID       
              move 4         to ACCEPT-CONTROL   
              move 1         to mod mod-k
              modify tool-modifica,  value = mod
              perform CANCELLA-COLORE                  
              perform VALORIZZA-NUOVO
              perform INIT-OLD-REC       
              set StatusIns to true
              perform STATUS-BAR-MSG  
              set nuovo to true       
              unlock agenti all records
           end-if 
           .
      * <TOTEM:END>

       ORDINAMENTO.
      * <TOTEM:PARA. ORDINAMENTO>
           inquire tool-ord, value in OrderBy.
      
           if OrderBy = zero
              perform DataSet1-CHANGETO-KEY1
           else
              perform DataSet1-CHANGETO-KEY2
           end-if.
      
           perform CURRENT-RECORD 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".
           copy "status.cpy" 
           .
      * <TOTEM:END>

       PARTITA-IVA.
      * <TOTEM:PARA. PARTITA-IVA>
           if not CheckPartitaIva
              set CheckPartitaIva to true
              exit paragraph
           end-if.

           set cf-partita-iva     to true.
           inquire ef-piva     value cf-piva.

           call   "codfis"     using link-codfis.
           cancel "codfis".

      * FLAG CHE INDICA SE IL CF O PIVA E' CORRETTO
           if cf-risultato not = ZERO
              set errori to true
              display message box MSG-Partita-Iva-Errata
                      title = tit-err
                      icon mb-default-icon
           end-if 
           .
      * <TOTEM:END>

       PRECEDENTE.
      * <TOTEM:PARA. PRECEDENTE>
           perform SALV-MOD.           
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock agenti all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-agenti-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-agenti-lock-flag
              end-if
                    
              perform CANCELLA-COLORE
              perform FORM1-PREVIOUS

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-agenti-lock-flag
                 move 1 to mod-k
                 perform FORM1-PREVIOUS
              end-if
           end-if 
           .
      * <TOTEM:END>

       PRIMO.
      * <TOTEM:PARA. PRIMO>
           perform SALV-MOD.           
           if tutto-ok    
 
              perform CHECK-INSERIMENTO
              unlock agenti all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-agenti-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-agenti-lock-flag
              end-if
                                  
              perform CANCELLA-COLORE
              perform FORM1-FIRST

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-agenti-lock-flag
                 move 1 to mod-k
                 perform FORM1-FIRST
              end-if
           end-if 
           .
      * <TOTEM:END>

       PUNTA-CELLA.
      * <TOTEM:PARA. PUNTA-CELLA>
           move 78-ID-gd-marche    to control-id store-id.
           move 4                  to accept-control.
           modify gd-marche, cursor-y riga, 
                             cursor-x colonna.
           perform COLORE-RIGA 
           .
      * <TOTEM:END>

       RESETTA-GRID.
      * <TOTEM:PARA. RESETTA-GRID>
           modify gd-marche, reset-grid = 1.
           perform GD-MARCHE-CONTENT 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE.
      * <TOTEM:PARA. RIEMPI-CHIAVE>
           inquire ef-codice, value in ef-codice-buf.
           move ef-codice-buf       to age-codice convert 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE-PROVINCIA.
      * <TOTEM:PARA. RIEMPI-CHIAVE-PROVINCIA>
           inquire ef-provincia, value in ef-provincia-buf.
           move ef-provincia-buf       to prv-codice 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE-NAZIONE.
      * <TOTEM:PARA. RIEMPI-CHIAVE-NAZIONE>
           inquire ef-nazione, value in ef-nazione-buf.
           move ef-nazione-buf       to naz-codice 
           .
      * <TOTEM:END>

       RIEMPI-GRID.
      * <TOTEM:PARA. RIEMPI-GRID>
           move 0 to idx.
           perform RESETTA-GRID.
           perform 10 times
              add   1 to idx
              move  age-marca(idx) to col-marca mar-codice
              read  tmarche no lock
                    invalid move spaces          to col-des-marca
                not invalid move mar-descrizione to col-des-marca
              end-read
              move age-perce-marca(idx) to col-perce
              modify gd-marche(idx + 1, 1), cell-data = col-marca
              modify gd-marche(idx + 1, 2), cell-data = col-des-marca
              modify gd-marche(idx + 1, 3), cell-data = col-perce
           end-perform 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           if mod = ZERO 
              exit paragraph 
           end-if.

           set tutto-ok to true.

           perform varying CONTROL-ID from 78-ID-ef-codice by 1
                     until CONTROL-ID > 78-ID-ef-listino
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if tutto-ok and age-listino = 0
              perform X-Y
              perform varying riga from 2 by 1 
                        until riga > tot-righe
                 perform VALORE-RIGA
                 perform varying colonna from 1 by 1
                           until colonna > 3
                    perform CONTROLLO-RIGA
                    if errori 
                       move 78-ID-gd-marche to store-id
                       exit perform 
                    end-if
                 end-perform
                 if errori exit perform end-if
              end-perform
           end-if.

           if errori
              perform ABILITAZIONI
              if store-id = 78-ID-gd-marche
                 perform PUNTA-CELLA
              else
                 move store-id to CONTROL-ID
                 move 4        to ACCEPT-CONTROL
              end-if
           else                       
              perform FORM1-BUF-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              perform CANCELLA-COLORE 
                                     
              accept como-data from century-date
              accept como-ora  from time
          
              write age-rec invalid rewrite age-rec end-write

              initialize G2Agg-linkage
              set G2Agg-age   to true
              move age-codice to G2Agg-agente
              if nuovo set G2Agg-insert to true
              else     set G2Agg-update to true
              end-if
              call   "G2Agg" using G2Agg-linkage
              cancel "G2Agg"

              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD.

           if SiSalvato and mod = 1
              move 0 to idx
              perform 10 times
                 add 1 to idx
                 if old-age-marca(idx) not = age-marca(idx)
                    set NoSalvato to true
                    move 78-ID-gd-marche to store-id
                    move 1 to colonna
                    add  1 to idx giving riga
                    exit perform
                 end-if
                 if old-age-perce-marca(idx) not = age-perce-marca(idx)
                    set NoSalvato to true
                    move 78-ID-gd-marche to store-id
                    add  1 to idx giving riga
                    move 3 to colonna
                    exit perform
                 end-if
              end-perform
           end-if.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel 
                            giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   inquire Screen1-Ta-1, value in pagina
                   evaluate pagina
                   when 1
                        if store-id > 78-ID-rb-pluri
                           move 666 to store-id
                        end-if
                   when 2
                        if store-id < 78-ID-rb-age-normale
                           move 666 to store-id
                        end-if
                   end-evaluate
                   if store-id = 78-ID-gd-marche
                      perform PUNTA-CELLA
                   else
                      move store-id to CONTROL-ID       
                      move 4        to ACCEPT-CONTROL   
                   end-if
              end-evaluate
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA.
      * <TOTEM:PARA. SELEZIONA>
           perform RIEMPI-CHIAVE.

           move "agenti"  to    como-file.
           call "zoom-gt" using como-file, age-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = ZERO
              if old-age-chiave not = age-chiave or
               ( age-codice = 0 and mod = 0 )
                 move age-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to age-chiave
                    set ReadSecca to true
                    modify ef-codice, value age-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-ragsoc-1 to CONTROL-ID       
                    move 4                 to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-ALFA.
      * <TOTEM:PARA. SELEZIONA-ALFA>
      * LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      * ANCORA GLI ZERI DAVANTI CHE ELIMINO
           inspect ef-codice-buf replacing leading ZERO by SPACES.
           move ef-codice-buf to age-ragsoc-1.

      * POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      * ALTERNATA
           call "C$JUSTIFY" using age-ragsoc-1, "L".

           start agenti key >= age-ragsoc-1
              invalid 
                 continue
              not invalid
                 read agenti next 
           end-start.

           move "agenti-alfa" to como-file.
           call "zoom-gt" using como-file, age-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = ZERO
              move age-codice    to codice-ed
              move codice-ed     to ef-codice-buf   
              call "C$JUSTIFY" using ef-codice-buf, "L"
              display ef-codice

              if old-age-ragsoc-1 not = age-ragsoc-1
                 move age-ragsoc-1 to save-key-1
                 perform SALV-MOD
                 if tutto-ok
                    move save-key-1 to age-ragsoc-1
                    set ReadSecca  to true
                    modify ef-codice, value age-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-ragsoc-1 to CONTROL-ID       
                    move 4           to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-NUMERICO.
      * <TOTEM:PARA. SELEZIONA-NUMERICO>
           read agenti no lock
              invalid
                 if vecchio
                    display message box MSG-record-inesistente
                            title = tit-err
                            icon mb-warning-icon
                    perform CLEAR-SCREEN
                    set errori to true
                 end-if
              not invalid      
                 if old-age-chiave not = age-chiave or
                    ( age-codice = 0 and mod = 0 )
                    perform CURRENT-RECORD 
                 end-if
           end-read 
           .
      * <TOTEM:END>

       SPOSTAMENTO.
      * <TOTEM:PARA. SPOSTAMENTO>
           set tutto-ok to true.
           perform X-Y.

           if event-data-2 not = riga

              perform VALORE-RIGA

              perform varying colonna from 1 by 1
                        until colonna > 3
                 perform CONTROLLO-RIGA
                 if errori exit perform end-if
              end-perform

              if tutto-ok
                 evaluate true
                 when event-data-2 < 2 
                      move 2            to riga
                      move event-data-1 to colonna

                 when event-data-2 > tot-righe
                      add  1                to tot-righe giving riga
                      set event-action      to event-action-fail
                      move 1                to colonna
                      perform PUNTA-CELLA
      *****                move 1                to colonna
      *****                move 78-ID-gd-marche  to control-id
      *****                move 4                to accept-control
      *****                modify gd-marche,  cursor-y = riga,
      *****                                   cursor-x = colonna
                 when other
                      move event-data-2 to riga
                      move event-data-1 to colonna
                 end-evaluate
                 perform VALORE-RIGA
              else
                 set event-action      to event-action-fail
                 perform PUNTA-CELLA
      *****           move 78-ID-gd-marche  to control-id
      *****           move 4                to accept-control
      *****           modify gd-marche, cursor-y = riga,
      *****                             cursor-x = colonna
              end-if
           else
              move event-data-1 to colonna
           end-if.

           perform COLORE-RIGA.
           perform HELP-GRID.

      ***---
       COLORE-RIGA.
           modify gd-marche, start-x = 1,
                                    x = 3,
                              start-y = riga,
                                    y = riga,
                         region-color = 257,
                         cursor-color = 481 
           .
      * <TOTEM:END>

       STAMPA.
      * <TOTEM:PARA. STAMPA>
           continue 
           .
      * <TOTEM:END>

       SUCCESSIVO.
      * <TOTEM:PARA. SUCCESSIVO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock agenti all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-agenti-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-agenti-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-agenti-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if 
           .
      * <TOTEM:END>

       TORNA-IN-VISUA.
      * <TOTEM:PARA. TORNA-IN-VISUA>
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-codice to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock agenti all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if.

           set NonCambiareTab   to false.
           set ControllaCampi   to true.
           
           move 1 to event-data-1 screen1-ta-1-tab-value.
           perform SCREEN1-TA-1-TABCHANGE  
           .
      * <TOTEM:END>

       ULTIMO.
      * <TOTEM:PARA. ULTIMO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock agenti all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-agenti-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-agenti-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-agenti-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if 
           .
      * <TOTEM:END>

       VALORE-RIGA.
      * <TOTEM:PARA. VALORE-RIGA>
           inquire gd-marche(riga, 1), cell-data in col-marca.
           inquire gd-marche(riga, 2), cell-data in col-des-marca.
           inquire gd-marche(riga, 3), cell-data in col-perce.
           
           move col-marca to mar-codice.
           move col-perce to como-numero.

           modify gd-marche(riga, 3), cell-data col-perce 
           .
      * <TOTEM:END>

       VALORIZZA-OLD.
      * <TOTEM:PARA. VALORIZZA-OLD>
           move age-rec to old-age-rec.
           set vecchio to true.

           move ZERO to StatusHelp
           perform STATUS-HELP 
           .
      * <TOTEM:END>

       VALORIZZA-NUOVO.
      * <TOTEM:PARA. VALORIZZA-NUOVO>
           move high-value to age-codice.

           start agenti key <= age-codice
                 invalid move 1 to age-codice
             not invalid
                 read agenti previous with no lock
                    at end
                       move 1 to age-codice
                    not at end
                       add  1 to age-codice
                 end-read
           end-start.

           move "00"    to status-agenti.

           initialize age-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                                       
           perform FORM1-IUD-DISPLAY.

           if OrderBy = 1                             
              move high-value to age-ragsoc-1
              start agenti key is <= age-ragsoc-1 
                    invalid continue 
                not invalid
                    read agenti previous no lock 
                       at end continue 
                    end-read
              end-start
           end-if.

           move 1 to Form1-RADIO-1-BUF.
           move 1 to Form1-RADIO-2-BUF.
           move 1 to Form1-RADIO-3-BUF.
           move 2 to Form1-RADIO-4-BUF.
           display   rb-mono rb-age-normale rb-pr-normale.
           move 50 to ef-marg-buf.
           
           
           move 1 to event-data-1 screen1-ta-1-tab-value.
           perform SCREEN1-TA-1-TABCHANGE  
           .
      * <TOTEM:END>

       VALORIZZA-DATI-COMUNI.
      * <TOTEM:PARA. VALORIZZA-DATI-COMUNI>
      * SE NON HO VALORIZZATO I CAMPI DI CREAZIONE LI VALORIZZO
           if age-data-creazione   = zero
              accept age-data-creazione from century-date
           end-if.
           if age-ora-creazione    = zero
              accept age-ora-creazione  from time
           end-if.
           if age-utente-creazione = space
              move user-codi to age-utente-creazione
           end-if.

           accept age-data-ultima-modifica from century-date.
           accept age-ora-ultima-modifica  from time.
           move user-codi    to age-utente-ultima-modifica 
           .
      * <TOTEM:END>

       X-Y.
      * <TOTEM:PARA. X-Y>
           inquire gd-marche, cursor-y in riga,
                              cursor-x in colonna,
                              last-row in tot-righe 
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       NUOVO-LinkTo.
      * <TOTEM:PARA. NUOVO-LinkTo>
           INQUIRE TOOL-NUOVO, ENABLED IN E-NUOVO.

           IF E-NUOVO = 1
              PERFORM NUOVO
           END-IF                    
           .
      * <TOTEM:END>
       CANCELLA-LinkTo.
      * <TOTEM:PARA. CANCELLA-LinkTo>
           INQUIRE TOOL-MODIFICA, VALUE IN MOD.
                              
           INQUIRE TOOL-CANCELLA, ENABLED IN E-CANCELLA
           IF E-CANCELLA = 1
              PERFORM CANCELLA
           END-IF                                                
           .
      * <TOTEM:END>
       TOOL-SALVA-LinkTo.
      * <TOTEM:PARA. TOOL-SALVA-LinkTo>
           INQUIRE TOOL-MODIFICA, VALUE IN MOD           

           INQUIRE TOOL-SALVA, ENABLED IN E-SALVA
           IF E-SALVA = 1
              PERFORM SALVA
           END-IF           
           .
      * <TOTEM:END>
       ANTEPRIMA-LinkTo.
      * <TOTEM:PARA. ANTEPRIMA-LinkTo>
           INQUIRE TOOL-ANTEPRIMA, ENABLED IN E-ANTEPRIMA.
           IF E-ANTEPRIMA = 1
                PERFORM ANTEPRIMA
           END-IF 
           .
      * <TOTEM:END>
       TOOL-MODIFICA-BeforeProcedure.
      * <TOTEM:PARA. TOOL-MODIFICA-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       TOOL-MODIFICA-AfterProcedure.
      * <TOTEM:PARA. TOOL-MODIFICA-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       TOOL-MODIFICA-LinkTo.
      * <TOTEM:PARA. TOOL-MODIFICA-LinkTo>
           INQUIRE TOOL-MODIFICA, ENABLED IN E-MODIFICA.

           IF E-MODIFICA = 1
              PERFORM MODIFICA
           END-IF 
           .
      * <TOTEM:END>
       STAMPA-LinkTo.
      * <TOTEM:PARA. STAMPA-LinkTo>
           INQUIRE TOOL-STAMPA, ENABLED IN E-STAMPA
           IF E-STAMPA = 1
                PERFORM STAMPA
           END-IF 
           .
      * <TOTEM:END>
       TOOL-CERCA-LinkTo.
      * <TOTEM:PARA. TOOL-CERCA-LinkTo>
           inquire tool-cerca, enabled in e-cerca
           if e-cerca = 1
              perform CERCA
           end-if 
           .
      * <TOTEM:END>
       TOOL-SELEZIONA-LinkTo.
      * <TOTEM:PARA. TOOL-SELEZIONA-LinkTo>
           inquire tool-seleziona, enabled in e-seleziona
           if e-seleziona = 1
              perform SELEZIONA
