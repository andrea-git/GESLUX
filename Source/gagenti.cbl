      * <TOTEM:PARA. gd-marche-Ev-Msg-Finish-Entry>
           set tutto-ok to true.
           perform X-Y.
           perform VALORE-RIGA.

           evaluate colonna
           when 1
                if mar-codice = 0
                   if como-numero not = 0 or col-des-marca not = spaces
                      display message "Cancellare la riga?"
                                title titolo
                                 type mb-yes-no
                              default mb-no
                            giving scelta
                      if scelta = mb-no
                         set errori to true
                      else
                         modify gd-marche, record-to-delete = riga
                         modify gd-marche, insertion-index  = 12,
                                           insert-rows = 1
                      end-if
                   end-if
                else
                   perform CONTROLLA-MARCA-GIA-INSERITA
                   if not trovato
                      perform CONTROLLO-RIGA
                   end-if
                end-if
           when 3
                perform CONTROLLO-RIGA
           end-evaluate.

           if errori
              move 78-ID-gd-marche to store-id
           end-if.

           if errori
              set event-action to event-action-fail
           end-if 
           .
