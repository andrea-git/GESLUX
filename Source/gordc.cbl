       VALORIZZA-NUMERO.
      * <TOTEM:PARA. VALORIZZA-NUMERO>
           set trovato-lock to false.
           set tutto-ok to true.
           move 78-evaart to lck-nome-pgm.
           read lockfile  no lock
                invalid
                move 78-evaart-GDO to lck-nome-pgm
                read lockfile no lock
                     invalid  continue
                 not invalid
                     if lck-operazione = "M"
                        set trovato-lock to true
                     end-if
                end-read
            not invalid
                if lck-operazione = "M"
                   set trovato-lock to true
                end-if
           end-read.

           |Controllo che sia in gestione altrimenti
           |se sessione si è inchiodata lo cancello
           if trovato-lock
              read lockfile lock
              if RecLocked
                 if lck-nome-pgm = 78-evaart
                    display message "Tabella contatori in uso da: " 
           lck-utente-creazione
                             x"0d0a""per evasione automatica ordini mast
      -    "er tradizionali"
                             x"0d0a""Operazione impossibile! Attendere t
      -    "ermine operazione!"
                              title titolo
                               icon 2
                 else
                    display message "Tabella contatori in uso da: " 
           lck-utente-creazione
                             x"0d0a""per evasione automatica ordini mast
      -    "er GDO"
                             x"0d0a""Operazione impossibile! Attendere t
      -    "ermine operazione!"
                              title titolo
                               icon 2
                 end-if
                 set errori to true
              else
                 delete lockfile record invalid continue end-delete
              end-if
           end-if.

           if tutto-ok
              inquire ef-anno value in ef-anno-buf
              move ef-anno-buf   to link-anno

              set  link-gordc    to true
              set  link-crea     to true
              move ef-cau-buf    to link-nambar-causale

              call   "nambar" using link-nambar
              cancel "nambar"
           
              if link-status-nambar = -1 set errori       to true
              else                       move link-numero to tor-numero
              end-if
           end-if 
           .
