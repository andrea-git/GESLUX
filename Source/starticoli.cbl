      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-sett
                inquire ef-sett, value in sme-codice
                move  "tsetmerc"      to como-file
                call   "zoom-gt"   using como-file, sme-rec
                                  giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move sme-codice      to ef-sett-buf
                   move sme-descrizione to lab-sett-buf
                   display ef-sett lab-sett
                end-if

           when 78-ID-ef-marca
                inquire ef-marca, value in mar-codice
                move   "tmarche"       to como-file
                call   "zoom-gt"    using como-file, mar-rec
                                   giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move mar-codice      to ef-marca-buf
                   move mar-descrizione to lab-marca-buf
                   display ef-marca lab-marca
                end-if

           when 78-ID-ef-classe
                inquire ef-classe value in cl1-codice
                move "tcla1art"         to como-file
                call   "zoom-gt"      using como-file, cl1-rec
                                    giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move cl1-codice      to ef-classe-buf
                   move cl1-descrizione to lab-classe-buf
                   display ef-classe lab-classe
                end-if
                
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-cod-a
                inquire ef-cod-da, value in ef-cod-da-buf
                inquire ef-cod-a,  value in ef-cod-a-buf

                if ef-cod-a-buf = 0
                   move 999999 to ef-cod-a-buf
                   display ef-cod-a
                end-if
                
                if ef-cod-a-buf < ef-cod-da-buf
                   set errori to true
                   display message "Intervallo codici errato"
                             title tit-err
                              icon 2
                end-if

           when 78-ID-ef-data
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-buf
                display ef-data

           when 78-ID-ef-sett
                inquire ef-sett, value in sme-codice
                if sme-codice = 0
                   move "Tutti i settori" to lab-sett-buf
                else
                   read tsetmerc no lock
                        invalid
                        move spaces to lab-sett-buf
                        set errori to true
                        display message "Settore NON valido"
                                  title tit-err
                                   icon 2
                    not invalid
                        move sme-descrizione to lab-sett-buf
                   end-read
                end-if
                display lab-sett

           when 78-ID-ef-marca
                inquire ef-marca, value in mar-codice
                if mar-codice = 0
                   move "Tutte le marche" to lab-marca-buf
                else
                   read tmarche no lock
                        invalid
                        set errori to true
                        display message "Marca NON valido"
                                  title tit-err
                                   icon 2
                        move spaces to lab-marca-buf
                    not invalid
                        move mar-descrizione to lab-marca-buf
                   end-read
                end-if
                display lab-marca

           when 78-ID-ef-classe
                inquire ef-classe, value in cl1-codice
                if cl1-codice = 0
                   move "Tutte le classi" to lab-classe-buf
                else
                   read tcla1art no lock
                        invalid
                        set errori to true
                        display message "Classe NON valido"
                                  title tit-err
                                   icon 2
                        move spaces to lab-classe-buf
                    not invalid
                        move cl1-descrizione to lab-classe-buf
                   end-read
                end-if
                display lab-classe

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

