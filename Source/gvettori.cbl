           move 1 to mod-k.
           set StatusVisua    to true. 
           set ControllaCampi to true.

           move 1 to Form1-RADIO-1-BUF.
           display   rb-regione 
           .
      * <TOTEM:END>

       INIT-OLD-REC.
      * <TOTEM:PARA. INIT-OLD-REC>
           initialize old-vet-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.

      * SPECIFICO PGM "gvettori"
           Set old-vet-regione        to true.
      * FINE
           .
      * <TOTEM:END>

       MESSAGE-DELETE-TARIFFA.
      * <TOTEM:PARA. MESSAGE-DELETE-TARIFFA>
           move mb-yes to scelta.

           if vecchio |Il nuovo vettore NON può avere tariffari associati
              inquire gd-peso, last-row in tot-righe

              if FromDeleteRigaPeso
                 set FromDeleteRigaPeso to false
                 set trovato            to true
              else
                 set trovato to false
                 perform varying store-riga from 2 by 1 
                           until store-riga > tot-righe
                    subtract 1 from store-riga giving idx
                    inquire gd-peso(store-riga, 1) cell-data in 
           peso-da-num
                    inquire gd-peso(store-riga, 2) cell-data in 
           peso-a-num
                    if peso-da-num not = vet-qli-da-peso(idx) or
                       peso-a-num  not = vet-qli-a-peso(idx)
                       set trovato to true
                       exit perform
                    end-if
                 end-perform
              end-if

              if trovato
                 set trovato to false
                 inquire ef-codice, value in vet-codice
                 move low-value  to tfv-rec
                 move vet-codice to tfv-codice
                 start tarifvet key is >= tfv-chiave
                       invalid continue
                   not invalid
                       read tarifvet next
                       if tfv-codice = vet-codice
                          set trovato to true
                       end-if
                 end-start
              end-if
              if trovato
                 move lab-des-buf to como-des
                 inspect como-des replacing trailing spaces by low-value
                 initialize messaggio
                 string "Modificati gli scalari. "         delimited 
           size
                        "Cancelli le tariffe del vettore " delimited 
           size
                        como-des                           delimited 
           low-value
                        " ?"                               delimited 
           size
                        into messaggio
                 end-string
                 display message messaggio
                           title titolo
                            type 3
                          giving scelta
                 if scelta = mb-yes
                    close tarifvet
                    open i-o tarifvet
                    if tutto-ok perform DELETE-TARIFFE
                    else        move mb-cancel to scelta
                    end-if
                    open input tarifvet
                 end-if
              end-if
           end-if  
           .
      * <TOTEM:END>

       MODIFICA.
      * <TOTEM:PARA. MODIFICA>
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if CONTROL-ID < 78-ID-ef-des
                       move 78-ID-ef-des to CONTROL-ID
                    end-if
                    move 0 to mod-k
                    move 1 to mod
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1    to mod
                 perform SALV-MOD            
                 move 0    to mod
                 move 1    to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    move 78-ID-ef-codice to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock tvettori all records
                 end-if
              end-if
                        
              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if 
           .
      * <TOTEM:END>

       NUOVO.
      * <TOTEM:PARA. NUOVO>
           perform SALV-MOD.

           if tutto-ok  
              move 78-ID-ef-codice to CONTROL-ID       
              move 4         to ACCEPT-CONTROL   
              move 1         to mod mod-k
              modify tool-modifica,  value = mod
              perform CANCELLA-COLORE                  
              perform VALORIZZA-NUOVO
              perform INIT-OLD-REC       
              set StatusIns to true
              perform STATUS-BAR-MSG  
              set nuovo to true       
              unlock tvettori all records
           end-if 
           .
      * <TOTEM:END>

       ORDINAMENTO.
      * <TOTEM:PARA. ORDINAMENTO>
           inquire tool-ord, value in OrderBy.
      
           if OrderBy = 0 perform DataSet1-CHANGETO-KEY1
           else           perform DataSet1-CHANGETO-KEY2
           end-if.
      
           perform CURRENT-RECORD 
           .
      * <TOTEM:END>

       PRIMO.
      * <TOTEM:PARA. PRIMO>
           perform SALV-MOD.           
           if tutto-ok    
 
              perform CHECK-INSERIMENTO
              unlock tvettori all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tvettori-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tvettori-lock-flag
              end-if
                                  
              perform CANCELLA-COLORE
              perform FORM1-FIRST

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tvettori-lock-flag
                 move 1 to mod-k
                 perform FORM1-FIRST
              end-if
           end-if 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".
           copy "status.cpy".
           copy "calcola-colore-trasparente.cpy" 
           .
      * <TOTEM:END>

       PRECEDENTE.
      * <TOTEM:PARA. PRECEDENTE>
           perform SALV-MOD.           
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tvettori all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tvettori-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tvettori-lock-flag
              end-if
                    
              perform CANCELLA-COLORE
              perform FORM1-PREVIOUS

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tvettori-lock-flag
                 move 1 to mod-k
                 perform FORM1-PREVIOUS
              end-if
           end-if 
           .
      * <TOTEM:END>

       PUNTA-CELLA-PESO.
      * <TOTEM:PARA. PUNTA-CELLA-PESO>
           move 78-ID-gd-peso      to control-id store-id.
           move 4                  to accept-control.
           modify gd-peso,   cursor-y riga, 
                             cursor-x colonna.
           perform COLORE-RIGA-PESO 
           .
      * <TOTEM:END>

       PUNTA-CELLA-ARROT.
      * <TOTEM:PARA. PUNTA-CELLA-ARROT>
           move 78-ID-gd-arrot      to control-id store-id.
           move 4                   to accept-control.
           modify gd-arrot,   cursor-y riga, 
                              cursor-x colonna.
           perform COLORE-RIGA-ARROT 
           .
      * <TOTEM:END>

       RESETTA-GRID-ARROT.
      * <TOTEM:PARA. RESETTA-GRID-ARROT>
           modify gd-arrot, reset-grid = 1.
           perform GD-ARROT-CONTENT 
           .
      * <TOTEM:END>

       RESETTA-GRID-PESO.
      * <TOTEM:PARA. RESETTA-GRID-PESO>
           modify gd-peso, reset-grid = 1.
           perform GD-PESO-CONTENT 
           .
      * <TOTEM:END>

       RESET-VIRGOLA.
      * <TOTEM:PARA. RESET-VIRGOLA>
           set environment "KEYSTROKE" to "DATA=44 44".
           set environment "KEYSTROKE" to "DATA=46 46" 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE.
      * <TOTEM:PARA. RIEMPI-CHIAVE>
           inquire ef-codice, value in ef-codice-buf.
           move ef-codice-buf       to vet-codice convert 
           .
      * <TOTEM:END>

       RIEMPI-GRID-ARROT.
      * <TOTEM:PARA. RIEMPI-GRID-ARROT>
           move 0 to idx.
           perform RESETTA-GRID-ARROT.
           perform 10 times
              add   1 to idx
              move vet-qli-da-arrot(idx)  to col-da-arrot
              move vet-qli-a-arrot(idx)   to col-a-arrot
              move vet-valore-arrot(idx)  to col-valore
              modify gd-arrot(idx + 1, 1), cell-data = col-da-arrot
              modify gd-arrot(idx + 1, 2), cell-data = col-a-arrot 
              modify gd-arrot(idx + 1, 3), cell-data = col-valore
           end-perform 
           .
      * <TOTEM:END>

       RIEMPI-GRID-PESO.
      * <TOTEM:PARA. RIEMPI-GRID-PESO>
           move 0 to idx.
           perform RESETTA-GRID-PESO.
           perform 20 times
              add   1 to idx
              move vet-qli-da-peso(idx) to col-da-peso
              move vet-qli-a-peso(idx)  to col-a-peso
              modify gd-peso(idx + 1, 1), cell-data = col-da-peso
              modify gd-peso(idx + 1, 2), cell-data = col-a-peso
           end-perform 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           if mod = 0 exit paragraph end-if.
      
           set tutto-ok to true.
      
           perform varying CONTROL-ID from 78-ID-ef-codice by 1
                     until CONTROL-ID > 78-ID-ef-url
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if vecchio
              evaluate form1-radio-1-buf
              when 1 |REGIONE
                   if old-vet-regione continue
                   else 
                      perform MESSAGE-DELETE-TARIFFA
                      if scelta = mb-cancel
                         set errori to true
                      end-if
                   end-if
              when 2 |PROVINCIA
                   if old-vet-prov continue
                   else 
                      perform MESSAGE-DELETE-TARIFFA
                      if scelta = mb-cancel
                         set errori to true
                      end-if
                   end-if
              when 3 |CLIENTE
                   if old-vet-cliente continue
                   else 
                      perform MESSAGE-DELETE-TARIFFA
                      if scelta = mb-cancel
                         set errori to true
                      end-if
                   end-if
              when 4 |CLIENTE/DESTINO
                   if old-vet-clides continue
                   else 
                      perform MESSAGE-DELETE-TARIFFA
                      if scelta = mb-cancel
                         set errori to true
                      end-if
                   end-if
              end-evaluate
           end-if.
      
           if tutto-ok
              perform X-Y-PESO
              perform varying riga from 2 by 1 
                        until riga > tot-righe
                 perform VALORE-RIGA-PESO
                 perform varying colonna from 1 by 1
                           until colonna > 2
                    perform CONTROLLO-RIGA-PESO
                    if errori 
                       move 78-ID-gd-peso to store-id
                       exit perform 
                    end-if
                 end-perform
                 if errori exit perform end-if
              end-perform
           end-if.     
      
           if tutto-ok
              perform X-Y-ARROT
              perform varying riga from 2 by 1 
                        until riga > tot-righe
                 perform VALORE-RIGA-ARROT
                 perform varying colonna from 1 by 1
                           until colonna > 2
                    perform CONTROLLO-RIGA-ARROT
                    if errori 
                       move 78-ID-gd-arrot to store-id
                       exit perform 
                    end-if
                 end-perform
                 if errori exit perform end-if
              end-perform
           end-if.

           if tutto-ok
              perform FORM1-BUF-TO-FLD
              if vet-abruzzo        = 0 or
                 vet-basilicata     = 0 or
                 vet-calabria       = 0 or
                 vet-campania       = 0 or
                 vet-emilia-romagna = 0 or
                 vet-friuli         = 0 or
                 vet-lazio          = 0 or
                 vet-liguria        = 0 or
                 vet-lombardia      = 0 or
                 vet-marche         = 0 or
                 vet-molise         = 0 or
                 vet-piemonte       = 0 or
                 vet-puglia         = 0 or
                 vet-sardegna       = 0 or
                 vet-sicilia        = 0 or
                 vet-toscana        = 0 or
                 vet-trentino       = 0 or
                 vet-umbria         = 0 or
                 vet-valle          = 0 or
                 vet-veneto         = 0
                 set errori to true
                 display message "Valorizzare i giorni per tutte le regi
      -    "oni"
                          title tit-err
                           icon 2
           end-if.
      
           if errori
              perform ABILITAZIONI
              evaluate store-id 
              when 78-ID-gd-arrot
                   perform PUNTA-CELLA-ARROT
              when 78-ID-gd-peso
                   perform PUNTA-CELLA-PESO
              when other
                   move store-id to CONTROL-ID
                   move 4        to ACCEPT-CONTROL
              end-evaluate
           else                       
              perform FORM1-BUF-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              perform CANCELLA-COLORE 
                                     
              accept como-data from century-date
              accept como-ora  from time
          
              write vet-rec invalid rewrite vet-rec end-write
              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD.
      
           if SiSalvato and mod = 1
              move 0 to idx
              perform 20 times
                 add 1 to idx

                 if old-vet-qli-da-peso(idx) not = vet-qli-da-peso(idx)
                    set NoSalvato to true
                    move 78-ID-gd-peso to store-id
                    move 1 to colonna
                    add  1 to idx giving riga
                    exit perform
                 end-if

                 if old-vet-qli-a-peso(idx) not = vet-qli-a-peso(idx)
                    set NoSalvato to true
                    move 78-ID-gd-peso to store-id
                    add  1 to idx giving riga
                    move 2 to colonna
                    exit perform
                 end-if

              end-perform
           end-if.                     
      
           if SiSalvato and mod = 1
              move 0 to idx
              perform 10 times
                 add 1 to idx

                 if old-vet-qli-da-arrot(idx) not = 
           vet-qli-da-arrot(idx)
                    set NoSalvato to true
                    move 78-ID-gd-arrot to store-id
                    move 1 to colonna
                    add  1 to idx giving riga
                    exit perform
                 end-if

                 if old-vet-qli-a-arrot(idx) not = vet-qli-a-arrot(idx)
                    set NoSalvato to true
                    move 78-ID-gd-arrot to store-id
                    add  1 to idx giving riga
                    move 2 to colonna
                    exit perform
                 end-if

                 if old-vet-valore-arrot(idx) not = 
           vet-valore-arrot(idx)
                    set NoSalvato to true
                    move 78-ID-gd-arrot to store-id
                    add  1 to idx giving riga
                    move 3 to colonna
                    exit perform
                 end-if

              end-perform
           end-if.
      
           if NoSalvato
              display message MSG-Salvare-le-modifiche
                        title titolo
                         type mb-yes-no-cancel 
                       giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no
                   continue
              when other
                   perform CANCELLA-COLORE
                   set errori to true
                   inquire Screen1-Ta-1, value in pagina
                   evaluate pagina
                   when 1
                        if store-id > 78-ID-rb-clides
                           move 666 to store-id
                        end-if
                   when 2
                        if store-id < 78-ID-gd-peso
                           move 666 to store-id
                        end-if
                   end-evaluate
                   if store-id = 78-ID-gd-peso or
                      store-id = 78-ID-gd-arrot
                      perform PUNTA-CELLA-PESO
                   else
                      move store-id to CONTROL-ID       
                      move 4        to ACCEPT-CONTROL   
                   end-if
              end-evaluate
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA.
      * <TOTEM:PARA. SELEZIONA>
           perform RIEMPI-CHIAVE.

           move "tvettori"  to    como-file.
           call "zoom-gt" using como-file, vet-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = 0
              if old-vet-chiave not = vet-chiave or
               ( vet-codice = 0 and mod = 0 )
                 move vet-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to vet-chiave
                    set ReadSecca to true
                    modify ef-codice, value vet-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-des to CONTROL-ID       
                    move 4            to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-ALFA.
      * <TOTEM:PARA. SELEZIONA-ALFA>
      * LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      * ANCORA GLI ZERI DAVANTI CHE ELIMINO
           inspect ef-codice-buf replacing leading ZERO by SPACES.
           move ef-codice-buf to vet-descrizione.
      
      * POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      * ALTERNATA
           call "C$JUSTIFY" using vet-descrizione, "L".
      
           start tvettori key >= vet-descrizione
                 invalid  continue
             not invalid  read tvettori next 
           end-start.
      
           move "tvettori-alfa" to como-file.
           call "zoom-gt" using como-file, vet-rec
                          giving stato-zoom.
           cancel "zoom-gt".

           if stato-zoom = 0
              move vet-codice     to codice-ed
              move codice-ed      to ef-codice-buf   
              call "C$JUSTIFY" using ef-codice-buf, "L"
              display ef-codice
      
              if old-vet-descrizione not = vet-descrizione
                 move vet-descrizione to save-key-1
                 perform SALV-MOD
                 if tutto-ok
                    move save-key-1 to vet-descrizione
                    set ReadSecca   to true
                    modify ef-codice, value vet-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-des to CONTROL-ID       
                    move 4            to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-NUMERICO.
      * <TOTEM:PARA. SELEZIONA-NUMERICO>
           read tvettori no lock
              invalid
                 if vecchio
                    display message MSG-record-inesistente
                              title tit-err
                               icon mb-warning-icon
                    perform CLEAR-SCREEN
                    set errori to true
                 end-if
              not invalid      
                 if old-vet-chiave not = vet-chiave or
                    ( vet-codice = 0 and mod = 0 )
                    perform CURRENT-RECORD 
                 end-if
           end-read 
           .
      * <TOTEM:END>

       SET-VIRGOLA.
      * <TOTEM:PARA. SET-VIRGOLA>
      *    sostituisco il punto come virgola
           set environment "KEYSTROKE" to "DATA=44 46" 
           .
      * <TOTEM:END>

       SPOSTAMENTO-ARROT.
      * <TOTEM:PARA. SPOSTAMENTO-ARROT>
           set tutto-ok to true.
           perform X-Y-ARROT.
      
           if event-data-2 not = riga
      
              perform VALORE-RIGA-ARROT
      
              perform varying colonna from 1 by 1
                        until colonna > 2
                 perform CONTROLLO-RIGA-ARROT
                 if errori exit perform end-if
              end-perform
      
              if tutto-ok
                 evaluate true
                 when event-data-2 < 2 
                      move 2            to riga
                      move event-data-1 to colonna
      
                 when event-data-2 > tot-righe
                      add  1                to tot-righe giving riga
                      set event-action      to event-action-fail
                      move 1                to colonna
                      perform PUNTA-CELLA-ARROT
                 when other
                      move event-data-2 to riga
                      move event-data-1 to colonna
                 end-evaluate
                 perform VALORE-RIGA-ARROT
              else
                 set event-action      to event-action-fail
                 perform PUNTA-CELLA-ARROT
              end-if
           else
              move event-data-1 to colonna
           end-if.
      
           perform COLORE-RIGA-ARROT.
           perform HELP-GRID-ARROT.
      
      ***---
       COLORE-RIGA-ARROT.
           modify gd-arrot, start-x = 1,
                                  x = 3,
                            start-y = riga,
                                  y = riga,
                       region-color = 257,
                       cursor-color = 481 
           .
      * <TOTEM:END>

       SPOSTAMENTO-PESO.
      * <TOTEM:PARA. SPOSTAMENTO-PESO>
           set tutto-ok to true.
           perform X-Y-PESO.
      
           if event-data-2 not = riga
      
              perform VALORE-RIGA-PESO
      
              perform varying colonna from 1 by 1
                        until colonna > 2
                 perform CONTROLLO-RIGA-PESO
                 if errori exit perform end-if
              end-perform
      
              if tutto-ok
                 evaluate true
                 when event-data-2 < 2 
                      move 2            to riga
                      move event-data-1 to colonna
      
                 when event-data-2 > tot-righe
                      add  1                to tot-righe giving riga
                      set event-action      to event-action-fail
                      move 1                to colonna
                      perform PUNTA-CELLA-PESO
                 when other
                      move event-data-2 to riga
                      move event-data-1 to colonna
                 end-evaluate
                 perform VALORE-RIGA-PESO
              else
                 set event-action      to event-action-fail
                 perform PUNTA-CELLA-PESO
              end-if
           else
              move event-data-1 to colonna
           end-if.
      
           perform COLORE-RIGA-PESO.
           perform HELP-GRID-PESO.
      
      ***---
       COLORE-RIGA-PESO.
           modify gd-peso, start-x = 1,
                                 x = 2,
                           start-y = riga,
                                 y = riga,
                      region-color = 257,
                      cursor-color = 481 
           .
      * <TOTEM:END>

       SUCCESSIVO.
      * <TOTEM:PARA. SUCCESSIVO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tvettori all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tvettori-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tvettori-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tvettori-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if 
           .
      * <TOTEM:END>

       TORNA-IN-VISUA.
      * <TOTEM:PARA. TORNA-IN-VISUA>
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-codice to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock tvettori all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if.

           set NonCambiareTab   to false.
           set ControllaCampi   to true.
           
           move 1 to event-data-1 screen1-ta-1-tab-value.
           perform SCREEN1-TA-1-TABCHANGE  
           .
      * <TOTEM:END>

       ULTIMO.
      * <TOTEM:PARA. ULTIMO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tvettori all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tvettori-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tvettori-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tvettori-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if 
           .
      * <TOTEM:END>

       VALORE-RIGA-ARROT.
