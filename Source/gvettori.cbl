       pb-pod-LinkTo.
      * <TOTEM:PARA. pb-pod-LinkTo>
           inquire ef-pod, value in ef-pod-buf.
           initialize opensave-data.
           if ef-pod-buf = spaces
              accept ef-pod-buf from environment "PATH_POD"           
              inspect ef-pod-buf replacing trailing spaces by low-value
              move ef-pod-buf to opnsav-default-dir
              initialize opnsav-filters
              string "|File Adobe PDF (*.pdf)|*.pdf"    delimited size
                     "TEXT files (*.txt)|*.txt"         delimited size
                     "|Documenti di Word (*.doc)|*.doc" delimited size
                     "|All files (*.*)|*.*"             delimited size
                     into  opnsav-filters
              end-string
              move 1 to opnsav-default-filter
              move "Geslux -  Selezione POD per vettore" to opnsav-title
              call "C$OPENSAVEBOX" using opensave-save-box, 
                                         opensave-data,
                                  giving opensave-status
      
              if opensave-status = 1
                 move opnsav-filename to ef-pod-buf
                 display ef-pod
              end-if
           else
              initialize comando
              move ef-pod-buf to opnsav-filename
              inspect ef-pod-buf replacing trailing spaces by low-value
              string "START "    delimited size
                    x"22222022"  delimited size
                     ef-pod-buf  delimited low-value
                    x"22"        delimited size
                     into comando
              end-string         
              inspect ef-pod-buf replacing trailing low-value by spaces
              call "C$SYSTEM" using comando, 64
                             giving ReturnCode
              if ReturnCode = -1
                 display message box "Comando fallito!"
                         title = tit-err
                         icon mb-error-icon
              end-if
           end-if.

           move opnsav-filename to ef-pod-buf.
           display ef-pod 
           .
      * <TOTEM:END>
