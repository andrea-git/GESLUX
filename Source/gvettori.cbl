       TVETTORI-ERR SECTION.
           use after error procedure on tvettori.
           set RecLocked to false.
           evaluate status-tvettori
           when "35"
                display message "File [TVETTORI] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [TVETTORI] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[TVETTORI] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99"
           when "93"
                set RecLocked to true
                set errori    to true
                move 0 to mod
                modify tool-modifica, value = mod
                move 1 to mod-k
                display form1
                set StatusVisua to true
                perform STATUS-BAR-MSG
                move "00" to status-tvettori
                if ReadSecca
                   read tvettori     no lock 
                        invalid      continue
                        not invalid  perform FORM1-IUD-DISPLAY
                   end-read
                end-if
                display message box MSG-Record-gia-in-uso
                          title tit-err
                           icon 2
           when "23"
                move 1 to mod-k
                move 0 to mod
                modify tool-modifica, value = mod
                set StatusVisua to true
                perform STATUS-BAR-MSG 
                if nuovo              
                   initialize vet-dati replacing numeric data by zeroes
                                            alphanumeric data by spaces
                   perform FORM1-FLD-TO-BUF
                   display form1-tb-1
                   set vecchio to true
                end-if
                perform ABILITAZIONI
                display form1
                |RISETTO IL VALORE A "23" PERCHE DENTRO AL PAR. CLEAR-SCREEN
                |CON L'OPERAZIONE UNLOCK VIENE RISETTATO A ZERO COSICCHÈ 
                |QUANDO TORNA DENTRO AI PAR. DI SCORRIMENTO DI TOTEM SI 
                |TROVA IN UNA SITUAZIONE CORRETTA E PROSEGUE CON LA READ
                |USCENDO CON UN MESSAGGIO DI ERRORE DI TIPO "46"
                move "23" to status-tvettori
           end-evaluate.

      ***---
       TARIFVET-ERR SECTION.
           use after error procedure on tarifvet.
           set tutto-ok  to true.
           evaluate status-tarifvet
           when "93"                     
                set  RecLocked    to true
                initialize geslock-messaggio
                string   "File già in uso!"
                  x"0d0a""Impossibile procedere!" delimited size
                      into geslock-messaggio
                end-string
                move 1 to geslock-v-riprova
                move 0 to geslock-v-ignora
                move 1 to geslock-v-termina
                move   "tarifvet"   to geslock-nome-file
                call   "geslock" using geslock-linkage
                cancel "geslock"
                evaluate true
                when riprova
                     open i-o tarifvet
                when termina
                     set errori to true
                end-evaluate
           when "99"
                set  RecLocked  to true
                move tfv-codice to codice-ed
                move tfv-prog   to riga-ed
                initialize geslock-messaggio
                string "Il tariffario per: "
                x"0d0a""Vettore: ", codice-ed, " Prog: ", riga-ed,
                x"0d0a""risulta in uso su altro terminale."
                x"0d0a""Questo comporta l'impossibilità"
                       " alla cencellazione."
                      delimited size
                      into geslock-messaggio
                end-string
                move 1 to geslock-v-riprova
                move 1 to geslock-v-ignora
                move 1 to geslock-v-termina
                move   "provvig"    to geslock-nome-file
                call   "geslock" using geslock-linkage
                cancel "geslock"
                evaluate true
                when riprova
                     set RecLocked to false
                     delete tarifvet record invalid continue end-delete
                when ignora
                     |RIPOSIZIONO IL CURSORE
                     read tarifvet no lock
                when termina
                     set errori to true
                end-evaluate
           end-evaluate.

