       scr-elab-PROC.
      * <TOTEM:EPT. FORM:scr-elab, FORM:scr-elab, BeforeAccept>
           move low-value           to tor-rec.
           move anno-consolidamento to tor-anno-fattura.
           move data-consolidamento to tor-data-fattura como-data.
           move 0                   to tot-provv.
           move 0                   to counter counter2 counter-edit.
           start tordini key is >= k4
                 invalid continue
             not invalid
                 perform until 1 = 2

                    add 1 to counter
                    add 1 to counter2
                    if counter2 = 5
                       move counter to counter-edit
                       display counter-edit
                          upon scr-elab-handle at column 20
                                                    line 03
                       move 0 to counter2
                    end-if

                    read tordini next at end exit perform end-read
                    if tor-data-fattura not = como-data 
                       exit perform 
                    end-if
                    if tor-si-agg-contab and tor-num-prenot = num-lotto
                       move 0 to tor-data-contab
                                 tor-ora-contab
                       set tor-no-agg-contab to true
                       rewrite tor-rec invalid continue end-rewrite
                       add 1 to tot-provv
                    end-if
                 end-perform
           end-start.

           if tot-provv = 0           
              move low-value           to tno-rec
              move anno-consolidamento to tno-anno-fattura
              move data-consolidamento to tno-data-fattura como-data
              move 0                   to tot-provv
              move 0                   to counter counter2 counter-edit
              start tnotacr key is >= k4
                    invalid continue
                not invalid
                    perform until 1 = 2
         
                       add 1 to counter
                       add 1 to counter2
                       if counter2 = 5
                          move counter to counter-edit
                          display counter-edit
                             upon scr-elab-handle at column 20
                                                       line 03
                          move 0 to counter2
                       end-if

                       read tnotacr next at end exit perform end-read
                       if tno-data-fattura not = como-data 
                          exit perform 
                       end-if
                       if tno-si-agg-contab and tno-num-prenot = 
           num-lotto
                          move 0 to tno-data-contab
                          set tno-no-agg-contab to true
                          rewrite tno-rec invalid continue end-rewrite
                          add 1 to tot-provv
                       end-if
                    end-perform
              end-start

           end-if.


           move 27 to key-status.

           .
