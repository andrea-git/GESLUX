                       move primo-id-dati to CONTROL-ID
                    end-if
                    move 0 to mod-k
                    move 1 to mod
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1 to mod
                 perform SALV-MOD            
                 move 0 to mod
                 move 1 to mod-k
                 if errori
                    move 1    to mod
                    move ZERO to mod-k
                    move prg-stato of progmag     to stato
                    perform CARICA-COMBO-STATO
                    evaluate true
                    when bloccato  move 1    to mod-campi
                    when attivo    move 1    to mod-campi
                    when disattivo move ZERO to mod-campi
                    end-evaluate
                 else
                    move 78-id-ef-cod-articolo to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock progmag all records

                    if KeyboardSaved
                       set KeyboardReleased to true
                       set environment "KEYSTROKE" to "DATA=44 44"
                       set environment "KEYSTROKE" to "DATA=46 46"
                    end-if

                 end-if
              end-if
                               
              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE
              move 4    to ACCEPT-CONTROL
              move ZERO to StatusHelp
              perform STATUS-HELP
           end-if 
