      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-vet-from
                inquire ef-vet-from, value vet-codice
                move "tvettori"      to como-file
                call   "zoom-gt"  using como-file, vet-rec
                                 giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move vet-codice  to ef-vet-from-buf
                   move 4 to accept-control
                end-if
                display ef-vet-from

           when 78-ID-ef-vet-to
                inquire ef-vet-to, value vet-codice
                move "tvettori"       to como-file
                call   "zoom-gt"   using como-file, vet-rec
                                  giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move vet-codice  to ef-vet-to-buf
                   move 4 to accept-control
                end-if
                display ef-vet-to

           when 78-ID-ef-cliente
                move "C"               to cli-tipo-cf
                inquire ef-cliente, value cli-codice
                move "clienti-all"     to como-file
                call   "zoom-gt" using como-file, cli-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-cliente, value cli-codice
                   move cli-ragsoc-1  to lab-cliente-buf
                   display lab-cliente
                   move 4 to accept-control
                end-if

           when 78-ID-ef-localita
                inquire ef-cliente,  value des-codice
                inquire ef-localita, value des-localita
                move "destini-citta" to como-file
                call   "zoom-gt" using como-file, des-rec
                                 giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-localita, value des-localita
                   move spaces to lab-localita-buf
                   display lab-localita
                   move 4 to accept-control
                end-if

           when 78-ID-ef-prov
                inquire ef-prov, value prv-codice
                move "tprov"        to como-file
                call   "zoom-gt" using como-file, prv-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-prov, value prv-codice
                   move prv-descrizione  to lab-prov-buf

                   initialize reg-rec
                   move prv-regione     to reg-codice
                   read tregioni no lock
                     invalid
                        initialize reg-rec
                   end-read
                   move reg-codice      to ef-reg-buf
                   move reg-descrizione to lab-reg-buf
                   move 0 to ena-reg

                   display lab-prov
                           ef-reg lab-reg 
                   move 4 to accept-control
                end-if

           when 78-ID-ef-reg
                inquire ef-reg, value reg-codice
                move "tregioni"     to como-file
                call   "zoom-gt" using como-file, reg-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-reg, value reg-codice
                   move reg-descrizione  to lab-reg-buf
                   display lab-reg
                   move 4 to accept-control
                end-if

           end-evaluate
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-ID-ef-data-from                          
                inquire ef-data-from, value in ef-data-from-buf
                move ef-data-from-buf to como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-from-buf
                else
                   string "01"     delimited size
                          "01"     delimited size
                          tge-anno delimited size
                          into como-data
                   end-string
                   move como-data to ef-data-from-buf
                end-if
                display ef-data-from

           when 78-ID-ef-data-to                              
                inquire ef-data-from, value in ef-data-from-buf
                inquire ef-data-to,   value in ef-data-to-buf
                move ef-data-to-buf to como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-to-buf
                else
                   move ef-data-from-buf      to como-data-from
                   string "31"                delimited size
                          "12"                delimited size
                          como-data-from(5:4) delimited size
                          into como-data
                   end-string
                end-if

                move como-data to ef-data-to-buf
                display ef-data-to

                perform DATE-TO-FILE
                move como-data        to como-data-to

                move ef-data-from-buf to como-data
                perform DATE-TO-FILE
                move como-data    to como-data-from

                if como-data-to < como-data-from
                   set errori to true
                   display message box "Intervallo limiti data errato!"
                           title = tit-err
                           icon 2
                   move 78-ID-ef-data-from to control-id
                else
                   if como-data-to(1:4) not = como-data-from(1:4)
                      set errori to true
                      display message "L'anno delle date NON coincide!"
                                title tit-err
                                 icon 2
                      move 78-ID-ef-data-from to control-id
                   end-if
                end-if

           when 78-ID-ef-vet-from
                inquire ef-vet-from, value in ef-vet-from-buf

           when 78-ID-ef-vet-to
                inquire ef-vet-from, value in ef-vet-from-buf
                inquire ef-vet-to,   value in ef-vet-to-buf

                if ef-vet-to-buf = spaces
                   move 99999    to ef-vet-to-buf
                   display ef-vet-to
                end-if

                if ef-vet-to-buf < ef-vet-from-buf
                   set errori to true
                   display message box
                   "Intervallo limiti vettore errato!"
                           title = tit-err
                           icon 2
                   move 78-ID-ef-vet-from to control-id
                end-if

           when 78-ID-ef-cliente
                move "C"                 to cli-tipo-cf
                inquire ef-cliente value in cli-codice
                if cli-codice = 0
                   move 0 to ef-cliente-buf
                   move "Tutti" to lab-cliente-buf
                else
                   read clienti no lock
                        invalid 
                        display message "Cliente NON valido"
                                  title tit-err
                                   icon 2
                        set errori  to true
                        move spaces to lab-cliente-buf
                    not invalid
                        move cli-ragsoc-1 to lab-cliente-buf
                   end-read
                end-if
                display ef-cliente lab-cliente

           when 78-ID-ef-localita
                initialize des-rec
                inquire ef-cliente,  value des-codice
                inquire ef-localita, value des-localita
                if des-localita = space
                   move spaces  to ef-localita-buf
                   move "Tutte" to lab-localita-buf
                else
                   read destini no lock key is k-localita
                      invalid
                         start destini key >= k-localita
                            invalid 
                               continue
                         end-start
                         move "destini-citta" to como-file
                         call   "zoom-gt" using como-file, des-rec
                                          giving stato-zoom
                         cancel "zoom-gt"
                         if stato-zoom = 0
                            move des-localita to ef-localita-buf
                            move spaces to lab-localita-buf
                         end-if
                      not invalid
                          move des-localita to ef-localita-buf
                          move spaces to lab-localita-buf
                   end-read
                end-if
                display ef-localita lab-localita

           when 78-ID-ef-prov
                inquire ef-prov, value prv-codice
                if prv-codice = spaces
                   move spaces  to ef-prov-buf
                   move "Tutte" to lab-prov-buf
                   move 1 to ena-reg
                else
                   read tprov no lock
                      invalid 
                         display message "Provincia NON valida"
                                   title tit-err
                                    icon 2
                         set errori to true
                         move spaces to lab-prov-buf
                      not invalid      
                        move prv-descrizione to lab-prov-buf
                        initialize reg-rec
                        move prv-regione     to reg-codice
                        read tregioni no lock
                          invalid
                             initialize reg-rec
                        end-read
                        move reg-codice      to ef-reg-buf
                        move reg-descrizione to lab-reg-buf
                        move 0 to ena-reg
                   end-read
                end-if
                display ef-prov lab-prov
                        ef-reg  lab-reg

           when 78-ID-ef-reg
                inquire ef-reg, value reg-codice
                if reg-codice = 0
                   move 0       to ef-reg-buf
                   move "Tutte" to lab-reg-buf
                else
                   read tregioni no lock
                      invalid 
                         display message "Regione NON valida"
                                   title tit-err
                                    icon 2
                         set errori to true
                         move 78-ID-ef-reg to control-id
                         move spaces to lab-reg-buf
                      not invalid      
                         move reg-descrizione to lab-reg-buf
                   end-read
                end-if
                display ef-reg lab-reg
                          
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to STORE-ID        
              move 4          to ACCEPT-CONTROL  
           end-if 
           .
      * <TOTEM:END>

