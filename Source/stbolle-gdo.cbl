      * tcontat
       DataSet1-tcontat-START.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tcontat-KEY1-Asc
                 START tcontat KEY >= con-chiave
              ELSE
                 START tcontat KEY <= con-chiave
              END-IF
           END-EVALUATE
           .

       DataSet1-tcontat-START-NOTGREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tcontat-KEY1-Asc
                 START tcontat KEY <= con-chiave
              ELSE
                 START tcontat KEY >= con-chiave
              END-IF
           END-EVALUATE
           .

       DataSet1-tcontat-START-GREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tcontat-KEY1-Asc
                 START tcontat KEY > con-chiave
              ELSE
                 START tcontat KEY < con-chiave
              END-IF
           END-EVALUATE
           .

       DataSet1-tcontat-START-LESS.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tcontat-KEY1-Asc
                 START tcontat KEY < con-chiave
              ELSE
