           modify PB-ESEGUI, bitmap-number 1 
           .
      * <TOTEM:END>
       PB-ESEGUI-LinkTo.
      * <TOTEM:PARA. PB-ESEGUI-LinkTo>
           perform CONTROLLO.

           if tutto-ok
              perform SCR-DATA-OPEN-ROUTINE

              if scelta = MB-YES
                 perform SCR-ELAB-OPEN-ROUTINE

                 if scelta = MB-YES

                    initialize stbolle-linkage
                    set  stbolle-stampa  to true
                    set  stbolle-k3      to true
                    set  stbolle-singola to false
                    move como-data       to stbolle-data
                    move con-anno        to stbolle-anno
                    move user-codi       to link-user
                    set stbolle-piu-stampe-si to true

                    set stbolle-GDO to true
                    call "W$MOUSE"  using set-mouse-shape, wait-pointer
                    call   "stbolle-p" using stbolle-linkage
                    cancel "stbolle-p"
                    call "W$MOUSE"  using set-mouse-shape, arrow-pointer
                    move 78-ID-ef-data to CONTROL-ID
                    move 4             to ACCEPT-CONTROL  

                    perform varying idx from 1 by 1 
                              until idx > stbolle-num-stampe
                       evaluate idx 
                       when 1 move stbolle-path1      to stbolle-path
                              move stbolle-stampante1 to printerToSet
                    
                       when 2 move stbolle-path2      to stbolle-path   
             
                              move stbolle-stampante2 to printerToSet
                    
                       when 3 move stbolle-path3      to stbolle-path
                              move stbolle-stampante3 to printerToSet
                    
                       when 4 move stbolle-path4      to stbolle-path
                              move stbolle-stampante4 to printerToSet
                    
                       when 5 move stbolle-path5      to stbolle-path
                              move stbolle-stampante5 to printerToSet
                       end-evaluate
                  
                       initialize file-info
                  
                       call "C$FILEINFO" using stbolle-path, file-info
                  
                       if file-size not = 0 
                  
                          accept ru-user from environment "USER_CODI"
                          call   "readutente" using ru-linkage
                          cancel "readutente"
                          |Mi salvo la stampante
                          evaluate true
                          when ru-SO-XP
                               accept como-stampante 
                                from environment "STAMPANTE_STBOLLE_GDO_
      -    "XP"
                          when ru-SO-VISTA
                               accept como-stampante 
                                      from environment "STAMPANTE_STBOLL
      -    "E_GDO_V"
                          when ru-SO-7
                               accept como-stampante 
                                      from environment "STAMPANTE_STBOLL
      -    "E_GDO_7"
                          end-evaluate
                          |Setto la nuova
                          evaluate true
                          when ru-SO-XP
                               set environment "STAMPANTE_STBOLLE_GDO_XP
      -    ""
                                            to printerToSet
                          when ru-SO-VISTA
                               set environment "STAMPANTE_STBOLLE_GDO_V"
                                            to printerToSet
                          when ru-SO-7
                               set environment "STAMPANTE_STBOLLE_GDO_7"
                                            to printerToSet
                          end-evaluate   
                       
                          move "S"            to operazione
                          move stbolle-path   to WSTAMPA
                          move "V"            to orientamento
                          call "spooler-A"  using operazione
                                                  WSTAMPA
                                                  orientamento
                          cancel "spooler-A"  
      
                          call "C$DELETE" using stbolle-path
                       
                          |Resetto quella del cblconfi
                          evaluate true
                          when ru-SO-XP
                               set environment "STAMPANTE_STBOLLE_GDO_XP
      -    ""
                                            to como-stampante
                          when ru-SO-VISTA
                               set environment "STAMPANTE_STBOLLE_GDO_V"
                                            to como-stampante
                          when ru-SO-7
                               set environment "STAMPANTE_STBOLLE_GDO_7"
                                            to como-stampante
                          end-evaluate
                       end-if

                    end-perform

                    initialize file-info

                    call "C$FILEINFO" using stbolle-split, file-info

                    if file-size not = 0
                       move "S"             to operazione
                       move stbolle-split   to WSTAMPA
                       move "V"             to orientamento
                       call "spooler-A"  using operazione
                                               WSTAMPA
                                               orientamento
                       cancel "spooler-A"
                    end-if

                    call "C$DELETE" using stbolle-path
                    
                    modify pb-esegui,  bitmap-number = 1
                    modify Form1-Pb-2, bitmap-number = 1

                 end-if
              end-if
           end-if 
           .
      * <TOTEM:END>
