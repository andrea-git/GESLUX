      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate CONTROL-ID   
      *
           when 78-ID-ef-tipo
                inquire ef-tipo, value in tcl-codice
                move "ttipocli" to como-file
                call   "zoom-gt" using como-file, tcl-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-tipo,   value tcl-codice
                   modify lab-tipo,  value tcl-descrizione
                end-if               
      *
           when 78-ID-ef-age                      
                inquire ef-age    value age-codice
                move "agenti"  to Como-File
                call   "zoom-gt" using como-file, age-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-age,   value age-codice
                end-if
      *
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-ID-ef-data-from                          
                continue
      *                       
           when 78-ID-ef-data-to   
                inquire ef-data-from, value in ef-data-from-buf
                inquire ef-data-to,   value in ef-data-to-buf
                move ef-data-to-buf   to como-data-to
                move ef-data-from-buf to como-data-from
                if como-data-to = 0
                   move 99999 to como-data-to
                   move como-data-to to ef-data-to-buf
                   display ef-data-to
                end-if
                if como-data-to < como-data-from
                   set errori to true
                   display message box "Intervallo limiti clienti errato
      -    "!"
                           title = tit-err
                           icon 2
                   move 78-ID-ef-data-from to control-id
                end-if       
      *         
           when 78-ID-ef-tipo
                inquire ef-tipo, value in tcl-codice
                if tcl-codice not = spaces
                   read ttipocli no lock
                        invalid
                        move spaces to tcl-descrizione
                        set errori to true
                        display message "Inserimento tipologia NON valid
      -    "a"
                                  title tit-err
                                   icon 2
                        move 78-ID-ef-tipo to control-id
                   end-read
                   move tcl-descrizione   to lab-tipo-buf
                   display ef-tipo lab-tipo 
                else
                   move "TUTTE LE TIPOLOGIE" to lab-tipo-buf
                   display lab-tipo
                end-if      
      *                        
           when 78-ID-ef-age
                inquire ef-age,   value in age-codice
                move spaces to age-ragsoc-1
                if age-codice not = 0   
                   read agenti no lock
                        invalid
                           set errori to true
                           display message box "Inserimento agente NON v
      -    "alido"
                                   title = tit-err
                                   icon 2
                           move 78-ID-ef-age to control-id 
                   end-read    
                else                        
                   move "TUTTI GLI AGENTI" to age-ragsoc-1
                end-if     
                move age-ragsoc-1 to lab-age-buf
                display lab-age 
                move age-codice to SaveAgente
      *               
