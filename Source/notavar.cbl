       CURRENT-RECORD.
      * <TOTEM:PARA. CURRENT-RECORD>
           move LinkAnno of LinkChiave to tno-anno.
           move LinkNumero             to tno-numero.

           set tutto-ok  to true.
           set ReadSecca to true.
           if mod = 1
              read tnotacr lock invalid
                   set errori to true
              end-read
           else
              read tnotacr no lock invalid
                   set errori to true
              end-read
           end-if.
           set ReadSecca to false.
           
           if RecLocked
              set RecLocked to false
              set errori    to true
           end-if.

           if tutto-ok
              if LinkPgm = "stdoc"
                 move 888 to control-id
                 move   4 to accept-control
              end-if
              move "00"          to status-tnotacr
              move riga          to save-riga
              move   tno-causale to Link-TipoNota
              perform FORM1-FLD-TO-BUF
              perform VALORIZZA-SCREEN
              perform CARICA-GRID
              perform CERCA-CONTESTAZIONI
           else
              move 0 to mod
              move 0 to mod-k
              modify tool-modifica, value = mod
              perform ABILITAZIONI
           end-if.
           perform VALORIZZA-OLD 
           .
      * <TOTEM:END>

       DELETE-RIGHE.
      * <TOTEM:PARA. DELETE-RIGHE>
           move tno-chiave to rno-chiave.
           move low-value  to rno-num-riga.

           start rnotacr key is >= rno-chiave
                 invalid continue
             not invalid 
                 perform until 1 = 2
                    read rnotacr next at end exit perform end-read
                    if tno-anno   not = rno-anno or
                       tno-numero not = rno-numero
                       exit perform
                    end-if
                    delete rnotacr record invalid continue end-delete
                    if CallWProgmag
                       initialize link-wprogmag            
                       set link-update        to true
                       set link-update-um     to true
                       set link-update-peso   to false
                       set link-update-valore to false
                       move rno-prg-chiave    to link-key
                       move tno-causale       to link-causale
                       move rno-qta           to link-valore
                       perform VALORIZZA-ARRAY-CAUSALI
                       |lo devo lasciare qui perchè l'unico
                       |caso in cui assegno -1 a questa causale
                       move -1             to multiplyer(1)
                       move -1             to multiplyer(15)
                       move user-codi      to link-user of link-wprogmag
                       call   "wprogmag" using link-wprogmag
                       cancel "wprogmag"
                    end-if
                 end-perform
           end-start 
           .
      * <TOTEM:END>

