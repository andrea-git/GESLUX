      * <TOTEM:EPT. INIT:tcontat, INIT:tcontat, BeforeDeclarative>
       TCONTAT-ERR SECTION.
           use after error procedure on tcontat.
           set RecLocked to false.
           evaluate status-tcontat
           when "35"
                display message "File [TCONTAT] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [TCONTAT] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[TCONTAT] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "93"
                display message "[TCONTAT] File locked!"
                           title titolo
                            icon 3
                set errori to true
                perform GINQUI-EV-AFTER-PROGRAM
                goback

           when "99"
                set RecLocked to true
                set errori    to true
                move 1 to mod-k
                move 0 to mod
                modify tool-modifica, value = mod
                display form1
                set StatusVisua to true
                perform STATUS-BAR-MSG
                move "00" to status-tcontat
                if ReadSecca
                   read tcontat       no lock 
                        invalid      continue
                        not invalid  perform FORM1-IUD-DISPLAY
                   end-read
                end-if
                display message box MSG-Record-gia-in-uso
                        title = tit-err
                        icon is mb-error-icon
           when "23"
                move 1 to mod-k
                move 0 to mod
                modify tool-modifica, value = mod
                set StatusVisua to true
                perform STATUS-BAR-MSG 
                if nuovo              
                   initialize con-dati replacing numeric data by zeroes
                                            alphanumeric data by spaces
