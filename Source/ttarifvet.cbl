           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-vet   
                move tfv-codice to vet-codice
                read tvettori no lock
                     invalid
                     display message "Codice Vettore NON valido"
                               title tit-err
                                icon 2
                     set errori to true
                     move spaces to lab-vet-buf
                 not invalid
                     if nuovo
                        move 0 to tot-scaglioni
                        perform varying idx from 1 by 1 
                                  until idx > 20
                           compute tot-scaglioni =
                                   tot-scaglioni        +
                                   vet-qli-da-peso(idx) +
                                   vet-qli-a-peso(idx)
                        end-perform
                        if tot-scaglioni = 0
                           set errori to true
                           display message "Vettore senza scaglioni"
                                     title tit-err
                                      icon 2
                        end-if
                     end-if
                     move vet-descrizione to lab-vet-buf
                end-read
                display lab-vet
                if vet-codice not = save-codice
                   perform AGG-DA-TVETTORI
                   move vet-codice to save-codice
                end-if

           when 78-ID-ef-campo1
                inquire ef-campo1, VALUE com-campo
                evaluate true
                when vet-regione
                     move com-campo to reg-codice convert
                     read tregioni no lock
                          invalid 
                          display message "Regione NON valida"
                                    title tit-err
                                     icon 2
                          set errori to true
                          move spaces to lab-des-buf
                      not invalid      
                          move reg-descrizione to lab-des-buf
                          perform SELEZIONA-NUMERICO
                          if errori
                             move 78-ID-ef-vet to control-id
                             move 4 to accept-control
                          end-if
                     end-read
                          
                when vet-prov
                     move com-campo to prv-codice
                     read tprov no lock
                          invalid 
                          display message "Provincia NON valida"
                                    title tit-err
                                     icon 2
                          set errori to true
                          move spaces to lab-des-buf
                      not invalid      
                          move prv-descrizione to lab-des-buf
                          perform SELEZIONA-NUMERICO
                          if errori
                             move 78-ID-ef-vet to control-id
                             move 4 to accept-control
                          end-if
                     end-read

                when vet-cliente
                when vet-clides
                     move "C"        to cli-tipo-cf
                     move com-campo  to cli-codice convert
                     read clienti no lock
                          invalid 
                          display message "Cliente NON valido"
                                    title tit-err
                                     icon 2
                          set errori  to true
                          move spaces to lab-des-buf
                      not invalid
                          move cli-ragsoc-1 to lab-des-buf
                          if vet-cliente
                             perform SELEZIONA-NUMERICO
                             if errori
                                move 78-ID-ef-vet to control-id
                                move 4 to accept-control
                             end-if
                          end-if
                     end-read
                end-evaluate
                display lab-des

           when 78-ID-EF-dest
                if vis-destino = 1
                   inquire ef-campo1, value com-campo
                   move com-campo  to des-codice convert
                   inquire ef-dest,   value des-prog
                   read destini no lock
                        invalid 
                        display message "Destino NON valido"
                                  title tit-err
                                   icon 2
                        set errori to true
                        move 78-ID-ef-campo1 to control-id
                        move spaces to lab-destino-buf lab-localita-buf
                    not invalid
                        move des-ragsoc-1 to lab-destino-buf  
                        move des-localita to lab-localita-buf
                        perform SELEZIONA-NUMERICO
                        if errori
                           move 78-ID-ef-vet to control-id
                           move 4 to accept-control
                        end-if
                   end-read
                end-if
                display lab-destino lab-localita

           when 78-ID-gd1             
                continue
           end-evaluate.  

      ****************
           if errori
              if control-id = 78-ID-gd1
                 perform PUNTA-CELLA
              end-if
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

       CONTROLLO-OLD.
      * <TOTEM:PARA. CONTROLLO-OLD>
           set SiSalvato to true.
           if mod = 0 exit paragraph end-if.
           perform X-Y.

           if nuovo
              if tot-righe > 2
                 set NoSalvato to true
                 |Punta alla prima cella modificabile
                 move 3 to colonna
                 move 2 to riga
              end-if
           else
              perform VALORE-RIGA
              if col-euro not = old-col-euro
                 move 3 to colonna
                 set NoSalvato to true
              end-if
           end-if 
           .
      * <TOTEM:END>

       CONTROLLO-RIGA.
      * <TOTEM:PARA. CONTROLLO-RIGA>
           set tutto-ok to true.
           |IN QUESTO PGM. LA GRID E' COMPLETAMENTE LIBERA!!!
           evaluate colonna
           when 1
           when 2
           when 3 continue
           end-evaluate 
           .
      * <TOTEM:END>

       CURRENT-RECORD.
      * <TOTEM:PARA. CURRENT-RECORD>
           unlock tarifvet all records.
           if ReadSecca
              perform RIEMPI-CHIAVE
              move tfv-codice      to save-codice
              evaluate true
              when vet-prov
                   move tfv-prov   to save-prov
              when other
                   move tfv-campo1 to save-campo1
              end-evaluate
              move tfv-campo2      to save-campo2
              move low-value       to tfv-prog
              move tfv-prog        to save-prog
           end-if.

           set tutto-ok  to true.
           set trovato   to false. 

           move tfv-codice      to vet-codice.
           read tvettori no lock
                invalid
                   continue
           end-read

           start tarifvet key >= tfv-chiave
                 invalid  set errori to true
           end-start.

           if tutto-ok
              modify gd1, mass-update = 1
              perform RESET-GRIGLIA
              move 2 to riga

              perform until 1 = 2
                 if nuovo
                    read tarifvet next no lock 
                         at end exit perform 
                    end-read
                 else
                    read tarifvet next no lock 
                         at end exit perform 
                    end-read
                 end-if

                 if not ReadSecca
                    evaluate true
                    when vet-prov
                         if save-codice = 0 and 
                            save-prov   = space and
                            save-campo2 = 0
                            move tfv-codice to save-codice
                            move tfv-prov   to save-prov
                            move tfv-campo2 to save-campo2
                         end-if
                    when other
                         if save-codice = 0 and 
                            save-campo1 = 0 and
                            save-campo2 = 0
                            move tfv-codice to save-codice
                            move tfv-campo1 to save-campo1
                            move tfv-campo2 to save-campo2
                         end-if
                    end-evaluate
                 end-if

                 evaluate true
                 when vet-prov
                      if tfv-codice not = save-codice or
                         tfv-prov   not = save-prov   or
                         tfv-campo2 not = save-campo2
                         exit perform
                      end-if
                 when other
                      if tfv-codice not = save-codice or
                         tfv-campo1 not = save-campo1 or
                         tfv-campo2 not = save-campo2
                         exit perform
                      end-if
                 end-evaluate
                 
                 if mod = 1
                    read tarifvet lock invalid continue end-read
                    if RecLocked
                       read tarifvet no lock invalid continue end-read
                    end-if
                 end-if


                 set trovato to true 

                 if nuovo
                    set vecchio to true
                    move 1 to mod-k
                    move 0 to mod
                 end-if

                 move tfv-qli-da   to col-daq
                 move tfv-qli-a    to col-aq
                 move tfv-euro     to col-euro
                 move tfv-prog     to HiddenPrg
                 modify gd1(riga, 1),   cell-data = col-daq
                 modify gd1(riga, 2),   cell-data = col-aq 
                 modify gd1(riga, 3),   cell-data = col-euro
                 modify gd1(riga, 1), hidden-data = HiddenPrg
                 add 1 to riga
              end-perform

              modify gd1, mass-update = 0

           end-if.

           if trovato
              evaluate true
              when vet-prov
                   move save-codice to tfv-codice  
                   move save-prov   to tfv-prov
                   move save-campo2 to tfv-campo2   
              when other
                   move save-codice to tfv-codice  
                   move save-campo1 to tfv-campo1   
                   move save-campo2 to tfv-campo2   
              end-evaluate
              set tutto-ok to true
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 perform PUNTA-CELLA
                 perform VALORE-OLD-RIGA
                 set  StatusModifica to true
              else
                 perform CANCELLA-COLORE
                 move 78-ID-ef-vet to control-id
                 move 4 to accept-control
                 set StatusVisua   to true
              end-if
              perform FORM1-FLD-TO-BUF
              perform DISPLAY-KEY
              move tfv-chiave to old-tfv-chiave
           else
              set errori to true
              unlock tarifvet all records
              if vecchio
                 if ReadSecca
                    display message MSG-record-inesistente
                              title tit-err
                               icon 2
                    perform CLEAR-SCREEN
                    initialize save-chiave
                 end-if
              end-if
           end-if.

           perform ABILITAZIONI.
           set ReadSecca to false.

           display form1.
           perform STATUS-BAR-MSG 
           .
      * <TOTEM:END>

       DISPLAY-DESTINO.
      * <TOTEM:PARA. DISPLAY-DESTINO>
           display lab-campo2.

           display ef-dest.
           display lab-destino.
           display lab-localita 
           .
      * <TOTEM:END>

       DISPLAY-KEY.
      * <TOTEM:PARA. DISPLAY-KEY>
           display ef-vet.
           display lab-vet.
           display lab-campo1.
           display ef-campo1.
           display lab-des.
           perform DISPLAY-DESTINO             
           .
      * <TOTEM:END>

       GRID-TO-FLD.
      * <TOTEM:PARA. GRID-TO-FLD>
           move col-daq   to tfv-qli-da.
           move col-aq    to tfv-qli-a.
           move col-euro  to tfv-euro.
           move HiddenPrg to tfv-prog 
           .
      * <TOTEM:END>

       GRID-HELP.
      * <TOTEM:PARA. GRID-HELP>
      * NON CI SONO ZOOM SULLE COLONNE 
      * QUINDI NON SERVE L'EVALUATE DELLA COLONNA. MA IL
      * BOTTONE CERCA SARA' SEMPRE DISABILITATO
           move BitmapZoomDisabled to BitmapNumZoom.
           move 0 to e-cerca.

           modify tool-cerca, bitmap-number = BitmapNumZoom,
                              enabled       = e-cerca.

           evaluate colonna
           when 1 move "Valore iniziale in quintali" to gd-hint-text
           when 2 move "Valore finale in quintali"   to gd-hint-text
           when 3 if mod = 1
                     move "Digitare il valore della tariffa in EURO"
                       to gd-hint-text
                  else
                     move "Valore della tariffa in EURO" 
                       to gd-hint-text
                  end-if
           end-evaluate.

           move gd-hint-text to totem-hint-text.
           
           perform FORM1-DISPLAY-STATUS-MSG 
           .
      * <TOTEM:END>

       INIT.
      * <TOTEM:PARA. INIT>
           set vecchio  to true.
           set tutto-ok to true.
           
           move 0       to vis-destino.
           move spaces  to lab-vet-buf
                           lab-des-buf
                           lab-localita-buf
                           lab-destino-buf.

           perform DISPLAY-KEY 
           .
      * <TOTEM:END>

       INIT-OLD-REC.
      * <TOTEM:PARA. INIT-OLD-REC>
           initialize old-rec-grid   replacing numeric data by zeroes
                                            alphabetic data by spaces.

           initialize old-tfv-chiave replacing numeric data by zeroes
                                            alphabetic data by spaces 
           .
      * <TOTEM:END>

       MODIFICA.
      * <TOTEM:PARA. MODIFICA>
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok  to true.
           set RecLocked to false.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else 
              if mod = 1
                 set YesMessage to true
                 set ReadSecca  to true
                 perform CURRENT-RECORD
                 
                 if errori
                    move 0 to mod
                    move 1 to mod-k
                    set StatusVisua to true
                 else
                    if not RecLocked
                       move 0 to mod-k
                       move 1 to mod
                       set StatusModifica to true
                    else
                       move 0 to mod
                       move 1 to mod-k
                       set StatusVisua to true
                    end-if
                    move 2 to riga
                    move 1 to colonna
                 end-if
              else
                 move 1 to mod-k
                 move 1 to mod |devo per un attimo simulare 
                               |ancora lo stato di modifica
                               |altrimenti il CONTROLLO-OLD
                               |non viene eseguito
                 perform SALV-MOD
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    move 0 to mod
                    set YesMessage to true
                    set ReadSecca  to true
                    perform CURRENT-RECORD
                 end-if
              end-if
                        
              display form1             

              perform CANCELLA-COLORE

              move 78-id-ef-vet to control-id
              move 4 to accept-control

           end-if.
           modify tool-modifica,  value mod.

           if vecchio
              if mod = 1 set StatusModifica to true
              else       set StatusVisua    to true
              end-if
           end-if.

           perform STATUS-BAR-MSG.
           perform ABILITAZIONI 
           .
      * <TOTEM:END>

       NUOVO.
      * <TOTEM:PARA. NUOVO>
           perform X-Y.
      *****     if control-id = 78-ID-gd1
      *****        if mod = 1
      *****           perform SALV-MOD
      *****           if tutto-ok
      *****              continue
      *****              if riga-nuova = 0
      *****                 modify gd1, insert-rows = 1
      *****                 add 1 to tot-righe giving riga
      *****                 move 2 to colonna
      *****                 perform PUNTA-CELLA
      *****                 move 1 to riga-nuova
      *****              end-if
      *****           end-if
      *****        end-if
      *****     else
              perform SALV-MOD
              if tutto-ok
                 initialize save-chiave
                 perform CANCELLA-COLORE
                 move 78-id-ef-vet   to control-id
                 move 4 to accept-control
                 move 1 to mod mod-k
                 modify tool-modifica,  value = mod
                 move 0      to vis-destino ef-vet-buf ef-dest-buf
                 move "Campo allegato 1" to lab-campo1-buf
                 move spaces to ef-campo1-buf 
                                lab-vet-buf 
                                lab-des-buf
                                lab-destino-buf
                                lab-localita-buf
                 perform DISPLAY-KEY
                 perform CLEAR-SCREEN
                 perform INIT-OLD-REC
                 set StatusIns to true
                 perform STATUS-BAR-MSG  
                 set nuovo to true
                 unlock tarifvet all records
                 |IN QUESTO PGM. LE RIGHE SCENDONO DA TVETTORI
                 |modify gd1, insert-rows = 1
                 perform ABILITAZIONI
              end-if
      *****     end-if 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".   
           copy "status.cpy" 
           .
      * <TOTEM:END>

       PRECEDENTE.
      * <TOTEM:PARA. PRECEDENTE>
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock tarifvet all records 

              perform RIEMPI-CHIAVE
              move low-value to tfv-prog

              start tarifvet key is < tfv-chiave
                    invalid  set errori to true
                not invalid 
                    read tarifvet previous no lock
                         at end   set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move tfv-chiave to save-chiave
                 move low-value  to tfv-prog
                 set ReadSecca   to false
                 perform CURRENT-RECORD
              end-if

              if errori or not trovato
                 display message "File [tarifvet]"
                          x"0d0a""Raggiunto inizio file"
                           title titolo
              end-if

           end-if 
           .
      * <TOTEM:END>

       PRIMO.
      * <TOTEM:PARA. PRIMO>
           perform SALV-MOD.
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock tarifvet all records 

              move low-value to tfv-chiave
              start tarifvet key is >= tfv-chiave
                    invalid  set errori to true
                not invalid 
                    read tarifvet next no lock
                         at end   set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move tfv-chiave to save-chiave
                 move low-value  to tfv-prog
                 set  ReadSecca  to false
                 perform CURRENT-RECORD
              end-if

              if errori
                 perform CLEAR-SCREEN
                 display message "File [tarifvet]"
                          x"0d0a""Archivio vuoto"
                           title titolo
                 perform CANCELLA-COLORE
                 move 78-ID-ef-vet to control-id
                 move 4            to accept-control
              end-if

           end-if 
           .
      * <TOTEM:END>

       PUNTA-CELLA.
      * <TOTEM:PARA. PUNTA-CELLA>
           move 78-ID-gd1          to control-id store-id.
           move 4                  to accept-control.
           modify gd1,       cursor-y riga, 
                             cursor-x colonna.
           perform COLORE-RIGA 
           .
      * <TOTEM:END>

       RESET-GRIGLIA.
      * <TOTEM:PARA. RESET-GRIGLIA>
           modify gd1, reset-grid = 1.
           perform GD1-CONTENT.
           initialize col-daq col-aq col-euro 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE.
      * <TOTEM:PARA. RIEMPI-CHIAVE>
           inquire ef-vet,      value in tfv-codice.
           evaluate true
           when vet-prov
                inquire ef-campo1,   value in tfv-prov
           when other
                inquire ef-campo1,   value in tfv-campo1
           end-evaluate
           inquire ef-dest,          value in tfv-campo2 
           .
      * <TOTEM:END>

       RIEMPI-GRID-FROM-TVETTORI.
      * <TOTEM:PARA. RIEMPI-GRID-FROM-TVETTORI>
           |Specifico del pgm. TTARIFVET
           set trovato to false.
           move save-codice to vet-codice.
           read tvettori no lock invalid continue end-read.
           perform varying idx from 1 by 1
                     until idx > 20
              if vet-qli-da-peso(idx) not = 0 or
                 vet-qli-a-peso(idx)  not = 0
                 move   vet-qli-da-peso(idx) to col-daq
                 move   vet-qli-a-peso(idx)  to col-aq
                 modify gd1, record-to-add = rec-grid
                 move   idx                  to HiddenPrg
                 modify gd1(idx + 1, 1), hidden-data = HiddenPrg
                 set trovato to true
              end-if
           end-perform.
           if not trovato
              display message 
                     "Impossibile inserire tariffe per il vettore"
              x"0d0a""selezionato in quanto non ha scaglioni."
                        title tit-err
                         icon 2
           end-if 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           perform CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel
                            giving scelta
       
              evaluate scelta
              when mb-yes
                   perform TOOL-SALVA-LINKTO
              when mb-no
                   continue
              when other
                   perform CANCELLA-COLORE
                   set errori    to true
                   if control-id= 78-id-gd1
                      perform PUNTA-CELLA
                   end-if
                   move store-id to control-id
                   move 4 to accept-control
              end-evaluate

           end-if 
           .
      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           if mod = 0 exit paragraph end-if.

           perform X-Y.

           perform VALORE-RIGA.

           if tutto-ok
              perform varying colonna from 1 by 1 
                        until colonna > 78-MaxCol
                 perform CONTROLLO-RIGA
                 if errori
                    exit perform 
                 end-if
              end-perform
           end-if.

           if errori
              if control-id = 78-ID-gd1
                 perform PUNTA-CELLA
              end-if
              move store-id to control-id
              move 4 to accept-control
           else
              perform RIEMPI-CHIAVE
              perform GRID-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              write tfv-rec invalid rewrite tfv-rec end-write
              |per il salvataggio singolo allo spostarsi di riga
              read tarifvet lock invalid continue end-read
              set SiSalvato to true
              set vecchio   to true
              move 0        to riga-nuova
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA.
      * <TOTEM:PARA. SELEZIONA>
           perform RIEMPI-CHIAVE.
           move   "tarifvet"  to    como-file.
           perform RIEMPI-TMP
           perform RIEMPI-CHIAVE-TMP

           if trovato
              move path-zoom-tarifvet to ext-file
              call   "zoom-gt"  using  como-file, zoom-tar-rec
                                giving stato-zoom
              cancel "zoom-gt"
           else
              move 1 to stato-zoom
           end-if.
           perform CANCELLA-TMP.
      
           if stato-zoom = 0
              if old-tfv-chiave not = zoom-tar-chiave
                 move zoom-tar-chiave to save-chiave
                 perform SALV-MOD
                 if tutto-ok
                    move save-chiave to zoom-tar-chiave
                    move zoom-tar-vettore to ef-vet-buf
                                            vet-codice
                    read tvettori no lock
                         invalid
                            continue
                    end-read
                    evaluate true
                    when vet-prov
                         move zoom-tar-campo1 to ef-campo1-buf
                    when other
                         move zoom-tar-campo1 to ef-campo1-buf
                    end-evaluate
                    move zoom-tar-campo2 to ef-dest-buf
                    display ef-vet ef-campo1 ef-dest
                    set ReadSecca to true
                    perform CANCELLA-COLORE
                    perform CURRENT-RECORD
                    move 78-ID-ef-vet to control-id
                    move 4 to accept-control
                 end-if
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-NUMERICO.
      * <TOTEM:PARA. SELEZIONA-NUMERICO>
           perform RIEMPI-CHIAVE.
           read tarifvet no lock key is k2
                invalid
                if vecchio
                   display message MSG-record-inesistente
                             title tit-err
                              icon 2
                   perform CLEAR-SCREEN
                   set errori to true
                else
                   |SPECIFICO PER IL PGM. TTARIFVET
                   perform X-Y
                   if tot-righe = 1
                      perform RIEMPI-GRID-FROM-TVETTORI
                      if trovato
                         move 1 to colonna
                         move 2 to riga
                         perform PUNTA-CELLA
                      else
                         set errori to true
                      end-if
                   end-if
                   |||||||||
                end-if
            not invalid
                |Se sono in NUOVO, ma ho delle righe precaricate
                |da tvettori non devo ricaricare la griglia
                inquire gd1, last-row in tot-righe
                if nuovo and tot-righe > 1
                   continue
                else
                   evaluate true
                   when vet-prov
                        if ( old-tfv-codice not = tfv-codice or
                             old-tfv-prov   not = tfv-prov   or
                             old-tfv-campo2 not = tfv-campo2 ) or
                           ( tfv-codice = 0 and mod = 0 )
                             set ReadSecca to true
                             perform CURRENT-RECORD 
                        end-if
                   when other
                        if ( old-tfv-codice not = tfv-codice or
                             old-tfv-campo1 not = tfv-campo1 or
                             old-tfv-campo2 not = tfv-campo2 ) or
                           ( tfv-codice = 0 and mod = 0 )
                             set ReadSecca to true
                             perform CURRENT-RECORD 
                        end-if
                   end-evaluate
                end-if
           end-read 
           .
      * <TOTEM:END>

       SPOSTAMENTO.
      * <TOTEM:PARA. SPOSTAMENTO>
           perform X-Y.

           if event-data-2 not = riga
              perform VALORE-RIGA

              if riga-nuova = 1 continue
      *****           if CAMPO_PRINCIPALE = vuoto
      *****              modify gd1, record-to-delete riga
      *****              move 0 to riga-nuova
      *****              set errori to true
      *****           end-if
              end-if

              if tutto-ok
                 |vecchio è specifico del pgm. ttarifvet
                 |perchè il salvataggio dev'essere globale
                 if vecchio
                    if rec-grid not = old-rec-grid
                       perform SALVA
                       if errori
                          perform PUNTA-CELLA
                          set event-action to event-action-fail
                       end-if
                    end-if
                 end-if
              end-if

           else
              if colonna not = event-data-1 and riga-nuova = 1
                 perform CONTROLLO
                 if errori
                    perform PUNTA-CELLA
                    set event-action to event-action-fail
                 end-if
              end-if
           end-if.

           if tutto-ok
      *       valorizzo l'old della riga su cui sono andato
              move event-data-2 to riga
              move event-data-1 to colonna
              perform VALORE-OLD-RIGA
      *       colorazione riga in grid
              perform COLORE-RIGA
           end-if.

           perform GRID-HELP.
           set tutto-ok to true 
           .
      * <TOTEM:END>

       STAMPA.
      * <TOTEM:PARA. STAMPA>
           set stampa       to true.
      *     perform paragrafo-stampa 
           .
      * <TOTEM:END>

       SUCCESSIVO.
      * <TOTEM:PARA. SUCCESSIVO>
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock tarifvet all records 

              perform RIEMPI-CHIAVE
              move high-value to tfv-prog

              start tarifvet key is > tfv-chiave
                    invalid  set errori to true
                not invalid 
                    read tarifvet next no lock
                         at end   set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move tfv-chiave to save-chiave
                 move low-value  to tfv-prog
                 set ReadSecca   to false
                 perform CURRENT-RECORD
              end-if

              if errori or not trovato
                 display message "File [tarifvet]"
                          x"0d0a""Raggiunta fine file"
                           title titolo
              end-if

           end-if 
           .
      * <TOTEM:END>

       TORNA-IN-VISUA.
      * <TOTEM:PARA. TORNA-IN-VISUA>
           move 0 to mod.
           move 1 to mod-k.
           set StatusVisua to true.
           display form1.
           modify tool-modifica, value = mod.
           perform CANCELLA-COLORE.
           move 78-ID-ef-vet to control-id.
           move 4            to accept-control.
           unlock tarifvet all records.
           perform STATUS-BAR-MSG.
           perform ABILITAZIONI 
           .
      * <TOTEM:END>

       ULTIMO.
      * <TOTEM:PARA. ULTIMO>
           perform SALV-MOD.           
           if tutto-ok                 
              perform CHECK-INSERIMENTO
              unlock tarifvet all records 

              move high-value to tfv-chiave
              start tarifvet key is < tfv-chiave
                    invalid  set errori to true
                not invalid 
                    read tarifvet previous no lock
                         at end   set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move tfv-chiave to save-chiave
                 move low-value  to tfv-prog
                 set  ReadSecca  to false
                 perform CURRENT-RECORD
              end-if

              if errori
                 perform CLEAR-SCREEN
                 display message "File [tarifvet]"
                          x"0d0a""Archivio vuoto"
                           title titolo
                 perform CANCELLA-COLORE
                 move 78-ID-ef-vet to control-id
                 move 4            to accept-control
              end-if

           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-DATI-COMUNI.
      * <TOTEM:PARA. VALORIZZA-DATI-COMUNI>
      *    se non ho valorizzato i campi di creazione li valorizzo
           if tfv-data-creazione = zero
              accept tfv-data-creazione from century-date
           end-if.
           if tfv-ora-creazione = zero
              accept tfv-ora-creazione from time
           end-if.
           if tfv-utente-creazione = space
              move user-codi to tfv-utente-creazione
           end-if.

           accept tfv-data-ultima-modifica from century-date.
           accept tfv-ora-ultima-modifica from time.
           move user-codi to tfv-utente-ultima-modifica 
           .
      * <TOTEM:END>

       VALORE-OLD-RIGA.
      * <TOTEM:PARA. VALORE-OLD-RIGA>
           inquire gd1(riga, 1), cell-data old-col-daq.
           inquire gd1(riga, 2), cell-data old-col-aq.
           inquire gd1(riga, 3), cell-data old-col-euro 
           .
      * <TOTEM:END>

       VALORE-RIGA.
      * <TOTEM:PARA. VALORE-RIGA>
           perform X-Y.
           inquire gd1(riga, 1),  cell-data col-daq.
           inquire gd1(riga, 2),  cell-data col-aq.
           inquire gd1(riga, 3),  cell-data col-euro.

           inquire gd1(riga, 1) hidden-data in HiddenPrg. 

           move col-daq    to como-col-daq.
           move col-aq     to como-col-aq.
           move col-euro   to como-col-euro 
           .
      * <TOTEM:END>

       X-Y.
      * <TOTEM:PARA. X-Y>
           inquire gd1, cursor-y in riga, cursor-x in colonna
                        last-row in tot-righe 
           .
      * <TOTEM:END>

       SET-VIRGOLA.
      * <TOTEM:PARA. SET-VIRGOLA>
      *    sostituisco il punto come virgola
           set environment "KEYSTROKE" to "DATA=44 46" 
           .
      * <TOTEM:END>

       RESET-VIRGOLA.
      * <TOTEM:PARA. RESET-VIRGOLA>
           set environment "KEYSTROKE" to "DATA=44 44".
           set environment "KEYSTROKE" to "DATA=46 46" 
           .
      * <TOTEM:END>

       RIEMPI-TMP.
      * <TOTEM:PARA. RIEMPI-TMP>
           set primo-passaggio to true
           set record-ok to true

           accept  path-zoom-tarifvet     from environment "PATH_ST".
           accept  como-data              from century-date.
           accept  como-ora               from time.
           inspect path-zoom-tarifvet     replacing trailing
                                          spaces by low-value.
           string path-zoom-tarifvet      delimited by low-value
                  "zoom-tarifvet"         delimited by size
                  "_"                     delimited by size
                  como-data               delimited by size
                  "_"                     delimited by size
                  como-ora                delimited by size
                  into path-zoom-tarifvet
           end-string.

           set trovato to false.
           open output zoom-tarifvet.
           initialize tfv-rec.

           start tarifvet key  is >= tfv-chiave
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read tarifvet next no lock at end exit perform 
           end-read
                    set record-ok to true

                    move tfv-codice   to vet-codice
                    read tvettori
                       invalid
                          initialize vet-rec
                    end-read
                    if primo-passaggio
                       evaluate true
                       when vet-prov
                            move tfv-codice to save-tfv-codice
                            move tfv-prov   to save-tfv-prov
                            move tfv-campo2 to save-tfv-campo2
                       when vet-regione
                       when vet-cliente
                       when vet-clides
                            move tfv-codice to save-tfv-codice
                            move tfv-campo1 to save-tfv-campo1
                            move tfv-campo2 to save-tfv-campo2
                       end-evaluate
                       set primo-passaggio to false
                    else
                       evaluate true
                       when vet-prov
                            if tfv-codice not = save-tfv-codice or
                               tfv-prov   not = save-tfv-prov   or
                               tfv-campo2 not = save-tfv-campo2
                               set record-ok to false
                               move tfv-codice to save-tfv-codice
                               move tfv-prov   to save-tfv-prov
                               move tfv-campo2 to save-tfv-campo2
                            end-if
                       when vet-regione
                       when vet-cliente
                       when vet-clides
                            if tfv-codice not = save-tfv-codice or
                               tfv-campo1 not = save-tfv-campo1 or
                               tfv-campo2 not = save-tfv-campo2
                               set record-ok to false
                               move tfv-codice to save-tfv-codice
                               move tfv-campo1 to save-tfv-campo1
                               move tfv-campo2 to save-tfv-campo2
                            end-if
                       end-evaluate
                    end-if

                    if record-ok
                       move tfv-codice         to zoom-tar-vettore
                                                  vet-codice
                       read tvettori
                            invalid
                               move space to zoom-tar-vet-descrizione
                        not invalid
                               move vet-descrizione to 
           zoom-tar-vet-descrizione
                       end-read
                       set cli-tipo-C          to true
                       evaluate true
                       when vet-prov
                            move tfv-prov      to zoom-tar-campo1
                                                  prv-codice
                            read tprov
                                 invalid
                                    initialize prv-rec
                            end-read
                            move prv-descrizione to 
           zoom-tar-campo1-descrizione
                            move space         to zoom-tar-campo2
                            move space         to 
           zoom-tar-campo2-destinazione
                       when vet-regione
                            move tfv-campo1    to como-campo1
                                                  reg-codice
                            inspect como-campo1 replacing trailing 
           low-value by space
                            call "c$justify" using como-campo1, "L"
                            move como-campo1   to zoom-tar-campo1
                            read tregioni
                                 invalid
                                    initialize reg-rec
                            end-read
                            move reg-descrizione to 
           zoom-tar-campo1-descrizione
                            move space         to zoom-tar-campo2
                            move space         to 
           zoom-tar-campo2-destinazione
                       when vet-cliente
                            move tfv-campo1    to como-campo1
                                                  cli-codice
                            inspect como-campo1 replacing trailing 
           low-value by space
                            call "c$justify" using como-campo1, "L"
                            move como-campo1   to zoom-tar-campo1
                            read clienti
                                 invalid
                                    initialize cli-rec
                            end-read
                            move cli-ragsoc-1  to 
           zoom-tar-campo1-descrizione
                            move space         to zoom-tar-campo2
                            move space         to 
           zoom-tar-campo2-destinazione
                       when vet-clides
                            move tfv-campo1    to como-campo1
                                                  des-codice
                                                  cli-codice
                            inspect como-campo1 replacing trailing 
           low-value by space
                            call "c$justify" using como-campo1, "L"
                            move como-campo1   to zoom-tar-campo1
                            move tfv-campo2    to zoom-tar-campo2
                                                  des-prog
                            read clienti
                                 invalid
                                    initialize cli-rec
                            end-read
                            read destini
                                 invalid
                                    initialize des-rec
                                    move cli-localita to des-localita
                                    move cli-ragsoc-1 to des-ragsoc-1
                            end-read
                            move des-ragsoc-1  to 
           zoom-tar-campo1-descrizione
                            move des-localita  to 
           zoom-tar-campo2-destinazione
                       end-evaluate

                       write zoom-tar-rec invalid continue end-write
                       set trovato to true
                    end-if

                 end-perform
           end-start.

           close zoom-tarifvet.

           move "zoom-tarifvet"   to como-file 
           .
      * <TOTEM:END>

       CANCELLA-TMP.
      * <TOTEM:PARA. CANCELLA-TMP>
           delete file zoom-tarifvet 
           .
      * <TOTEM:END>

       RIEMPI-CHIAVE-TMP.
      * <TOTEM:PARA. RIEMPI-CHIAVE-TMP>
           inquire ef-vet,    value in zoom-tar-vettore.
           inquire ef-campo1, value in zoom-tar-campo1.
           inquire ef-dest,   value in zoom-tar-campo2 
           .
      * <TOTEM:END>

      * EVENT PARAGRAPH
       NUOVO-LinkTo.
      * <TOTEM:PARA. NUOVO-LinkTo>
           inquire tool-nuovo, enabled in e-nuovo.
           if e-nuovo = 1
              perform NUOVO
           end-if 

           .
      * <TOTEM:END>
       CANCELLA-LinkTo.
      * <TOTEM:PARA. CANCELLA-LinkTo>
           inquire tool-cancella, enabled in e-cancella.
           if e-cancella = 1
              perform CANCELLA
           end-if 
           .
      * <TOTEM:END>
       TOOL-SALVA-LinkTo.
      * <TOTEM:PARA. TOOL-SALVA-LinkTo>
           inquire tool-salva, enabled in e-salva.
           if e-salva = 1
              |Specifico del pgm. TTARIFVET
              if nuovo
                 perform X-Y
                 if tot-righe > 1

                    perform varying control-id from 78-ID-ef-vet by 1
                              until control-id > 78-ID-ef-dest
                       perform CONTROLLO
                       if errori
                          exit perform
                       end-if
                    end-perform

                    if tutto-ok
                       perform varying riga from 2 by 1
                                 until riga > tot-righe
                          modify gd1, cursor-y = riga
                          set nuovo to true
                          perform SALVA
                       end-perform
                       if tutto-ok
                          perform TORNA-IN-VISUA
                       end-if
                    end-if

                 end-if
              else
                 perform SALVA
                 if tutto-ok
                    perform TORNA-IN-VISUA
                 end-if
              end-if
           end-if 
           .
      * <TOTEM:END>
       ANTEPRIMA-LinkTo.
      * <TOTEM:PARA. ANTEPRIMA-LinkTo>
           inquire tool-anteprima, enabled in e-anteprima.
           if e-anteprima = 1
              perform ANTEPRIMA
           end-if 
           .
      * <TOTEM:END>
       TOOL-MODIFICA-LinkTo.
      * <TOTEM:PARA. TOOL-MODIFICA-LinkTo>
           inquire tool-modifica, enabled in e-modifica
           if e-modifica = 1
              perform MODIFICA
           end-if 
           .
      * <TOTEM:END>
       STAMPA-LinkTo.
      * <TOTEM:PARA. STAMPA-LinkTo>
           inquire tool-stampa, enabled in e-stampa
           if e-stampa = 1
              perform STAMPA
           end-if 
           .
      * <TOTEM:END>
       I-O-BLOCCO.
      * <TOTEM:PARA. I-O-BLOCCO>
           SET LK-BL-CANCELLAZIONE TO TRUE.
           MOVE COMO-PROG-ID       TO LK-BL-PROG-ID.
           CALL "BLOCKPGM"  USING LK-BLOCKPGM 
           .
      * <TOTEM:END>
       PAS004P-Ev-Before-Program.
      * <TOTEM:PARA. PAS004P-Ev-Before-Program>
           move LK-BL-PROG-ID    TO COMO-PROG-ID 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Finish-Entry.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Finish-Entry>
           set tutto-ok to true.
           perform X-Y.
           perform VALORE-RIGA.
      
           perform CONTROLLO-RIGA.
      
           if errori
              set event-action to event-action-fail
           else
              perform RESET-VIRGOLA
              ||Editazione!!
              modify gd1(riga, colonna), cell-data = col-euro
           end-if 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Begin-Entry.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Begin-Entry>
           if mod = 0
              set event-action to event-action-fail
              exit paragraph
           end-if.

           inquire gd1, cursor-x = colonna,
                        cursor-y in riga.
                         
           perform VALORE-RIGA.

           evaluate colonna
           when 1
           when 2
                set event-action to event-action-fail
           when 3
                perform SET-VIRGOLA
                continue
           end-evaluate 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-End-Drag.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-End-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Goto-Cell.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Goto-Cell>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Goto-Cell-Drag.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Goto-Cell-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Goto-Cell-Mouse.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Goto-Cell-Mouse>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       form1-gd-1-Ev-Msg-Begin-Drag.
      * <TOTEM:PARA. form1-gd-1-Ev-Msg-Begin-Drag>
           perform SPOSTAMENTO 
           .
      * <TOTEM:END>
       TOOL-CERCA-LinkTo.
      * <TOTEM:PARA. TOOL-CERCA-LinkTo>
           inquire tool-cerca, enabled in e-cerca.
           if e-cerca = 1
              perform CERCA
           end-if 
           .
      * <TOTEM:END>
       TOOL-MODIFICA-BeforeProcedure.
      * <TOTEM:PARA. TOOL-MODIFICA-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       TOOL-MODIFICA-AfterProcedure.
      * <TOTEM:PARA. TOOL-MODIFICA-AfterProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR
           .
      * <TOTEM:END>
       TOOL-PRIMO.
      * <TOTEM:PARA. TOOL-PRIMO>
           PERFORM PRIMO 
           .
      * <TOTEM:END>
       TOOL-PRECEDENTE.
      * <TOTEM:PARA. TOOL-PRECEDENTE>
           if nuovo             
              perform PRIMO
           else
              perform PRECEDENTE
           end-if 
           .
      * <TOTEM:END>
       TOOL-SUCCESSIVO.
      * <TOTEM:PARA. TOOL-SUCCESSIVO>
           if nuovo             
              perform ULTIMO
           else
              perform SUCCESSIVO
           end-if 
           .
      * <TOTEM:END>
       TOOL-ULTIMO.
      * <TOTEM:PARA. TOOL-ULTIMO>
           PERFORM ULTIMO 
           .
      * <TOTEM:END>
       tool-seleziona-LinkTo.
      * <TOTEM:PARA. tool-seleziona-LinkTo>
           perform SELEZIONA 
           .
      * <TOTEM:END>
       ef-vet-BeforeProcedure.
      * <TOTEM:PARA. ef-vet-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-campo1-BeforeProcedure.
      * <TOTEM:PARA. ef-campo1-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-dest-BeforeProcedure.
      * <TOTEM:PARA. ef-dest-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-vet-AfterProcedure.
      * <TOTEM:PARA. ef-vet-AfterProcedure>
              INQUIRE ef-vet, VALUE IN tfv-codice
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-vet-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

       ef-campo1-AfterProcedure.
      * <TOTEM:PARA. ef-campo1-AfterProcedure>
              INQUIRE ef-campo1, VALUE IN com-campo
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-campo1-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

       ef-dest-AfterProcedure.
      * <TOTEM:PARA. ef-dest-AfterProcedure>
              INQUIRE ef-dest, VALUE IN tfv-campo2
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-dest-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
