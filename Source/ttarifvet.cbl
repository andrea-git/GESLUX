      * <TOTEM:PARA. ANTEPRIMA>
           set anteprima       to true.
      *     perform paragrafo-stampa 
           .
      * <TOTEM:END>

       CANCELLA.
      * <TOTEM:PARA. CANCELLA>
           if mod = 0       exit paragraph end-if.
           perform X-Y.
           if tot-righe <= 1 exit paragraph end-if.

           initialize gesdelete-linkage.
           |In questo pgm. non è possibile cancellare righe
           move 0 to gesdelete-v-tasto1.
           move 1 to gesdelete-v-tasto2.
           move 1 to gesdelete-v-tasto3.
           move 3 to gesdelete-bitmap-tasto1.
           move 5 to gesdelete-bitmap-tasto2.
           move 1 to gesdelete-bitmap-tasto3.
           move "tarifvet" to gesdelete-nome-file.
           move titolo     to gesdelete-titolo.

           call   "gesdelete" using gesdelete-linkage
           cancel "gesdelete".

           if gesdelete-yes-delete
              move mb-yes to scelta
              evaluate gesdelete-scelta 
              when 1   set DeleteRow to true
              when 2   set DeleteRec to true
              end-evaluate
           else
              move mb-no to scelta
           end-if.
      *
           if scelta = mb-yes
              perform RIEMPI-CHIAVE
              set tutto-ok to true
              perform X-Y
              if tot-righe = 2
                 |COSI' MI FA ANCHE IL RIPOSIZIONAMENTO
                 |SUL PRIMO RECORD
                 set DeleteRec to true
              end-if
                   
              if DeleteRow
                 
                 perform VALORE-RIGA
      
                 delete tarifvet record invalid continue end-delete
                 modify gd1,  record-to-delete = riga
                 move 0 to riga-nuova
                 perform VALORE-RIGA
                 perform VALORE-OLD-RIGA

              else
                 if vecchio
                    move low-value to tfv-prog
                    start tarifvet key is >= tfv-chiave
                          invalid  continue
                      not invalid
                          move tfv-chiave to save-chiave
                          perform until 1 = 2
                             read tarifvet next no lock 
                                  at end exit perform 
                             end-read
                             evaluate true
                             when vet-prov
                                  if tfv-codice not = save-codice or
                                     tfv-prov   not = save-prov   or
                                     tfv-campo2 not = save-campo2
                                     exit perform
                                  end-if
                             when other
                                  if tfv-codice not = save-codice or
                                     tfv-campo1 not = save-campo1 or
                                     tfv-campo2 not = save-campo2
                                     exit perform
                                  end-if
                             end-evaluate
                             delete tarifvet invalid continue end-delete
                          end-perform
                          display message 
           MSG-cancellazione-avvenuta-con-successo
                                    title titolo
                          move 0 to mod
                          move 1 to mod-k
                          modify tool-modifica, value = mod
                    end-start
                 else
                    perform RESET-GRIGLIA
                 end-if
                 perform PRIMO
              end-if
           end-if 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate CONTROL-ID
           when 78-ID-EF-vet
                inquire ef-vet, VALUE tfv-codice
                move tfv-codice to vet-codice
                move "tvettori"      to Como-File
                call   "zoom-gt" using como-file, vet-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move vet-codice  To tfv-codice
                   modify ef-vet value tfv-codice
                   move vet-descrizione  to lab-vet-buf
                   move 4 to accept-control
                end-if
                perform AGG-DA-TVETTORI
                display lab-vet

           when 78-ID-EF-campo1
                inquire ef-campo1, VALUE com-campo
                evaluate true
                when vet-regione
                     move com-campo to reg-codice convert
                     move "tregioni"     to Como-File
                     call   "zoom-gt" using como-file, reg-rec
                                     giving stato-zoom
                     cancel "zoom-gt"
                     if stato-zoom = 0
                        move reg-codice to com-campo
                        modify ef-campo1, value com-campo
                        move reg-descrizione  to lab-des-buf
                        display lab-des
                        move 4 to accept-control
                     end-if

                when vet-prov
                     move com-campo to prv-codice 
                     move "tprov"        to Como-File
                     call   "zoom-gt" using como-file, prv-rec
                                     giving stato-zoom
                     cancel "zoom-gt"
                      if stato-zoom = 0
                         move prv-codice to com-campo
                         modify ef-campo1, value com-campo
                         move prv-descrizione  to lab-des-buf
                         display lab-des
                         move 4 to accept-control
                      end-if

                when vet-cliente
                when vet-clides
                     set cli-tipo-C to true
                     move com-campo  to cli-codice convert
                     move "clienti-all"        to Como-File
                     call   "zoom-gt" using como-file, cli-rec
                                     giving stato-zoom
                     cancel "zoom-gt"
                     if stato-zoom = ZERO
                        modify ef-campo1, value cli-codice
                        move cli-ragsoc-1  to lab-des-buf
                        display lab-des
                        move 4 to accept-control
                     end-if

                end-evaluate

           when 78-ID-EF-dest
                if vet-clides
                   move 1 to vis-destino
                   perform DISPLAY-DESTINO
                   inquire ef-campo1, VALUE com-campo
                   move com-campo to des-codice convert
                   inquire EF-dest,  value des-prog
                   move "clienti-des"   to como-file
                   call   "zoom-gt"  using como-file, des-rec
                                    giving stato-zoom
                   cancel "zoom-gt"
                   if stato-zoom = 0
                      modify EF-dest, value des-prog
                      move des-ragsoc-1  to lab-destino-buf
                      display lab-destino
                      move 4 to accept-control
                   end-if
                end-if
           end-evaluate  
           .
      * <TOTEM:END>

