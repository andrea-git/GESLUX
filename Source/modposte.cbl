                                giving stato-zoom

              cancel "zoom-gt"
           else
              move 1 to stato-zoom
           end-if.
           perform CANCELLA-TMP.

           if stato-zoom = 0
              move zoom-top-numero to numero
              display ef-num
           end-if 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
