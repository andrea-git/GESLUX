      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-codice
                if tipo-codice = 1 set cli-tipo-C to true
                else               set cli-tipo-F to true
                end-if
                inquire ef-codice, value in cli-codice
                move "clienti-CF-all"        to como-file
                call   "zoom-gt"      using como-file, cli-rec
                                     giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move cli-codice   to ef-codice-buf
                   move cli-ragsoc-1 to lab-codice-buf
                   display ef-codice lab-codice
                end-if
                
           when 78-ID-ef-tipo
                inquire ef-marca, value in tcl-codice
                move "ttipocli"         to como-file
                call   "zoom-gt"     using como-file, tcl-rec
                                    giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tcl-codice      to ef-tipo-buf
                   move tcl-descrizione to lab-tipo-buf
                   display ef-tipo lab-tipo
                end-if

           when 78-ID-ef-gdo
                inquire ef-gdo,   value in gdo-codice
                move "tgrupgdo"         to como-file
                call   "zoom-gt"     using como-file, gdo-rec
                                    giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move gdo-codice       to ef-gdo-buf
                   move gdo-intestazione to lab-gdo-buf
                   display ef-gdo lab-gdo
                end-if

           when 78-ID-ef-art
                inquire ef-art,   value in art-codice
                move "articoli"         to como-file
                call   "zoom-gt"     using como-file, art-rec
                                    giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move art-codice      to ef-art-buf
                   move art-descrizione to lab-art-buf
                   display ef-art lab-art
                end-if

           when 78-ID-ef-marca
                inquire ef-marca, value in mar-codice
                move "tmarche"          to como-file
                call   "zoom-gt"     using como-file, mar-rec
                                    giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move mar-codice      to ef-marca-buf
                   move mar-descrizione to lab-marca-buf
                   display ef-marca lab-marca
                end-if
                
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-data-da
                inquire ef-data-da, value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to data-da
                   display ef-data-da
                end-if

           when 78-ID-ef-data-a
                inquire ef-data-a, value in como-data
                if como-data not = 99999999
                   perform DATE-FORMAT
                   move como-data to data-a
                   display ef-data-a
                end-if

                inquire ef-data-da,  value in como-data
                perform DATE-TO-FILE
                move como-data to link-stm-data-da

                inquire ef-data-a,  value in como-data
                perform DATE-TO-FILE
                move como-data to link-stm-data-a

                if link-stm-data-a < link-stm-data-da
                   set errori to true
                   display message "Intervallo date errato"
                             title tit-err
                              icon 2
                end-if

           when 78-ID-ef-codice
                evaluate tipo-codice
                when 1 set cli-tipo-C to true
                when 2 set cli-tipo-F to true
                when 3 initialize cli-tipo-CF
                end-evaluate
                inquire ef-codice value in cli-codice
                if cli-codice = 0
                   evaluate true
                   when cli-tipo-C
                        move 1 to e-tipo
                        move "Tutti i Clienti"   to lab-codice-buf
                   when cli-tipo-F
                        move "Tutti i Fornitori" to lab-codice-buf
                   when other
                        move "Tutti i Codici"    to lab-codice-buf
                   end-evaluate
                else
                   move 0 to e-tipo 
                   move 0 to e-gdo
                   move spaces to lab-tipo-buf
                   move spaces to lab-gdo-buf
                   read clienti no lock
                        invalid
                        move spaces to ef-tipo-buf
                        move spaces to ef-gdo-buf
                        set errori  to true
                        move spaces to lab-codice-buf
                        display message "Codice NON valido"
                                  title tit-err
                                   icon 2
                    not invalid
                        move cli-ragsoc-1 to lab-codice-buf
                        move cli-tipo     to ef-tipo-buf tcl-codice
                        read ttipocli no lock 
                             invalid  move spaces          to 
           lab-tipo-buf
                         not invalid  move tcl-descrizione to 
           lab-tipo-buf
                        end-read
                        move cli-gdo      to ef-gdo-buf gdo-codice
                        read tgrupgdo no lock
                             invalid  move spaces           to 
           lab-gdo-buf
                         not invalid  move gdo-intestazione to 
           lab-gdo-buf
                        end-read
                   end-read
                end-if
                display ef-tipo ef-gdo lab-codice lab-tipo lab-gdo

           when 78-ID-ef-tipo
                if e-tipo = 1
                   inquire ef-tipo value in tcl-codice
                   if tcl-codice = spaces
                      move "Tutte le tipologie" to lab-tipo-buf
                   else
                      read ttipocli no lock
                           invalid
                           move 0 to e-gdo
                           move spaces to ef-gdo-buf
                           move spaces to lab-gdo-buf
                           set errori to true
                           move spaces to lab-tipo-buf
                           display message "Tipologia NON valida"
                                     title tit-err
                                      icon 2
                       not invalid
                           move tcl-descrizione to lab-tipo-buf
                           if tcl-gdo-si
                              move 1 to e-gdo
                           else
                              move 0 to e-gdo
                              move spaces to ef-gdo-buf
                              move spaces to lab-gdo-buf
                           end-if
                      end-read
                   end-if
                   display lab-tipo ef-gdo lab-gdo
                end-if

           when 78-ID-ef-gdo
                if e-gdo = 1
                   inquire ef-gdo value in gdo-codice
                   if gdo-codice = spaces
                      move "Tutti i codici GDO" to lab-tipo-buf
                   else
                      read tgrupgdo no lock
                           invalid
                           set errori to true
                           move spaces to lab-gdo-buf
                           display message "Gruppo GDO NON valido"
                                     title tit-err
                                      icon 2
                       not invalid
                           move gdo-intestazione to lab-gdo-buf
                      end-read
                   end-if
                   display lab-gdo
                end-if

           when 78-ID-ef-art
                inquire ef-art, value in art-codice
                if art-codice = 0
                   move 1 to e-marca
                   move "Tutti gli articoli" to lab-art-buf
                else
                   move 0 to e-marca
                   move spaces to lab-marca-buf
                   read articoli no lock
                        invalid
                        set errori  to true
                        move spaces to lab-art-buf
                        display message "Articolo NON valido"
                                  title tit-err
                                   icon 2
                    not invalid
                        move art-descrizione    to lab-art-buf
                        move art-marca-prodotto to ef-marca-buf 
                                                   mar-codice
                        read tmarche no lock
                             invalid move spaces          to 
           lab-marca-buf
                         not invalid move mar-descrizione to 
           lab-marca-buf
                        end-read
                   end-read
                end-if
                display lab-art ef-marca lab-marca

           when 78-ID-ef-marca
                if e-marca = 1
                   inquire ef-marca, value in mar-codice
                   if mar-codice = 0
                      move "Tutte le marche" to lab-marca-buf
                   else
                      read tmarche no lock
                           invalid
                           set errori to true
                           display message "Marca NON valida"
                                     title tit-err
                                      icon 2
                           move spaces to lab-marca-buf
                       not invalid
                           move mar-descrizione to lab-marca-buf
                      end-read
                      display lab-marca
                   end-if
                end-if

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

