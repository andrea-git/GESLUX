              when "Numero"  set numero  to true
              when "Fattura" set fattura to true
              when "Nota Cr" set nota-cr to true
              end-evaluate
              modify form1-Handle, visible 0
              |Possono esserci più fatture che fanno 
              |riferimento alla stessa fattura
              if tipo-elab = 1 and fattura
                 set tutte to true
                 move 0 to como-riga
                 move cnt-anno-fatt  to como-anno
                 move cnt-num-fatt   to como-numero-nota
                 start contestazioni key >= k-fattura
                       invalid continue
                   not invalid
                       perform until 1 = 2 or como-riga > 1
                          read contestazioni next at end exit perform 
           end-read
                          if cnt-num-fatt    not = como-numero-nota or
                             cnt-anno-fatt   not = como-anno
                             exit perform
                          end-if
                          add 1 to como-riga

                          if como-riga = 1
                             move cnt-anno   to save-anno
                             move cnt-numero to save-numero
                          end-if

                       end-perform
                 end-start
                 if como-riga > 1
                    move como-anno        to anno-to   anno-to
                    move como-numero-nota to nota-from nota-to
                    move 2 to tipo-elab
                 else
                    move save-anno   to cnt-anno
                    move save-numero to cnt-numero
                 end-if
              end-if

              if tipo-elab = 1
                 evaluate true
                 when fattura
                 when nota-cr
                      move cnt-anno   to como-anno
                      move cnt-numero to como-numero-nota
                 when numero
                      move anno-to    to como-anno
                      move nota-from  to como-numero-nota
                 end-evaluate
                 perform CHIAMA-MANUTENZIONE
              else
                 perform FORM2-OPEN-ROUTINE 
              end-if

              modify form1-Handle, visible 1
           end-if 
           .
      * <TOTEM:END>
