      * USER DEFINE PARAGRAPH
       ABILITAZIONI.
      * <TOTEM:PARA. ABILITAZIONI>
           if tor-data-fattura not = 0 or 
              tor-anno-fattura not = 0 or
               tor-num-fattura not = 0

              move tor-data-fattura   to como-data
              perform DATE-TO-SCREEN
              move como-data          to lab-data-fatt-buf

              move tor-num-fattura    to lab-num-fatt-buf

              move BitmapEditDisabled to BitmapNumEdit
              move 0 to e-modifica
              move 1 to v-fatt
           else
              move BitmapEditEnabled to BitmapNumEdit
              move 1 to e-modifica
              move 0 to v-fatt
           end-if.

           modify tool-modifica, 
                  enabled e-modifica, 
                  bitmap-number = BitmapNumEdit.

           if mod = 1                  
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
                                 
              move 0 to mod-campi

              inquire cbo-stato,   value cbo-stato-buf

              if cbo-stato-buf not = "Disattivo" 
                 move 1 to mod-campi 
              end-if  

           else         

              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move 0 to e-salva e-cancella         
              move 0 to mod-campi 
           end-if.
           move BitmapNewDisabled     to BitmapNumNew.
                                                                 
           modify tool-nuovo,    enabled       = 0
                                 bitmap-number = BitmapNumNew.
           modify tool-cancella, enabled       = e-cancella,
                                 bitmap-number = BitmapNumDelete.
           modify tool-salva,    enabled       = e-salva,
                                 bitmap-number = BitmapNumSave 
           .
      * <TOTEM:END>

       AGGIORNA-BOZZA.
      * <TOTEM:PARA. AGGIORNA-BOZZA>
           string   "La Bozza di partenza risulta"
             x"0d0a""in uso su altro terminale."
             x"0d0a""Questo comporta l'impossibilità"
             x"0d0a""ad aggiornarne lo stato." delimited size
                 into geslock-messaggio
           end-string.
           set RecLocked to false.
                               
           open i-o btnotacr.
           move tor-anno   to btno-anno-fm.
           move tor-numero to btno-num-Fm.
           perform until 1 = 2
              set RecLocked to false
              read btnotacr lock key k-fatman invalid continue end-read
              if RecLocked
                 move "BTNOTACR" to geslock-nome-file
                 move 1 to geslock-v-riprova
                 move 0 to geslock-v-ignora
                 move 0 to geslock-v-termina
                 call   "geslock" using geslock-linkage
                 cancel "geslock"
              else
                 move 0 to btno-anno-fm
                 move 0 to btno-data-fm
                 move 0 to btno-num-fm
                 rewrite btno-rec invalid continue end-rewrite
                 unlock  btnotacr all records
                 exit perform
              end-if
           end-perform.
           close    btnotacr 
           .
      * <TOTEM:END>

