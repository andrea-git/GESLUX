              ELSE
                 START clienti KEY <= cli-chiave OF clienti
              END-IF
           END-EVALUATE
           .

       DataSet1-clienti-START-NOTGREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-clienti-KEY1-Asc
                 START clienti KEY <= cli-chiave OF clienti
              ELSE
                 START clienti KEY >= cli-chiave OF clienti
              END-IF
           END-EVALUATE
