      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-tipo
                inquire ef-tipo, value in tcl-codice

                move "ttipocli"    to como-file
                call "zoom-gt"  using como-file, tcl-rec
                                giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move tcl-codice      to ef-tipo-buf
                   move tcl-descrizione to lab-tipo-buf
                   display ef-tipo lab-tipo
                end-if

           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.
      
           evaluate control-id
           when 78-ID-ef-cod-a
                inquire ef-cod-da, value in cliente-da
                inquire ef-cod-a,  value in cliente-a

                if cliente-a = 0
                   move "99999" to ef-cod-a-buf cliente-a
                   display ef-cod-a
                end-if

                if cliente-da > cliente-a
                   display message "Range Cliente errato"
                             title tit-err
                              icon 2
                   set errori to true
                   move 78-ID-ef-cod-da to control-id
                end-if

           when 78-ID-ef-tipo
                inquire ef-tipo, value in tcl-codice
                if tcl-codice = spaces
                   move "TUTTE LE TIPOLOGIE" to lab-tipo-buf
                else
                   read ttipocli no lock
                        invalid
                        set errori to true
                        display message "Tipologia NON valida"
                                  title tit-err
                                   icon 2
                        move spaces to tcl-descrizione
                   end-read
                   move tcl-descrizione to lab-tipo-buf
                end-if
                display lab-tipo

           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

