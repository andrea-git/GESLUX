           PERFORM Form1-DUMMY-CURR
           PERFORM Form1-IUD-Display
           .

       Form1-DUPLICATE-PREV.
           EVALUATE DataSet1-KEYIS
           WHEN 2
              IF mar-descrizione = Form1-KEYISTMP2
                 MOVE DataSet1-tmarche-KEY2-ORDER TO 
           TMP-Form1-KEY2-ORDER
                 MOVE "A" TO DataSet1-tmarche-KEY2-ORDER
                 PERFORM DataSet1-tmarche-START-EQUAL
                 IF Valid-STATUS-tmarche
                    PERFORM DataSet1-tmarche-Read-Next
                    PERFORM UNTIL NOT Valid-STATUS-tmarche OR 
                         mar-chiave = Form1-PKEYTMP OR
                         mar-descrizione NOT = Form1-KEYISTMP2
                       PERFORM DataSet1-tmarche-Read-Next
                    END-PERFORM
                 END-IF
                 MOVE TMP-Form1-KEY2-ORDER TO 
           DataSet1-tmarche-KEY2-ORDER
              ELSE
                 PERFORM DataSet1-tmarche-START-LESS
              END-IF
           WHEN OTHER
              PERFORM DataSet1-tmarche-START-LESS
           END-EVALUATE
           .

       Form1-DUPLICATE-NEXT.
           EVALUATE DataSet1-KEYIS
           WHEN 2
              IF mar-descrizione = Form1-KEYISTMP2
                 MOVE DataSet1-tmarche-KEY2-ORDER TO 
           TMP-Form1-KEY2-ORDER
                 MOVE "A" TO DataSet1-tmarche-KEY2-ORDER
                 PERFORM DataSet1-tmarche-START
                 IF Valid-STATUS-tmarche
                    PERFORM DataSet1-tmarche-Read-Next
                    PERFORM UNTIL NOT Valid-STATUS-tmarche OR 
                         mar-chiave = Form1-PKEYTMP OR
                         mar-descrizione NOT = Form1-KEYISTMP2
                       PERFORM DataSet1-tmarche-Read-Next
                    END-PERFORM
                 END-IF
                 MOVE TMP-Form1-KEY2-ORDER TO 
           DataSet1-tmarche-KEY2-ORDER
              ELSE
                 PERFORM DataSet1-tmarche-START-GREATER
              END-IF
           WHEN OTHER
              PERFORM DataSet1-tmarche-START-GREATER
           END-EVALUATE
           .

       Form1-DUPLICATE-MOVEKEY.
           MOVE mar-chiave TO Form1-PKEYTMP
           EVALUATE DataSet1-KEYIS
           WHEN 2
              MOVE mar-descrizione TO 
                   Form1-KEYISTMP2
           END-EVALUATE
