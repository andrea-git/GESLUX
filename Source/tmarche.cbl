      * <TOTEM:END>

       SALVA.
      * <TOTEM:PARA. SALVA>
           if mod = ZERO 
              exit paragraph 
           end-if.

           set tutto-ok to true.

           perform varying CONTROL-ID from 78-ID-ef-codice by 1
                     until CONTROL-ID > 78-ID-chk-cou
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if errori
              perform ABILITAZIONI
              move store-id to CONTROL-ID       
              move 4        to ACCEPT-CONTROL   
           else                       
              perform FORM1-BUF-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              perform cancella-colore 

              write mar-rec 
                 invalid 
                    rewrite mar-rec
              end-write

              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1 
           .
      * <TOTEM:END>

       SALV-MOD.
      * <TOTEM:PARA. SALV-MOD>
           set tutto-ok to true.
           perform Form1-CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel 
                            giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA.
      * <TOTEM:PARA. SELEZIONA>
           perform RIEMPI-CHIAVE.

           move "tmarche" to    como-file.
           call "zoom-gt" using como-file, mar-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = ZERO
              if old-mar-chiave not = mar-chiave
                 move mar-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to mar-chiave
                    set ReadSecca to true
                    modify ef-codice, value mar-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-codice to CONTROL-ID       
                    move 4               to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-ALFA.
      * <TOTEM:PARA. SELEZIONA-ALFA>
      * LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      * ANCORA GLI ZERI DAVANTI CHE ELIMINO
           inspect ef-codice-buf replacing leading ZERO by SPACES.
           move ef-codice-buf to mar-descrizione.

      * POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      * ALTERNATA
           call "C$JUSTIFY" using mar-descrizione, "L".

           start tmarche key >= mar-descrizione
              invalid 
                 continue
              not invalid
                 read tmarche next 
           end-start.

           move "tmarche-alfa" to como-file.
           call "zoom-gt" using   como-file, mar-rec
                          giving  stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = ZERO
              move mar-codice    to codice-ed
              move codice-ed     to ef-codice-buf   
              call "C$JUSTIFY" using ef-codice-buf, "L"
              display ef-codice

              if old-mar-descrizione not = mar-descrizione
                 move mar-descrizione to save-key-1
                 perform SALV-MOD
                 if tutto-ok
                    move save-key-1 to mar-descrizione
                    set ReadSecca  to true
                    modify ef-codice, value mar-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-codice to CONTROL-ID       
                    move 4               to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if 
           .
      * <TOTEM:END>

       SELEZIONA-NUMERICO.
      * <TOTEM:PARA. SELEZIONA-NUMERICO>
           if mar-codice not > ZERO
              set errori to true
              display message box MSG-Codice-obbligatorio
                      title tit-err
                      type  mb-ok
                      icon  mb-warning-icon
           else          
              read tmarche no lock
                 invalid
                    if vecchio
                       display message box MSG-record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                       perform CLEAR-SCREEN
                       set errori to true
                    end-if
                 not invalid      
                    if old-mar-chiave not = mar-chiave
                       perform CURRENT-RECORD 
                       set errori to true
                    end-if
              end-read
           end-if 
           .
      * <TOTEM:END>

       SUCCESSIVO.
      * <TOTEM:PARA. SUCCESSIVO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tmarche all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tmarche-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tmarche-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tmarche-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if 
           .
      * <TOTEM:END>

       STAMPA.
      * <TOTEM:PARA. STAMPA>
           continue 
           .
      * <TOTEM:END>

       ULTIMO.
      * <TOTEM:PARA. ULTIMO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tmarche all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tmarche-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tmarche-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tmarche-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-OLD.
      * <TOTEM:PARA. VALORIZZA-OLD>
           move mar-rec to old-mar-rec.
           set vecchio to true.

           move ZERO to StatusHelp
           perform STATUS-HELP 
           .
      * <TOTEM:END>

       VALORIZZA-NUOVO.
      * <TOTEM:PARA. VALORIZZA-NUOVO>
           move high-value to mar-codice.

           start tmarche key <= mar-codice
                 invalid move 1 to mar-codice
             not invalid
                 read tmarche previous with no lock
                    at end
                       move 1 to mar-codice
                    not at end
                       add  1 to mar-codice
                 end-read
           end-start.

           move "00"    to status-tmarche.

           initialize mar-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                                       
           perform FORM1-IUD-DISPLAY.

           if OrderBy = 1                             
              move high-value to mar-descrizione
              start tmarche key is <= mar-descrizione 
                    invalid continue 
                not invalid
                    read tmarche previous no lock 
                       at end continue 
                    end-read
              end-start
           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-DATI-COMUNI.
      * <TOTEM:PARA. VALORIZZA-DATI-COMUNI>
      * SE NON HO VALORIZZATO I CAMPI DI CREAZIONE LI VALORIZZO
           if mar-data-creazione   = zero
              accept mar-data-creazione from century-date
           end-if.
           if mar-ora-creazione    = zero
              accept mar-ora-creazione  from time
           end-if.
           if mar-utente-creazione = space
              move user-codi to mar-utente-creazione
           end-if.

           accept mar-data-ultima-modifica from century-date.
           accept mar-ora-ultima-modifica  from time.
           move user-codi    to mar-utente-ultima-modifica 
           .
      * <TOTEM:END>

