      * tmarche
       DataSet1-tmarche-START.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tmarche-KEY1-Asc
                 START tmarche KEY >= mar-chiave
              ELSE
                 START tmarche KEY <= mar-chiave
              END-IF
           WHEN 2
              IF DataSet1-tmarche-KEY2-Asc
                 START tmarche KEY >= mar-descrizione
              ELSE
                 START tmarche KEY <= mar-descrizione
              END-IF
           END-EVALUATE
           .

       DataSet1-tmarche-START-NOTGREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tmarche-KEY1-Asc
                 START tmarche KEY <= mar-chiave
              ELSE
                 START tmarche KEY >= mar-chiave
              END-IF
           WHEN 2
              IF DataSet1-tmarche-KEY2-Asc
                 START tmarche KEY <= mar-descrizione
              ELSE
                 START tmarche KEY >= mar-descrizione
              END-IF
           END-EVALUATE
           .

       DataSet1-tmarche-START-GREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tmarche-KEY1-Asc
                 START tmarche KEY > mar-chiave
              ELSE
                 START tmarche KEY < mar-chiave
              END-IF
           WHEN 2
              IF DataSet1-tmarche-KEY2-Asc
                 START tmarche KEY > mar-descrizione
              ELSE
                 START tmarche KEY < mar-descrizione
              END-IF
           END-EVALUATE
           .

       DataSet1-tmarche-START-LESS.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tmarche-KEY1-Asc
                 START tmarche KEY < mar-chiave
              ELSE
                 START tmarche KEY > mar-chiave
              END-IF
           WHEN 2
              IF DataSet1-tmarche-KEY2-Asc
                 START tmarche KEY < mar-descrizione
              ELSE
                 START tmarche KEY > mar-descrizione
              END-IF
           END-EVALUATE
           .

