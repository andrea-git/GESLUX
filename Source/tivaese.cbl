       tivaese-key01-MERGE-SPLITBUF.
           INITIALIZE tivaese-key01-SPLITBUF
           MOVE TBLIV-CODICE1(1:2) TO tivaese-key01-SPLITBUF(1:2)
           MOVE TBLIV-DESCRIZIONE1(1:30) TO tivaese-key01-SPLITBUF(3:30)
           MOVE TBLIV-CODICE2(1:20) TO tivaese-key01-SPLITBUF(33:20)
           .

       DataSet1-tivaese-INITSTART.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tivaese-KEY1-Asc
                 MOVE Low-Value TO TBLIV-CODICE
              ELSE
                 MOVE High-Value TO TBLIV-CODICE
              END-IF
           WHEN 2
              IF DataSet1-tivaese-KEY2-Asc
                 MOVE Low-Value TO TBLIV-CODICE1
              ELSE
                 MOVE High-Value TO TBLIV-CODICE1
              END-IF
              IF DataSet1-tivaese-KEY2-Asc
                 MOVE Low-Value TO TBLIV-DESCRIZIONE1
              ELSE
                 MOVE High-Value TO TBLIV-DESCRIZIONE1
              END-IF
              IF DataSet1-tivaese-KEY2-Asc
                 MOVE Low-Value TO TBLIV-CODICE2
              ELSE
                 MOVE High-Value TO TBLIV-CODICE2
              END-IF
           END-EVALUATE
           .

       DataSet1-tivaese-INITEND.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tivaese-KEY1-Asc
                 MOVE High-Value TO TBLIV-CODICE
              ELSE
                 MOVE Low-Value TO TBLIV-CODICE
              END-IF
           WHEN 2
              IF DataSet1-tivaese-KEY2-Asc
                 MOVE High-Value to TBLIV-CODICE1
              ELSE          
                 MOVE Low-Value to TBLIV-CODICE1
              END-IF
              IF DataSet1-tivaese-KEY2-Asc
                 MOVE High-Value to TBLIV-DESCRIZIONE1
              ELSE          
                 MOVE Low-Value to TBLIV-DESCRIZIONE1
              END-IF
              IF DataSet1-tivaese-KEY2-Asc
                 MOVE High-Value to TBLIV-CODICE2
              ELSE          
                 MOVE Low-Value to TBLIV-CODICE2
              END-IF
           END-EVALUATE
           .

       DataSet1-tivaese-TMPBUF-TO-FLD.
           EVALUATE DataSet1-KEYIS
           WHEN 2
              MOVE DataSet1-tivaese-SPLIT-BUF2(1:2) TO
                   TBLIV-CODICE1(1:2)
              MOVE DataSet1-tivaese-SPLIT-BUF2(3:30) TO
                   TBLIV-DESCRIZIONE1(1:30)
              MOVE DataSet1-tivaese-SPLIT-BUF2(33:20) TO
