      * tivaese
       DataSet1-tivaese-START.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tivaese-KEY1-Asc
                 START tivaese KEY >= TBLIV-CODICE
              ELSE
                 START tivaese KEY <= TBLIV-CODICE
              END-IF
           WHEN 2
              IF DataSet1-tivaese-KEY2-Asc
                 START tivaese KEY >= key01
              ELSE
                 START tivaese KEY <= key01
              END-IF
           END-EVALUATE
           .

       DataSet1-tivaese-START-NOTGREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tivaese-KEY1-Asc
                 START tivaese KEY <= TBLIV-CODICE
              ELSE
                 START tivaese KEY >= TBLIV-CODICE
              END-IF
           WHEN 2
              IF DataSet1-tivaese-KEY2-Asc
                 START tivaese KEY <= key01
              ELSE
                 START tivaese KEY >= key01
              END-IF
           END-EVALUATE
           .

       DataSet1-tivaese-START-GREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tivaese-KEY1-Asc
                 START tivaese KEY > TBLIV-CODICE
              ELSE
                 START tivaese KEY < TBLIV-CODICE
              END-IF
           WHEN 2
              IF DataSet1-tivaese-KEY2-Asc
                 START tivaese KEY > key01
              ELSE
                 START tivaese KEY < key01
              END-IF
           END-EVALUATE
           .

       DataSet1-tivaese-START-LESS.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tivaese-KEY1-Asc
                 START tivaese KEY < TBLIV-CODICE
              ELSE
                 START tivaese KEY > TBLIV-CODICE
              END-IF
           WHEN 2
              IF DataSet1-tivaese-KEY2-Asc
                 START tivaese KEY < key01
              ELSE
                 START tivaese KEY > key01
              END-IF
           END-EVALUATE
           .

       DataSet1-tivaese-START-EQUAL.
