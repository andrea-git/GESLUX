       Form2-PROC.
      * <TOTEM:EPT. FORM:Form2, FORM:Form2, BeforeAccept>
           initialize PathFile.
           if mese not = mese-delta
               move "Stampa Statistica giornaliera in corso..." 
                to lab-oper-buf
              display lab-oper
              call   "statgio-p" using PathFile, mese
              cancel "statgio-p"
           else
               move "Stampa Statistica NEW in corso..." 
                to lab-oper-buf
              display lab-oper                     
              |Deve replicare il programma di creazione notturna del file
              |sttendenza-p
              move tge-data-consolid-progmag(5:2) to como-anno
              if como-anno = 12
                 move tge-data-consolid-progmag(1:4) to como-anno
                 add 1 to como-anno
              else
                 move tge-data-consolid-progmag(1:4) to como-anno
              end-if
              accept  PathFile    from environment "PATH_STAT"        
              inspect PathFile    replacing trailing spaces by low-value
              |Uno per giorno altrimenti non lo trovo più
              string  PathFile    delimited low-value
                      "statsett-" delimited size
                      como-anno   delimited size
                      "_"         delimited size
                      mese        delimited size
                      ".txt"      delimited size
                 into PathFile
              end-string       

              accept  path-dest from environment "PATH-ST"        
              inspect path-dest replacing trailing spaces by low-value
              accept como-data from century-date
              accept como-ora  from time
              |Uno per giorno altrimenti non lo trovo più
              string  path-dest   delimited low-value
                      "statsett-" delimited size
                      como-data   delimited size
                      "_"         delimited size
                      como-ora    delimited size
                      ".txt"      delimited size
                 into path-dest
              end-string       

              initialize cmd-lancio
              string "copy "   delimited size
                     x"22"     delimited size  
                     PathFile  delimited low-value
                     x"22"     delimited size  
                     " "       delimited size     
                     x"22"     delimited size  
                     path-dest delimited low-value
                     x"22"     delimited size  
                into cmd-lancio
              end-string
      *        move "copy F:\Lubex\GESLUX\a.txt F:\Lubex\GESLUX\v\a.txt"
      *        to cmd-lancio

              call "C$SYSTEM" using cmd-lancio, 225
                             giving return-code
              inspect path-dest   replacing trailing low-value by spaces

              call "C$SLEEP" using 2                                
           end-if.
                                   
           modify form2-handle, visible = 0
          
           call "C$FILEINFO" using path-dest, file-info.
      
           if file-size not = 0
              set  splcrt2graf-stampa        to true
              set  splcrt2graf-windows       to true
              move path-dest
                to splcrt2graf-percorso-stampa
              set  splcrt2graf-orizzontale   to true
              set  splcrt2graf-forza-crt     to true
              set  splcrt2graf-6pt           to true
              call   "splcrt2graf" using splcrt2graf-link 
              cancel "splcrt2graf"
           end-if.
      
           if mese not = mese-delta
              call "C$DELETE" using PathFile
           end-if.

           move 27 to key-status.

           .
      * <TOTEM:END>
