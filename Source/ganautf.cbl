       pb-importa-LinkTo.
      * <TOTEM:PARA. pb-importa-LinkTo>
           perform RIEMPI-CHIAVE.
           move utf-chiave to save-chiave.
           initialize utf-rec.
           move   "anautf" to     como-file.
           call   "zoom-gt"  using  como-file, utf-rec
                             giving stato-zoom.
           cancel "zoom-gt".
      
           if stato-zoom = 0
              move utf-desc1         to ef-desc1-buf         
              move utf-desc2         to ef-desc2-buf         
              move utf-modello       to ef-modello-buf       
              move utf-ragsoc        to ef-ragsoc-buf        
              move utf-indirizzo     to ef-indirizzo-buf     
              move utf-cap           to ef-cap-buf           
              move utf-citta         to ef-citta-buf         
              move utf-provincia     to ef-provincia-buf     
              move utf-partita-iva   to ef-partita-iva-buf   
              move utf-num-lic-utf   to ef-num-lic-utf-buf   
              move utf-data-lic      to como-data
              perform DATE-TO-SCREEN
              move como-data         to ef-data-lic-buf      
              move utf-uffico-appart to ef-ufficio-appart-buf
              move utf-num-reg-ass   to ef-num-reg-ass-buf
              display ef-desc1 ef-desc2 ef-modello ef-ragsoc
                      ef-indirizzo ef-cap ef-citta ef-provincia
                      ef-partita-iva ef-num-lic-utf ef-data-lic
                      ef-ufficio-appart ef-num-reg-ass
              move save-chiave to utf-chiave
           end-if 
           .
      * <TOTEM:END>
       Screen1-Ta-1-Ev-Cmd-Tabchanged.
      * <TOTEM:PARA. Screen1-Ta-1-Ev-Cmd-Tabchanged>
           evaluate event-data-1
           when 1
                if nuovo move 1 to v-importa
                else     move 0 to v-importa
                end-if
                move 1 to BitmapNumImporta
                if form1-radio-1-buf = 1
                   move 1 to v-dettaglio
                else
                   move 0 to v-dettaglio
                end-if
                move 0 to v-giacenze
           when 2
                move 0 to v-importa
                move 0 to v-dettaglio
                move 1 to BitmapNumImporta
                if form1-radio-1-buf = 1
                   move 1 to v-giacenze
                else
                   move 0 to v-giacenze
                end-if
           end-evaluate.

           modify pb-importa, visible v-importa, 
                              bitmap-number BitmapNumImporta 
           .
      * <TOTEM:END>
