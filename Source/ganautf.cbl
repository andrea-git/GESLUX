                      end-if

                      if tutto-ok
                         inquire ef-serie-1, value in utf-reg-serie-1
                         inquire ef-serie-2, value in utf-reg-serie-2
                         inquire ef-serie-3, value in utf-reg-serie-3
      *****                   if utf-reg-serie-1 = utf-reg-serie-2
      *****                      set errori to true
      *****                      display message "Differenziare le due serie di registri per i dettagli"
      *****                                title tit-err
      *****                                 icon 2
      *****                   else
      *****                      if utf-reg-serie-2 = utf-num-reg
      *****                         set errori to true
      *****                         display message "Differenziare la serie 2 dal principale"
      *****                                   title tit-err
      *****                                    icon 2
      *****                      end-if
      *****                   end-if
                      end-if
                   end-if
                else
                   move 0 to ef-serie-2-buf
                end-if

           when 78-ID-ef-serie-3
                if Form1-RADIO-1-BUF = 1
                   inquire ef-serie-3, value in utf-reg-serie-3
                   if utf-reg-serie-3 = 0
                      set errori to true
                      display message "Registro serie 3 obbligatorio"
