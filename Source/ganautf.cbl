      *****                   if utf-reg-serie-1 = utf-reg-serie-2
      *****                      set errori to true
      *****                      display message "Differenziare le due serie di registri per i dettagli"
      *****                                title tit-err
      *****                                 icon 2
      *****                   else
      *****                      if utf-reg-serie-2 = utf-num-reg
      *****                         set errori to true
      *****                         display message "Differenziare la serie 2 dal principale"
      *****                                   title tit-err
      *****                                    icon 2
