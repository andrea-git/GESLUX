       pb-ok-LinkTo.
      * <TOTEM:PARA. pb-ok-LinkTo>
           display message "Procedere con la copia?"
                     title titolo
                      type mb-yes-no
                   default mb-no
                    giving scelta
           if scelta = mb-yes
              move LinkChiave to utf-chiave
              read anautf no lock 
                   invalid 
                   display message "Registro non valido"
                             title tit-err
                              icon 2
                   set errori to true
               not invalid
                    move ef-anno-c-buf to utf-anno
                    initialize utf-dati-giacenza 
                               replacing numeric data by zeroes
                                    alphanumeric data by spaces
                    write utf-rec
                          invalid 
                          display message "Registro già presente"
                                   x"0d0a""Copia impossibile!"
                                    title tit-err
                                     icon 2
                          set errori to true
                      not invalid
                          display message "Copia effettuata"
                                    title titolo
                                     icon 2
                          set tutto-ok to true
                    end-write
              end-read      
           end-if.

           if tutto-ok
              move 27 to key-status
           else
              modify pb-ok, bitmap-number = 1
              move 78-ID-ef-anno-c to control-id
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>
