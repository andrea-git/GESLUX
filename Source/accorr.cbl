       scr-visua-PROC.
      * <TOTEM:EPT. FORM:scr-visua, FORM:scr-visua, BeforeAccept>
           accept data-stampa from century-date.
           move spaces to tge-chiave.
           read tparamge no lock invalid continue end-read.

           move 0 to sel-counter.
           perform RESET-GRIGLIA.
           set tutto-ok to true.
           set trovato  to false.
           move low-value to tor-rec.
           move tge-causale-corrisp to tor-causale.
           start tordini key is >= k-causale
                 invalid set errori to true
           end-start.
           if tutto-ok
              modify form1-gd-1, mass-update = 1
              move 2  to riga
              perform until 1 = 2
                 read tordini next at end exit perform end-read
                 if tor-causale not = tge-causale-corrisp
                    exit perform
                 end-if
                 if tor-data-passaggio-ordine = 0
                    set  trovato    to true
                    move tor-anno   to col-anno
                    move tor-numero to col-numero

                    move 1 to hidden-sel
                    perform SOMMA-TOTALE

                    modify form1-gd-1(riga, 1), cell-data col-anno
                    modify form1-gd-1(riga, 2), cell-data col-numero
                    modify form1-gd-1(riga, 3), cell-data col-tot
                    modify form1-gd-1(riga, 4), bitmap spunta-bmp,
                                                bitmap-number = 1,
                                                bitmap-width 16
                    modify form1-gd-1(riga, 1), hidden-data hidden-sel
                    modify form1-gd-1(riga) 
                           row-font = Verdana12BI-Occidentale
                    add 1 to riga
                    add 1 to sel-counter
                 end-if
              end-perform
              modify form1-gd-1, mass-update = 0
           end-if.

           if not trovato
              display message box "Nessuno corrispettivo ancora aperto!"
                      title = titolo
                      icon  2
              perform SCR-VISUA-EXIT
           else
              move 1 to v-screen
              modify form1-handle, visible v-screen
           end-if.

           .
      * <TOTEM:END>
           PERFORM UNTIL Exit-Pushed
              ACCEPT scr-visua
                 ON EXCEPTION
                    PERFORM scr-visua-Evaluate-Func
                 MOVE 1 TO TOTEM-Form-Index
              END-ACCEPT
      * <TOTEM:EPT. FORM:scr-visua, FORM:scr-visua, AfterEndAccept>
      * <TOTEM:END>
           END-PERFORM
