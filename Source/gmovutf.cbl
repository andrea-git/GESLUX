      * DB_RADIOBOX : rb-entrata, rb-uscita, 
           EVALUATE Form1-RADIO-1-BUF
           WHEN 1
              MOVE "+" TO mov-tipo
           WHEN 2
              MOVE "-" TO mov-tipo
           WHEN OTHER
              MOVE SPACE TO mov-tipo
           END-EVALUATE
      * DB_RADIOBOX : rb-a, rb-b, rb-c, rb-d, 
           EVALUATE Form1-RADIO-3-BUF
           WHEN 1
              MOVE "A" TO mov-col-reg-utf
           WHEN 2
              MOVE "B" TO mov-col-reg-utf
           WHEN 3
              MOVE "C" TO mov-col-reg-utf
           WHEN 4
              MOVE "D" TO mov-col-reg-utf
           WHEN OTHER
              MOVE SPACE TO mov-col-reg-utf
           END-EVALUATE
      * DB_RADIOBOX : rb-cliente, rb-forn, 
           EVALUATE Form1-RADIO-2-BUF
           WHEN 1
              MOVE "C" TO mov-tipo-CF
           WHEN 2
              MOVE "F" TO mov-tipo-CF
           WHEN OTHER
              MOVE SPACE TO mov-tipo-CF
           END-EVALUATE
      * DB_Entry-Field : ef-codice
           MOVE ef-codice-BUF TO mov-cod-clifor
      * DB_Entry-Field : ef-numdoc
           MOVE ef-numdoc-BUF TO mov-num-doc
      * DB_Entry-Field : ef-movim
           MOVE ef-movim-BUF TO mov-num-movim
      * <TOTEM:EPT. FORM:Form1, FORM:Form1, AfterBufToFld>
           perform RIEMPI-CHIAVE.
                                  
           move ef-data-buf to como-data.
           perform DATE-TO-FILE.
           move como-data   to mov-data.

