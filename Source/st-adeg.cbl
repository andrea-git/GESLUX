      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
                
           when 78-ID-ef-tipo
                inquire ef-marca, value in tcl-codice
                move "ttipocli"         to como-file
                call   "zoom-gt"     using como-file, tcl-rec
                                    giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tcl-codice      to ef-tipo-buf
                   move tcl-descrizione to lab-tipo-buf
                   display ef-tipo lab-tipo
                end-if

           when 78-ID-ef-marca
                inquire ef-marca, value in mar-codice
                move "tmarche"          to como-file
                call   "zoom-gt"     using como-file, mar-rec
                                    giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move mar-codice      to ef-marca-buf
                   move mar-descrizione to lab-marca-buf
                   display ef-marca lab-marca
                end-if
                
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-mese-da
                inquire ef-mese-da, value in mese-da
                if mese-da > 12
                   move 12 to mese-da
                   display ef-mese-da
                end-if

           when 78-ID-ef-mese-a
                inquire ef-mese-a, value in mese-a
                if mese-a > 12 or = 0
                   move 12 to mese-a
                   display ef-mese-a
                end-if

                inquire ef-mese-da,  value in mese-da

                if mese-da > mese-a
                   move 78-ID-ef-mese-da to control-id
                   set errori to true
                   display message "Intervallo mesi errato"
                             title tit-err
                              icon 2
                end-if


           when 78-ID-ef-tipo
                inquire ef-tipo value in tcl-codice
                if tcl-codice = spaces
                   move "Tutte le tipologie" to lab-tipo-buf
                else
                   read ttipocli no lock
                        invalid
                        set errori to true
                        move spaces to lab-tipo-buf
                        display message "Tipologia NON valida"
                                  title tit-err
                                   icon 2
                    not invalid
                        move tcl-descrizione to lab-tipo-buf
                   end-read
                end-if
                display lab-tipo

           when 78-ID-ef-marca
                inquire ef-marca, value in mar-codice
                if mar-codice = 0
                   move "Tutte le marche" to lab-marca-buf
                else
                   read tmarche no lock
                        invalid
                        set errori to true
                        display message "Marca NON valida"
                                  title tit-err
                                   icon 2
                        move spaces to lab-marca-buf
                    not invalid
                        move mar-descrizione to lab-marca-buf
                   end-read
                end-if
                display lab-marca

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

