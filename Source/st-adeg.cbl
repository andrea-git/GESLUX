                 MOVE Low-Value TO mar-chiave
              ELSE
                 MOVE High-Value TO mar-chiave
              END-IF
           END-EVALUATE
           .

       DataSet1-tmarche-INITEND.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tmarche-KEY1-Asc
                 MOVE High-Value TO mar-chiave
              ELSE
                 MOVE Low-Value TO mar-chiave
              END-IF
           END-EVALUATE
           .

       DataSet1-CHANGETO-KEY1.
           MOVE 1 TO DataSet1-KEYIS
           .   

       DataSet1-Change-CurrentKey-Asc.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              MOVE "A" TO DataSet1-tmarche-KEY1-ORDER
           END-EVALUATE
           .

       DataSet1-Change-CurrentKey-Desc.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              MOVE "D" TO DataSet1-tmarche-KEY1-ORDER
           END-EVALUATE
           .

      * tmarche
       DataSet1-tmarche-START.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tmarche-KEY1-Asc
                 START tmarche KEY >= mar-chiave
              ELSE
                 START tmarche KEY <= mar-chiave
              END-IF
           END-EVALUATE
           .

       DataSet1-tmarche-START-NOTGREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
              IF DataSet1-tmarche-KEY1-Asc
                 START tmarche KEY <= mar-chiave
              ELSE
                 START tmarche KEY >= mar-chiave
              END-IF
           END-EVALUATE
           .

       DataSet1-tmarche-START-GREATER.
           EVALUATE DataSet1-KEYIS
           WHEN 1
