      * USER DEFINE PARAGRAPH
       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID

           when 78-ID-ef-anno-from
                inquire ef-anno-from, value in anno-from
                if anno-from = 0
                   display message "Inserimento anno mancante"
                           title tit-err
                           icon  mb-warning-icon
                   set errori              to true
                   move 78-ID-ef-anno-from to store-id
                   move 4                  to accept-control
                else
                   if anno-from not = tge-anno
                      display message "L'anno di stampa NON coincide"
                                      " con quello d'esercizio."
                               x"0d0a""Anno d'esercizio: ", tge-anno
                              title = tit-err
                              icon 2                           
                      set errori              to true
                      move 78-ID-ef-anno-from to store-id
                      move 4                  to accept-control
                   end-if
                end-if

      *****     when 78-ID-ef-num-from
      ****** ORDINE DI FINE SELEZIONE
      *****          if num-from > 900001
      *****             move 900001 to num-from
      *****             display ef-num-from
      *****          end-if

           when 78-ID-ef-num-to
      ****** ORDINE DI FINE SELEZIONE
      *****          if num-to = 0
      *****             move 900000 to num-to
      *****             display ef-num-to
      *****          end-if
      *****
      *****          if num-to > 900000
      *****             move 900000 to num-to
      *****             display ef-num-to
      *****          end-if

                if num-to < num-from
                   set errori to true
                   display message MSG-intervallo-num-ord-err
                           title tit-err
                           icon  mb-warning-icon
                   move 78-ID-ef-num-to to store-id
                   move 4               to accept-control
                end-if
           end-evaluate.

           perform CANCELLA-COLORE 
           .
      * <TOTEM:END>

