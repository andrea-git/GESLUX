       PB-ESEGUI-LinkTo.
      * <TOTEM:PARA. PB-ESEGUI-LinkTo>
           perform varying control-id from 78-ID-ef-anno-from by 1 
                     until control-id    > 78-ID-ef-num-to
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if tutto-ok
              display message box "Confermi la ristampa?"
                      title = titolo
                      type MB-YES-NO
                      giving scelta

              if scelta = MB-YES
                 initialize stbolle-linkage
                 set stbolle-ristampa to true
                 move anno-from       to stbolle-anno
                 move num-from        to stb-numero-da
                 move num-to          to stb-numero-a
                 set stbolle-k-bolla  to true
                 set stbolle-singola  to false
      *****           set stbolle-gdo      to true
                 set stbolle-piu-stampe-si to true

                 move spaces to stbolle-path
                 move 1 to stbolle-giro
                 perform RISTAMPA-BOLLA
                 if stbolle-giro not = 9
                    move 2 to stbolle-giro
                    perform RISTAMPA-BOLLA                  
                 end-if

                 initialize file-info                     
                 call "C$FILEINFO" using stbolle-path-riepilogo, 
           file-info

                 if file-size not = 0                                  
                    accept printerToSet from environment "STAMPANTE_RIEP
      -    "ILOGO_CORRIERI"
                    move printerToSet to splcrt2graf-percorso-stampa-u
                    move stbolle-path-riepilogo              
                      to   splcrt2graf-percorso-stampa
                    set    splcrt2graf-stampa        to true
                    set    splcrt2graf-windows       to true
                    set    splcrt2graf-orizzontale   to true
                    set    splcrt2graf-forza-crt     to true
                    set    splcrt2graf-10pt          to true
                    set    splcrt2graf-si-grasssetto to false
                    call   "splcrt2graf2"   using splcrt2graf-link
                    cancel "splcrt2graf2"                              
                 end-if

                 call "C$DELETE" using stbolle-path-riepilogo

                 modify pb-esegui,  bitmap-number = 1
                 modify Form1-Pb-2, bitmap-number = 1
              else
                 move 500 to control-id
                 move 4   to accept-control
              end-if
           end-if.

      ***---
       RISTAMPA-BOLLA.
           call "W$MOUSE"     using set-mouse-shape, wait-pointer
           call   "stbolle-A4-p" using stbolle-linkage
           cancel "stbolle-A4-p"       
           call "W$MOUSE"     using set-mouse-shape, arrow-pointer

           move 78-ID-ef-anno-from to CONTROL-ID    
           move 4                  to ACCEPT-CONTROL

           perform varying idx from 1 by 1 
                     until idx > stbolle-num-stampe
              evaluate idx 
              when 1 move stbolle-path1      to stbolle-path
                     move stbolle-stampante1 to printerToSet

              when 2 move stbolle-path2      to stbolle-path     
                     move stbolle-stampante2 to printerToSet

              when 3 move stbolle-path3      to stbolle-path
                     move stbolle-stampante3 to printerToSet

              when 4 move stbolle-path4      to stbolle-path
                     move stbolle-stampante4 to printerToSet

              when 5 move stbolle-path5      to stbolle-path
                     move stbolle-stampante5 to printerToSet
              end-evaluate

              initialize file-info
          
              call "C$FILEINFO" using stbolle-path, file-info

              if file-size not = 0                                    
                 move printerToSet to splcrt2graf-percorso-stampa-u
                 move stbolle-path              
                   to splcrt2graf-percorso-stampa
                 set  splcrt2graf-stampa        to true
                 if stbolle-giro = 1
                    set  splcrt2graf-windows       to true
                 else                                     
                    set  splcrt2graf-unix          to true |usato come comodo
                 end-if                 
      *****           set  splcrt2graf-windows       to true
                 set  splcrt2graf-verticale     to true
                 set  splcrt2graf-forza-crt     to true
                 set  splcrt2graf-7pt           to true
                 set  splcrt2graf-si-grasssetto to false 
                 call   "splcrt2graf2" using splcrt2graf-link
                 cancel "splcrt2graf2"                    
      *****           call   "splcrt2graf" using splcrt2graf-link
      *****           cancel "splcrt2graf"                             
              end-if

              call "C$DELETE" using stbolle-path

           end-perform 
           .
      * <TOTEM:END>

