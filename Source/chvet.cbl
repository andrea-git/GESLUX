      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-reg
                inquire ef-reg, value in reg-codice
                move  "tregioni"      to como-file
                call   "zoom-gt"   using como-file, reg-rec
                                  giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move reg-codice      to ef-reg-buf
                   move reg-descrizione to lab-reg-buf
                   display ef-reg lab-reg
                end-if

           when 78-ID-ef-prov
                inquire ef-prov, value in prv-codice
                move   "tprov"         to como-file
                call   "zoom-gt"    using como-file, prv-rec
                                   giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move prv-codice      to ef-prov-buf
                   move prv-descrizione to lab-prov-buf
                   display ef-prov lab-prov
                end-if

           when 78-ID-ef-old-vet
                inquire ef-old-vet value in vet-codice
                move "tvettori"          to como-file
                call   "zoom-gt"      using como-file, vet-rec
                                    giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move vet-codice      to ef-old-vet-buf
                   move vet-descrizione to lab-old-vet-buf
                   display ef-old-vet lab-old-vet
                end-if

           when 78-ID-ef-new-vet
                inquire ef-new-vet value in vet-codice
                move "tvettori"          to como-file
                call   "zoom-gt"      using como-file, vet-rec
                                    giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move vet-codice       to ef-new-vet-buf
                   move vet-descrizione  to lab-new-vet-buf
                   display ef-new-vet lab-new-vet
                end-if
                
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-reg
                inquire ef-reg, value in reg-codice
                if reg-codice = 0
                   move spaces to lab-reg-buf
                else
                   read tregioni no lock
                        invalid
                        set errori to true
                        display message "Regione NON valida"
                                  title tit-err
                                   icon 2
                        move spaces          to lab-reg-buf
                    not invalid
                        move reg-descrizione to lab-reg-buf
                   end-read
                end-if
                display lab-reg

           when 78-ID-ef-prov
                inquire ef-reg,  value in reg-codice
                inquire ef-prov, value in prv-codice
                if prv-codice = spaces
                   if reg-codice = 0
                      set errori to true
                      display message "Regione o provincia obbligatorie"
                                title tit-err
                                 icon 2
                      move 78-ID-ef-reg to control-id
                      move spaces to lab-prov-buf
                   else
                      move "Tutte le provincie" to lab-prov-buf
                   end-if
                else
                   read tprov no lock
                        invalid
                        display message "Provincia NON valida"
                                  title tit-err
                                   icon 2
                        set errori   to true
                        move spaces  to lab-prov-buf
                    not invalid
                        move prv-descrizione to lab-prov-buf
                        move prv-regione     to ef-reg-buf reg-codice
                        read tregioni no lock
                             invalid move spaces          to lab-reg-buf
                         not invalid move reg-descrizione to lab-reg-buf
                        end-read
                   end-read
                end-if
                display lab-prov lab-reg ef-reg

           when 78-ID-ef-tipo
                inquire ef-tipo, value in tcl-codice
                if tcl-codice = spaces
                   move "TUTTE le tipologie" to lab-tipo-buf
                else
                   read ttipocli no lock
                        invalid
                        set errori to true
                        display message "Tipologia NON valida"
                                  title tit-err
                                   icon 2
                        move spaces          to lab-tipo-buf
                    not invalid
                        move tcl-descrizione to lab-tipo-buf
                   end-read
                end-if
                display lab-tipo

           when 78-ID-ef-old-vet
                inquire ef-old-vet, value in vet-codice
                if vet-codice = 0
                   move "Vettore corrente" to lab-old-vet-buf
                else
                   read tvettori no lock
                        invalid
                        set errori to true
                        display message "Vettore NON valido"
                                  title tit-err
                                   icon 2
                        move spaces to lab-old-vet-buf
                    not invalid
                        move vet-descrizione to lab-old-vet-buf
                   end-read
                end-if
                display lab-old-vet

           when 78-ID-ef-new-vet
                inquire ef-new-vet, value in vet-codice
                read tvettori no lock
                     invalid
                     set errori to true
                     display message "Vettore NON valido"
                               title tit-err
                                icon 2
                     move spaces to lab-new-vet-buf
                 not invalid
                     move vet-descrizione to lab-new-vet-buf
                end-read
                display lab-new-vet

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

