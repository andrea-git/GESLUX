      * <TOTEM:END>
       pb-sib-BeforeProcedure.
      * <TOTEM:PARA. pb-sib-BeforeProcedure>
           modify pb-sib, bitmap-number = 2 
           .
      * <TOTEM:END>
       pb-sib-LinkTo.
      * <TOTEM:PARA. pb-sib-LinkTo>
           perform varying control-id from 78-ID-ef-num-from by 1
                     until control-id > 78-ID-ef-prodener-new
               perform CONTROLLO
               if errori
                  exit perform
               end-if
           end-perform.

           if tutto-ok
              display message "Verrà sostituito il codice " 
                       x"0d0a"prodener-old
                       x"0d0a""con quello indicato"
                       x"0d0a"prodener-new
                       x"0d0da""per gli articoli indicati."
                       x"0d0a""Confermi?"
                        title titolo
                         icon 2
                         type mb-yes-no
                      default mb-no
                       giving scelta
              if scelta = mb-no
                 set errori to true
              end-if
           end-if.

           if tutto-ok
              move 0 to deleted
              call "W$MOUSE" using set-mouse-shape, wait-pointer
              move num-from to art-codice
              start articoli key >= art-chiave
                    invalid continue
                not invalid
                    perform until 1 = 2
                       read articoli next at end exit perform end-read
                       if art-codice > num-to exit perform end-if
                       if art-cod-prodener = prodener-old
                          move prodener-new to art-cod-prodener
                          rewrite art-rec
                          add 1 to deleted
                       end-if
                    end-perform
              end-start

              call "W$MOUSE" using set-mouse-shape, arrow-pointer
              display message "OPERAZIONE TERMINATA!!!"
                       x"0d0a""SOSTITUITI " deleted " CODICI"
                        title titolo
                         icon 2
              modify pb-sib, bitmap-number = 1
              move 78-ID-ef-num-from to control-id
              move                 4 to accept-control
           end-if 
           .
      * <TOTEM:END>
       check-utf-Ev-Before-Program.
      * <TOTEM:PARA. check-utf-Ev-Before-Program>
           perform CALCOLA-COLORE-TRASPARENTE 
           .
      * <TOTEM:END>
       ef-num-from-BeforeProcedure.
      * <TOTEM:PARA. ef-num-from-BeforeProcedure>
           MODIFY CONTROL-HANDLE COLOR = COLORE-NU
           .
      * <TOTEM:END>
       ef-num-from-AfterProcedure.
      * <TOTEM:PARA. ef-num-from-AfterProcedure>
              INQUIRE ef-num-from, VALUE IN num-from
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-num-from-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-prodener-old, VALUE IN prodener-old
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-prodener-old-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
              INQUIRE ef-prodener-new, VALUE IN prodener-new
              SET TOTEM-CHECK-OK TO FALSE
              PERFORM ef-prodener-new-VALIDATION
              IF NOT TOTEM-CHECK-OK
                 MOVE 1 TO ACCEPT-CONTROL
              END-IF
           MODIFY CONTROL-HANDLE COLOR = COLORE-OR

           .
      * <TOTEM:END>

