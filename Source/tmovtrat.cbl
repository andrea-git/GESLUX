       SUCCESSIVO.
      * <TOTEM:PARA. SUCCESSIVO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tmovtrat all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tmovtrat-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tmovtrat-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tmovtrat-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if 
           .
      * <TOTEM:END>

       STAMPA.
      * <TOTEM:PARA. STAMPA>
           continue 
           .
      * <TOTEM:END>

       ULTIMO.
      * <TOTEM:PARA. ULTIMO>
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tmovtrat all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tmovtrat-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tmovtrat-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tmovtrat-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if 
           .
      * <TOTEM:END>

       VALORIZZA-OLD.
      * <TOTEM:PARA. VALORIZZA-OLD>
           move tra-rec to old-tra-rec.
           set vecchio to true.

           move ZERO to StatusHelp
           perform STATUS-HELP 
           .
      * <TOTEM:END>

       VALORIZZA-NUOVO.
      * <TOTEM:PARA. VALORIZZA-NUOVO>
           |ricerca ultimo anno su tmovtrat, se esiste propongo il
           |successivo, altrimenti utilzzo l'anno d'esercizio
           move 9999 to tra-anno
           move 0 to sw-tmovtrat
           start tmovtrat key <= tra-chiave
             invalid
               move 1 to sw-tmovtrat
           end-start
           perform until sw-tmovtrat = 1
             read tmovtrat previous no lock
               at end
                 move 1 to sw-tmovtrat
               not at end
                 move 1 to sw-tmovtrat
                 move tra-anno to sav-anno
                 add 1 to sav-anno
             end-read
           end-perform

           move sav-anno to tra-anno.

           initialize tra-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                                       
           perform FORM1-IUD-DISPLAY 

           .
      * <TOTEM:END>

       VALORIZZA-DATI-COMUNI.
      * <TOTEM:PARA. VALORIZZA-DATI-COMUNI>
      * SE NON HO VALORIZZATO I CAMPI DI CREAZIONE LI VALORIZZO
           if tra-data-creazione   = 0
              accept tra-data-creazione from century-date
           end-if.
           if tra-ora-creazione    = 0
              accept tra-ora-creazione  from time
           end-if.
           if tra-utente-creazione = space
              move user-codi to tra-utente-creazione
           end-if.

           accept tra-data-modifica from century-date.
           accept tra-ora-modifica  from time.
           move user-codi to tra-utente-modifica 
           .
      * <TOTEM:END>

       TORNA-IN-VISUA.
      * <TOTEM:PARA. TORNA-IN-VISUA>
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-anno to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock tmovtrat all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL 
           .
      * <TOTEM:END>

