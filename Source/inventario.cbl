           inquire chk1, value in chk1-buf.
           if chk1-buf = 1
              move 1 to v1
              display scr-elab
              move 1 to inv-chiamata
              call   "inventario-p" using inventario-linkage
              cancel "inventario-p"
              move "Azzeramento eseguito." to lab2-buf
           else
              move "Azzeramento NON eseguito." to lab2-buf
           end-if.
           move 1 to v2.
           display scr-elab.  

           inquire chk4, value in chk4-buf.
           if chk4-buf = 1
              move 1 to v3
              display scr-elab
              move 2 to inv-chiamata
              move ef-mag-buf to inv-mag-azz
              call   "inventario-p" using inventario-linkage
              cancel "inventario-p"
              move "Copertura giacenze eseguita." to lab4-buf
           else
              move "Copertura giacenze NON eseguita." to lab4-buf
           end-if.
           move 1 to v4.
           display scr-elab.                               

           inquire chk2, value in chk2-buf.
           move 1 to v5.
           if ef-file1-buf = spaces or chk2-buf = 0
              move "Nessuna elaborazione per file 1" to lab5-buf
              display lab5
           else
              move ef-file1-buf to inv-file1
              move "Elaborazione file 1 in corso..." to lab5-buf
              display lab5
              move 3 to inv-chiamata
              move ef-mag-c3-buf to inv-mag-c3
              move ef-mag-s3-buf to inv-mag-s3
              call   "inventario-p" using inventario-linkage
              cancel "inventario-p"
              move 1 to v6
              display scr-elab
           end-if.       

           inquire chk3, value in chk3-buf.
           move 1 to v7.
           if ef-file2-buf = spaces or chk3-buf = 0
              move "Nessuna elaborazione per file 2" to lab7-buf
              display lab7
           else
              move ef-file2-buf to inv-file2
              move "Elaborazione file 2 in corso..." to lab7-buf
              display lab7
              move 4 to inv-chiamata
              call   "inventario-p" using inventario-linkage
              cancel "inventario-p"
              move 1 to v8
              display scr-elab
           end-if.      

           move 1 to v-push.
           display pb-fatto.
           modify  pb-fatto, bitmap-number = 2.

           call "W$MOUSE" using set-mouse-shape, arrow-pointer.

           .
