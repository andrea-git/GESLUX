      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-mag-c
                move "tmagaz" to como-file
                inquire ef-mag-c, value in mag-codice
                call "zoom-gt"   using como-file, mag-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move mag-codice      to ef-mag-c-buf
                   move mag-descrizione to lab-mag-c-buf
                   display ef-mag-c lab-mag-c
                end-if

           when 78-ID-ef-mag-c3
                move "tmagaz" to como-file
                inquire ef-mag-c3, value in mag-codice
                call "zoom-gt"   using como-file, mag-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move mag-codice      to ef-mag-c3-buf
                   move mag-descrizione to lab-mag-c3-buf
                   display ef-mag-c3 lab-mag-c3
                end-if

           when 78-ID-ef-mag-s3
                move "tmagaz" to como-file
                inquire ef-mag-s3, value in mag-codice
                call "zoom-gt"   using como-file, mag-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move mag-codice      to ef-mag-s3-buf
                   move mag-descrizione to lab-mag-s3-buf
                   display ef-mag-s3 lab-mag-s3
                end-if

           when 78-ID-ef-cau-c
                set tca-imponibile-pos to true
                inquire ef-mag-c, value in tca-cod-magaz
                move "tcaumag-imp-pos-mag"  to como-file
                inquire ef-cau-c, value in tca-codice
                call "zoom-gt"   using como-file, tca-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tca-codice      to ef-cau-c-buf
                   move tca-descrizione to lab-cau-c-buf
                   display ef-cau-c lab-cau-c
                end-if

           when 78-ID-ef-mag-s
                move "tmagaz" to como-file
                inquire ef-mag-s, value in mag-codice
                call "zoom-gt"   using como-file, mag-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move mag-codice      to ef-mag-s-buf
                   move mag-descrizione to lab-mag-s-buf
                   display ef-mag-s lab-mag-s
                end-if

           when 78-ID-ef-cau-s
                set tca-imponibile-neg to true
                inquire ef-mag-s, value in tca-cod-magaz
                move "tcaumag-imp-neg-mag"  to como-file
                inquire ef-cau-s, value in tca-codice
                call "zoom-gt"   using como-file, tca-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tca-codice      to ef-cau-s-buf
                   move tca-descrizione to lab-cau-s-buf
                   display ef-cau-s lab-cau-s
                end-if                 
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.
           evaluate control-id

           when 78-ID-ef-data
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                perform DATE-FORMAT
                perform DATE-TO-FILE
                if como-data <= tge-data-consolid-progmag
                   set errori to true
                   display message "La data dev'essere superiore a quell
      -    "a dell'utlimo consolidamento"
                            x"0d0a""DATA DI RIFERIMENTO: " 
                                   tge-data-consolid-progmag(7:2)
                                   "/"
                                   tge-data-consolid-progmag(5:2)
                                   "/"
                                   tge-data-consolid-progmag(1:4)
                             title tit-err
                              icon 2
                end-if
                perform DATE-TO-SCREEN
                move como-data to ef-data-buf
                display ef-data

           when 78-ID-ef-mag-s
                inquire chk1, value in chk1-buf
                if chk1-buf = 1
                   inquire ef-mag-s, value in mag-codice
                   read tmagaz no lock 
                        invalid
                        move spaces to mag-descrizione lab-cau-s-buf 
           ef-cau-s-buf
                        set errori to true
                        display message "Magazzino di scarico non valido
      -    ""
                                  title tit-err
                                   icon 2
                    not invalid
                        move mag-cau-s to tca-codice
                        read tcaumag no lock
                        move tca-codice      to ef-cau-s-buf
                        move tca-descrizione to lab-cau-s-buf     
                   end-read
                   move mag-descrizione to lab-mag-s-buf
                   display lab-mag-s
                   display lab-cau-s ef-cau-s
                end-if

           when 78-ID-ef-cau-s
                inquire chk1, value in chk1-buf
                if chk1-buf = 1
                   inquire ef-cau-s, value in tca-codice
                   read tcaumag no lock 
                        invalid
                        move spaces to tca-descrizione
                        set errori to true
                        display message "Causale di scarico non valida"
                                  title tit-err
                                   icon 2
                    not invalid
                        if tca-imponibile-pos
                           move spaces to tca-descrizione
                           set errori to true
                           display message "Causale di scarico non valid
      -    "a"
                                    x"0d0a""Indicare una causale ad impo
      -    "nibile negativo"
                                     title tit-err
                                      icon 2
                        else
                           if tca-cod-magaz not = ef-mag-s-buf
                              move spaces to tca-descrizione
                              set errori to true
                              display message "Causale di scarico non va
      -    "lida"
                                       x"0d0a""per il magazzino indicato
      -    ""
                                        title tit-err
                                         icon 2
                           end-if    
                        end-if
                   end-read
                   move tca-descrizione to lab-cau-s-buf
                   display lab-cau-s
                end-if

           when 78-ID-ef-mag-c
                inquire chk1, value in chk1-buf
                if chk1-buf = 1
                   inquire ef-mag-c, value in mag-codice
                   read tmagaz no lock 
                        invalid
                        move spaces to mag-descrizione lab-cau-c-buf 
           ef-cau-c-buf
                        set errori to true
                        display message "Magazzino di carico non valido"
                                  title tit-err
                                   icon 2
                    not invalid
                        move mag-cau-c to tca-codice
                        read tcaumag no lock
                        move tca-codice to ef-cau-c-buf
                        move tca-descrizione to lab-cau-c-buf     
                   end-read
                   move mag-descrizione to lab-mag-c-buf
                   display lab-mag-c
                   display lab-cau-c ef-cau-c
      *****             if tutto-ok
      *****                inquire ef-mag-c, value in ef-mag-c-buf
      *****                if ef-mag-c-buf = mag-codice
      *****                   set errori to true
      *****                   display message "Magazzini coincidenti"
      *****                             title tit-err
      *****                              icon 2
      *****                   move 78-ID-ef-mag-c to control-id
      *****                end-if
      *****             end-if
                end-if

           when 78-ID-ef-cau-c
                inquire chk1, value in chk1-buf
                if chk1-buf = 1
                   inquire ef-mag-c, value in ef-mag-c-buf
                   move ef-mag-c-buf to tca-cod-magaz
                   inquire ef-cau-c, value in tca-codice
                   read tcaumag no lock 
                        invalid
                        move spaces to tca-descrizione
                        set errori to true
                        display message "Causale di carico non valida"
                                  title tit-err
                                   icon 2
                    not invalid
                        if tca-imponibile-neg
                           move spaces to tca-descrizione
                           set errori to true
                           display message "Causale di carico non valida
      -    ""
                                    x"0d0a""Indicare una causale ad impo
      -    "nibile positivo"
                                     title tit-err
                                      icon 2
                        else
                           if tca-cod-magaz not = ef-mag-c-buf
                              move spaces to tca-descrizione
                              set errori to true
                              display message "Causale di carico non val
      -    "ida"
                                       x"0d0a""per il magazzino indicato
      -    ""
                                        title tit-err
                                         icon 2
                           end-if   
                        end-if               
                   end-read
                   move tca-descrizione to lab-cau-c-buf
                   display lab-cau-c
                end-if

           when 78-ID-ef-file1
                inquire chk2, value in chk2-buf
                if chk2-buf = 1
                   inquire ef-file1, value ef-file1-buf
                   if ef-file1-buf = spaces
                      display message "Confermi nessuna elaborazione per
      -    " file 1?"
                               title titolo
                                icon 2
                              giving scelta
                      if scelta = mb-no
                         set errori to true
                      else
                         move spaces to ef-mag-c3-buf lab-mag-c3-buf
                         move spaces to ef-mag-s3-buf lab-mag-s3-buf
                         display ef-mag-c3 lab-mag-c3 ef-mag-s3 
           lab-mag-s3
                         modify ef-mag-c3, read-only
                         modify ef-mag-s3, read-only
                      end-if
                   else
                      modify ef-mag-c3, not read-only
                      modify ef-mag-s3, not read-only
                   end-if
                end-if

           when 78-ID-ef-mag-c3
                inquire chk2, value in chk2-buf
                if chk2-buf = 1
                   inquire ef-file1, value ef-file1-buf
                   if ef-file1-buf not = spaces
                      inquire ef-mag-c3, value in mag-codice
                      read tmagaz no lock 
                           invalid
                           move spaces to mag-descrizione
                           set errori to true
                           display message "Magazzino di carico non vali
      -    "do"
                                     title tit-err
                                      icon 2
                      end-read
                      move mag-descrizione to lab-mag-c3-buf
                   end-if
                   display lab-mag-c3 lab-mag-c3
                end-if

           when 78-ID-ef-mag-s3
                inquire chk2, value in chk2-buf
                if chk2-buf = 1
                   inquire ef-file1, value ef-file1-buf
                   if ef-file1-buf not = spaces
                      inquire ef-mag-s3, value in mag-codice
                      read tmagaz no lock 
                           invalid
                           move spaces to mag-descrizione
                           set errori to true
                           display message "Magazzino di carico non vali
      -    "do"
                                     title tit-err
                                      icon 2
                      end-read
                      move mag-descrizione to lab-mag-s3-buf
                   end-if
                   display lab-mag-s3 lab-mag-s3
                end-if  

           when 78-ID-ef-file2
                inquire chk3, value in chk3-buf
                if chk3-buf = 1
                   inquire ef-file2, value ef-file2-buf
                   if ef-file2-buf = spaces
                      display message "Confermi nessuna elaborazione per
      -    " file 2?"
                               title titolo
                                icon 2
                              giving scelta
                      if scelta = mb-no
                         set errori to true
                      end-if
                   end-if
                   if tutto-ok
                      inquire ef-file1, value ef-file1-buf
                      if ef-file1-buf not = spaces or
                         ef-file2-buf not = spaces
                         if ef-file1-buf = ef-file2-buf
                            set errori to true
                            move 78-ID-ef-file1 to control-id
                            display message "File di elaborazione coinci
      -    "denti"
                                      title tit-err
                                       icon 2
                         end-if
                      end-if
                   end-if
                end-if

           when 78-ID-ef-mag
                inquire chk4, value in chk4-buf
                if chk4-buf = 1
                   inquire ef-mag, value ef-mag-buf
                   move ef-mag-buf to mag-codice
                   read tmagaz no lock
                        invalid
                        display message "Magazzino errato"
                                  title tit-err
                                   icon 2
                                 giving scelta
                        set errori to true
                        move spaces to mag-descrizione
                   end-read
                   move mag-descrizione to lab-mag-buf
                   display lab-mag
                end-if

           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

