      ***---
       CLIENTI-ERR SECTION.
           use after error procedure on CLIENTI.
           set tutto-ok  to true.
           evaluate status-clienti
           when "35"
                display message "File [CLIENTI] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [CLIENTI] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[CLIENTI] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           end-evaluate.

      ***---
       ASSORCLI-ERR SECTION.
           use after error procedure on assorcli.
           set tutto-ok  to true.
           evaluate status-assorcli
           when "35"
                display message "File [ASSORCLI] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [ASSORCLI] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[ASSORCLI] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           end-evaluate.

      ***---
       ASSORCLI1-ERR SECTION.
           use after error procedure on assorcli1.
           set tutto-ok  to true.
           evaluate status-assorcli1
           when "35"
                display message "File [ASSORCLI1] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [ASSORCLI1] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[ASSORCLI1] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "93"
                move "assorcli1" to geslock-nome-file
                initialize geslock-messaggio
                string   "File assorcli1 già in uso."
                  x"0d0a""Elaborazione impossibile!" delimited size
                     into geslock-messaggio
                end-string
                move 1     to geslock-v-termina
                move 0     to geslock-v-riprova
                move 0     to geslock-v-ignora
                call   "geslock" using geslock-linkage
                cancel "geslock"

                goback
                
                
           when "99"
                move "assorcli1" to geslock-nome-file
                initialize geslock-messaggio
                string   "Il Record ", asc-chiave of assorcli1
                  x"0d0a""risulta in uso su altro terminale."
                  x"0d0a""Questo ne comporta l'impossibilità al trattame
      -    "nto." delimited size
                     into geslock-messaggio
                end-string
                move 1     to geslock-v-termina
                move 1     to geslock-v-riprova
                move 1     to geslock-v-ignora
                call   "geslock" using geslock-linkage
                cancel "geslock"

                evaluate true
                when ignora  continue
                when riprova perform CANCELLA-ASSORCLI1
                when termina display message "Operazione interrotta!"
                                       title tit-err
                                        icon 2
                             set errori to true
                end-evaluate
   
           end-evaluate.

