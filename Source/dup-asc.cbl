      * USER DEFINE PARAGRAPH
       CANCELLA-ASSORCLI1.
      * <TOTEM:PARA. CANCELLA-ASSORCLI1>
           delete assorcli1 record invalid continue end-delete 
           .
      * <TOTEM:END>

       CANCELLAZIONE.
      * <TOTEM:PARA. CANCELLAZIONE>
           set  tutto-ok       to true.
           set  RecLocked      to true.
           move low-value      to asc-rec of assorcli1.
           move save-a-gdo     to asc-cod-gruppo-gdo of assorcli1.
           move ef-a-cli-buf   to asc-cod-cliente    of assorcli1.
           start assorcli1 key is >= asc-chiave      of assorcli1
                 invalid   set errori to true
           end-start.

           |CANCELLAZIONE
           move 0 to counter counter2.
           if tutto-ok
              perform until 1 = 2

                 add 1 to counter
                 add 1 to counter2
                 if counter2 = 50
                    if counter = 50
                       display "DELETE"
                          upon scr-calprovv-handle at column 13,00
                                                        line  3,00
                    end-if
                    move counter to counter-edit
                    display counter-edit
                       upon scr-calprovv-handle at column 17,00
                                                     line  3,00
                    move 0 to counter2
                 end-if

                 read assorcli1 next no lock at end exit perform 
           end-read
                 if asc-cod-gruppo-gdo of assorcli1 not = save-a-gdo or
                    asc-cod-cliente    of assorcli1 not = ef-a-cli-buf
                    exit perform
                 end-if
                 perform CANCELLA-ASSORCLI1
                 if errori exit perform end-if
              end-perform
           end-if 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate control-id
           when 1001
                inquire ef-da-cli, value in cli-codice
                set cli-tipo-C to true
                read clienti no lock
                     invalid
                     set errori  to true
                     move spaces to cli-ragsoc-1
                     display message "Cliente di partenza errato"
                               title tit-err
                                icon 2
                 not invalid
                     move cli-gdo to save-da-gdo
LUBEXX               move low-value     to asc-rec of assorcli
LUBEXX               move save-da-gdo   to asc-cod-gruppo-gdo of 
           assorcli
LUBEXX               move ef-da-cli-buf to asc-cod-cliente    of 
           assorcli
LUBEXX               start assorcli key is >= asc-chiave of assorcli
LUBEXX                     invalid set errori to true
LUBEXX                 not invalid
LUBEXX                     read assorcli next no lock end-read
LUBEXX                     if asc-cod-gruppo-gdo of assorcli not = 
           save-da-gdo or
LUBEXX                        asc-cod-cliente    of assorcli not = 
           ef-da-cli-buf
LUBEXX                        set errori to true
LUBEXX                     end-if
LUBEXX               end-start
LUBEXX               if errori
LUBEXX                  display message "Cliente di partenza senza assor
      -    "timento"
LUBEXX                            title tit-err
LUBEXX                             icon 2
LUBEXX               end-if
                end-read
                move cli-ragsoc-1 to lab-da-cli-buf
                display lab-da-cli

           when 1002
                inquire ef-a-cli, value in cli-codice
                set cli-tipo-C to true
                read clienti no lock
                     invalid
                     set errori  to true
                     move spaces to cli-ragsoc-1
                     display message "Cliente di destinazione errato"
                               title tit-err
                                icon 2
                 not invalid
                     if ef-a-cli-buf = ef-da-cli-buf
                        display message "Il Cliente di partenza coincide
      -    " col Cliente di destinazione"
                                  title tit-err
                                   icon 2
                        set errori to true
                     else
                        move cli-gdo to save-a-gdo
                        if save-a-gdo not = save-da-gdo
                           set errori to true
                           display message "Assortimento non duplicabile
      -    "."
                                    x"0d0a""Il gruppo GDO non coincide"
                                     title tit-err
                                      icon 2
                        end-if
                     end-if
                end-read
                move cli-ragsoc-1 to lab-a-cli-buf
                display lab-a-cli

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 1001
                set cli-tipo-c to true
                move   "clienti-all"  to como-file
                call   "zoom-gt"   using como-file, cli-rec
                                  giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move cli-codice     to ef-da-cli-buf   
                   move cli-ragsoc-1   to lab-da-cli-buf
                   display ef-da-cli lab-da-cli
                end-if

           when 1002
                set cli-tipo-c to true
                move   "clienti-all"  to como-file
                call   "zoom-gt"   using como-file, cli-rec
                                  giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move cli-codice     to ef-a-cli-buf   
                   move cli-ragsoc-1   to lab-a-cli-buf
                   display ef-a-cli lab-a-cli
                end-if

           end-evaluate 
           .
      * <TOTEM:END>

       PARAGRAFO-COPY.
      * <TOTEM:PARA. PARAGRAFO-COPY>
           copy "utydata.cpy".
           copy "color-custom.cpy" 
           .
      * <TOTEM:END>

       SOSTITUZIONE.
      * <TOTEM:PARA. SOSTITUZIONE>
           set tutto-ok to true.
           move 0 to tot-rec.

           move low-value     to asc-rec of assorcli.
           move save-da-gdo   to asc-cod-gruppo-gdo of assorcli.
           move ef-da-cli-buf to asc-cod-cliente    of assorcli.
           start assorcli key is >= asc-chiave of assorcli
                 invalid set errori to true
           end-start.

           |SCRITTURA
           move 0 to counter counter2.
           if tutto-ok
              display "                                                "
                    upon scr-calprovv-handle at column 13,00

              perform until 1 = 2

                 add 1 to counter
                 add 1 to counter2
                 if counter2 = 50
                    if counter = 50
                       display "SOSTITUZIONE"
                          upon scr-calprovv-handle at column 13,00
                                                        line  3,00
                    end-if
                    move counter to counter-edit
                    display counter-edit
                       upon scr-calprovv-handle at column 17,00
                                                     line  3,00
                    move 0 to counter2
                 end-if

                 read assorcli next no lock at end exit perform end-read
                 if asc-cod-gruppo-gdo of assorcli not = save-a-gdo or
                    asc-cod-cliente    of assorcli not = ef-da-cli-buf
                    exit perform
                 end-if
                 move asc-rec of assorcli to asc-rec      of assorcli1
                 move save-a-gdo    to asc-cod-gruppo-gdo of assorcli1
                 move ef-a-cli-buf  to asc-cod-cliente    of assorcli1
                 write asc-rec of assorcli1 invalid continue end-write
                 add 1 to tot-rec
              end-perform
           end-if 
           .
      * <TOTEM:END>

