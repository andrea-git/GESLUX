                    ror-imp-consumo   = 0 and
                    ror-add-piombo    = 0 and prenotabile
                    if ror-cod-iva  not = tge-cod-iva-omag
                       
                       set prenotabile to false
                       move riga to store-riga
                       set  errore-iva-no-E15 to true
                       move "A" to lprenf-mod
                    end-if
                 end-if

                 |Per evitare (così come già successo) che
                 |possa rientrare nella fatturazione una riga
                 |tge-cod-iva-omag, non omaggio, ma con imposte valorizzate...
                 if ror-cod-iva = tge-cod-iva-omag
                    if ror-prz-unitario  not = 0 or
                       ror-imponib-merce not = 0 or
                       ror-imp-cou-cobat not = 0 or
                       ror-imp-consumo   not = 0 or
                       ror-add-piombo    not = 0 and prenotabile
                       set prenotabile to false
                       move riga       to store-riga
                       set  errore-E15-no-zero to true
                    end-if
                 end-if
                 
                 |10/09: Modifica per gli ordini con doppia aliquota
                 |imposte. In base alla data bolla (che sarà poi la
                 |data fattura) le imposte devono utilizzare il vecchio
                 |o il nuovo moltiplicatore delle imposte
                 if tor-data-bolla < 20091101 and tor-data-bolla > 0
                    move 0,842 to imp-imposta-consumo
                    move 0,075 to imp-cou
                 else
                    move tor-anno to con-anno
                    read tcontat no lock

                    move con-ult-stampa-fatt to imp-data

                    start timposte key <= imp-chiave
                          invalid continue
                      not invalid
                          read timposte previous
                    end-start
                 end-if
                 perform CONTROLLA-IMPOSTE
                 |Controllo se c'è un errore che sia uguale al nuovo.
                 |Serve per gli ordini con data bolla < 20091101, ma
                 |predatate quindi con imposte calcolate col nuovo valore
                 if imposte-ko and tor-data-bolla < 20091101 and
                    tor-data-bolla > 0
                    move tor-anno to con-anno
                    read tcontat no lock
                    move con-ult-stampa-fatt to imp-data

                    start timposte key <= imp-chiave
                          invalid continue
                      not invalid
                          read timposte previous
                    end-start
                    perform CONTROLLA-IMPOSTE
                 end-if
                 if imposte-ko
                    set errore-imposte to true
                    set prenotabile    to false
                 end-if

                 if tor-da-ordine-si
                    move ror-chiave-ordine-testa  to mro-chiave-testa
                    move ror-progr-master         to mro-progr
                    read mrordini no lock key mro-k-progr
                         invalid
                         if mro-progr  not = 0 or
                            mro-anno   not = 0 or
                            mro-numero not = 0
                            set prenotabile        to false
                            set errore-prog-master to true
                            move riga              to store-riga
                         end-if
                     not invalid
                         if ror-prz-unitario  not = mro-prz-unitario 
                            set prenotabile          to false
                            set errore-prezzo-master to true
                            move riga                to store-riga
                         end-if
                         if ror-prg-cod-articolo  not = 
           mro-prg-cod-articolo
                            set prenotabile        to false
                            set errore-prog-master to true
                            move riga              to store-riga
                         end-if
                    end-read
                 end-if
                                       
                 |Per evitare una fatturazione ancora al 20%
                 if ror-cod-iva = "020" and prenotabile
                    set  errori            to true
                    move riga              to store-riga
                    set  errore-iva-020    to true
                    set  prenotabile       to false
                 end-if

                 |Per evitare una fatturazione ancora al 21%
                 if ror-cod-iva = "021" and prenotabile
                    set  errori            to true
                    move riga              to store-riga
                    set  errore-iva-021    to true
                    set  prenotabile       to false
                 end-if

LUBEXX        end-if

              initialize rec-grid-ord

              move ror-cod-articolo to col-art-ord art-codice

              read articoli invalid continue end-read

              move art-unita-di-misura to col-udm-ord
              move art-descrizione     to col-des-ord
              move ror-num-colli       to col-colli-ord
              perform DESCRIZIONE-IMBALLO
              move ror-peso-utf        to col-utf-ord
              move ror-qta             to col-qta-ord

              modify gd-ord, record-to-add = rec-grid-ord

              add ror-num-colli    to tot-colli
              compute tot-utf =
              ( ror-peso-utf * ror-qta ) + tot-utf
              compute tot-non-utf =
              ( ror-peso-non-utf * ror-qta ) + tot-non-utf
           end-perform.

           |Per evitare una AAAA a prezzo zero
           if importo-totale = 0 and prenotabile
              move tor-causale to tca-codice
              read tcaumag no lock
              if tca-si-emissione
                 set  errori            to true
