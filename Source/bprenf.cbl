      * USER DEFINE PARAGRAPH
       CANCELLA-TMP.
      * <TOTEM:PARA. CANCELLA-TMP>
           delete file zoom-tordini 
           .
      * <TOTEM:END>

       CERCA.
      * <TOTEM:PARA. CERCA>
           perform RIEMPI-TMP.

           move "zoom-tordini"   to como-file.

           inquire ef-anno, value in zoom-tor-anno.
           inquire ef-num,  value in zoom-tor-numero.

           if trovato
              move path-zoom-tordini to ext-file
              call "zoom-gt"   using como-file, zoom-tor-rec
                              giving stato-zoom
              end-call
              cancel "zoom-gt"
           else
              move 1 to stato-zoom
           end-if.
           perform CANCELLA-TMP.

           if stato-zoom = 0
              move zoom-tor-numero to numero
              display ef-num
           end-if 
           .
      * <TOTEM:END>

       CONTROLLA-IMPOSTE.
      * <TOTEM:PARA. CONTROLLA-IMPOSTE>
           set imposte-ko to false.
           |Controllo che I.Merce, le imposte
           |e l'addizionale merce sia corretto
           |Solo per ordini x' le note vengono
           |inserite a mano
           if prenotabile and ror-no-omaggio
              move ror-cod-articolo to art-codice
              read articoli no lock invalid continue end-read
              if art-si-cobat and tcl-si-piombo
                 move ror-prz-unitario   to como-prz-unitario
                 move art-marca-prodotto to tpb-marca
                 |IN CASO DI EVASIONE DA PIU' MASTER
                 if tor-data-ordine = 0
                    if tor-da-ordine-si
                       move ror-chiave-ordine-testa to mto-chiave
                       read mtordini no lock 
                            invalid continue
                        not invalid
                            move mto-data-ordine to como-data-ordine
                       end-read
                    end-if
                    if como-data-ordine = 0
                       accept como-data-ordine from century-date
                    end-if
                 end-if
                 move como-data-ordine to tpb-data
                 move tor-cod-cli      to como-prm-cliente
                 move tor-prg-destino  to como-prm-destino
                 perform ADDIZIONALE-PIOMBO
              else
                 move 0 to add-piombo
              end-if   

              if tor-prg-destino = 0
                 move cli-nazione to naz-codice
              else
                 move des-nazione to naz-codice
              end-if
              read tnazioni no lock

              if naz-imp-esenti-si
                 move 0 to add-piombo
                 move 0 to imposta-consumo
                 move 0 to imposta-cou-cobat
                 move 0 to imponibile-merce
              else
                 move ror-prg-chiave to prg-chiave
                 read progmag  no lock invalid continue end-read
                 move art-marca-prodotto to mar-codice
                 read tmarche  no lock invalid continue end-read
                 perform CALCOLA-IMPOSTE
                 |Faccio la move in una varibile a 2 decimali
                 move risultato-imposte to imposta-cou-cobat
                 |risultato-imposte contiene il COBAT o il COU
                 |in base all'assoggetamento dell'articolo.
                 |Se c'è COU non c'è COBAT e viceversa.
              end-if

              if not TrattamentoGDO
                 move ror-prz-unitario to imponibile-merce
              else
                 compute imponibile-merce  =
                         ror-prz-unitario  -
                         add-piombo        -
                         imposta-cou-cobat -
                         imposta-consumo
              end-if

              if ror-add-piombo    not = add-piombo        or
                 ror-imp-consumo   not = imposta-consumo   or
                 ror-imp-cou-cobat not = imposta-cou-cobat or
                 ror-imponib-merce not = imponibile-merce
                 set errori          to true
                 move riga           to store-riga
                 set  imposte-ko     to true
              end-if
           end-if 
           .
      * <TOTEM:END>

