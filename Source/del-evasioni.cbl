                              icon 2
                end-if

           when 78-ID-ef-num-to
                inquire ef-num-to, value in num-to
                if num-to = 0
                   move 99999999 to num-to ef-num-to-buf
                   display ef-num-to
                end-if
                inquire ef-num-from, value in num-from
                if num-from > num-to
                   set errori to true
                   move 78-ID-ef-num-from to control-id
                   display message "Range errato"
                             title tit-err
                              icon 2
                end-if

           end-evaluate.

           if errori
              move 4 to accept-control
           end-if 
           .
      * <TOTEM:END>

