      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id   
           when 78-ID-ef-cli
                set cli-tipo-c  to true
                inquire ef-cli, value in cli-codice
                move "clienti" to como-file
                call "zoom-gt"  using como-file, cli-rec
                               giving stato-zoom
                end-call
                cancel "zoom-gt"

                if stato-zoom = 0
                   move cli-codice     to ef-cli-buf
                   move cli-ragsoc-1   to lab-cli-buf
                   display ef-cli lab-cli
                end-if

           when 78-ID-ef-gdo
                inquire ef-gdo, value in gdo-codice
                move "tgrupgdo" to como-file
                call "zoom-gt"  using como-file, gdo-rec
                               giving stato-zoom
                end-call
                cancel "zoom-gt"

                if stato-zoom = 0
                   move gdo-codice       to ef-gdo-buf
                   move gdo-intestazione to lab-gdo-buf
                   display ef-gdo lab-gdo
                end-if
                
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-ID-ef-anno
                if anno = 0
                   display message box "Inserimento anno mancante!"
                           title = tit-err
                           icon 2
                   move 78-ID-ef-anno to control-id
                   move 4             to accept-control
                   set errori         to true
                end-if
      *
           when 78-ID-ef-num-to
                if e-num = 1                                
                   inquire ef-num-from, value in ef-num-from-buf
                   inquire ef-num-to,   value in ef-num-to-buf
                   if ef-num-to-buf = 0
                      move all "9" to ef-num-to-buf
                      display ef-num-to
                   end-if
                   if ef-num-to-buf < ef-num-from-buf
                      set errori to true
                      display message box "Intervallo limiti numero erra
      -    "to!"
                              title = tit-err
                              icon 2
                      move 78-ID-ef-num-from to control-id
                   end-if
                end-if
      *
           when 78-ID-ef-lotto
                if e-lotto = 1
                   inquire ef-lotto, value in ef-lotto-buf
                   if ef-lotto-buf = 0
                      set errori to true
                      display message box "Inserimento numero di lotto m
      -    "ancante!"
                              title = tit-err
                              icon 2
                      move 78-ID-ef-lotto to control-id
                   end-if
                end-if  
      *
           when 78-ID-ef-cli
                inquire ef-cli, value in cli-codice
                if cli-codice = 0
                   move "TUTTI I CLIENTI" to cli-ragsoc-1
                else
                   set cli-tipo-C to true
                   read clienti no lock
                        invalid
                        move spaces to cli-ragsoc-1
                        set errori to true
                        display message "Cliente non valido"
                                  title tit-err
                                   icon 2
                   end-read
                end-if
                move cli-ragsoc-1 to lab-cli-buf
                display lab-cli
      *
           when 78-ID-ef-gdo
                inquire ef-gdo, value in gdo-codice
                if gdo-codice = spaces
                   move "TUTTI I GRUPPI" to gdo-intestazione
                else
                   read tgrupgdo no lock
                        invalid
                        move spaces to gdo-intestazione
                        set errori to true
                        display message "Gruppo GDO non valido"
                                  title tit-err
                                   icon 2
                   end-read
                end-if
                move gdo-intestazione to lab-gdo-buf
                display lab-gdo
      *
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move control-id to store-id
              move 4          to accept-control
           end-if.

           display form1 
           .
      * <TOTEM:END>

