       PB-ESEGUI-LinkTo.
      * <TOTEM:PARA. PB-ESEGUI-LinkTo>
           perform varying control-id from 78-ID-ef-gdo by 1
                     until control-id    > 78-ID-ef-gdo
              perform CONTROLLO

              if errori exit perform end-if
           end-perform.

           if tutto-ok

              initialize stgdo-p-linkage

              if tipo-stampa = 1
                 set  stgdo-p-gdo to true
                 move ef-gdo-buf  to stgdo-p-codice
              else
                 move cli-gdo     to stgdo-p-codice
                 move ef-cli-buf  to stgdo-p-cliente
                 set  stgdo-p-cli to true
              end-if

              move user-codi   to stgdo-p-user
              modify lab, visible true
              call   "stgdo-p" using stgdo-p-linkage
              cancel "stgdo-p"
              modify lab, visible false

              modify pb-esegui,  bitmap-number = 1
              modify Form1-Pb-2, bitmap-number = 1

              move 78-ID-ef-gdo  to control-id      
              move 4             to accept-control  

           end-if 
           .
