      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-gdo
                move "tgrupgdo"  to como-file
                inquire ef-gdo,  value in gdo-codice
      
                call "zoom-gt"   using como-file, gdo-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move gdo-codice to ef-gdo-buf
                   display ef-gdo
                end-if

           when 78-ID-ef-cli
                move "clienti-all"   to como-file
                set cli-tipo-C   to true
                inquire ef-cli,  value in cli-codice
      
                call "zoom-gt"   using como-file, cli-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move cli-codice to ef-cli-buf
                   display ef-cli
                end-if

           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID

           when 78-ID-ef-gdo
                if tipo-stampa = 1
                   inquire ef-gdo value in ef-gdo-buf
                   if ef-gdo-buf = spaces
                      move 78-des-gdo-std to lab-gdo-buf
                   else
                      move ef-gdo-buf to gdo-codice
                      read tgrupgdo no lock
                           invalid
                           move spaces to gdo-intestazione
                           set errori  to true
                           display message "Gruppo GDO non valido"
                                     title tit-err
                                      icon 2
                      end-read
                      move gdo-intestazione to lab-gdo-buf
                   end-if
                   display lab-gdo
                end-if

           when 78-ID-ef-cli
                if tipo-stampa = 2
                   inquire ef-cli, value in cli-codice
                   if cli-codice = 0
                      move 78-des-gdo-std to lab-gdo-buf
                      move spaces         to cli-gdo
                   else
                      set cli-tipo-C to true
                      read clienti no lock
                           invalid
                           move spaces to cli-ragsoc-1
                           set errori  to true
                           display message "Codice Cliente non valido"
                                     title tit-err
                                      icon 2
                      end-read
                      move cli-ragsoc-1 to lab-cli-buf
                   end-if
                   display lab-cli
                end-if
                     
           end-evaluate.
                        
           if errori                  
              move store-id to control-id
              move 4        to accept-control
           end-if.

           perform CANCELLA-COLORE 
           .
      * <TOTEM:END>

