       pb-ok-LinkTo.
      * <TOTEM:PARA. pb-ok-LinkTo>
           perform varying control-id from 78-ID-ef-data by 1
                     until control-id > 78-ID-ef-art
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if tutto-ok
              if como-data  = 0  and 
                 art-codice = 0  and
                 gdo-codice = spaces 
                 display message "ATTENZIONE!!"
                          x"0d0a""Cancellare TUTTO il file?"
                           title titolo
                            type mb-yes-no
                            icon 2
                         default mb-no
                          giving scelta
                 if scelta = mb-yes
                    close       listini
                    open output listini
                    if status-listini = "00"
                       display message "Cancellazione effettuata con suc
      -    "cesso!"
                                 title titolo
                    else
                       display message "Cancellazione non effettuata!"
                                 title titolo
                    end-if
                    close       listini
                    open i-o    listini
                 end-if
              else
                 display message "ATTENZIONE!!"
                          x"0d0a""Procedere alla cancellazione?"
                           title titolo
                            type mb-yes-no
                            icon 2
                         default mb-no
                          giving scelta
                 if scelta = mb-yes
                    call "W$MOUSE" using set-mouse-shape, wait-pointer
                    move 0 to num-rec
                    perform DATE-TO-FILE
                    move low-value  to lst-rec
                    move gdo-codice to lst-gdo
                    move como-data  to lst-data
                    if art-codice = 0
                       start listini key >= lst-chiave
                             invalid continue
                         not invalid
                             perform until 1 = 2
                                read listini next at end exit perform 
           end-read
                                if gdo-codice not = spaces  and
                                   gdo-codice not = lst-gdo
                                   exit perform
                                end-if
                                if como-data not = 0 and
                                   como-data not = lst-data
                                   exit perform
                                end-if
                                delete listini record
                                       invalid continue
                                end-delete
                                add 1 to num-rec
                             end-perform
                       end-start
                    else
                       move art-codice to lst-articolo
                       start listini key >= lst-k-gdo-articolo
                             invalid continue
                         not invalid
                             perform until 1 = 2
                                read listini next at end exit perform 
           end-read
                                if gdo-codice not = spaces and
                                   gdo-codice not = lst-gdo
                                   exit perform
                                end-if
                                if gdo-codice not = spaces  and
                                   art-codice not = lst-articolo
                                   exit perform
                                end-if
                                if lst-articolo = art-codice
                                   if como-data not = 0
                                      if como-data <= lst-data
                                         delete listini record
                                                invalid continue
                                         end-delete
                                         add 1 to num-rec
                                      end-if
                                   else
                                      delete listini record
                                             invalid continue
                                      end-delete
                                      add 1 to num-rec
                                   end-if
                                end-if
                             end-perform
                       end-start
                    end-if
                 end-if
                 if num-rec = 0
                    display message "Nessuna cancellazione effettuata"
                              title titolo
                               icon 2
                 else
                    display message "Effettuate "num-rec, " cancellazion
      -    "i"
                              title titolo
                               icon 2
                 end-if
              end-if
              modify pb-ok, bitmap-number = 1
              move 78-ID-ef-data to control-id
              move 4             to accept-control
              call "W$MOUSE"  using set-mouse-shape, arrow-pointer
           end-if 
           .
      * <TOTEM:END>
