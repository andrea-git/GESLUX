       SCRIVI-ELENCO.
           set trovato to true.
           if ElaboraListini
              if prima-volta
                 set prima-volta to false
                 write line-riga from spaces
                 move "Elenco articolo/blister già presente in listini"
                   to line-riga
                 write line-riga
                 write line-riga from spaces
                 string "GDO  "      delimited size
                        "  "         delimited size
                        "Valido dal" delimited size
                        "  "         delimited size
                        "   Prezzo"  delimited size
                        into line-riga
                 end-string
                 write line-riga
                 move "+---------------------------+" to line-riga
                 write line-riga
              end-if
              initialize line-riga
              move lst-prezzo to r-prz
              string lst-gdo delimited size
                     "  "    delimited size
                     lst-data(7:2) delimited size
                     "/"           delimited size
                     lst-data(5:2) delimited size
                     "/"           delimited size
                     lst-data(1:4) delimited size
                     into line-riga
              end-string
              write line-riga
           else
              if prima-volta
                 set prima-volta to false
                 write line-riga from spaces
                 move "=================================================
      -    "===========================" to line-riga
                 write line-riga
                 write line-riga from spaces
                 move"Elenco articolo/blister già presente in volantini"
                   to  line-riga
                 write line-riga
                 write line-riga from spaces
                 string "GDO  "          delimited size
                        "  "             delimited size
                        "Inizio DPO"     delimited size
                        "  "             delimited size
                        " Fine DPO "     delimited size
                        "  "             delimited size
                        "   Prezzo"      delimited size
                        "  "             delimited size
                        "Nome Volantino" delimited size
                        into line-riga
                 end-string
                 write line-riga
                 move "+------------------------------------------------
      -    "--------------------------+" to line-riga
                 write line-riga
              end-if
              move rpr-prz-acq to r-prz
              move rpr-codice  to tpr-codice
              read tpromo no lock invalid continue end-read
              string tpr-gdo           delimited size
                     "  "              delimited size
                     tpr-ini-dpo(7:2)  delimited size
                     "/"               delimited size
                     tpr-ini-dpo(5:2)  delimited size
                     "/"               delimited size
                     tpr-ini-dpo(1:4)  delimited size
                     "  "              delimited size
                     tpr-fine-dpo(7:2) delimited size
                     "/"               delimited size
                     tpr-fine-dpo(5:2) delimited size
                     "/"               delimited size
                     tpr-fine-dpo(1:4) delimited size
                     "  "              delimited size
                     r-prz             delimited size
                     "  "              delimited size
                     tpr-descrizione   delimited size
                     into line-riga
              end-string
              write line-riga
           end-if 
           .
