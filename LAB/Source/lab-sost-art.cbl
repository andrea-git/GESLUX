      * USER DEFINE PARAGRAPH
       CERCA.
      * <TOTEM:PARA. CERCA>
           evaluate control-id
           when 78-ID-ef-gdo
                inquire ef-gdo, value in gdo-codice
                move   "tgrupgdo"     to como-file
                call   "zoom-gt"   using como-file, gdo-rec
                                  giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move gdo-codice       to ef-gdo-buf
                   move gdo-intestazione to lab-gdo-buf
                   display ef-gdo lab-gdo
                end-if

           when 78-ID-ef-orig
                inquire ef-orig, value in art-codice
                move   "articoli"     to como-file
                call   "zoom-gt"   using como-file, art-rec
                                  giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move art-codice      to ef-orig-buf
                   move art-descrizione to lab-orig-buf
                   display ef-orig lab-orig
                end-if

           when 78-ID-ef-nuovo
                inquire ef-nuovo, value in art-codice
                move   "articoli"     to como-file
                call   "zoom-gt"   using como-file, art-rec
                                  giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move art-codice      to ef-nuovo-buf
                   move art-descrizione to lab-nuovo-buf
                   display ef-nuovo lab-nuovo
                end-if
                
           end-evaluate 
           .
      * <TOTEM:END>

       CONTROLLO.
      * <TOTEM:PARA. CONTROLLO>
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-ID-ef-gdo
                inquire ef-gdo, value in gdo-codice
                if gdo-codice = spaces
                   move "Tutti i gruppi GDO" to lab-gdo-buf
                else
                   read tgrupgdo no lock
                        invalid
                        set errori to true
                        display message "Gruppo GDO non valido"
                                  title tit-err
                                   icon 2
                        move spaces to lab-gdo-buf
                    not invalid
                         move gdo-intestazione to lab-gdo-buf
                   end-read
                end-if
                display lab-gdo
      *
           when 78-ID-ef-orig
                inquire ef-orig, value in ef-orig-buf
                move ef-orig-buf   to art-codice
                read articoli no lock
                     invalid
                     move art-codice to bli-codice
                     read blister no lock
                          invalid
                          move spaces to art-descrizione
                          set errori to true
                          display message "Codice non valido"
                                    title tit-err
                                     icon 2
                      not invalid
                          move bli-descrizione to art-descrizione
                     end-read
                end-read
                move art-descrizione to lab-orig-buf
                display lab-orig
      *
           when 78-ID-ef-nuovo
                inquire ef-orig,  value in ef-orig-buf
                inquire ef-nuovo, value in ef-nuovo-buf
                inquire ef-nuovo, value in art-codice
                read articoli no lock
                     invalid
                     move art-codice to bli-codice
                     read blister no lock
                          invalid
                          move spaces to art-descrizione
                          set errori to true
                          display message "Articolo non valido"
                                    title tit-err
                                     icon 2
                      not invalid
                          move bli-descrizione to art-descrizione
                     end-read
                end-read
                move art-descrizione to lab-nuovo-buf
                display lab-nuovo
                if ef-orig-buf = ef-nuovo-buf
                   set errori to true
                   display message "I codici devono essere differenti"
                             title tit-err
                              icon 2
                   move 78-ID-ef-orig to control-id
                end-if
      *               
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move control-id to store-id
              move 4          to accept-control
           end-if.

           display form1 
           .
      * <TOTEM:END>

