       pb-ok-LinkTo.
      * <TOTEM:PARA. pb-ok-LinkTo>
           perform varying control-id from 78-ID-ef-gdo by 1
                     until control-id > 78-ID-ef-nuovo
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.
           set trovato to false.

           if tutto-ok
              display message "ATTENZIONE!!"
                       x"0d0a""Procedere alla sostituzione su listini e 
      -    "volantini?"
                        title titolo
                         type mb-yes-no
                         icon 2
                      default mb-no
                       giving scelta
              if scelta = mb-yes
                 accept como-data from century-date
                 accept como-ora  from time
                 initialize wstampa
                 accept  wstampa from environment "PATH_ST"
                 inspect wstampa replacing trailing spaces by low-value
                 string  wstampa        delimited low-value
                         "LAB-SOST-ART" delimited size
                         "_"            delimited size
                         como-data      delimited size
                         "_"            delimited size
                         como-ora       delimited size
                         into wstampa
                 end-string
                 open output lineseq

                 call "W$MOUSE"  using set-mouse-shape, wait-pointer
                 move 0 to num-rec-l num-rec-p
                 move low-value to lst-rec
                 start listini key >= lst-chiave
                       invalid continue
                   not invalid
                       set prima-volta    to true
                       set ElaboraListini to true
                       perform until 1 = 2
                          read listini next at end exit perform end-read
                          if lst-articolo = ef-orig-buf
                             if ef-gdo-buf = lst-gdo or 
                                ef-gdo-buf = spaces
                                move lst-chiave   to save-lst-chiave
                                move ef-nuovo-buf to lst-articolo
                                write lst-rec
                                      invalid
                                      perform SCRIVI-ELENCO
                                  not invalid
                                      add 1 to num-rec-l
                                      move save-lst-chiave to lst-chiave
                                      delete listini record
                                            invalid continue
                                      end-delete
                                      start listini key >= lst-chiave
                                            invalid exit perform
                                      end-start
                                end-write
                             end-if
                          end-if
                       end-perform
                 end-start

                 move low-value to rpr-rec
                 start rpromo key >= rpr-chiave
                       invalid continue
                   not invalid
                       set prima-volta  to true
                       set ElaboraPromo to true
                       perform until 1 = 2
                          read rpromo next at end exit perform end-read
                          if rpr-articolo = ef-orig-buf
                             move rpr-codice to tpr-codice
                             read tpromo no lock invalid continue 
           end-read
                             if ef-gdo-buf = tpr-gdo or
                                ef-gdo-buf = spaces
                                move rpr-chiave   to save-rpr-chiave
                                move ef-nuovo-buf to rpr-articolo
                                write rpr-rec 
                                      invalid
                                      perform SCRIVI-ELENCO
                                  not invalid
                                      add 1 to num-rec-p
                                      move save-rpr-chiave to rpr-chiave
                                      delete rpromo record 
                                             invalid continue 
                                      end-delete
                                      start rpromo key >= rpr-chiave 
                                            invalid exit perform
                                      end-start
                                end-write
                             end-if
                          end-if
                       end-perform
                 end-start

                 if ( num-rec-l + num-rec-p) = 0
                    display message "Nessuna sostituzione effettuata"
                              title titolo
                               icon 2
                 else
                    display message"Effettuate:"
                            x"0d0a"num-rec-l, " sostituzioni su listini"
                            x"0d0a"num-rec-p, " sostituzioni su promo"
                              title titolo
                               icon 2
                 end-if

                 if trovato
                    call   "spooler-a" using "A", wstampa, "V"
                    cancel "spooler-a"
                 end-if
                 close       lineseq
                 delete file lineseq

              end-if
              modify pb-ok, bitmap-number = 1
              move 78-ID-ef-orig to control-id
              move 4             to accept-control
              call "W$MOUSE"  using set-mouse-shape, arrow-pointer

           end-if.

