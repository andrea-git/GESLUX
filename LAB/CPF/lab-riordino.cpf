BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-ok-LinkTo
BEGIN-TEXT
           move CONTROL-ID to mem-id.

           perform  varying CONTROL-ID from 78-ID-ef-cli by 1
                      until CONTROL-ID > 78-ID-gd-art
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if tutto-ok
              call "W$MOUSE" using set-mouse-shape, wait-pointer
              initialize riordino-linkage replacing numeric data by zeroes
                                               alphanumeric data by spaces
              move ef-data-buf to como-data
              perform DATE-TO-FILE
              move como-data    to riord-data
              move form1-handle to riord-handle
              move ef-cli-buf   to riord-cliente
              move lab-cli-buf  to riord-ragsoc
              move ef-des-buf   to riord-destino
              move lab-des-buf  to riord-localita
              inquire cbo-tipo, value in cbo-tipo-buf
              evaluate cbo-tipo-buf
              when "Inevasi"  set riord-inevasi to true
              when "Bolle"    set riord-bolle   to true
              when "Fatture"  set riord-fatture to true
              when "Tutti"    set riord-tutti   to true
              end-evaluate

              move 0 to riord-idx
              perform varying riga from 2 by 1 
                        until riga > tot-righe
                 add 1 to riord-idx
                 inquire gd-art(riga, 1), 
                         cell-data in riord-el-articolo(riord-idx)
              end-perform

              call   "lab-riordino-p" using riordino-linkage
              cancel "lab-riordino-p"
              move 78-ID-ef-cli to control-id     
              move 4            to accept-control 
              call "W$MOUSE" using set-mouse-shape, arrow-pointer
           end-if.            
END-TEXT
