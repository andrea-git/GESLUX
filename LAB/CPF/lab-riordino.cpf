BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-cli
                inquire ef-cli, value in cli-codice
                set cli-tipo-c to true
                move   "clienti-all"  to como-file
                call   "zoom-gt"   using como-file, cli-rec
                                  giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move cli-codice   to ef-cli-buf
                   move cli-ragsoc-1 to lab-cli-buf
                   display ef-cli lab-cli
                end-if

           when 78-ID-ef-des
                inquire ef-cli,  value in des-codice
                inquire ef-des, value in des-prog
                move "clienti-des"   to como-file
                call   "zoom-gt"  using como-file, des-rec
                                 giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move des-prog     to ef-des-buf
                   move des-localita to lab-des-buf
                   display ef-des lab-des
                   move 4 to accept-control
                end-if

           when 78-ID-gd-art
                inquire gd-art, cursor-y in riga
                inquire gd-art(riga, 1)
                        cell-data in art-codice
                move "articoli"      to como-file
                call   "zoom-gt"  using como-file, art-rec
                                 giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   set tutto-ok to true
                   move riga to store-riga
                   inquire gd-art, last-row in tot-righe
                   add 1 to tot-righe
                   perform TROVA-ARTICOLO-GIA-INSERITO
                   if tutto-ok
                      move store-riga      to riga
                      move art-codice      to col-codice
                      move art-descrizione to col-descrizione
                      modify gd-art(riga, 1), cell-data col-codice
                      modify gd-art(riga, 2), cell-data col-descrizione
                   end-if
                end-if
                
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA-2
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-bolla           
                perform RIEMPI-TMP
                move "zoom-tordini-b"   to como-file

                inquire ef-anno,  value in zoom-tor-anno
                inquire ef-bolla, value in zoom-tor-numero

                if trovato
                   move path-zoom-tordini to ext-file
                   call "zoom-gt"   using como-file, zoom-tor-rec
                                   giving stato-zoom
                   end-call
                   cancel "zoom-gt"
                else
                   move 1 to stato-zoom
                end-if
                perform CANCELLA-TMP

                if stato-zoom = 0
                   move zoom-tor-numero to ef-bolla-buf
                   display ef-bolla
                end-if
           end-evaluate.

      ***---
       RIEMPI-TMP.
           accept  path-zoom-tordini      from environment "PATH_ST".
           accept  como-data              from century-date.
           accept  como-ora               from time.
           inspect path-zoom-tordini      replacing trailing
                                          spaces by low-value.
           string path-zoom-tordini       delimited by low-value
                  "zoom-tordini"          delimited by size
                  "_"                     delimited by size
                  como-data               delimited by size
                  "_"                     delimited by size
                  como-ora                delimited by size
                  into path-zoom-tordini
           end-string.

           set trovato to false.
           open output zoom-tordini.
           initialize tor-rec.
           move ef-anno-buf to tor-anno-bolla.
           
           start tordini key  is >= k-bolla
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read tordini next no lock at end exit perform end-read

                    if tor-anno-bolla not = ef-anno-buf or
                       tor-anno-bolla = 0
                       exit perform
                    end-if

                    move tor-anno-bolla     to zoom-tor-anno
                    move tor-num-bolla      to zoom-tor-numero
                    move tor-data-bolla     to zoom-tor-data-ordine
                    move tor-causale        to tca-codice
                    read tcaumag invalid continue end-read
                    move tca-caus-trasporto to zoom-tor-causale
                    move tor-cod-cli        to zoom-tor-cod-cli
                                               cli-codice
                                               des-codice
                    set cli-tipo-C          to true
                    move tor-prg-destino    to zoom-tor-prg-destino
                                               des-prog

                    read clienti no lock
                         invalid initialize cli-rec
                    end-read
                    move cli-ragsoc-1       to zoom-tor-cli-ragsoc-1

                    read destini no lock
                         invalid
                            initialize des-rec
                            move cli-localita to des-localita
                    end-read
                    move des-localita       to zoom-tor-des-localita

                    write zoom-tor-rec invalid continue end-write
                    set trovato to true

                 end-perform
           end-start.

           close zoom-tordini.

      ***---
       CANCELLA-TMP.
           delete file zoom-tordini.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
      *                       
           when 78-ID-ef-data
                inquire ef-data, value in como-data
                if como-data = 0
                   accept como-data from century-date
                   perform DATE-TO-SCREEN
                end-if
                perform DATE-FORMAT
                move como-data to ef-data-buf
                perform DATE-TO-FILE
                if como-data > data-oggi
                   move data-oggi to como-data
                   perform DATE-TO-SCREEN
                   move como-data to ef-data-buf
                end-if
                display ef-data
      *    
           when 78-ID-ef-cli
                inquire ef-cli, value in cli-codice
                set cli-tipo-C to true
                read clienti no lock
                     invalid
                     move spaces to cli-ragsoc-1
                     set errori to true
                     display message "Cliente non valido"
                               title tit-err
                                icon 2
                end-read
                move cli-ragsoc-1 to lab-cli-buf
                display lab-cli
      *    
           when 78-ID-ef-des                        
                move cli-codice to des-codice
                inquire ef-des, value in des-prog
                read destini no lock
                     invalid
                     move spaces to des-localita
                     set errori to true
                     display message "Destino non valido"
                               title tit-err
                                icon 2
                end-read
                move des-localita to lab-des-buf
                display lab-des
      *
           when 78-ID-gd-art
                inquire gd-art, last-row in tot-righe
                if tot-righe < 2
                   set errori to true
                   display message "Selezionare almeno un articolo"
                             title tit-err
                              icon 2
                end-if
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to STORE-ID        
              move 4          to ACCEPT-CONTROL  
           end-if.

           display form1.
END-TEXT
END-DATA
