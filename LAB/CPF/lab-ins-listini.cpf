   PARAGRAPH         = pb-ok-LinkTo
BEGIN-TEXT
           move CONTROL-ID to mem-id.

           perform  varying CONTROL-ID from 78-ID-ef-gdo by 1
                      until CONTROL-ID > 78-ID-ef-prezzo
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if tutto-ok
              perform INSERISCI-LISTINO
              perform INIT-SCREEN
              modify pb-ok,        bitmap-number = 1
              perform CANCELLA-COLORE
              move 78-ID-ef-gdo to control-id
              move 4            to accept-control
           end-if.            
