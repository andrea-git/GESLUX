BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-gdo
                inquire ef-gdo, value in gdo-codice
                move   "tgrupgdo"     to como-file
                call   "zoom-gt"   using como-file, gdo-rec
                                  giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move gdo-codice       to ef-gdo-buf
                   move gdo-intestazione to lab-gdo-buf
                   display ef-gdo lab-gdo
                end-if

           when 78-ID-ef-art
                inquire ef-art, value in art-codice
                move   "articoli"     to como-file
                call   "zoom-gt"   using como-file, art-rec
                                  giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move art-codice      to ef-art-buf
                   move art-descrizione to lab-art-buf
                   display ef-art lab-art
                end-if
                
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
      *    
           when 78-ID-ef-gdo
                inquire ef-gdo, value in gdo-codice
                read tgrupgdo no lock
                     invalid
                     move spaces to gdo-intestazione
                     set errori to true
                     display message "Gruppo GDO non valido"
                               title tit-err
                                icon 2
                end-read
                move gdo-intestazione to lab-gdo-buf
                display lab-gdo

           when 78-ID-ef-data
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-buf
                display ef-data

           when 78-ID-ef-art
                inquire ef-art, value in art-codice
                 read articoli no lock
                      invalid
                      move art-codice to bli-codice
                      read blister no lock
                           invalid
                           move spaces to lab-art-buf
                           set errori to true
                           display message "Articolo non valido"
                                     title tit-err
                                      icon 2
                       not invalid
                           move bli-descrizione to lab-art-buf
                      end-read
                  not invalid
                      move art-descrizione to lab-art-buf
                 end-read
                display lab-art
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to STORE-ID        
              move 4          to ACCEPT-CONTROL  
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "color-custom.cpy".
           copy "utydata.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INSERISCI-LISTINO
BEGIN-TEXT
LUBEXX     |Richiesta di Walter 18/09/07. Se il cod art cli
LUBEXX     |è vuoto recupero l'ultimo in base alla data
LUBEXX     if ef-art-cli-buf = spaces
              move ef-gdo-buf      to lst-gdo
              move ef-art-buf to lst-articolo
              move high-value to lst-data
              start listini key not > lst-k-articolo
                 invalid 
                    continue
                 not invalid
                    perform until 1 = 2
                       read listini previous 
                          at end 
                             exit perform 
                       end-read
                       if lst-gdo      not = ef-gdo-buf   or
                          lst-articolo not = ef-art-buf
                          exit perform
                       end-if
                       if lst-cod-art-cli not = spaces
                          move lst-cod-art-cli to ef-art-cli-buf
                          exit perform
                       end-if
                    end-perform
              end-start
           end-if
           ||||

           initialize lst-rec replacing numeric data by zeroes
                                   alphanumeric data by spaces

           move ef-gdo-buf      to lst-gdo
           move ef-data-buf     to como-data
           move como-data(1:2)  to lst-data(7:2)
           move como-data(3:2)  to lst-data(5:2)
           move como-data(5:4)  to lst-data(1:4)
           move ef-art-buf      to lst-articolo

           read listini no lock
              invalid
                 perform SCRIVI-LISTINO
              not invalid
                 display message box "Riga Listino già inserita."
                                     x"0D0A"
                                     "Sovrascirvere i dati?"
                          type mb-yes-no
                          giving scelta
                          title titolo
                 if scelta = mb-yes
                    perform RISCRIVI-LISTINO

                 end-if
           end-read.

      ***---
       SCRIVI-LISTINO.
           move ef-art-cli-buf  to lst-cod-art-cli 
           move ef-prezzo-buf   to lst-prezzo
           accept lst-data-modifica   from century-date
           accept lst-ora-modifica    from time
           move user-codi          to lst-utente-modifica
                                      lst-utente-creazione
           move lst-data-modifica  to lst-data-creazione
           move lst-ora-modifica   to lst-ora-creazione

           write lst-rec
              invalid
                 rewrite lst-rec
           end-write.

      ***---
       RISCRIVI-LISTINO.
           move ef-art-cli-buf  to lst-cod-art-cli 
           move ef-prezzo-buf   to lst-prezzo

           accept lst-data-modifica   from century-date
           accept lst-ora-modifica    from time
           move user-codi to  lst-utente-modifica
           rewrite lst-rec 
              invalid
                 continue
           end-rewrite.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-SCREEN
BEGIN-TEXT
           initialize ef-gdo-buf
                      lab-gdo-buf
                      ef-art-buf
                      lab-art-buf
                      ef-art-cli-buf
                      ef-prezzo-buf

           accept como-data from century-date.
           perform DATE-TO-SCREEN.
           move como-data to ef-data-buf.

           modify ef-gdo     value ef-gdo-buf
           modify lab-gdo    title lab-gdo-buf.
           modify ef-data    value ef-gdo-buf.
           modify ef-art     value ef-art-buf.
           modify lab-art    title lab-art-buf.
           modify ef-art-cli value ef-art-cli-buf.
           modify ef-prezzo  value ef-prezzo-buf.
