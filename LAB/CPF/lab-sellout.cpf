BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-ok-LinkTo
BEGIN-TEXT
           perform varying control-id from 78-ID-ef-data by 1
                     until control-id >    78-ID-ef-data
              perform CONTROLLO
              if errori
                 exit perform
              end-if
           end-perform.

           if tutto-ok
              inquire ef-data, value in ef-data-buf
              move ef-data-buf to como-data
              perform DATE-TO-FILE

              call "W$MOUSE" using set-mouse-shape, wait-pointer
              call   "lab-sellout-p" using como-data,
                                           form1-handle
              cancel "lab-sellout-p"
              move 78-ID-ef-data  to control-id
              move 4              to accept-control
              perform CANCELLA-COLORE
              modify pb-ok,       bitmap-number = 1
              modify pb-annulla,  bitmap-number = 1
              call "W$MOUSE" using set-mouse-shape, arrow-pointer
           end-if.

      *****     inquire cbo-mese, value in cbo-mese-buf.
      *****     evaluate cbo-mese-buf
      *****     when "Gennaio"    move 01 to como-mese
      *****     when "Febbraio"   move 02 to como-mese
      *****     when "Marzo"      move 03 to como-mese
      *****     when "Aprile"     move 04 to como-mese
      *****     when "Maggio"     move 05 to como-mese
      *****     when "Giugno"     move 06 to como-mese
      *****     when "Luglio"     move 07 to como-mese
      *****     when "Agosto"     move 08 to como-mese
      *****     when "Settembre"  move 09 to como-mese
      *****     when "Ottobre"    move 10 to como-mese
      *****     when "Novembre"   move 11 to como-mese
      *****     when "Dicembre"   move 12 to como-mese
      *****     end-evaluate.
      *****
      *****     move anno-attuale to como-anno.
      *****     if como-mese < mese-attuale
      *****        add 1 to como-anno
      *****     end-if.

      *****     call "W$MOUSE" using set-mouse-shape, wait-pointer.
      *****     call   "lab-sellout-p" using como-data,
      *****                                  como-anno
      *****                                  form1-handle.
      *****     cancel "lab-sellout-p".
      *****     move 78-ID-cbo-mese to control-id.
      *****     move 4              to accept-control .
      *****     perform CANCELLA-COLORE.
      *****     modify pb-ok,       bitmap-number = 1.
      *****     modify pb-annulla,  bitmap-number = 1.
      *****     call "W$MOUSE" using set-mouse-shape, arrow-pointer.
END-TEXT
END-DATA
