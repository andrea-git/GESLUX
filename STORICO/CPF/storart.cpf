BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = scr-elab
   ACTOBJ            = scr-elab
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           call "W$MOUSE" using set-mouse-shape, wait-pointer.

           if tipo-op = 1
              move "Storicizzazione in corso..." to lab1-buf
           else
              move "Ripristino in corso..."      to lab1-buf
           end-if.
           move 0 to v-push.
           display scr-elab.

           move tipo-op         to storart-tipo-op.
           move ef-anno-da-buf  to storart-anno-da.
           move ef-anno-a-buf   to storart-anno-a.
           move scr-elab-handle to storart-handle.

           call   "storart-p" using storart-linkage.
           cancel "storart-p".

           move 1 to v-push.
           display pb-fatto.
           modify  pb-fatto, bitmap-number = 2.

           call "W$MOUSE" using set-mouse-shape, arrow-pointer.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.
           evaluate control-id

           when 78-ID-ef-anno-da
                inquire ef-anno-da, value in ef-anno-da-buf
                if ef-anno-da-buf = 0
                   set errori to true
                   display message "Anno obbligatorio"
                             title tit-err
                              icon 2
                end-if

           when 78-ID-ef-anno-a             
                inquire ef-anno-da, value in ef-anno-da-buf
                inquire ef-anno-a, value in ef-anno-a-buf
                if ef-anno-da-buf > ef-anno-a-buf
                   set errori to true
                   move 78-ID-ef-anno-da to control-id
                   display message "Range anno errato"
                             title tit-err
                              icon 2
                end-if


           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move 4 to accept-control
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "color-custom.cpy".                                            
           copy "calcola-colore-trasparente.cpy".
