BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = PB-ESEGUI-BeforeProcedure
BEGIN-TEXT
           modify PB-ESEGUI, bitmap-number 2.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = PB-ESEGUI-AfterProcedure
BEGIN-TEXT
           modify PB-ESEGUI, bitmap-number 1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = PB-ESEGUI-LinkTo
BEGIN-TEXT
           set tutto-ok to true.
           perform varying control-id from 78-ID-ef-anno-da by 1
                     until control-id > 78-ID-ef-anno-a
              perform CONTROLLO
              if errori
                 exit perform
              end-if
           end-perform.

           if tutto-ok
              display message "Confermi l'elaborazione?"
                        title titolo
                         icon 2
                         type mb-yes-no
                       default mb-no
                        giving scelta
              if scelta = mb-yes
                 modify Form1-Handle, visible false
                 perform SCR-ELAB-OPEN-ROUTINE
                 modify Form1-Handle, visible true
                 move 27 to key-status
              end-if
           else
              modify pb-esegui, bitmap-number = 1
           end-if
END-TEXT
END-DATA
