BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           inquire ef-codice, value in ef-codice-buf.
           move ef-codice-buf       to age-codice convert.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE-PROVINCIA
BEGIN-TEXT
           inquire ef-provincia, value in ef-provincia-buf.
           move ef-provincia-buf       to prv-codice.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE-NAZIONE
BEGIN-TEXT
