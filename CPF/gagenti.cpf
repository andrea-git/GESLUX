BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterBufToFld
BEGIN-TEXT
           move ef-codice-buf to age-codice convert.
                                  
           move 0 to idx.
           perform 10 times
              add  1  to idx
              move 0  to age-marca(idx)
              move 0  to age-perce-marca(idx)
           end-perform.

           if age-listino not = 0
              perform RESETTA-GRID
           else
              perform X-Y
              perform varying riga from 2 by 1
                        until riga > tot-righe
                 perform VALORE-RIGA
                 move col-marca to age-marca(riga - 1)
                 move col-perce to age-perce-marca(riga - 1)
              end-perform
           end-if.
END-TEXT
END-DATA
