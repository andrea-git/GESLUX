BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PUNTA-CELLA
BEGIN-TEXT
           move 78-ID-gd-marche    to control-id store-id.
           move 4                  to accept-control.
           modify gd-marche, cursor-y riga, 
                             cursor-x colonna.
           perform COLORE-RIGA.
END-TEXT
END-DATA
