   BASETYPE          = 2
   PARAGRAPH         = Screen1-Ta-1-Ev-Cmd-Tabchanged
BEGIN-TEXT
           perform CHANGE-TAB.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd-marche-Ev-Msg-Begin-Entry
BEGIN-TEXT
           if mod = 0
              set event-action to event-action-fail
           else
              inquire ef-listino, value in lis-codice
              if lis-codice not = 0
                 set event-action to event-action-fail
              else
                 perform X-Y
                 evaluate colonna
                 when 1 continue
                 when 2 set event-action to event-action-fail
                        move 1 to colonna
                        perform PUNTA-CELLA
                 when 3 perform VALORE-RIGA
                        if mar-codice = 0
                           set event-action to event-action-fail
                           move 1 to colonna
                           perform PUNTA-CELLA
                        end-if
                 end-evaluate
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = rb-age-normale-LinkTo
BEGIN-TEXT
           inquire ef-marg, value in age-marg.
           if age-marg = 0
              move 50 to ef-marg-buf
              display ef-marg
           end-if
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = rb-age-speciale-LinkTo
BEGIN-TEXT
           inquire ef-marg, value in age-marg.
           if age-marg = 0
              move 100 to ef-marg-buf
              display ef-marg
           end-if
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd-marche-Ev-Msg-Begin-Drag
BEGIN-TEXT
           perform SPOSTAMENTO.
END-TEXT
END-DATA
