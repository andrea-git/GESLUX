END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-reso-AfterProcedure
BEGIN-TEXT
           modify pb-reso, bitmap-number = 1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-reso-LinkTo
BEGIN-TEXT
           move mag-cau-reso to tca-codice.
           read tcaumag no lock 
                invalid
                display message "Impostare la causale per il reso all'interno del magazzino"
                          title tit-err
                           icon 2
            not invalid
                perform ELABORA-CREAZIONE
                modify pb-reso, bitmap-number = 2
                move 100 to control-id
                move   4 to accept-control
           end-read.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-diff-BeforeProcedure
BEGIN-TEXT
           modify pb-diff, bitmap-number = 4.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-diff-AfterProcedure
BEGIN-TEXT
           modify pb-diff, bitmap-number = 3.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-diff-LinkTo
BEGIN-TEXT
           move mag-cau-diff to tca-codice.
           read tcaumag no lock 
                invalid
                display message "Impostare la causale per il reso all'interno del magazzino"
                          title tit-err
                           icon 2
            not invalid
                perform ELABORA-CREAZIONE
                modify pb-diff, bitmap-number = 4
                move 200 to control-id
                move   4 to accept-control
           end-read.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ef-mag-BeforeProcedure
BEGIN-TEXT
      *<<** Customized_Default, SP-G, Screen2-Custom1-1 **>>
           move 1 to e-cerca.
           modify tool-cerca2, enabled e-cerca.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ef-mag-AfterProcedure
BEGIN-TEXT
      *<<** Customized_Default, SP-G, Screen2-Custom1-1 **>>
           inquire ef-mag, value in ef-mag-buf.
           move ef-mag-buf to mag-codice.
           read tmagaz  no lock
                invalid
                move 4 to accept-control
                display message "Magazzino non valido"
                         title tit-err
                          icon 2
                move spaces to lab-mag-buf
            not invalid
                move 0 to e-cerca
                modify tool-cerca2, enabled e-cerca
                move mag-descrizione to lab-mag-buf
           end-read.
           display lab-mag.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-CERCAa-LinkTo
BEGIN-TEXT
           inquire tool-cerca, enabled in e-cerca.

           if e-cerca = 1 
              inquire ef-mag     value mag-codice
              move "tmagaz"      to Como-File
              call   "zoom-gt" using como-file, mag-rec
                              giving stato-zoom
              cancel "zoom-gt"
              if stato-zoom = 0
                 move mag-codice       to ef-mag-buf
                 move mag-descrizione  to lab-mag-buf
                 display lab-mag ef-mag
              end-if             
           end-if.
END-TEXT
END-DATA

