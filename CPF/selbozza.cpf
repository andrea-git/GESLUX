BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate control-id
           when 1002
                perform RIEMPI-TMP

                inquire ef-anno, value in zoom-tor-anno
                inquire ef-num,  value in zoom-tor-numero

                move "zoom-fatture"    to como-file

                if trovato
                   move path-zoom-tordini to ext-file
                   call   "zoom-gt"   using como-file, zoom-tor-rec
                                     giving stato-zoom

                   cancel "zoom-gt"
                else
                   move 1 to stato-zoom
                end-if
                perform CANCELLA-TMP

                if stato-zoom = 0
                   move zoom-tor-numero to numero
                   display ef-num
                end-if

           when 1003
                perform RIEMPI-TMP-BOLLE

                inquire ef-anno,  value in zoom-tor-anno
                inquire ef-bolla, value in zoom-tor-numero

                move "zoom-fatture-b"   to como-file

                if trovato
                   move path-zoom-tordini to ext-file
                   call   "zoom-gt"   using como-file, zoom-tor-rec
                                     giving stato-zoom

                   cancel "zoom-gt"
                else
                   move 1 to stato-zoom
                end-if
                perform CANCELLA-TMP

                if stato-zoom = 0
                   move zoom-tor-numero to numero-b
                   display ef-bolla
                end-if
           end-evaluate.
END-TEXT
END-DATA
