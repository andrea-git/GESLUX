BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-aggiungi-LinkTo
BEGIN-TEXT
           perform TOTALE-RIGHE. 
           add 1 to tot-righe giving riga.
              
           modify  form1-gd-1, insert-rows = 1.                

           initialize bat-rec replacing numeric data by zeroes
                                   alphanumeric data by spaces.
                                          
           add 1        to LastPrg.
           move LastPrg to bat-prg col-prg.
           set  bat-abilitato-no  to true.   
           set  bat-bloccante-no  to true.  
           set  bat-bloccabile-no to true.  
           move bat-abilitato  to hid-abil.
           move bat-bloccante  to hid-bloc.
           move bat-bloccabile to hid-bile.

           modify form1-gd-1(riga, 1), cell-data = col-prg.
           modify form1-gd-1(riga, 3), bitmap spunta2-bmp,
                                       bitmap-number = 1,
                                       bitmap-width 20,
                                       hidden-data hid-abil.
           modify form1-gd-1(riga, 4), bitmap spunta2-bmp,
                                       bitmap-number = 1,
                                       bitmap-width 20,
                                       hidden-data hid-bloc.
           modify form1-gd-1(riga, 5), bitmap spunta2-bmp,
                                       bitmap-number = 1,
                                       bitmap-width 20,
                                       hidden-data hid-bile.

           write bat-rec.          

           move riga to event-data-2.
           perform SPOSTAMENTO.
           modify form1-gd-1, cursor-y riga, cursor-x 2.


END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-elimina-LinkTo
BEGIN-TEXT
           perform TOTALE-RIGHE.
           if tot-righe = 1
              exit paragraph
           end-if.
           display message "Elimina?"
                     title titolo
                      type mb-yes-no
                    giving scelta
           if scelta = mb-yes
              inquire form1-gd-1, cursor-y in riga
              inquire form1-gd-1(riga, 1), cell-data in col-prg
              move col-prg to bat-prg LastPrg
              delete batnott record
              modify  form1-gd-1, record-to-delete = riga
              perform TOTALE-RIGHE
              if riga > tot-righe and tot-righe > 1
                 move tot-righe to riga
              end-if                   
              subtract 1 from tot-righe
              move LastPrg to bat-prg
              start batnott key >= bat-chiave
                    invalid continue
                not invalid
                    perform until 1 = 2
                       read batnott next at end exit perform end-read
                       delete batnott record
                    end-perform                      
                    perform TOTALE-RIGHE
                    modify form1-gd-1, mass-update = 1
                    move riga to store-riga
                    perform varying store-riga from store-riga by 1 
                              until store-riga > tot-righe
                       initialize bat-rec replacing numeric data by zeroes
                                               alphanumeric data by spaces     
                       move LastPrg to bat-prg col-prg
                       inquire form1-gd-1(store-riga, 2), cell-data in col-programma
                       inquire form1-gd-1(store-riga, 3), hidden-data in hid-abil
                       inquire form1-gd-1(store-riga, 4), hidden-data in hid-bloc
                       inquire form1-gd-1(store-riga, 5), hidden-data in hid-bile
                       move col-prg       to bat-prg
                       move col-programma to bat-programma
                       move hid-abil      to bat-abilitato
                       move hid-bloc      to bat-bloccante
                       move hid-bile      to bat-bloccabile
                       write bat-rec                            
                       modify form1-gd-1(store-riga, 1), cell-data col-prg
                       add 1 to LastPrg
                    end-perform
                    modify form1-gd-1, mass-update = 0
              end-start  
              move riga to event-data-2
              perform SPOSTAMENTO
              modify form1-gd-1, cursor-y riga, cursor-x 2
           end-if.                              
           move high-value to bat-chiave.
           start batnott key <= bat-chiave
                 invalid move 0 to LastPrg
             not invalid
                 read batnott previous
                 move bat-prg to LastPrg
           end-start.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-su-LinkTo
BEGIN-TEXT
           inquire form1-gd-1, cursor-y = riga.
           if riga not = 2
              move riga to event-data-2
              subtract 1 from event-data-2 giving riga
              perform SPOSTA-RIGA
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-giu-LinkTo
BEGIN-TEXT
           perform TOTALE-RIGHE.
           if riga not = tot-righe and tot-righe not = 1
              move riga to event-data-2
              add 1 to event-data-2 giving riga
              perform SPOSTA-RIGA
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Finish-Entry
BEGIN-TEXT
           set tutto-ok to true.
           if event-data-1 = 6
              inquire form1-gd-1 (event-data-2, 6), cell-data in col-invio
              if not ( col-invio = "S" or = "N" or = "E" )
                 set event-action to event-action-fail
                 display message "Valori validi: "
                          x"0d0a""S - Sempre"
                          x"0d0a""E - Solo se errori"
                          x"0d0a""N - Nessun invio"
                 set errori to true
              end-if
           end-if.
           if tutto-ok
              perform SCRIVI-RIGA
           end-if.
END-TEXT
END-DATA

