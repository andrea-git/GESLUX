BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = scr-visua
   ACTOBJ            = scr-visua
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           perform RESET-GRIGLIA.
           set tutto-ok to true.
           move low-value to bat-rec.
           start batnott key is >= bat-chiave
                 invalid set errori to true
           end-start.
           if tutto-ok
              modify form1-gd-1, mass-update = 1
              move 2  to riga
              perform until 1 = 2
                 read batnott next at end exit perform end-read
                 move bat-prg       to col-prg
                 move bat-programma to col-programma
                 if bat-abilitato-si
                    modify form1-gd-1(riga, 3), bitmap spunta2-bmp,
                                                bitmap-number = 2,
                                                bitmap-width 20
                 else                                          
                    modify form1-gd-1(riga, 3), bitmap spunta2-bmp,
                                                bitmap-number = 1,
                                                bitmap-width 20
                 end-if
                 if bat-bloccante-si
                    modify form1-gd-1(riga, 4), bitmap spunta2-bmp,
                                                bitmap-number = 2,
                                                bitmap-width 20
                 else                                          
                    modify form1-gd-1(riga, 4), bitmap spunta2-bmp,
                                                bitmap-number = 1,
                                                bitmap-width 20
                 end-if
                 if bat-bloccabile-si
                    modify form1-gd-1(riga, 5), bitmap spunta2-bmp,
                                                bitmap-number = 2,
                                                bitmap-width 20
                 else                                          
                    modify form1-gd-1(riga, 5), bitmap spunta2-bmp,
                                                bitmap-number = 1,
                                                bitmap-width 20
                 end-if
                 move bat-abilitato  to hid-abil
                 move bat-bloccante  to hid-bloc
                 move bat-bloccabile to hid-bile 
                 move bat-invio-log  to col-invio
                 modify form1-gd-1,(riga, 1), cell-data col-prg
                 modify form1-gd-1,(riga, 2), cell-data col-programma
                 modify form1-gd-1,(riga, 3), hidden-data hid-abil
                 modify form1-gd-1,(riga, 4), hidden-data hid-bloc
                 modify form1-gd-1,(riga, 5), hidden-data hid-bile
                 modify form1-gd-1,(riga, 6), cell-data col-invio
                 add 1 to riga
              end-perform
              move bat-prg to LastPrg
              modify form1-gd-1, mass-update = 0
              if LastPrg not = 0
                 move 2 to event-data-2
                 perform SPOSTAMENTO
                 modify form1-gd-1, cursor-y 2, cursor-x 2
                 move 78-ID-form1-gd-1 to control-id
                 move 4 to accept-control
              end-if
           end-if.
END-TEXT
END-DATA
