BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TOTALE-RIGHE
BEGIN-TEXT
           move 0 to tot-righe.
           move 2 to store-riga.
           perform until 1 = 2                       
              inquire form1-gd-1(store-riga, 1), cell-data in col-prg
              move col-prg to bat-prg
              if bat-prg = 0
                 subtract 1 from store-riga
                 exit perform
              end-if
              add 1 to store-riga
           end-perform.
           move store-riga to tot-righe.
END-TEXT
END-DATA
