BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SCRIVI-RIGA
BEGIN-TEXT
           inquire form1-gd-1(event-data-2, 1), cell-data in col-prg.
           inquire form1-gd-1(event-data-2, 2), cell-data in col-programma.
           inquire form1-gd-1(event-data-2, 3), hidden-data in hid-abil.
           inquire form1-gd-1(event-data-2, 4), hidden-data in hid-bloc.
           inquire form1-gd-1(event-data-2, 5), hidden-data in hid-bile.
           inquire form1-gd-1(event-data-2, 6), cell-data in col-invio.

           move col-prg       to bat-prg.
           move col-programma to bat-programma.
           move hid-abil      to bat-abilitato.     
           move hid-bloc      to bat-bloccante.
           move hid-bile      to bat-bloccabile.
           move col-invio     to bat-invio-log.

           rewrite bat-rec.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SEL-COLONNE
BEGIN-TEXT
           move event-data-2 to riga.
           if event-data-1 = 3
              inquire form1-gd-1(riga, 3), hidden-data in hid-abil
              if hid-abil = 0
                 move 1 to hid-abil   
                 modify form1-gd-1(riga, 3), bitmap spunta2-bmp,
                                             bitmap-number = 2,
                                             bitmap-width 20
              else
                 move 0 to hid-abil                           
                 modify form1-gd-1(riga, 3), bitmap spunta2-bmp,
                                             bitmap-number = 1,
                                             bitmap-width 20
              end-if
              modify form1-gd-1(riga, 3), hidden-data hid-abil
           end-if.

           if event-data-1 = 4
              inquire form1-gd-1(riga, 4), hidden-data in hid-bloc
              if hid-bloc = 0
                 move 1 to hid-bloc
                 modify form1-gd-1(riga, 4), bitmap spunta2-bmp,
                                             bitmap-number = 2,
                                             bitmap-width 20
              else
                 move 0 to hid-bloc
                 modify form1-gd-1(riga, 4), bitmap spunta2-bmp,
                                             bitmap-number = 1,
                                             bitmap-width 20
              end-if
              modify form1-gd-1(riga, 4), hidden-data hid-bloc
           end-if.             
           if event-data-1 = 5
              inquire form1-gd-1(riga, 5), hidden-data in hid-bile
              if hid-bile = 0
                 move 1 to hid-bile
                 modify form1-gd-1(riga, 5), bitmap spunta2-bmp,
                                             bitmap-number = 2,
                                             bitmap-width 20
              else
                 move 0 to hid-bile
                 modify form1-gd-1(riga, 5), bitmap spunta2-bmp,
                                             bitmap-number = 1,
                                             bitmap-width 20
              end-if
              modify form1-gd-1(riga, 5), hidden-data hid-bile
           end-if.
           perform SCRIVI-RIGA.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTA-RIGA
BEGIN-TEXT
           inquire form1-gd-1(event-data-2, 2), cell-data in col-programma.
           inquire form1-gd-1(event-data-2, 3), hidden-data in hid-abil.
           inquire form1-gd-1(event-data-2, 4), hidden-data in hid-bloc.   
           inquire form1-gd-1(event-data-2, 5), hidden-data in hid-bile.
           inquire form1-gd-1(event-data-2, 6), cell-data in col-invio.
                                                                           
           inquire form1-gd-1(riga, 2), cell-data in col-programma2.
           inquire form1-gd-1(riga, 3), hidden-data in hid-abil2.
           inquire form1-gd-1(riga, 4), hidden-data in hid-bloc2.
           inquire form1-gd-1(riga, 5), hidden-data in hid-bile2.
           inquire form1-gd-1(riga, 6), cell-data in col-invio2.

           modify form1-gd-1(event-data-2, 2), cell-data col-programma2.
           modify form1-gd-1(event-data-2, 3), hidden-data hid-abil2.
           modify form1-gd-1(event-data-2, 4), hidden-data hid-bloc2.
           modify form1-gd-1(event-data-2, 5), hidden-data hid-bile2.
           modify form1-gd-1(event-data-2, 6), cell-data col-invio2.

           modify form1-gd-1(riga, 2), cell-data col-programma.
           modify form1-gd-1(riga, 3), hidden-data hid-abil.
           modify form1-gd-1(riga, 4), hidden-data hid-bloc.
           modify form1-gd-1(riga, 5), hidden-data hid-bile.
           modify form1-gd-1(riga, 6), cell-data col-invio. 
                  
           if hid-abil2 = 0
              modify form1-gd-1(event-data-2, 3), bitmap spunta2-bmp,
                                                  bitmap-number = 1,
                                                  bitmap-width 20
           else                          
              modify form1-gd-1(event-data-2, 3), bitmap spunta2-bmp,
                                                  bitmap-number = 2,
                                                  bitmap-width 20
           end-if.

           if hid-bloc2 = 0
              modify form1-gd-1(event-data-2, 4), bitmap spunta2-bmp,
                                                  bitmap-number = 1,
                                                  bitmap-width 20
           else
              modify form1-gd-1(event-data-2, 4), bitmap spunta2-bmp,
                                                  bitmap-number = 2,
                                                  bitmap-width 20
           end-if.

           if hid-bile2 = 0
              modify form1-gd-1(event-data-2, 5), bitmap spunta2-bmp,
                                                  bitmap-number = 1,
                                                  bitmap-width 20
           else
              modify form1-gd-1(event-data-2, 5), bitmap spunta2-bmp,
                                                  bitmap-number = 2,
                                                  bitmap-width 20
           end-if.

           if hid-abil = 0
              modify form1-gd-1(riga, 3), bitmap spunta2-bmp,
                                          bitmap-number = 1,
                                          bitmap-width 20
           else                          
              modify form1-gd-1(riga, 3), bitmap spunta2-bmp,
                                          bitmap-number = 2,
                                          bitmap-width 20
           end-if.                             

           if hid-bloc = 0
              modify form1-gd-1(riga, 4), bitmap spunta2-bmp,
                                          bitmap-number = 1,
                                          bitmap-width 20
           else
              modify form1-gd-1(riga, 4), bitmap spunta2-bmp,
                                          bitmap-number = 2,
                                          bitmap-width 20
           end-if.                  

           if hid-bile = 0
              modify form1-gd-1(riga, 5), bitmap spunta2-bmp,
                                          bitmap-number = 1,
                                          bitmap-width 20
           else
              modify form1-gd-1(riga, 5), bitmap spunta2-bmp,
                                          bitmap-number = 2,
                                          bitmap-width 20
           end-if.                  

           perform SCRIVI-RIGA.

           move riga to event-data-2.
           perform SCRIVI-RIGA.

           perform SPOSTAMENTO.
           modify form1-gd-1, cursor-y riga.  
END-TEXT
END-DATA
