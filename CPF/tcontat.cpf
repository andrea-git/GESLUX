BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRECEDENTE
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tcontat all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tcontat-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tcontat-lock-flag
              end-if
                    
              perform CANCELLA-COLORE
              perform FORM1-PREVIOUS

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tcontat-lock-flag
                 move 1 to mod-k
                 perform FORM1-PREVIOUS
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok    
 
              perform CHECK-INSERIMENTO
              unlock tcontat all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tcontat-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tcontat-lock-flag
              end-if
                                  
              perform CANCELLA-COLORE
              perform FORM1-FIRST

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tcontat-lock-flag
                 move 1 to mod-k
                 perform FORM1-FIRST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = ZERO 
              exit paragraph 
           end-if.

           set tutto-ok to true.

           perform varying CONTROL-ID from 78-ID-ef-anno by 1
                     until CONTROL-ID > 78-ID-ef-data-nc
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if errori
              perform ABILITAZIONI
              move store-id to CONTROL-ID       
              move 4        to ACCEPT-CONTROL   
           else                       
              perform FORM1-BUF-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              perform CANCELLA-COLORE 

              write con-rec invalid rewrite con-rec end-write
              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform Form1-CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel 
                            giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA
BEGIN-TEXT
           perform RIEMPI-CHIAVE.

           move "tcontat" to    como-file.
           call "zoom-gt" using como-file, con-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = ZERO
              if old-con-chiave not = con-chiave
                 move con-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to con-chiave
                    set ReadSecca to true
                    modify ef-anno, value con-anno
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-anno to CONTROL-ID       
                    move 4             to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA-ALFA
BEGIN-TEXT
      ** LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      ** ANCORA GLI ZERI DAVANTI CHE ELIMINO
      *     inspect ef-codice-buf replacing leading ZERO by SPACES.
      *     move ef-codice-buf to mar-descrizione.
      *
      ** POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      ** ALTERNATA
      *     call "C$JUSTIFY" using mar-descrizione, "L".
      *
      *     start tmarche key >= mar-descrizione
      *        invalid 
      *           continue
      *        not invalid
      *           read tmarche next 
      *     end-start.
      *
      *     move "tmarche-alfa" to como-file.
      *     call "zoom-gt" using   como-file, mar-rec
      *                    giving  stato-zoom.
      *     cancel "zoom-gt".
      *     if stato-zoom = ZERO
      *        move mar-codice    to codice-ed
      *        move codice-ed     to ef-codice-buf   
      *        call "C$JUSTIFY" using ef-codice-buf, "L"
      *        display ef-codice
      *
      *        if old-mar-descrizione not = mar-descrizione
      *           move mar-descrizione to save-key-1
      *           perform SALV-MOD
      *           if tutto-ok
      *              move save-key-1 to mar-descrizione
      *              set ReadSecca  to true
      *              modify ef-codice, value mar-codice
      *              perform CURRENT-RECORD
      *              perform CANCELLA-COLORE
      *              move 78-ID-ef-codice to CONTROL-ID       
      *              move 4               to ACCEPT-CONTROL   
      *           end-if                                  
      *        end-if
      *     end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SUCCESSIVO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tcontat all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tcontat-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tcontat-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tcontat-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA
BEGIN-TEXT
           continue.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ULTIMO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tcontat all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tcontat-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tcontat-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tcontat-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move con-rec to old-con-rec.
           set vecchio to true.

           move ZERO to StatusHelp
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-NUOVO
BEGIN-TEXT
      * ISTRUZIONE SPECIFICA DEL PGM tcontat
      *     perform Form1-Clear
      * FINE
           move "00" to status-tcontat.
           
           move tge-anno to con-anno.

           initialize con-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                                       
           perform FORM1-IUD-DISPLAY.

      *     if OrderBy = 1                             
      *        move high-value to con-descrizione
      *        start tcontat key is <= con-descrizione 
      *              invalid continue 
      *          not invalid
      *              read tcontat previous no lock 
      *                 at end continue 
      *              end-read
      *        end-start
      *     end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      * SE NON HO VALORIZZATO I CAMPI DI CREAZIONE LI VALORIZZO
           if con-data-creazione   = zero
              accept con-data-creazione from century-date
           end-if.
           if con-ora-creazione    = zero
              accept con-ora-creazione  from time
           end-if.
           if con-utente-creazione = space
              move user-codi to con-utente-creazione
           end-if.

           accept con-data-ultima-modifica from century-date.
           move user-codi    to con-utente-ultima-modifica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-anno to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock tcontat all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.
END-TEXT
END-DATA
