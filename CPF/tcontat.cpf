BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-INSERIMENTO
BEGIN-TEXT
      * MODIFICA LA STATUS-BAR
      * CONTROLLO SE DA INSERIMENTO SCORRO. ALLORA DEVO SEGNALARE CHE
      * SONO TORNATO IN MODIFICA.
           inquire tool-modifica, value in mod.

           evaluate mod
           when 1
                set StatusModifica to true
           when ZERO
                set StatusVisua to true   
           end-evaluate.
      
           perform STATUS-BAR-MSG.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-ID-ef-anno
                perform RIEMPI-CHIAVE
                if con-anno not > ZERO
                   set errori to true
                   display message box "Inserimento anno mancante"
                      title tit-err
                      type  mb-ok
                      icon  mb-warning-icon
                else          
                   read tcontat no lock
                      invalid
                         if vecchio
                            display message box MSG-record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                            perform CLEAR-SCREEN
                            set errori to true
                         end-if
                      not invalid      
                        if old-con-chiave not = con-chiave
                           perform CURRENT-RECORD 
                           set errori to true
                        end-if
                  end-read
               end-if

           when 78-ID-ef-bolle-1
                inquire ef-bolle-1 value in como-numero
                if como-numero > 500000
                   set errori to true
                   display message "Range corretto 0 - 500.000"
                             title tit-err
                              icon 2
                end-if

           when 78-ID-ef-data-bolle-1
                inquire ef-data-bolle-1 value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-bolle-1-buf
                   display ef-data-bolle-1
                end-if

           when 78-ID-ef-bolle-2
                inquire ef-bolle-2 value in como-numero
                if como-numero < 900001
                   set errori to true
                   display message "Range corretto 900.001 - 999.999"
                             title tit-err
                              icon 2
                end-if

           when 78-ID-ef-data-bolle-2
                inquire ef-data-bolle-2 value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-bolle-2-buf
                   display ef-data-bolle-2
                end-if

           when 78-ID-ef-bolle-3
                inquire ef-bolle-3 value in como-numero
                if como-numero < 500001 or
                   como-numero > 900000
                   set errori to true
                   display message "Range corretto 500.001 - 900.000"
                             title tit-err
                              icon 2
                end-if

           when 78-ID-ef-data-bolle-3
                inquire ef-data-bolle-3 value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-bolle-3-buf
                   display ef-data-bolle-3
                end-if

           when 78-ID-ef-data-fatture
                inquire ef-data-fatture value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-fatture-buf
                   display ef-data-fatture
                end-if

           when 78-ID-ef-data-nc
                inquire ef-data-nc value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-nc-buf
                   display ef-data-nc
                end-if

           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move control-id to store-id
              move 4          to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if mod = 1
              read tcontat    lock invalid 
                 set errori to true 
              end-read
           else
              read tcontat no lock invalid 
                 set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move ZERO          to mod
                    move 1             to mod-k
                    move 78-ID-ef-anno to CONTROL-ID
                    move 4             to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else                         
                    set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG
                 perform VALORIZZA-OLD
              else
                 move ZERO to mod
                 move 1    to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-anno to CONTROL-ID
                       move 4             to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT
BEGIN-TEXT
           move ZERO to StatusHelp.
           move 1    to mod-k.
           set StatusVisua to true.     
           set errore-des  to false.
END-TEXT
