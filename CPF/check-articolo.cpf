   ENTRYPOINT        = AfterCreateWin
BEGIN-TEXT
      *<<** Customized_Default, SP-G, scr-elab-blockpgm-1 **>>
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = form1
   ACTOBJ            = form1
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           accept esercizio-x from environment "ESERCIZIO".
           move   esercizio-x to anno-to  ef-anno-buf.
           move        0      to num-from ef-num-from-buf.
           move 99999999      to num-to   ef-num-to-buf.
           modify cbo-tipo, item-to-add = "Ordini".
           modify cbo-tipo, item-to-add = "Note Credito".
           move "Ordini" to cbo-tipo-buf.
           modify cbo-tipo, value cbo-tipo-buf.

           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = form1
   ACTOBJ            = form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterDisplay
BEGIN-TEXT
      *<<** Customized_Default, SP-G, scr-elab-blockpgm-1 **>>
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.
           evaluate control-id
           when 78-ID-ef-anno
                inquire ef-anno, value in anno-to
                if anno-to = 0
                   set errori to true
                   display message "Anno obbligatorio"
                             title tit-err
                              icon 2
                end-if

           when 78-ID-ef-num-to
                inquire ef-num-to, value in num-to
                if num-to = 0
                   move 99999999 to num-to ef-num-to-buf
                   display ef-num-to
                end-if
                inquire ef-num-from, value in num-from
                if num-from > num-to
                   set errori to true
                   move 78-ID-ef-num-from to control-id
                   display message "Range errato"
                             title tit-err
                              icon 2
                end-if

           end-evaluate.

           if errori
              move 4 to accept-control
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STATUS-HELP
BEGIN-TEXT

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "calcola-colore-trasparente.cpy".
END-TEXT
END-DATA
BEGIN-DATA
