END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-noa-AfterProcedure
BEGIN-TEXT
           modify pb-noa, bitmap-number = 1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-sib-BeforeProcedure
BEGIN-TEXT
           modify pb-sib, bitmap-number = 2.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-sib-LinkTo
BEGIN-TEXT
           perform varying control-id from 78-ID-ef-anno by 1
                     until control-id > 78-ID-ef-num-to
               perform CONTROLLO
               if errori
                  exit perform
               end-if
           end-perform.

           if tutto-ok
              inquire cbo-tipo, value in cbo-tipo-buf
              if cbo-tipo-buf = "Ordini" set ordini       to true
              else                       set note-credito to true
              end-if
              move 0 to link-result
              call "W$MOUSE" using set-mouse-shape, wait-pointer
              call   "check-articolo-p" using form1-handle, 
                                              link-result,
                                              anno-to,
                                              num-from,
                                              num-to,
                                              user-codi,
                                              tipo-doc
              cancel "check-articolo-p"
              call "W$MOUSE" using set-mouse-shape, arrow-pointer

              if link-result = 0
                 display message "NESSUN ERRORE RISCONTRATO!!!"
                           title titolo
                            icon 2
              end-if
              modify pb-sib, bitmap-number = 1
              move 78-ID-ef-anno to control-id
              move             4 to accept-control
           end-if.
END-TEXT
END-DATA
