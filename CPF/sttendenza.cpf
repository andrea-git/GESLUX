BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form2
   ACTOBJ            = Form2
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           initialize PathFile.
           if mese not = mese-delta
               move "Stampa Statistica giornaliera in corso..." 
                to lab-oper-buf
              display lab-oper
              call   "statgio-p" using PathFile, mese
              cancel "statgio-p"
           else
               move "Stampa Statistica NEW in corso..." 
                to lab-oper-buf
              display lab-oper
              accept como-data from century-date
              initialize PathFile
              accept  PathFile   from environment "PATH_ST"
              inspect PathFile   replacing trailing spaces by low-value
              |Ne trovo solamente uno per giorno
              |creato in notturna dalla funziona apposita
              string  PathFile   delimited by low-value
                      "statsett" delimited by size
                      "_"        delimited by size
                      como-data  delimited by size
                      ".txt"     delimited by size
                      into PathFile
              end-string
      *****        call   "sttendenza-p" using PathFile, mese
      *****        cancel "sttendenza-p"
           end-if.
                                   
           modify form2-handle, visible = 0
          
           call "C$FILEINFO" using PathFile, file-info.
      
           if file-size not = 0
              set  splcrt2graf-stampa        to true
              set  splcrt2graf-windows       to true
              move PathFile
                to splcrt2graf-percorso-stampa
              set  splcrt2graf-orizzontale   to true
              set  splcrt2graf-forza-crt     to true
              set  splcrt2graf-6pt           to true
              call   "splcrt2graf" using splcrt2graf-link 
              cancel "splcrt2graf"
           end-if.
      
           if mese not = mese-delta
              call "C$DELETE" using PathFile
           end-if.

           move 27 to key-status.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           move spaces to tge-chiave.
           read tparamge no lock invalid continue end-read.
           move tge-data-consolid-progmag(5:2) to mese.
           if mese = 12 move 1 to mese
           else         add  1 to mese
           end-if.  
           display ef-mese.

           move mese to mese-delta.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = sttendenza
   ACTOBJ            = sttendenza
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
      ***---
       TPARAMGE-ERR SECTION.
           use after error procedure on tparamge.
           set tutto-ok  to true.
           evaluate status-tparamge
           when "35"
                display message "File [TPARAMGE] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [TPARAMGE] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[TPARAMGE] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           end-evaluate.
END-TEXT
END-DATA
