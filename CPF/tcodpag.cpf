                end-if
           when 5
                move col-importo to col-importo-edit
                modify form1-gd-1(riga, 5), cell-data = col-importo-edit
           end-evaluate.

           if scelta = mb-no
              perform CONTROLLO-RIGA
              if errori
                 set event-action to event-action-fail
              end-if
           end-if.
