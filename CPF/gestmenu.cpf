BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           modify cmb-tipo, RESET-LIST = 1

           set PROG-T-ELABORAZIONE of prog to true
           modify cmb-tipo, ITEM-TO-ADD = PROG-TYPE-2 of prog

           set PROG-T-ARCHIVIO of prog to true
           modify cmb-tipo, ITEM-TO-ADD = PROG-TYPE-2 of prog

           set PROG-T-STAMPA of prog to true
           modify cmb-tipo, ITEM-TO-ADD = PROG-TYPE-2 of prog

           perform INIT.
           perform ABILITA-TOOLBAR.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
                         
           perform PRIMO.
           modify pb-sposta, bitmap-number = 1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterAccept
BEGIN-TEXT
           evaluate key-status
           when 5 |modifica
                inquire tool-modifica, enabled in e-modifica
                if e-modifica = 1
                   if mod = 
                      zero move 1 to mod
                   else          
                      move zero to mod
                   end-if
                   modify tool-modifica, value = mod
                   perform MODIFICA
                end-if
           when 10 |ordinamento alfabetico 
                if OrderBy = 0 
                   move 1 to OrderBy
                else           
                   move 0 to OrderBy
                end-if
                modify TOOL-ORD, value OrderBy
                perform ORDINAMENTO
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterFldToBuf
BEGIN-TEXT
           perform ABILITAZIONI.
           perform VALORIZZA-OLD.
                                  
           if prog-type of prog = 1
              modify cmb-tipo, VISIBLE = 1
           else 
              modify cmb-tipo, VISIBLE = zero
              MOVE SPACES TO PROG-TYPE-2 of prog1
           end-if.


      *    vado a valorizzare le descrizioni dei vari livelli
           move PROG-KEY of prog   to save-PROG-KEY.

           move save-prog-level-1  to prog-level-1 of prog1.
           move zero               to prog-level-2 of prog1.
           move zero               to prog-level-3 of prog1.
           move zero               to prog-level-4 of prog1.
           move zero               to prog-level-5 of prog1.

           read Prog1
              invalid
                 move space to prog-s-desc of prog1
                 move 78-bmp-niente to como-num-bitmap
              not invalid
                 perform VAL-COMO-NUM-BITMAP
           end-read

           move prog-s-desc of prog1  to des-level-1.
           move como-num-bitmap       to num-bmp-level-1.

           display lbl-level-1.
      *     modify bmp-level-1, bitmap-number = como-num-bitmap
           
           if save-prog-level-2 = zero and
              save-prog-level-3 = zero and 
              save-prog-level-4 = zero and 
              save-prog-level-5 = zero
