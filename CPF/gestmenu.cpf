BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-id-ef-level-1
                move ef-level-1-buf   to prog-level-1 of prog1
                move zero             to prog-level-2 of prog1 
                move zero             to prog-level-3 of prog1 
                move zero             to prog-level-4 of prog1 
                move zero             to prog-level-5 of prog1 

                read Prog1
                   invalid
                     move space to prog-s-desc of prog1
                     move 78-bmp-niente to como-num-bitmap
                   not invalid
                     perform VAL-COMO-NUM-BITMAP
                end-read
                move prog-s-desc of prog1   to des-level-1
                move prog-type of prog1     to como-type1
                modify bmp-level-1, bitmap-number = como-num-bitmap
                move como-num-bitmap to num-bmp-level-1
                display lbl-level-1
           when 78-id-ef-level-2
                if ef-level-2-buf = zero and
                   ef-level-3-buf = zero and 
                   ef-level-4-buf = zero and 
                   ef-level-5-buf = zero
                   move space to des-level-2
                   move 78-bmp-niente to como-num-bitmap
                else
                   move ef-level-1-buf   to prog-level-1 of prog1
                   move ef-level-2-buf   to prog-level-2 of prog1 
                   move zero             to prog-level-3 of prog1 
                   move zero             to prog-level-4 of prog1 
                   move zero             to prog-level-5 of prog1 
       
                   read Prog1
                      invalid
                         move space to prog-s-desc of prog1
                         move 78-bmp-niente to como-num-bitmap
                      not invalid
                         perform VAL-COMO-NUM-BITMAP
                   end-read
                   move prog-s-desc of prog1   to des-level-2
                   move prog-type of prog1     to como-type2
                end-if

                modify bmp-level-2, bitmap-number = como-num-bitmap
                display lbl-level-2
                move como-num-bitmap to num-bmp-level-2
                if nuovo
                   if ef-level-2-buf not = zero and como-type1 = 1
                      display message box
                      "Non puoi creare un nuovo sottolivello ad un eseguibile!"
                      title titolo
                      move 78-ID-ef-level-2 to control-id
                      move 4                to accept-control
                      exit paragraph
                   end-if
                end-if

           when 78-id-ef-level-3
                if ef-level-3-buf = zero and 
                   ef-level-4-buf = zero and 
                   ef-level-5-buf = zero
                   move space to des-level-3
                   move 78-bmp-niente to como-num-bitmap
                else
                   move ef-level-1-buf   to prog-level-1 of prog1
                   move ef-level-2-buf   to prog-level-2 of prog1 
                   move ef-level-3-buf   to prog-level-3 of prog1 
                   move zero             to prog-level-4 of prog1 
                   move zero             to prog-level-5 of prog1 
       
                   read Prog1
                      invalid
                        move space to prog-s-desc of prog1
                         move 78-bmp-niente to como-num-bitmap
                      not invalid
                         perform VAL-COMO-NUM-BITMAP
                    end-read
                   move prog-s-desc of prog1   to des-level-3
                   move prog-type of prog1     to como-type3
                end-if

                modify bmp-level-3, bitmap-number = como-num-bitmap
                display lbl-level-3
                move como-num-bitmap to num-bmp-level-3
                if nuovo
                   if ef-level-3-buf not = zero and como-type2 = 1
                      display message box
                      "Non puoi creare un nuovo sottolivello ad un eseguibile!"
                      title titolo
                      move 78-ID-ef-level-3 to control-id
                      move 4                to accept-control
                      exit paragraph
                   end-if
                end-if
                   
           when 78-id-ef-level-4
                if ef-level-4-buf = zero and 
                   ef-level-5-buf = zero
                   move space to des-level-4
                   move 78-bmp-niente to como-num-bitmap
                else
                   move ef-level-1-buf   to prog-level-1 of prog1
                   move ef-level-2-buf   to prog-level-2 of prog1 
                   move ef-level-3-buf   to prog-level-3 of prog1 
                   move ef-level-4-buf   to prog-level-4 of prog1 
                   move zero             to prog-level-5 of prog1 
       
                   read Prog1
                      invalid
                         move space to prog-s-desc of prog1
                         move 78-bmp-niente to como-num-bitmap
                      not invalid
                         perform VAL-COMO-NUM-BITMAP
                   end-read
                   move prog-s-desc of prog1   to des-level-4
                   move prog-type of prog1     to como-type4
                end-if
                modify bmp-level-4, bitmap-number = como-num-bitmap
                display lbl-level-4
                move como-num-bitmap to num-bmp-level-4
                if nuovo
                   if ef-level-4-buf not = zero and como-type3 = 1
                      display message box
                      "Non puoi creare un nuovo sottolivello ad un eseguibile!"
                      title titolo
                      move 78-ID-ef-level-4 to control-id
                      move 4                to accept-control
                      exit paragraph
                   end-if
                end-if

           when 78-id-ef-level-5
      * LETTURA DEL RECORD SE TUTTI I CONTROLLI SONO PASSATI OK
                perform RIEMPI-CHIAVE
                read prog with no lock
                   invalid 
                      if vecchio
                         display message  MSG-record-inesistente
                                 title = tit-err
                                 icon mb-warning-icon
                         |perform CLEAR-SCREEN
                         set errori     to true
                         move 78-id-ef-level-1 to store-id
                      end-if
                   not invalid      
                      if old-PROG-KEY not = PROG-KEY of prog
                         perform CURRENT-RECORD 
                         set errori to true
                      end-if
                      move 78-id-ef-level-5 to CONTROL-ID
                end-read

      *    codice per la valorizzazione della bitmap
                if ef-level-5-buf = zero
                   move space to des-level-5
                   move 78-bmp-niente to como-num-bitmap
                else
                   move ef-level-1-buf   to prog-level-1 of prog1
                   move ef-level-2-buf   to prog-level-2 of prog1 
                   move ef-level-3-buf   to prog-level-3 of prog1 
                   move ef-level-4-buf   to prog-level-4 of prog1 
                   move ef-level-5-buf   to prog-level-5 of prog1 
       
                   read Prog1
                      invalid
                         move space to prog-s-desc of prog1
                         move 78-bmp-niente to como-num-bitmap
                      not invalid
                         perform VAL-COMO-NUM-BITMAP
                   end-read
                   move prog-s-desc of prog1   to des-level-5
                   move prog-type of prog1     to como-type5
                end-if
                modify bmp-level-5, bitmap-number = como-num-bitmap
                display lbl-level-5
                move como-num-bitmap to num-bmp-level-5
                if nuovo
                   if ef-level-5-buf not = zero and como-type5 = 1
                      display message box
                      "Non puoi creare un nuovo sottolivello ad un eseguibile!"
                      title titolo
                      move 78-ID-ef-level-5 to control-id
                      move 4                to accept-control
                      exit paragraph
                   end-if
                end-if
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move store-id to CONTROL-ID
              move 4        to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if mod = 1
              read prog    lock invalid 
                 set errori to true 
              end-read
           else
              read prog no lock invalid 
                 set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0      to mod
                    move 1      to mod-k
                    move 78-ID-ef-level-1   to CONTROL-ID
                    move 4         to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else                         
                    set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG
                 perform VALORIZZA-OLD
              else
                 move ZERO to mod
                 move 1    to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-level-1   to CONTROL-ID
                       move 4                  to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
