BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA
BEGIN-TEXT
           if mod = ZERO
              exit paragraph 
           end-if.
      *
           display message box MSg-Cancellare-il-record-corrente
                   title   titolo
                   type    mb-yes-no 
                   default mb-no
                   giving  scelta

           if scelta = mb-yes
              perform RIEMPI-CHIAVE
              delete Prog record 
                 invalid 
                    continue 
              end-delete     
              move zero to mod
              move 1    to mod-k
              perform FORM1-CLEAR
              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC

              
              display message box MSG-Cancellazione-avvenuta-con-successo
                      title titolo                         

              unlock prog all records

              perform PRIMO 
              move 78-ID-ef-level-1   to  CONTROL-ID     
              move 4                  to  ACCEPT-CONTROL 

              set Statusvisua to true
              perform STATUS-BAR-MSG
              modify TOOL-MODIFICA value = mod
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate CONTROL-ID
           when 78-ID-ef-level-2
                perform RIEMPI-CHIAVE-CERCA
                move "prog-l2"  to Como-File
                call   "zoom-gt" using como-file, PROG-R of prog1
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-level-2, value prog-level-2 of prog1
                   move prog-level-2 of prog1  to ef-level-2-buf
                   perform CONTROLLO
                   move 4 to accept-control
                end-if
           when 78-ID-ef-level-3
                perform RIEMPI-CHIAVE-CERCA
                move "prog-l3"  to Como-File
                call   "zoom-gt" using como-file, PROG-R of prog1
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef-level-3, value prog-level-3 of prog1
                   move prog-level-3 of prog1  to ef-level-3-buf
                   perform CONTROLLO
                   move 4 to accept-control
                end-if
           when 78-ID-ef-level-4
