BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTA-FIGLI
BEGIN-TEXT
           if PROG-TYPE of PROG = 1
              exit paragraph
           end-if
           set livello-5  to true
           if work-LEVEL-5 = zero
              set livello-4  to true
           end-if

           if work-LEVEL-4 = zero
              set livello-3  to true
           end-if

           if work-LEVEL-3 = zero
              set livello-2  to true
           end-if

           if work-LEVEL-2 = zero
              set livello-1  to true
           end-if
           evaluate true
           when livello-1
                move 2    to como-offset
           when livello-2
                move 4    to como-offset
           when livello-3
                move 6    to como-offset
           when livello-4
                move 8    to como-offset
           when livello-5
                move 10   to como-offset
           end-evaluate
           move low-value                   to PROG-MENU-LEVEL of PROG1
           move old-PROG-KEY(1:como-offset) to PROG-KEY of PROG1(1:como-offset)

           start Prog1 key is >= PROG-KEY of PROG1
              invalid
                 continue
              not invalid
                 perform until 1 = 2
                    read Prog1 next
                       at end
                          exit perform
                    end-read
                    if old-PROG-KEY(1:como-offset) not = 
                       PROG-MENU-LEVEL of PROG1(1:como-offset)
                       exit perform
                    end-if
                    move PROG-R of PROG1 to PROG-R of PROG
                    move work-KEY(1:como-offset)
                                   to PROG-KEY of PROG(1:como-offset)
                    write PROG-R of PROG
                       invalid
                          continue
                    end-write
                    move PROG-R of PROG1 to PROG-R of PROG
                    delete Prog record
                 end-perform
           end-start.
END-TEXT
END-DATA
