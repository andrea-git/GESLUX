   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-prog-r   replacing numeric data by zeroes
                                   alphanumeric data by spaces.


END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 move 3   to b-sposta
                 modify pb-sposta, BITMAP-NUMBER = 3
                 modify pb-sposta, enabled = false
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if CONTROL-ID < 78-ID-ef-prog-id
                       move 78-ID-ef-prog-id to CONTROL-ID
                    end-if
                    move ZERO to mod-k
                    move 1    to mod
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1   to b-sposta
                 modify pb-sposta, BITMAP-NUMBER = 1
                 modify pb-sposta, enabled = true
                 move 1    to mod
                 perform SALV-MOD            
                 move ZERO to mod
                 move 1    to mod-k
                 if errori
                    move 1    to mod
                    move ZERO to mod-k
                 else
                    move 78-ID-ef-level-1 to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock prog all records
                 end-if
              end-if
              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           perform SALV-MOD.

           if tutto-ok  
              move 78-ID-ef-level-1 to CONTROL-ID       
              move 4                to ACCEPT-CONTROL   
              move 1                to mod mod-k
              modify tool-modifica, value = mod
              perform CANCELLA-COLORE                  
              perform VALORIZZA-NUOVO
              perform INIT-OLD-REC       
              set StatusIns to true
              perform STATUS-BAR-MSG  
              set nuovo to true       
              unlock prog all records
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ORDINAMENTO
BEGIN-TEXT
           inquire tool-ord, value in OrderBy.
      
           if OrderBy = zero
              perform DataSet1-CHANGETO-KEY1
           else
              perform DataSet1-CHANGETO-KEY2
           end-if.
      
           perform CURRENT-RECORD.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           inquire ef-level-1, value in ef-level-1-buf.
           move ef-level-1-buf  to PROG-LEVEL-1 of prog | convert.
           inquire ef-level-2, value in ef-level-2-buf.
           move ef-level-2-buf  to PROG-LEVEL-2 of prog | convert.
           inquire ef-level-3, value in ef-level-3-buf.
           move ef-level-3-buf  to PROG-LEVEL-3 of prog | convert.
           inquire ef-level-4, value in ef-level-4-buf.
           move ef-level-4-buf  to PROG-LEVEL-4 of prog | convert.
           inquire ef-level-5, value in ef-level-5-buf.
           move ef-level-5-buf  to PROG-LEVEL-5 of prog | convert.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRECEDENTE
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock prog all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-prog-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-prog-lock-flag
              end-if
                    
              perform CANCELLA-COLORE
              perform FORM1-PREVIOUS

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-prog-lock-flag
                 move 1 to mod-k
                 perform FORM1-PREVIOUS
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok    
              perform CHECK-INSERIMENTO
              unlock prog all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-prog-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-prog-lock-flag
              end-if
                                  
              perform CANCELLA-COLORE
              perform FORM1-FIRST

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-prog-lock-flag
                 move 1 to mod-k
                 perform FORM1-FIRST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = ZERO 
              exit paragraph 
           end-if.

           set tutto-ok to true.

           perform varying CONTROL-ID from 78-ID-ef-level-1 by 1
                     until CONTROL-ID > 78-ID-ef-descr-compl
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if errori
              perform ABILITAZIONI
              move store-id to CONTROL-ID       
              move 4        to ACCEPT-CONTROL   
           else                       
              perform FORM1-BUF-TO-FLD
              perform cancella-colore 
                                     
              accept como-data from century-date
              accept como-ora  from time
          
              write prog-r of prog
                 invalid 
                    rewrite PROG-R of prog
              end-write
              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel 
                            giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA
BEGIN-TEXT
           perform RIEMPI-CHIAVE.

           move "prog"    to    como-file.
           call "zoom-gt" using como-file, prog-r of prog
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = ZERO
              if old-PROG-KEY not = PROG-KEY of prog  or
               ( prog-key of prog = 0 and mod = 0 )
                 move prog-key of prog to save-prog-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-prog-key to PROG-KEY of prog 
                    set ReadSecca to true
                    modify ef-level-1, value prog-level-1 of prog 
                    modify ef-level-2, value prog-level-2 of prog 
                    modify ef-level-3, value prog-level-3 of prog 
                    modify ef-level-4, value prog-level-4 of prog 
                    modify ef-level-5, value prog-level-5 of prog 

                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-prog-id to CONTROL-ID       
                    move 4                 to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SUCCESSIVO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock prog all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-prog-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-prog-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-prog-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ULTIMO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock prog all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-prog-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-prog-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-prog-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move PROG-R  of prog to old-PROG-R.
           move como-cmb-tipo   to old-como-cmb-tipo.
           move prog-acuthin of prog to old-prog-acuthin.
           set vecchio to true.
           if old-PROG-acuthin = space
              move "N" to old-PROG-acuthin
           end-if.

           move ZERO to StatusHelp
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-NUOVO
BEGIN-TEXT
           initialize  prog-id     of prog
                       prog-s-desc of prog
                       prog-type   of prog
                       prog-type-2 of prog
                       prog-l-desc of prog
                       prog-filler of prog.
      
           perform FORM1-IUD-DISPLAY.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-level-1 to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock prog all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VAL-COMO-NUM-BITMAP
BEGIN-TEXT
           if prog-type of prog1 = zero
              move 78-bmp-cartella          to como-num-bitmap
           else
              evaluate true
              when prog-t-elaborazione of prog1
                   move 78-bmp-elaborazione to como-num-bitmap
              when prog-t-archivio of prog1
                   move 78-bmp-archivio     to como-num-bitmap
              when prog-t-stampa of prog1
                   move 78-bmp-stampa       to como-num-bitmap
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE-CERCA
BEGIN-TEXT
           inquire ef-level-1, value in ef-level-1-buf.
           move ef-level-1-buf  to PROG-LEVEL-1 of prog1.
           inquire ef-level-2, value in ef-level-2-buf.
           move ef-level-2-buf  to PROG-LEVEL-2 of prog1.
           inquire ef-level-3, value in ef-level-3-buf.
           move ef-level-3-buf  to PROG-LEVEL-3 of prog1.
           inquire ef-level-4, value in ef-level-4-buf.
           move ef-level-4-buf  to PROG-LEVEL-4 of prog1.
           inquire ef-level-5, value in ef-level-5-buf.
           move ef-level-5-buf  to PROG-LEVEL-5 of prog1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTA-TREE
BEGIN-TEXT
           if work-LEVEL-5 = zero
              if work-LEVEL-4 = zero
                 if work-LEVEL-3 = zero
                    if work-LEVEL-2 = zero
                       if work-LEVEL-1 = zero
                          continue
                       else
                          move 78-ID-ef3-level-1 to control-id
                       end-if
                    else
                       move 78-ID-ef3-level-2 to control-id
                    end-if
                 else
                    move 78-ID-ef3-level-3 to control-id
                 end-if
              else
                 move 78-ID-ef3-level-4 to control-id
              end-if
           else
              move 78-ID-ef3-level-5 to control-id
           end-if.

           move 4 to accept-control.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTA
BEGIN-TEXT
           perform FORM2-BUF-TO-FLD
           move PROG-KEY of PROG to old-PROG-KEY
           move work-KEY to PROG-KEY of PROG
           read Prog
              invalid
                 perform CONTROLLO-SPOSTA
                 if not problema-livello
                    move old-PROG-KEY to PROG-KEY of PROG
                    read Prog
                    move work-KEY to PROG-KEY of PROG
                    write PROG-R of PROG
                    move old-PROG-KEY to PROG-KEY of PROG
                    delete Prog record
                    perform SPOSTA-FIGLI
                    set cambio-fatto  to true
                    perform Form2-Exit
                 end-if

              not invalid
                 display message box "Record già presente..."
           end-read.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA-SPOSTA
BEGIN-TEXT
           evaluate CONTROL-ID
           when 78-ID-ef3-level-1
                perform RIEMPI-CHIAVE-SPOSTA
                move "prog"    to    como-file
                call "zoom-gt" using como-file, prog-r of prog
                              giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef3-level-1, value prog-level-1 of prog1
                   move prog-level-1 of prog1  to ef3-level-1-buf
                   move prog-s-desc of prog1 to des3-level-1
                   move 4 to accept-control
                end-if
           when 78-ID-ef3-level-2
                perform RIEMPI-CHIAVE-SPOSTA
                move "prog-l2"  to Como-File
                call   "zoom-gt" using como-file, PROG-R of prog1
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef3-level-2, value prog-level-2 of prog1
                   move prog-level-2 of prog1  to ef3-level-2-buf
                   move prog-s-desc of prog1 to des3-level-2
                   move 4 to accept-control
                end-if
           when 78-ID-ef3-level-3
                perform RIEMPI-CHIAVE-SPOSTA
                move "prog-l3"  to Como-File
                call   "zoom-gt" using como-file, PROG-R of prog1
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef3-level-3, value prog-level-3 of prog1
                   move prog-level-3 of prog1  to ef3-level-3-buf
                   move prog-s-desc of prog1 to des3-level-3
                   move 4 to accept-control
                end-if
           when 78-ID-ef3-level-4
                perform RIEMPI-CHIAVE-SPOSTA
                move "prog-l4"  to Como-File
                call   "zoom-gt" using como-file, PROG-R of prog1
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef3-level-4, value prog-level-4 of prog1
                   move prog-level-4 of prog1  to ef3-level-4-buf
                   move prog-s-desc of prog1 to des3-level-4
                   move 4 to accept-control
                end-if
           when 78-ID-ef3-level-5
                perform RIEMPI-CHIAVE-SPOSTA
                move "prog-l5"  to Como-File
                call   "zoom-gt" using como-file, PROG-R of prog1
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = ZERO
                   modify ef3-level-5, value prog-level-5 of prog1
                   move prog-level-5 of prog1  to ef3-level-5-buf
                   move prog-s-desc of prog1 to des3-level-5
                   move 4 to accept-control
                end-if
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE-SPOSTA
BEGIN-TEXT
           inquire ef3-level-1, value in ef3-level-1-buf.
           move ef3-level-1-buf  to PROG-LEVEL-1 of prog1.
           inquire ef3-level-2, value in ef3-level-2-buf.
           move ef3-level-2-buf  to PROG-LEVEL-2 of prog1.
           inquire ef3-level-3, value in ef3-level-3-buf.
           move ef3-level-3-buf  to PROG-LEVEL-3 of prog1.
           inquire ef3-level-4, value in ef3-level-4-buf.
           move ef3-level-4-buf  to PROG-LEVEL-4 of prog1.
           inquire ef3-level-5, value in ef3-level-5-buf.
           move ef3-level-5-buf  to PROG-LEVEL-5 of prog1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-SPOSTA
BEGIN-TEXT
      *********     if work-level-2 not = zero and work-level-3 not = zero
      *********        move work-level-1 to prog-level-1 of prog1
      *********        read prog1
      *********        invalid display message box "Livello 1 non presente!"
      *********        set problema-livello to true
      *********        not invalid
      *********           if work-level-3 not = zero and work-level-4 not = zero
      *********              move work-level-2 to prog-level-2 of prog1
      *********              read prog1
      *********              invalid display message box "Livello 2 non presente!"
      *********              set problema-livello to true
      *********              not invalid
      *********                 if work-level-4 not = zero and work-level-5 not = zero
      *********                    move work-level-3 to prog-level-3 of prog1
      *********                    read prog1
      *********                    invalid display message box "Livello 3 non presente!"
      *********                    set problema-livello to true
      *********                    not invalid
      *********                       if work-level-5 not = zero
      *********                          move work-level-4 to prog-level-4 of prog1
      *********                          read prog1
      *********                          invalid display message box "Livello 4 non presente!"
      *********                          set problema-livello to true
      *********                       end-if
      *********                  end-if
      *********           end-if
      *********     end-if

           set problema-livello to false
           evaluate control-id
           when 78-id-ef3-level-1
              move ef3-level-1-buf  to prog-level-1 of prog1
              move zero             to prog-level-2 of prog1 
              move zero             to prog-level-3 of prog1 
              move zero             to prog-level-4 of prog1 
              move zero             to prog-level-5 of prog1 

              read Prog1
                 invalid
                   move space to prog-s-desc of prog1
                   move 78-bmp-niente to como-num-bitmap
                 not invalid
                   set no-livello to false
                   perform VAL-COMO-NUM-BITMAP
              end-read
              move prog-s-desc of prog1   to des3-level-1
              move prog-type of prog1     to como-type1
              modify bmp3-level-1, bitmap-number = como-num-bitmap
              move como-num-bitmap to num-bmp-level-1
              display lbl3-level-1
              if ef3-level-1-buf = zero and ef3-level-2-buf = zero
                                        and cartella
                 display message box "Operazione non consentita!"
                 move 78-id-ef3-level-1  to control-id
                 move 4                  to accept-control
              end-if

           when 78-id-ef3-level-2
              if ef3-level-2-buf = zero and
                 ef3-level-3-buf = zero and 
                 ef3-level-4-buf = zero and 
                 ef3-level-5-buf = zero
                 move space to des3-level-2
                 move 78-bmp-niente to como-num-bitmap
              else
                 move ef3-level-1-buf  to prog-level-1 of prog1
                 move ef3-level-2-buf  to prog-level-2 of prog1 
                 move zero             to prog-level-3 of prog1 
                 move zero             to prog-level-4 of prog1 
                 move zero             to prog-level-5 of prog1 
       
                 read Prog1
                    invalid
                       move space to prog-s-desc of prog1
                       move 78-bmp-niente to como-num-bitmap
                       if no-livello
                          display message box "Livello 1 non presente"
                          move 78-id-ef-level-1 to control-id
                          move 4 to accept-control
                          set no-livello to false
                       else
                          set no-livello to true
                       end-if
                    not invalid
                       set no-livello to false
                       perform VAL-COMO-NUM-BITMAP
                 end-read
                 move prog-s-desc of prog1   to des3-level-2
                 move prog-type of prog1     to como-type2
              end-if

              modify bmp3-level-2, bitmap-number = como-num-bitmap
              display lbl3-level-2
              move como-num-bitmap to num-bmp-level-2
              if ef3-level-2-buf not = zero and como-type1 = 1
                   display message box
                   "Non puoi creare un nuovo sottolivello ad un eseguibile!"
                   title titolo-sposta
                   move 78-ID-ef3-level-2 to control-id
                   move 4                 to accept-control
                   set problema-livello to true
              end-if
              if ef3-level-2-buf = zero and ef3-level-3-buf = zero
                                        and cartella
                 display message box "Operazione non consentita!"
                 move 78-id-ef3-level-2  to control-id
                 move 4                  to accept-control
              end-if
           when 78-id-ef3-level-3
              if ef3-level-3-buf = zero and 
                 ef3-level-4-buf = zero and 
                 ef3-level-5-buf = zero
                 move space to des3-level-3
                 move 78-bmp-niente to como-num-bitmap
              else
                 move ef3-level-1-buf  to prog-level-1 of prog1
                 move ef3-level-2-buf  to prog-level-2 of prog1 
                 move ef3-level-3-buf  to prog-level-3 of prog1 
                 move zero             to prog-level-4 of prog1 
                 move zero             to prog-level-5 of prog1 
       
                 read Prog1
                    invalid
                       move space to prog-s-desc of prog1
                       move 78-bmp-niente to como-num-bitmap
                       if no-livello
                          display message box "Livello 2 non presente"
                          move 78-id-ef-level-2 to control-id
                          move 4 to accept-control
                          set no-livello to false
                       else
                          set no-livello to true
                       end-if
                    not invalid
                       set no-livello to false
                       perform VAL-COMO-NUM-BITMAP
                 end-read
                 move prog-s-desc of prog1   to des3-level-3
                 move prog-type of prog1     to como-type3
              end-if

              modify bmp3-level-3, bitmap-number = como-num-bitmap
              display lbl3-level-3
              move como-num-bitmap to num-bmp-level-3
              if ef3-level-3-buf not = zero and como-type2 = 1
                   display message box
                   "Non puoi creare un nuovo sottolivello ad un eseguibile!"
                   title titolo-sposta
                   move 78-ID-ef-level-2 to control-id
                   move 4                to accept-control
                   set problema-livello to true
              end-if
              if ef3-level-3-buf = zero and ef3-level-4-buf = zero
                                        and cartella
                 display message box "Operazione non consentita!"
                 move 78-id-ef3-level-3  to control-id
                 move 4                  to accept-control
              end-if
           when 78-id-ef3-level-4
              if ef3-level-4-buf = zero and 
                 ef3-level-5-buf = zero
                 move space to des3-level-4
                 move 78-bmp-niente to como-num-bitmap
              else
                 move ef3-level-1-buf  to prog-level-1 of prog1
                 move ef3-level-2-buf  to prog-level-2 of prog1 
                 move ef3-level-3-buf  to prog-level-3 of prog1 
                 move ef3-level-4-buf  to prog-level-4 of prog1 
                 move zero             to prog-level-5 of prog1 
       
                 read Prog1
                    invalid
                       move space to prog-s-desc of prog1
                       move 78-bmp-niente to como-num-bitmap
                       if no-livello
                          display message box "Livello 3 non presente"
                          move 78-id-ef-level-3 to control-id
                          move 4 to accept-control
                          set no-livello to false
                       else
                          set no-livello to true
                       end-if
                    not invalid
                       perform VAL-COMO-NUM-BITMAP
                       set no-livello to false
                 end-read
                 move prog-s-desc of prog1   to des3-level-4
                 move prog-type of prog1     to como-type4
              end-if
              modify bmp3-level-4, bitmap-number = como-num-bitmap
              display lbl3-level-4
              move como-num-bitmap to num-bmp-level-4
              if ef3-level-4-buf not = zero and como-type3 = 1
                   display message box
                   "Non puoi creare un nuovo sottolivello ad un eseguibile!"
                   title titolo-sposta
                   move 78-ID-ef3-level-2 to control-id
                   move 4                 to accept-control
                   set problema-livello to true
              end-if
              if ef3-level-4-buf = zero and ef3-level-5-buf = zero
                                        and cartella
                 display message box "Operazione non consentita!"
                 move 78-id-ef3-level-4  to control-id
                 move 4                  to accept-control
              end-if
           when 78-id-ef3-level-5
              perform RIEMPI-CHIAVE-SPOSTA
              read prog with no lock
                 invalid
                    if vecchio
                       display message  MSG-record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                       set errori     to true
                       move 78-id-ef3-level-1 to store-id
                    end-if
                 not invalid      
                    if old-PROG-KEY not = PROG-KEY of prog
                       perform CURRENT-RECORD 
                       set errori to true
                    end-if
                    move 78-id-ef3-level-5 to CONTROL-ID
              end-read

      *    codice per la valorizzazione della bitmap
              if ef3-level-5-buf = zero
                 move space to des3-level-5
                 move 78-bmp-niente to como-num-bitmap
              else
                 move ef3-level-1-buf   to prog-level-1 of prog1
                 move ef3-level-2-buf   to prog-level-2 of prog1 
                 move ef3-level-3-buf   to prog-level-3 of prog1 
                 move ef3-level-4-buf   to prog-level-4 of prog1 
                 move ef3-level-5-buf   to prog-level-5 of prog1 
       
                 read Prog1
                    invalid
                       move space to prog-s-desc of prog1
                       move 78-bmp-niente to como-num-bitmap
                    not invalid
                       perform VAL-COMO-NUM-BITMAP
                 end-read
                 move prog-s-desc of prog1   to des3-level-5
                 move prog-type of prog1     to como-type5
              end-if
              modify bmp3-level-5, bitmap-number = como-num-bitmap
