   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true. 
      
           evaluate CONTROL-ID
           when 78-ID-ef-codice
                perform RIEMPI-CHIAVE
                perform SELEZIONA-NUMERICO
      
           when 78-ID-ef-des
                inquire ef-des value in tpa-descrizione
                if tpa-descrizione = spaces
                   set errori to true
                   display message "Descrizione obbligatoria"
                             title tit-err
                              icon 2
                end-if
      
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if nuovo move 0 to mod end-if.

           if mod = 1
              read tparameva lock invalid 
                 set errori to true 
              end-read
           else
              read tparameva no lock invalid 
                 set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0      to mod
                    move 1      to mod-k
                    move 78-ID-ef-codice to CONTROL-ID
                    move 4         to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1  set StatusModifica to true
                 else        set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG
                 perform VALORIZZA-OLD
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-codice to CONTROL-ID
                       move 4       to ACCEPT-CONTROL
                       display message MSG-Record-inesistente
                                 title tit-err
                                  icon 2
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT
BEGIN-TEXT
           move 0 to StatusHelp.
           move 1 to mod-k.
           set StatusVisua    to true. 
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-tpa-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if CONTROL-ID < 78-ID-ef-des
                       move 78-ID-ef-des to CONTROL-ID
                    end-if
                    move 0 to mod-k
                    move 1 to mod
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1    to mod
                 perform SALV-MOD            
                 move 0    to mod
                 move 1    to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    move 78-ID-ef-codice to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock tparameva all records
                 end-if
              end-if
                        
              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           perform SALV-MOD.

           if tutto-ok  
              move 78-ID-ef-codice to CONTROL-ID       
              move 4         to ACCEPT-CONTROL   
              move 1         to mod mod-k
              modify tool-modifica,  value = mod
              perform CANCELLA-COLORE
              perform VALORIZZA-NUOVO
              perform INIT-OLD-REC
              set StatusIns to true
              perform STATUS-BAR-MSG
              set nuovo to true
              unlock tparameva all records
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ORDINAMENTO
BEGIN-TEXT
           inquire tool-ord, value in OrderBy.
      
           if OrderBy = 0 perform DATASET1-CHANGETO-KEY1
           else           perform DATASET1-CHANGETO-KEY2
           end-if.
      
           perform CURRENT-RECORD.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok    
 
              perform CHECK-INSERIMENTO
              unlock tparameva all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tparameva-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tparameva-lock-flag
              end-if
                                  
              perform CANCELLA-COLORE
              perform FORM1-FIRST

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tparameva-lock-flag
                 move 1 to mod-k
                 perform FORM1-FIRST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".
           copy "status.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRECEDENTE
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tparameva all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tparameva-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tparameva-lock-flag
              end-if
                    
              perform CANCELLA-COLORE
              perform FORM1-PREVIOUS

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tparameva-lock-flag
                 move 1 to mod-k
                 perform FORM1-PREVIOUS
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           inquire ef-codice, value in ef-codice-buf.
           move ef-codice-buf       to tpa-codice.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.
      
           set tutto-ok to true.
      
           perform varying CONTROL-ID from 78-ID-ef-codice by 1
                     until CONTROL-ID > 78-ID-ef-des
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if tutto-ok
              inquire gd-mag, last-row in tot-righe
              if tot-righe = 1
                 set errori to true
                 move 78-ID-gd-mag to store-id
                 perform CANCELLA-COLORE
                 display message "Selezionare almeno un deposito"
                           title tit-err
                            icon 2
              end-if
           end-if.
      
           if errori
              perform ABILITAZIONI
              move store-id to CONTROL-ID
              move 4        to ACCEPT-CONTROL
           else                       
              perform FORM1-BUF-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              perform CANCELLA-COLORE 
                                     
              accept como-data from century-date
              accept como-ora  from time
          
              write tpa-rec invalid rewrite tpa-rec end-write
              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD.
      
           if NoSalvato
              display message MSG-Salvare-le-modifiche
                        title titolo
                         type mb-yes-no-cancel 
                       giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no
                   continue
              when other
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA
BEGIN-TEXT
           perform RIEMPI-CHIAVE.

           move "tparameva"   to como-file.
           call "zoom-gt"  using como-file, tpa-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = 0
              if old-tpa-chiave not = tpa-chiave or
               ( tpa-codice = 0 and mod = 0 )
                 move tpa-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to tpa-chiave
                    set ReadSecca to true
                    modify ef-codice, value tpa-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-des to CONTROL-ID       
                    move 4            to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA-NUMERICO
BEGIN-TEXT
           read tparameva no lock
                invalid
                if vecchio
                   display message MSG-record-inesistente
                             title tit-err
                              icon 2
                   perform CLEAR-SCREEN
                   set errori to true
                end-if
            not invalid      
                if old-tpa-chiave not = tpa-chiave or
                 ( tpa-codice = 0 and mod = 0 )
                   perform CURRENT-RECORD 
                end-if
           end-read.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           if event-data-1 = 1 and mod = 1
              move 1 to StatusHelp
           else
              move 0 to StatusHelp
           end-if.
           perform STATUS-HELP.
           inquire gd-mag, last-row in tot-righe.
           if event-data-2 < 2 or event-data-2 > tot-righe
              exit paragraph
           end-if.
           modify gd-mag, start-y = event-data-2, y = event-data-2 
                          start-x = 1,            x = 3,
                          region-color 481.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SUCCESSIVO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tparameva all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tparameva-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tparameva-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tparameva-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-codice to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock tparameva all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if. 
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ULTIMO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tparameva all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tparameva-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tparameva-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tparameva-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move tpa-rec to old-tpa-rec.
           set vecchio to true.

           move 0 to StatusHelp
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-NUOVO
BEGIN-TEXT
           move high-value to tpa-codice.

           start tparameva key <= tpa-codice
                 invalid move 1 to tpa-codice
             not invalid
                 read tparameva previous with no lock
                      at end    move 1 to tpa-codice
                  not at end    add  1 to tpa-codice
                 end-read
           end-start.

           move "00"    to status-tparameva.

           initialize tpa-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                                       
           perform FORM1-IUD-DISPLAY.

           if OrderBy = 1                             
              move high-value to tpa-descrizione
              start tparameva key <= tpa-descrizione
                    invalid continue 
                not invalid
                    read tparameva previous no lock 
                       at end continue 
                    end-read
              end-start
           end-if.

           modify gd-mag, reset-grid = 1.
           perform GD-MAG-CONTENT.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      * SE NON HO VALORIZZATO I CAMPI DI CREAZIONE LI VALORIZZO
           if tpa-data-creazione   = 0
              accept tpa-data-creazione from century-date
           end-if.
           if tpa-ora-creazione    = 0
              accept tpa-ora-creazione  from time
           end-if.
           if tpa-utente-creazione = space
              move user-codi to tpa-utente-creazione
           end-if.

           accept tpa-data-modifica from century-date.
           accept tpa-ora-modifica  from time.
           move user-codi    to tpa-utente-modifica.
END-TEXT
END-DATA
