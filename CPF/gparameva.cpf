BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd-mag-Ev-Msg-Begin-Entry
BEGIN-TEXT
           if mod = 0
              set event-action to event-action-fail
           else
              inquire gd-mag, cursor-x in colonna
              if colonna = 2
                 set event-action to event-action-fail
              else
                 inquire gd-mag(event-data-2, 1), cell-data in old-mag-codice
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd-mag-Ev-Msg-Finish-Entry
BEGIN-TEXT
           set tutto-ok to true.
           inquire gd-mag, cursor-y in riga.
           inquire gd-mag(riga, 1), cell-data in col-codice.
           move col-codice to mag-codice.
           if mag-codice = spaces
              add 1 to riga
              inquire gd-mag(riga, 1), cell-data in col-codice
              subtract 1 from riga
              modify gd-mag, record-to-delete riga
              modify gd-mag(riga, 1), cell-data col-codice
           else
              read tmagaz no lock 
                   invalid 
                   move spaces to mag-descrizione
                   set errori to true
                   set event-action to event-action-fail
                   display message "Magazzino NON valido"
                             title tit-err
                              icon 2
               not invalid
                   if mag-causale-eva  = spaces or
                      mag-causale-omag = spaces
                      set errori to true
                      set event-action to event-action-fail
                      display message "Magazzino NON valido: Causali non valorizzate"
                                title tit-err
                                 icon 2
                   end-if
              end-read
              move mag-descrizione to col-descrizione
              modify gd-mag(event-data-2, 2), cell-data col-descrizione
           end-if.

           if tutto-ok
              inquire gd-mag, last-row in tot-righe
              perform varying riga from 2 by 1 
                        until riga > tot-righe 
                 inquire gd-mag(riga, 1), cell-data in col-codice
                 if col-codice = mag-codice and riga not = event-data-2
                    set errori to true
                    subtract 1 from riga
                    display message "Magazzino già presente a riga " riga
                              title tit-err
                               icon 2
                    set event-action to event-action-fail
                    exit perform
                 end-if
              end-perform
           end-if.
END-TEXT
END-DATA

