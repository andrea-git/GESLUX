BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = gparameva
   ACTOBJ            = gparameva
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       TPARAMEVA-ERR SECTION.
           use after error procedure on tparameva.
           set RecLocked to false.
           evaluate status-tparameva
           when "35"
                display message "File [TPARAMEVA] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [TPARAMEVA] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[TPARAMEVA] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99"
           when "93"
                set RecLocked to true
                set errori    to true
                move 0 to mod
                modify tool-modifica, value = mod
                move 1 to mod-k
                display form1
                set StatusVisua to true
                perform STATUS-BAR-MSG
                move "00" to status-tparameva
                if ReadSecca
                   read tparameva    no lock 
                        invalid      continue
                        not invalid  perform FORM1-IUD-DISPLAY
                   end-read
                end-if
                display message box MSG-Record-gia-in-uso
                          title tit-err
                           icon 2
           when "23"
                move 1 to mod-k
                move 0 to mod
                modify tool-modifica, value = mod
                set StatusVisua to true
                perform STATUS-BAR-MSG 
                if nuovo              
                   initialize tpa-dati replacing numeric data by zeroes
                                            alphanumeric data by spaces
                   perform FORM1-FLD-TO-BUF
                   display form1-tb-1
                   set vecchio to true
                end-if
                perform ABILITAZIONI
                display form1
                |RISETTO IL VALORE A "23" PERCHE DENTRO AL PAR. CLEAR-SCREEN
                |CON L'OPERAZIONE UNLOCK VIENE RISETTATO A ZERO COSICCHÈ 
                |QUANDO TORNA DENTRO AI PAR. DI SCORRIMENTO DI TOTEM SI 
                |TROVA IN UNA SITUAZIONE CORRETTA E PROSEGUE CON LA READ
                |USCENDO CON UN MESSAGGIO DI ERRORE DI TIPO "46"
                move "23" to status-tparameva
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterFldToBuf
BEGIN-TEXT
           perform ABILITAZIONI.

           perform VALORIZZA-OLD.

           modify gd-mag, reset-grid = 1.
           perform GD-MAG-CONTENT.

           perform varying idx from 1 by 1 
                     until idx > 25
              add 1 to idx giving riga
              if tpa-el-deposito(idx) = spaces
                 exit perform
              end-if                                        
              move tpa-el-deposito(idx) to mag-codice
              move tpa-el-gg-cons-max(idx) to col-gg
              read tmagaz no lock
              move mag-codice      to col-codice
              move mag-descrizione to col-descrizione
              modify gd-mag(riga, 1), cell-data = col-codice        
              modify gd-mag(riga, 2), cell-data = col-descrizione
              modify gd-mag(riga, 3), cell-data = col-gg
           end-perform.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterAccept
BEGIN-TEXT
           evaluate key-status
           when 5 |MODIFICA
                inquire tool-modifica, enabled in e-modifica
                if e-modifica = 1
                   if mod = 0 move 1 to mod
                   else       move 0 to mod
                   end-if
                   modify tool-modifica, value = mod
                   perform MODIFICA
                end-if
           when 10 |ORDINAMENTO ALFABETICO 
                if OrderBy = 0 move 1 to OrderBy
                else           move 0 to OrderBy
                end-if
                modify TOOL-ORD, value OrderBy
                perform ORDINAMENTO
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeExit
BEGIN-TEXT
           perform SALV-MOD.

           if errori
              move 26 to key-status
              exit paragraph
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterBufToFld
BEGIN-TEXT
           perform varying idx from 1 by 1 
                     until idx > 25
              move spaces to tpa-el-deposito(idx)
           end-perform.

           inquire gd-mag, last-row in tot-righe.
           perform varying riga from 2 by 1 
                     until riga > tot-righe                   
              inquire gd-mag(riga, 1), cell-data in col-codice
              inquire gd-mag(riga, 3), cell-data in col-gg
              subtract 1 from riga giving idx        
              move col-codice to tpa-el-deposito(idx)
              move col-gg     to tpa-el-gg-cons-max(idx)
           end-perform.
END-TEXT
END-DATA
