BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CAMBIA-FONT-RIGA
BEGIN-TEXT
           modify gd-mov(riga), ROW-FONT = small-font.

           modify gd-mov(riga, 1),
                  bitmap        = elemento-bmp,
                  bitmap-number = 1,
                  bitmap-width  = 16.

           move event-data-2 to riga.
           modify gd-mov(riga), ROW-FONT = font-evidenzia-griglia.


           modify gd-mov(riga, 1),
                  bitmap        = elemento-bmp,
                  bitmap-number = 2,
                  bitmap-width  = 16.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CARICA-MOVIMENTI
BEGIN-TEXT
           move link-tof-anno   to reva-anno-ordf
           move link-tof-numero to reva-numero-ordf
           move low-value       to reva-riga-ordf

           start reva key not < reva-chiave-ordf
              invalid
                 continue
              not invalid
                 perform until 1 = 2
                    read reva next no lock
                       at end
                          exit perform
                    end-read
                    if link-tof-anno not = reva-anno-ordf or 
                       link-tof-numero not = reva-numero-ordf
                       exit perform
                    end-if
                    move reva-anno    to tmp-emc-anno
                    move reva-numero  to tmp-emc-numero

                    move zero         to tmp-emc-prg
                    write tmp-emc-rec
                       invalid
                          continue
                    end-write
                 end-perform
           end-start.


           set trovato to false.

           modify gd-mov, mass-update = 1.

           move low-value to tmp-emc-chiave
           start tmp-elmovc key not < tmp-emc-chiave
              invalid
                 continue
              not invalid
                 perform until 1 = 2
                    read tmp-elmovc next 
                       at end
                          exit perform
                    end-read
                    move tmp-emc-anno    to teva-anno
                    move tmp-emc-numero  to teva-numero

                    read teva no lock
                       invalid 
                          continue
                       not invalid
                          perform METTI-IN-GRIGLIA
                    end-read
                 end-perform
           end-start.
           modify gd-mov, mass-update = 0.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHIAMA-MANUTENZIONE
BEGIN-TEXT
           move como-anno          to LinkAnno.
           move como-numero-ordine to linkNumero.

           modify form2-Handle, visible false.
           call   "gbozze" using lk-blockpgm
                                   user-codi
                                   1|livello-abil
                                   gbozze-linkage.
           cancel "gbozze".                 
           modify form2-Handle, visible true.

           inquire gd-mov, cursor-y in riga.
           
           set CallManutenzione to false
           if RicaricaGrid
              move riga to store-riga
              perform FORM3-OPEN-ROUTINE
              move store-riga to riga
           end-if.
           
           move riga to event-data-2.
           modify gd-mov, CURSOR-Y = riga.
           perform CAMBIA-FONT-RIGA.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHIAMA-MANUTENZIONE-M
BEGIN-TEXT
           modify form2-handle, visible false.
           call   "gmovcvar" using lk-blockpgm, 
                                   user-codi,
                                   1,
                                   teva-k-mov,
                                   0.

           cancel "gmovcvar".
           modify form2-handle, visible true.

           inquire gd-mov, cursor-y in riga.
           
           set CallManutenzione to false
           if RicaricaGrid
              move riga to store-riga
              perform FORM3-OPEN-ROUTINE
              move store-riga to riga
           end-if.
           
           move riga to event-data-2.
           modify gd-mov, CURSOR-Y = riga.
           perform CAMBIA-FONT-RIGA.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = METTI-IN-GRIGLIA
BEGIN-TEXT
           move teva-anno       to Col-anno.

           move teva-numero     to Col-numero

           move teva-data       to como-data.
           perform DATE-TO-SCREEN.
           move como-data       to col-data.

      *     move teva-causale          to tca-codice.
      *     move "** NON TROVATA **"  to tca-descrizione.
      *     move "*"                  to tca-tipo.
      *     read tcaumag no lock invalid continue end-read.
      *     move tca-descrizione      to col-descau.
           
           set cli-tipo-F   to true.
           move teva-cod-forn         to cli-codice.
           move "** NON TROVATA **"   to cli-ragsoc-1.
           read clienti no lock 
              invalid 
                 continue 
           end-read.
           move cli-ragsoc-1         to col-rag-soc-clifor.


           move teva-mag  to mag-codice
           read tmagaz
              invalid
                 move "** NON TROVATO **"   to mag-descrizione
           end-read.
           move mag-descrizione to Col-magazzino
           evaluate true
           when teva-aperta
                move "APERTA"   to Col-stato
                move space            to col-anno-mov
                                         col-num-mov
           when teva-chiusa
                move "CHIUSA"   to Col-stato
                move teva-num-movim   to num-ed
                move num-ed           to col-num-mov
                move teva-anno-movim  to col-anno-mov
           end-evaluate

           modify gd-mov, record-to-add rec-grid

           inquire gd-mov, last-row = riga.
           set trovato to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MOVIM-DA-GRIGLIA
BEGIN-TEXT
           inquire gd-mov(riga), record-data = rec-grid.

           move col-anno-mov        to como-anno          teva-anno-movim.
           move col-num-mov         to como-numero-ordine teva-num-movim.
           move riga                to como-riga.

           if teva-anno-movim not = 0
              perform CHIAMA-MANUTENZIONE-M
           end-if.

           move como-riga to riga, event-data-2.
           modify gd-mov, cursor-y riga.
           perform SPOSTAMENTO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ORDINE-DA-GRIGLIA
BEGIN-TEXT
           inquire gd-mov(riga), record-data = rec-grid.

           move col-anno            to como-anno.
           move Col-numero          to como-numero-ordine.
           move riga                to como-riga.

           perform CHIAMA-MANUTENZIONE.

           move como-riga to riga, event-data-2.
           modify gd-mov, cursor-y riga.
           perform SPOSTAMENTO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "utydata.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           inquire gd-mov, last-row in tot-righe.
           if event-data-2 < 2 move 2 to event-data-2 end-if.
           if event-data-2 > tot-righe move tot-righe to event-data-2 end-if.
           if riga not = event-data-2 
              perform CAMBIA-FONT-RIGA
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ginqui-Ev-Before-Program
BEGIN-TEXT
      *     perform CALCOLA-COLORE-TRASPARENTE.

      * Verdana10B-Occidentale
           INITIALIZE WFONT-DATA, font-evidenzia-griglia
           MOVE 10               TO WFONT-SIZE
           MOVE "Arial"          TO WFONT-NAME
           SET WFONT-BOLD        TO true
           SET WFONT-ITALIC      TO true
           SET WFONT-UNDERLINE   TO FALSE
           SET WFONT-STRIKEOUT   TO FALSE
           SET WFONT-FIXED-PITCH TO FALSE
           MOVE 0                TO WFONT-CHAR-SET
           CALL "W$FONT" USING WFONT-GET-FONT, 
                     font-evidenzia-griglia, WFONT-DATA


           copy resource "elemento.bmp".
           call "W$BITMAP" using wbitmap-load, "elemento.bmp"
                          giving elemento-bmp.

           accept path-tmp-elmovc from environment "TEMP"
           accept como-data  from century-date
           accept como-ora   from time

           inspect path-tmp-elmovc replacing trailing space by low-value
           string path-tmp-elmovc  delimited by low-value
                  "/tmp-elmovc_"   delimited by size
                  como-data        delimited by size
                  "_"              delimited by size
                  como-ora         delimited by size
                  into path-tmp-elmovc

