BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form2
   ACTOBJ            = Form2
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           set CallManutenzione to false.
           perform Form3-Open-Routine.

           if not trovato
      *        display message box "Nessun movimento presente associato all'ordine"
      *                title = tit-err
      *                icon mb-error-icon
              perform FORM2-EXIT
           else                                  
              modify form2-Handle, visible = 1 
              move 2 to riga event-data-2
              modify gd-mov, CURSOR-Y = 2
              perform CAMBIA-FONT-RIGA
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = form3
   ACTOBJ            = form3
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           call "w$mouse" using set-mouse-shape, wait-pointer
           modify gd-mov, reset-grid  = 1.
           modify gd-mov, mass-update = 1. 
           perform GD-MOV-CONTENT.
           perform CARICA-MOVIMENTI.
           modify gd-mov, mass-update = 0.

           call "w$mouse" using set-mouse-shape, arrow-pointer.

           perform Form3-Exit.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form2
   ACTOBJ            = Form2
   TEXTMODE          = 0
   ENTRYPOINT        = AfterEndAccept
BEGIN-TEXT
           if CallManutenzione
              if event-data-1 >= 8
                 perform MOVIM-DA-GRIGLIA
              else
                 perform ORDINE-DA-GRIGLIA
              end-if
              set CallManutenzione to false
           end-if
END-TEXT
END-DATA
