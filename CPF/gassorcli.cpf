   PARAGRAPH         = chk-da-articoli-LinkTo
BEGIN-TEXT
           if chk-da-articoli-buf = 1
              inquire ef-art, value in art-codice
              read articoli no lock invalid continue end-read
              move art-prezzo-vendita to ef-prezzo-buf
              move colore-protetto    to c-prezzo
              display ef-prezzo
              perform CALCOLA-IMPONIBILE
           else
              move colore-non-protetto to c-prezzo
              display ef-prezzo
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-duplica-BeforeProcedure
BEGIN-TEXT
           move 2 to BitmapNumDuplica.
           modify pb-duplica, bitmap-number = BitmapNumDuplica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-duplica-AfterProcedure
BEGIN-TEXT
           move 1 to BitmapNumDuplica.
           modify pb-duplica, bitmap-number = BitmapNumDuplica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-duplica-LinkTo
BEGIN-TEXT
           if mod = 1 and vecchio
              perform RIEMPI-CHIAVE
              move asc-chiave to save-key
              unlock assorcli all records
              call   "dup-asc"
              cancel "dup-asc"
              move save-key to asc-chiave
              read assorcli lock invalid continue end-read
           else
              call   "dup-asc"
              cancel "dup-asc"
           end-if.

           perform CANCELLA-COLORE.
           move 666 to control-id.
           move   4 to accept-control.
           move   2 to BitmapNumDuplica.
           modify pb-duplica, bitmap-number = BitmapNumDuplica.
END-TEXT
END-DATA

