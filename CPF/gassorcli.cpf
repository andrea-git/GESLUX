           perform SALV-MOD.

           if errori
              move 26 to key-status
              exit paragraph
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterAccept
BEGIN-TEXT
           evaluate key-status
           when 5 |MODIFICA
                inquire tool-modifica, enabled in e-modifica
                if e-modifica = 1
                   if mod = 0
                      move 1 to mod
                   else          
                      move 0 to mod
                   end-if
                   modify tool-modifica, value = mod
                   perform MODIFICA
                end-if
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = gassorcli
   ACTOBJ            = gassorcli
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       ASSORCLI-ERR SECTION.
           use after error procedure on assorcli.
           set RecLocked to false.
           evaluate status-assorcli
           when "35"
                display message "File [ASSORCLI] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [ASSORCLI] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[ASSORCLI] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99" 
           when "93" 
                set RecLocked to true
                set errori    to true
                move colore-non-protetto to c-chiave
                move colore-protetto     to c-stato c-campi
                move 0 to mod
                modify tool-modifica, value  mod
                display form1
                set StatusVisua to true
                perform STATUS-BAR-MSG
                move "00" to status-assorcli
                if ReadSecca
                   read  assorcli    no lock 
                         invalid  continue
                     not invalid  perform FORM1-IUD-DISPLAY
                   end-read
                end-if
                display message MSG-Record-gia-in-uso
                          title tit-err
                        icon is mb-error-icon
           when "23"            
                move colore-non-protetto to c-chiave
                move 0 to mod
                modify tool-modifica, value = mod
