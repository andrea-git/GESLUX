              add 1 to idx
              add 1 to riga
              perform VALORE-RIGA
              perform GRID-TO-FLD
           end-perform.

           if v-riga = 0
              move ef-art-buf             to dis-articolo-finale
              move ef-mag-buf             to dis-magazzino-finale
              move ef-imb-buf             to dis-imballo-finale
              move ef-peso-buf            to dis-peso-finale
