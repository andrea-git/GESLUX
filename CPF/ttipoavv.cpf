BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ABILITAZIONI
BEGIN-TEXT
           if mod = 1
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else
              |SE SONO IN VISUA           
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move ZERO to e-salva e-cancella
           end-if.     

      * ISACCO (CORREZIONE SU LIVELLO ABILITAZIONE 2) - 10/11/2003
      * (CANCELLAZIONE NON PERMESSA)
           if livello-abil = 2 
              move ZERO to e-cancella
              move BitmapDeleteDisabled to BitmapNumDelete
           end-if.
      * FINE

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-nuovo, tool-salva, tool-cancella, tool-modifica.
      * FINE.

           perform GRID-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ANTEPRIMA
BEGIN-TEXT
           set anteprima       to true.
      *     perform paragrafo-stampa.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.

           display message MSG-cancellare-il-record-corrente
                     title titolo
                      type mb-yes-no
                   default mb-no
                    giving scelta.

           if scelta = mb-yes

              inquire form1-gd-1, cursor-y  in riga
              perform VALORE-RIGA

              delete ttipoavv record 
                 invalid 
                    continue 
              end-delete

              modify  form1-gd-1, record-to-delete = riga 
              inquire form1-gd-1, last-row in tot-righe
              if riga > tot-righe 
                 move tot-righe to riga 
              end-if
              modify form1-gd-1, cursor-y = riga, cursor-x = 1
              move riga to event-data-2
              perform COLORE-RIGA
              display message MSG-cancellazione-avvenuta-con-successo
                        title titolo
              set vecchio to true
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           continue.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = COLORE-RIGA
BEGIN-TEXT
           if riga < 2 
              move 2 to riga 
           end-if.

           modify form1-gd-1, start-x = 1, x     = 9,
                                  start-y = riga,
                                        y = riga,
                                  region-color 257,
                                  cursor-color 481.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           inquire form1-gd-1, cursor-y in riga.

           perform VALORE-RIGA.

           evaluate colonna
           when 1
                if nuovo
                   if tav-codice = zero or spaces 
                      set errori to true
                      move 1     to colonna
                      display message box MSG-Codice-obbligatorio
                              title = tit-err
                              icon MB-WARNING-ICON
                   else  
                      read ttipoavv
                         not invalid
                            set errori to true
                            display message MGS-codice-gia-inserito|"Codice gi inserito!"
                                    title = tit-err
                                    icon MB-WARNING-ICON
                      end-read
                   end-if
                end-if
           when 2
                if tav-messaggio= spaces
                   set errori to true
                   move 2 to colonna
                   display message box MSG-Descrizione-mancante|"Descrizione mancante"
                           title = tit-err
                           icon MB-WARNING-ICON
                end-if
           end-evaluate.

           if errori
              modify form1-gd-1, cursor-y = riga, cursor-x = colonna
              move riga    to event-data-2
              move colonna to event-data-1
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = GRID-HELP
BEGIN-TEXT
      * NON CI SONO ZOOM SULLE COLONNE 
      * QUINDI NON SERVE L'EVALUATE DELLA COLONNA. MA IL BOTTONE CERCA
      * SARA' SEMPRE DISABILITATO
           move BitmapZoomDisabled to BitmapNumZoom
           move 0 to e-cerca

           modify tool-cerca, bitmap-number = BitmapNumZoom,
                              enabled       = e-cerca 
           .

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT
BEGIN-TEXT
           set vecchio  to true.
           set tutto-ok to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INTESTAZIONE
BEGIN-TEXT
           perform FORM1-GD-1-CONTENT

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = LOAD-RECORD
BEGIN-TEXT
           modify form1-gd-1, mass-update = 1.

           move 1   to riga
           move low-value to tav-codice.
           start ttipoavv key is >= tav-chiave
              invalid 
                 continue
              not invalid
                 perform varying riga from 2 by 1 until 1 = 2
                    read ttipoavv next
                       at end 
                          exit perform
                       not at end

                          move tav-codice      to col-id 
                          move tav-messaggio   to col-messaggio
                          move space           to col-automatismo

                          modify form1-gd-1, record-to-add = rec-grid
                          perform SETTA-BMP
                     end-read                                           
                  end-perform
           end-start.

           modify form1-gd-1, mass-update = 0.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           inquire tool-modifica, value in mod.

      *--->MODIFICA (ISACCO)
           if nuovo 
              move 1 to mod 
           else
              |SPECIFICO PER I PGM. CON GRID-CONTROL
              if mod = 1
                 close    ttipoavv
                 open i-o ttipoavv allowing readers
              else
                 set tutto-ok to true
                 if NoSalvato
                    move 1 to mod
                    perform SALV-MOD
                 end-if   
                 if tutto-ok
                    move zero  to mod
                    close      ttipoavv
                    open input ttipoavv

      *             | SPECIFICO X LE TABELLE.
                    | FA IN MODO CHE RESTO SULLA CELLA IN CUI ERO
                    if ricarica
                       set ricarica to false
                       modify form1-gd-1, reset-grid = 1
                       perform INTESTAZIONE
                       perform LOAD-RECORD
                    end-if

                 else
                    move 1 to mod
                 end-if
              end-if
           end-if.

           perform ABILITAZIONI.

           modify tool-modifica, value mod.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           set tutto-ok to true.

           if nuovo
              perform VALORE-RIGA
              if tav-codice not = SPACES
                 perform SALVA
              else
                 set errori to true
              end-if
           end-if.

           if tutto-ok
              set nuovo to true
              inquire form1-gd-1,  last-row in tot-righe
              modify  form1-gd-1,  insert-rows = 1
              modify  form1-gd-1,  cursor-x    = 1 
                                   cursor-y    = tot-righe + 1
              move 1 to mod
              modify  tool-modifica,   value = mod

              add 1 to tot-righe giving riga
              set tav-no-auto  to true
              perform SETTA-BMP

      *        move 2   to num-bitmap
      *        modify form1-gd-1(riga, 3), 
      *                                      bitmap-number = num-bitmap
      *                                      bitmap conferma-bmp
      *                                      bitmap-width 19
      *                                      hidden-data = num-bitmap

              perform ABILITAZIONI

              initialize old-tav-rec replacing numeric data by zeroes
                                          alphanumeric data by spaces

              add 1 to tot-righe giving event-data-2
              perform COLORE-RIGA
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".   
           copy "cerca-testo.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           if nuovo
              perform VALORE-RIGA
              if tav-rec not = old-tav-rec
                 set NoSalvato to true
              else
                 set SiSalvato to true
              end-if
           end-if.

           set tutto-ok to true.
           if NoSalvato
              display message MSG-Salvare-le-modifiche, 
                        title titolo
                         type mb-yes-no-cancel
                       giving scelta

              evaluate scelta
              when mb-yes    
                   perform SALVA
              when mb-cancel 
                   set errori    to true
              when mb-no     
                   set tutto-ok  to true
                   set SiSalvato to true
                   set ricarica  to true
              end-evaluate

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if sisalvato or mod = 0 
              exit paragraph 
           end-if.

           inquire form1-gd-1, cursor-x in colonna, 
                               cursor-y in riga.


           perform varying colonna from 1 by 1 
                     until colonna > 9
              perform CONTROLLO

              if errori 
                 exit perform 
              end-if
           end-perform.

           if tutto-ok
              perform VALORIZZA-DATI-COMUNI
              write tav-rec 
                 invalid 
                    rewrite tav-rec 
              end-write

              set sisalvato to true
              set vecchio   to true
           else                   
              perform ABILITAZIONI
           end-if.                 
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           inquire form1-gd-1, cursor-x = colonna, cursor-y = riga.
                                                                       
           evaluate event-data-1
           when 3
                modify Form1-St-1-Handle, panel-index 1
                                          panel-style 1
                                          panel-text 
                 "Invio o Doppio Click per modificare l'automatismo"
           when other
                modify Form1-St-1-Handle, panel-index 1
                                          panel-style 1
                                          panel-text  spaces
           end-evaluate.

           if event-data-2 not = riga
              perform VALORE-RIGA
              if tav-codice = spaces or 0
                 modify form1-gd-1, record-to-delete riga
                 set vecchio to true
              else  
                 perform SALVA
                 if errori
                    move riga    to event-data-2
                    move colonna to event-data-1
                    set event-action to event-action-fail
                 end-if
              end-if
           else
              if colonna not = event-data-1
                 perform CONTROLLO
                 if errori
                    set event-action to event-action-fail
                 end-if
              end-if
           end-if.

      * VALORIZZO L'OLD DELLA RIGA SU CUI SONO ANDATO
           move event-data-2 to riga.
           move event-data-1 to colonna.
           perform VALORE-OLD-RIGA.

      * COLORAZIONE RIGA IN GRID
           perform COLORE-RIGA.

           perform GRID-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA
BEGIN-TEXT
           set stampa       to true.
      *     perform paragrafo-stampa.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-RIGA
BEGIN-TEXT
           inquire form1-gd-1(riga, 1), cell-data tav-codice.
           inquire form1-gd-1(riga, 2), cell-data tav-messaggio 
           inquire form1-gd-1(riga, 1), hidden-data tav-automatismi. 

      *     evaluate num-bitmap
      *     when 2 
      *          set tav-no-auto to true
      *     when 1 
      *          set tav-promo   to true
      *     end-evaluate

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-OLD-RIGA
BEGIN-TEXT
           if vecchio
              inquire form1-gd-1(riga, 1) cell-data tav-codice
              read ttipoavv               no lock
              move tav-rec                to old-tav-rec
           else
              initialize old-tav-rec replacing numeric data by zeroes
                                          alphanumeric data by spaces
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      * SE NON HO VALORIZZATO I CAMPI DI CREAZIONE LI VALORIZZO
           if tav-data-creazione = zero
              accept tav-data-creazione from century-date
           end-if.
           if tav-ora-creazione = zero
              accept tav-ora-creazione from time
           end-if.
           if tav-utente-creazione = space
              move user-codi to tav-utente-creazione
           end-if.

           accept tav-data-ultima-modifica from century-date.
           accept tav-ora-ultima-modifica from time.
           move user-codi to tav-utente-ultima-modifica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CAMBIA-BITMAP
BEGIN-TEXT
      *     inquire form1-gd-1(riga, 3), 
      *             hidden-data = num-bitmap

      *     if num-bitmap = 1
      *        move 2 to num-bitmap
      *     else
      *        move 1 to num-bitmap
      *     end-if
      *   
      *     modify form1-gd-1(riga, 3), bitmap-number = num-bitmap
      *                                 hidden-data = num-bitmap.
           perform SETTA-BMP

           perform VALORE-RIGA
           set NoSalvato to true
           perform SALVA
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SETTA-BMP
BEGIN-TEXT
           evaluate true
           when tav-no-auto
                move 1 to num-bitmap
                move 2 to num-bitmap-promo
                move 2 to num-bitmap-banner-t
                move 2 to num-bitmap-banner-g
           when tav-promo
                move 2 to num-bitmap
                move 1 to num-bitmap-promo
                move 2 to num-bitmap-banner-t
                move 2 to num-bitmap-banner-g
           when tav-banner-trad
                move 2 to num-bitmap
                move 2 to num-bitmap-promo
                move 1 to num-bitmap-banner-t
                move 2 to num-bitmap-banner-g
           when tav-banner-gdo
                move 2 to num-bitmap
                move 2 to num-bitmap-promo 
                move 2 to num-bitmap-banner-t
                move 1 to num-bitmap-banner-g
           end-evaluate

           modify form1-gd-1(riga, 3), 
                             bitmap-number = num-bitmap
                             bitmap conferma-bmp
                             bitmap-width 19
           modify form1-gd-1(riga, 4), 
                             bitmap-number = num-bitmap-promo
                             bitmap conferma-bmp
                             bitmap-width 19
           modify form1-gd-1(riga, 5), 
                             bitmap-number = num-bitmap-banner-t
                             bitmap conferma-bmp
                             bitmap-width 19
           modify form1-gd-1(riga, 6),
                             bitmap-number = num-bitmap-banner-g
                             bitmap conferma-bmp
                             bitmap-width 19

           modify form1-gd-1(riga, 1), hidden-data tav-automatismi

END-TEXT
END-DATA
