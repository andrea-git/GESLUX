BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = PAS004P-Ev-Before-Program
BEGIN-TEXT
      *<<** Customized_Default, SP-G, CST-BLOCKPGM **>>
           COPY RESOURCE "..\RESOURCE\CONFERMA.BMP".
           CALL "w$bitmap" USING WBITMAP-LOAD "CONFERMA.BMP", 
                   GIVING conferma-bmp.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Finish-Entry
BEGIN-TEXT
           inquire form1-gd-1, cursor-y in riga.
           perform VALORE-RIGA.

           perform  CONTROLLO.

           if errori 
              set event-action to event-action-fail 
              perform VALORE-RIGA
           end-if.

           if tav-rec not = old-tav-rec
              set NoSalvato to true                            
           else 
              set SiSalvato to true
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Begin-Entry
BEGIN-TEXT
           inquire form1-gd-1, cursor-x in colonna,
                               cursor-y in riga.
                         
           perform VALORE-RIGA.

           evaluate colonna
           when 1
                if tav-codice not = 0 and tav-codice not = spaces
                   set event-action to event-action-fail
                end-if
           when 2
                if mod = 0
                   set event-action to event-action-fail
                else
                   if tav-codice = 0 or spaces
                      set event-action to event-action-fail
                      modify form1-gd-1, cursor-y = riga, cursor-x = 1
                   end-if   
                end-if
           when 3
                set event-action to event-action-fail-terminate
                if mod = 1
                   inquire form1-gd-1, entry-reason in como-x
                   evaluate como-x
                   when X"0D"|invio
                   when X"00"|doppio click
                        move event-data-1 to colonna
                        move event-data-2 to riga
                        set tav-no-auto  to true
                        perform CAMBIA-BITMAP
                   end-evaluate 
                end-if
           when 4
                set event-action to event-action-fail-terminate
                if mod = 1
                   inquire form1-gd-1, entry-reason in como-x
                   evaluate como-x
                   when X"0D"|invio
                   when X"00"|doppio click
                        move event-data-1 to colonna
                        move event-data-2 to riga
                        set tav-promo  to true
                        perform CAMBIA-BITMAP
                   end-evaluate 
                end-if
           when 5
                set event-action to event-action-fail-terminate
                if mod = 1
                   inquire form1-gd-1, entry-reason in como-x
                   evaluate como-x
                   when X"0D"|invio
                   when X"00"|doppio click
                        move event-data-1 to colonna
                        move event-data-2 to riga
                        set tav-banner-trad  to true
                        perform CAMBIA-BITMAP
                   end-evaluate 
                end-if
           when 6
                set event-action to event-action-fail-terminate
                if mod = 1
                   inquire form1-gd-1, entry-reason in como-x
                   evaluate como-x
                   when X"0D"|invio
                   when X"00"|doppio click
                        move event-data-1 to colonna
                        move event-data-2 to riga
                        set tav-banner-gdo  to true
                        perform CAMBIA-BITMAP
                   end-evaluate 
                end-if
           end-evaluate 

END-TEXT
END-DATA
