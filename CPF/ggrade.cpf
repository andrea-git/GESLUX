   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           inquire form1-gd-1, last-row in tot-righe
           initialize gra-tab replacing numeric data by zeroes
                                   alphanumeric data by spaces
           perform varying riga from 2 by 1 
                     until riga > tot-righe
              perform VALORE-RIGA
              subtract 1 from riga giving idx
              move col-da    to gra-da(idx)
              move col-a     to gra-a(idx)
              move col-perce to gra-perce(idx)
           end-perform
           if gra-tab not = old-gra-tab and mod = 1
              set NoSalvato to true
           else
              set SiSalvato to true
           end-if.

           set tutto-ok to true.
           if NoSalvato
              display message box MSG-Salvare-le-modifiche, 
                             title = titolo
                             type = mb-yes-no-cancel
                             giving scelta

              evaluate scelta
              when mb-yes    
                   perform SALVA
              when mb-cancel 
                   set errori    to true
              when mb-no     
                   set tutto-ok  to true
                   set SiSalvato to true
                   set ricarica  to true
              end-evaluate

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.
      
           inquire form1-gd-1, cursor-x in colonna, 
                               cursor-y in riga.
      
           perform VALORE-RIGA.
           perform varying colonna from 1 by 1 
                     until colonna > 3
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           

           if tutto-ok
              initialize gra-tab replacing numeric data by zeroes
                                      alphanumeric data by spaces
              inquire form1-gd-1, last-row in tot-righe
              move 0 to idx
              perform varying riga from 2 by 1 
                        until riga > tot-righe
                 perform VALORE-RIGA             
                 move col-da    to como-da
                 move col-a     to como-a
                 move col-perce to como-perce
                 if como-da > 0 or como-a > 0 or como-perce > 0
                    add 1 to idx
                    move col-da    to gra-da(idx)
                    move col-a     to gra-a(idx)
                    move col-perce to gra-perce(idx)
                 end-if
              end-perform
              perform VALORIZZA-DATI-COMUNI
              write gra-rec invalid rewrite gra-rec end-write
              set sisalvato to true   

              modify form1-gd-1, reset-grid = 1
              perform INTESTAZIONE
              perform LOAD-RECORD

              move gra-tab  to old-gra-tab
           else
              perform ABILITAZIONI
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SET-SALVATO
BEGIN-TEXT
      *     if gra-tab       not = old-gra-tab     
      *        set NoSalvato to true
      *     else
      *        set SiSalvato to true
      *     end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           inquire form1-gd-1, cursor-x = colonna, cursor-y = riga.
      * colorazione riga in grid
           perform COLORE-RIGA.  
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-RIGA
BEGIN-TEXT
           move spaces to gra-codice.
           inquire form1-gd-1(riga, 1), cell-data in col-da.
           inquire form1-gd-1(riga, 2), cell-data in col-a.
           inquire form1-gd-1(riga, 3), cell-data in col-perce.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      *    se non ho valorizzato i campi di creazione li valorizzo
           if gra-data-creazione = 0
              accept gra-data-creazione from century-date
           end-if.
           if gra-ora-creazione = 0
              accept gra-ora-creazione from time
           end-if.
           if gra-utente-creazione = space
              move user-codi to gra-utente-creazione
           end-if.

           accept gra-data-ultima-modifica from century-date.
           accept gra-ora-ultima-modifica  from time.
           move user-codi to gra-utente-ultima-modifica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = NUOVO-LinkTo
BEGIN-TEXT
           inquire tool-nuovo, enabled in e-nuovo.
           if e-nuovo = 1
              perform NUOVO
           end-if.


END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = CANCELLA-LinkTo
BEGIN-TEXT
           inquire tool-cancella, enabled in e-cancella.
           if e-cancella = 1
              perform CANCELLA
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-SALVA-LinkTo
BEGIN-TEXT
           inquire tool-salva, enabled in e-salva.
           if e-salva = 1
              perform salva
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-MODIFICA-LinkTo
BEGIN-TEXT
           inquire tool-modifica, enabled in e-modifica
           if e-modifica = 1
              perform MODIFICA
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = I-O-BLOCCO
BEGIN-TEXT
