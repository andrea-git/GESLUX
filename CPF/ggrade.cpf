   PARENT            = ggrade
   ACTOBJ            = ggrade
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       GRADE-ERR SECTION.
           use after error procedure on grade.

           evaluate status-grade
           when "35"
                display message "File [GRADE] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [GRADE] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[GRADE] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "93"             
                move 0 to mod
                modify tool-modifica, value = mod  
                set StatusVisua to true
                open input grade
                display message box MSG-Gia-in-uso-su-altro-terminale
                            x"0d0a" MSG-SOLA-VISUA
                        title = tit-err
                        icon is mb-error-icon      
           end-evaluate.                         
END-TEXT
END-DATA
