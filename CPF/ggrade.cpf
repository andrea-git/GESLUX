BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = PAS004P-Ev-Before-Program
BEGIN-TEXT
      *<<** Customized_Default, SP-G, CST-BLOCKPGM **>>

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Finish-Entry
BEGIN-TEXT
           inquire form1-gd-1, cursor-y in riga.
           perform VALORE-RIGA.

           evaluate event-data-1
           when 3
                modify form1-gd-1(riga, 3), cell-data col-perce
                set environment "KEYSTROKE" to "DATA=44 44"
                set environment "KEYSTROKE" to "DATA=46 46"
           end-evaluate.

           perform  CONTROLLO.

           if errori
              set event-action to event-action-fail
              perform VALORE-RIGA
           end-if.

           perform SET-SALVATO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Begin-Entry
BEGIN-TEXT
           if mod = 0 
              set event-action to event-action-fail
           else
              inquire form1-gd-1, cursor-x in colonna,
                                  cursor-y in riga
                         
              perform VALORE-RIGA
              evaluate event-data-1
              when 3
                   set environment "KEYSTROKE" to "DATA=44 46"
              end-evaluate
           end-if.
END-TEXT
END-DATA
