BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
      *****     perform RIEMPI-TMP.
      *****
      *****     move "zoom-tordini"   to como-file.
      *****
      *****     inquire ef-anno, value in zoom-tor-anno.
      *****     inquire ef-num,  value in zoom-tor-numero.
      *****
      *****     if trovato
      *****        move path-zoom-tordini to ext-file
      *****        call "zoom-gt"   using como-file, zoom-tor-rec
      *****                        giving stato-zoom
      *****        end-call
      *****        cancel "zoom-gt"
      *****     else
      *****        move 1 to stato-zoom
      *****     end-if.
      *****     perform CANCELLA-TMP.
      *****
      *****     if stato-zoom = 0
      *****        move zoom-tor-num-fatt to ef-num-buf
      *****        display ef-num
      *****     end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
      *    
           when 78-ID-ef-anno
                inquire ef-anno, value in ef-anno-buf
                if ef-anno-buf = 0
                   display message "Anno obbligatorio"
                             title tit-err
                              icon 2
                   set errori to true
                end-if

           when 78-ID-ef-num
                inquire ef-anno, value in ef-anno-buf
                inquire ef-num,  value in ef-num-buf
                inquire cbo-doc, value in cbo-doc-buf
                if cbo-doc-buf = "Fatture Manuali"
                   set note-credito to false
                   move ef-anno-buf to tor-anno-fattura
                   move ef-num-buf  to tor-num-fattura
                   read tordini key k-fattura
                        invalid
                        set errori to true
                        display message "Fattura non registrata"
                                  title tit-err
                                   icon 2
                    not invalid
                        if tor-ordine
                           set errori to true
                           display message "Documento non valido"
                                     title tit-err
                                      icon 2
                        end-if
                   end-read
                else
                   set note-credito to true
                   move ef-anno-buf to tno-anno-fattura
                   move ef-num-buf  to tno-num-fattura
                   read tnotacr key k-fattura
                        invalid
                        set errori to true
                        display message "Nota Credito non registrata"
                                  title tit-err
                                   icon 2
                   end-read
                end-if
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move control-id to store-id        
              move 4          to accept-control  
           end-if.

END-TEXT
END-DATA
