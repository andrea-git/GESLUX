BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-ok2-LinkTo
BEGIN-TEXT
           display message "Salvare le modifiche?"
                     title titolo
                      type mb-yes-no
                    giving scelta
           if scelta = mb-yes
              if note-credito
                 inquire ef-note, value in tno-note
                 rewrite tno-rec
              else
                 inquire ef-note, value in tor-note
                 rewrite tor-rec
              end-if
              move 27 to key-status
              unlock tordini all records
              unlock tnotacr all records
           end-if.
END-TEXT
