BEGIN-TEXT
       USER-ERR SECTION.
           use after error procedure on user.
           set RecLocked to false.
           evaluate status-user
           when "35"
                display message "File [USER] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [USER] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[USER] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99"
           when "93"
                set RecLocked to true
                set errori    to true

                display message MSG-Record-gia-in-uso
                          title tit-err
                           icon 2
           end-evaluate.

      ***---
