BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterFldToBuf
BEGIN-TEXT
           move spaces to lab-des-marca-buf.
           move pvv-articolo to art-codice.
           read articoli no lock 
                invalid continue
            not invalid
                move art-descrizione to lab-art-buf
                move pvv-marca       to mar-codice
                read tmarche no lock
                     invalid continue
                 not invalid move mar-descrizione to lab-des-marca-buf
                end-read
           end-read.
           
           move pvv-agente to age-codice.
           read agenti no lock 
                invalid continue
            not invalid                 
                move age-ragsoc-1 to lab-age-buf
           end-read.

           set cli-tipo-C   to true.
           move pvv-cliente to cli-codice.
           read clienti no  lock 
                invalid continue
            not invalid
                move cli-ragsoc-1       to lab-cli-buf
           end-read.

           move pvv-data-fat to como-data.
           perform DATE-TO-SCREEN.
           move como-data    to ef-data-buf.

           move pvv-data-liq to como-data.
           perform DATE-TO-SCREEN.
           move como-data    to ef-liq-buf.
                                
           perform VALORIZZA-OLD.

           perform ABILITAZIONI.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterAccept
BEGIN-TEXT
           evaluate key-status
           when 5 |modifica
                inquire tool-modifica, enabled in e-modifica
                if e-modifica = 1
                   if mod = 0 move 1 to mod
                   else       move 0 to mod
                   end-if
                   modify tool-modifica, value = mod
                   perform MODIFICA
                end-if
           |when 10 |ordinamento alfabetico 
           |     if OrderBy = 0 move 1 to OrderBy
           |     else           move 0 to OrderBy
           |     end-if
           |     modify TOOL-ORD, value OrderBy
           |     perform ORDINAMENTO
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = gprovvig
   ACTOBJ            = gprovvig
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       PROVVIG-ERR SECTION.
           use after error procedure on provvig.
           set RecLocked to false.
           evaluate status-provvig
           when "35"
                display message "File [PROVVIG] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [PROVVIG] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[PROVVIG] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99" 
           when "93" 
                set RecLocked to true
                set errori    to true
                move 0 to mod mod-campi
                move 1 to mod-k
                modify tool-modifica, value = mod
                display form1
                set StatusVisua to true
                perform STATUS-BAR-MSG
                move "00" to status-provvig
                if ReadSecca
                   read provvig no lock 
                        invalid continue
                    not invalid perform FORM1-IUD-DISPLAY
                   end-read
                end-if
                display message box MSG-Record-gia-in-uso
                        title = tit-err
                        icon is mb-error-icon
           when "23"
                move 1 to mod-k
                move 0 to mod mod-campi
                modify tool-modifica, value = mod
                set StatusVisua to true
                perform STATUS-BAR-MSG 
                if nuovo              
                   initialize pvv-dati 
                                       replacing numeric data by zeroes
                                            alphanumeric data by spaces
                   perform FORM1-FLD-TO-BUF
                   display form1-tb-1
                   set vecchio to true
                end-if
                perform ABILITAZIONI
                display form1
                |risetto il valore a "23" perche dentro al par. CLEAR-SCREEN
                |con l'operazione unlock viene risettato a zero cosicchè 
                |quando torna dentro ai par. di scorrimento di Totem si 
                |trova in una situazione corretta e prosegue con la read
                |uscendo con un messaggio di errore di tipo "46"
                move "23" to status-provvig
           end-evaluate.
 
      ***---
       ARTICOLI-ERR SECTION.
           use after error procedure on articoli.
           set tutto-ok  to true.
           evaluate status-articoli
           when "35"
                display message "File [ARTICOLI] not found!"
