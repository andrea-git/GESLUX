BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CARICA-MOVIMENTI
BEGIN-TEXT
      *     move anno-from    to tmo-anno.     

           initialize path-tmp-k-ord.
           accept  path-tmp-k-ord from environment "PATH_ST".
           inspect path-tmp-k-ord replacing trailing spaces by low-value.
           accept  como-data from century-date.
           accept  como-ora  from time.
           string  path-tmp-k-ord delimited low-value
                   "ORD_"         delimited size
                   como-data      delimited size
                   "_"            delimited size
                   como-ora       delimited size
                   ".tmp"         delimited size
              into path-tmp-k-ord
           end-string.
           open output tmp-k-ord.
           close       tmp-k-ord.
           open i-o    tmp-k-ord.

           move anno-to      to tmo-anno.
           move ordine-from  to tmo-numero.

           start tmovmag key not < tmo-chiave
                 invalid continue
             not invalid perform SCORRI-ORDINI
           end-start.             
           close tmp-k-ord.


END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-causale-to
                move "tcaumag"  to como-file         
                inquire ef-causale-to, value in tca-codice
                call "zoom-gt"   using como-file, tca-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tca-codice      to causale-to
                   display ef-causale-to
                end-if
           when 78-ID-ef-causale-from
                move "tcaumag"  to como-file         
                inquire ef-causale-from, value in tca-codice
                call "zoom-gt"   using como-file, tca-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tca-codice      to causale-from
                   display ef-causale-from
                end-if
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHIAMA-MANUTENZIONE
BEGIN-TEXT
           move como-anno          to tmo-anno.
           move como-numero-ordine to tmo-numero.

           modify form2-Handle, visible false.
           call   "gmovcvar" using lk-blockpgm
                                   user-codi
                                   livello-abil
                                   tmo-chiave
                                   LinkRicaricaGrid.
           cancel "gmovcvar".                 
           modify form2-Handle, visible true.

           inquire gd-mov, cursor-y in riga.
           
           if tipo-elab = 2
              set CallManutenzione to false
              if RicaricaGrid
                 move riga to store-riga
                 perform FORM3-OPEN-ROUTINE
                 move store-riga to riga
              end-if
           end-if.
           
           move riga to event-data-2.
           modify gd-mov, CURSOR-Y = riga.
           perform CAMBIA-FONT-RIGA.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
      *
           when 78-ID-ef-anno
                inquire ef-anno, value in anno-to
                if anno-to = ZERO
                   set errori  to true
                   display message "Inserimento anno mancante"
                           title tit-err
                           icon  mb-warning-icon
                   move 78-ID-ef-anno   to store-id
                end-if
           when 78-ID-ef-ordine-from
                if tipo-elab = 1
                   if ordine-from = zero
                      if controllo-finale
                         set errori to true                                                

                         move 78-ID-ef-ordine-from to CONTROL-ID
                         display message "Il numero di movimento  obbligatorio!"
                                 title tit-err
                                 icon  MB-WARNING-ICON
                      end-if
                   else
      *                move anno-from     to tmo-anno
                      move anno-to       to tmo-anno
                      move ordine-from   to tmo-numero
                      read tmovmag
                         invalid 
                             set errori to true
                             move 78-ID-ef-ordine-from to CONTROL-ID
                             display message "Movimento inesistente!"
                                      title tit-err
                                      icon  MB-WARNING-ICON
                      end-read
                   end-if
                end-if
      *
           when 78-ID-ef-ordine-to
                if ordine-to = ZERO
                   move all "9" to ordine-to
                   modify EF-ORDINE-TO, VALUE ordine-to
                end-if
                if ordine-to < ordine-from
                   set errori to true
                   move 78-ID-ef-ordine-to to CONTROL-ID
                   display message MSG-intervallo-num-ord-err
                      title tit-err
                      icon  MB-WARNING-ICON
                end-if
      *
           when 78-ID-ef-data-from
                if data-from not = zero
                   move data-from to como-data
                   perform DATE-FORMAT
                   move como-data to data-from
                end-if

           when 78-ID-ef-data-to
                if data-to = zero
                   move 99999999 to data-to
                end-if
                if data-to not = 99999999
                   move data-to to como-data
                   perform DATE-FORMAT
                   move como-data to data-to
                end-if

                move data-from to como-data
                perform DATE-TO-FILE
                move como-data to data-from-AAAAMMGG

                move data-to   to como-data
                perform DATE-TO-FILE
                move como-data to data-to-AAAAMMGG

                if data-to-AAAAMMGG < data-from-AAAAMMGG
                   set errori to true
                   move 78-ID-ef-data-to to CONTROL-ID
                   display message MSG-intervallo-date-err
                      title tit-err
                      icon  MB-WARNING-ICON
                end-if
      *
           when 78-ID-ef-causale-to
                if causale-to = ZERO
                   move all "9" to causale-to
                   modify ef-causale-to, value causale-to
                end-if
                if causale-to < causale-from
                   set errori to true
                   move 78-ID-ef-causale-to to control-id
                   display message MSG-intervallo-causale-err
                      title tit-err
                      icon  MB-WARNING-ICON
                end-if

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if.

           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = METTI-IN-GRIGLIA
BEGIN-TEXT
           move tmo-anno       to Col-anno.

           move tmo-numero     to Col-numero

           move tmo-data-movim to como-data.
           perform DATE-TO-SCREEN.
           move como-data      to col-data.

           move tmo-causale          to tca-codice.
           move "** NON TROVATA **"  to tca-descrizione.
           move "*"                  to tca-tipo.
           read tcaumag no lock invalid continue end-read.
           move tca-descrizione      to col-descau.
           
           move tca-tipo to col-clifor cli-tipo-CF.

           move tmo-cod-clifor       to cli-codice.
           move "** NON TROVATA **"  to cli-ragsoc-1.
           read clienti no lock invalid continue end-read.
           move cli-ragsoc-1         to col-rag-soc-clifor.

           move tmo-numdoc-clifor    to col-numdoc.

           move tmo-data-doc         to como-data.
           perform DATE-TO-SCREEN.
           move como-data            to col-datadoc.

           modify gd-mov, record-to-add rec-grid

           inquire gd-mov, last-row = riga.

           modify gd-mov(riga), row-color = como-colore. 
           set trovato to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ORDINE-DA-GRIGLIA
BEGIN-TEXT
           inquire gd-mov(riga), record-data = rec-grid.

           move col-anno            to como-anno.
           move Col-numero          to como-numero-ordine.
           move riga                to como-riga.

           perform CHIAMA-MANUTENZIONE.

           move como-riga to riga, event-data-2.
           modify gd-mov, cursor-y riga.
           perform SPOSTAMENTO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "utydata.cpy".
           copy "color-custom.cpy".
           copy "calcola-colore-trasparente.cpy".

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           inquire gd-mov, last-row in tot-righe.
           if event-data-2 < 2 move 2 to event-data-2 end-if.
           if event-data-2 > tot-righe move tot-righe to event-data-2 end-if.
           if riga not = event-data-2 
              perform CAMBIA-FONT-RIGA
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SCORRI-ORDINI
BEGIN-TEXT
           set trovato to false.

           modify gd-mov, mass-update = 1.
           perform until 1 = 2
              read tmovmag next at end exit perform end-read

              if tmo-anno > anno-to    exit perform end-if
              
              perform VALIDA-MOVIMENTO
              
              if record-ok perform METTI-IN-TMP end-if

           end-perform.                        

           if trovato
              perform TMP-TO-GRID
           end-if.

           modify gd-mov, mass-update 0.


      ***---
       METTI-IN-TMP.
           move tmo-data-movim to tko-data.
           move tmo-chiave     to tko-chiave-pri.
           write tko-rec.
           set trovato to true.

      ***---
       TMP-TO-GRID.     
           move low-value to tko-rec.
           start tmp-k-ord key >= tko-chiave
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read tmp-k-ord next at end exit perform end-read
                    move tko-chiave-pri to tmo-chiave
                    read tmovmag no lock
                    perform METTI-IN-GRIGLIA
                 end-perform
           end-start.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALIDA-MOVIMENTO
BEGIN-TEXT
           set record-ok to true.

           if tmo-numero < ordine-from or 
              tmo-numero > ordine-to
              set record-ok to false
           end-if.

           if tmo-data-movim < data-from-AAAAMMGG or 
              tmo-data-movim > data-to-AAAAMMGG
              set record-ok to false
           end-if.

           if tmo-causale < causale-from or 
              tmo-causale > causale-to
              set record-ok to false
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ginqui-Ev-Before-Program
BEGIN-TEXT
      *<<** Customized_Default, SP-G, Screen4-blockpgm-1 **>>
           perform CALCOLA-COLORE-TRASPARENTE.

      * Verdana10B-Occidentale
           INITIALIZE WFONT-DATA, font-evidenzia-griglia
           MOVE 10               TO WFONT-SIZE
           MOVE "Arial"          TO WFONT-NAME
           SET WFONT-BOLD        TO true
           SET WFONT-ITALIC      TO true
           SET WFONT-UNDERLINE   TO FALSE
           SET WFONT-STRIKEOUT   TO FALSE
           SET WFONT-FIXED-PITCH TO FALSE
           MOVE 0                TO WFONT-CHAR-SET
           CALL "W$FONT" USING WFONT-GET-FONT, 
                     font-evidenzia-griglia, WFONT-DATA


           copy resource "elemento.bmp".
           call "W$BITMAP" using wbitmap-load, "elemento.bmp"
                          giving elemento-bmp.

           move spaces to tge-anno.
           open input tparamge.
           move spaces to tge-codice.
           read tparamge no lock invalid continue end-read.
           close tparamge.
END-TEXT
