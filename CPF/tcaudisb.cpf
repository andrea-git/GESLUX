BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ABILITAZIONI
BEGIN-TEXT
           if mod = 1
              move BitmapDeleteEnabled to BitmapNumDelete
              move BitmapSaveEnabled   to BitmapNumSave
              move 1 to e-salva e-cancella
           else
              |se sono in visua           
              move BitmapDeleteDisabled to BitmapNumDelete
              move BitmapSaveDisabled   to BitmapNumSave
              move 0 to e-salva e-cancella
           end-if.     

      * ISACCO (CORREZIONE SU LIVELLO ABILITAZIONE 2) - 10/11/2003
      * (CANCELLAZIONE NON PERMESSA)
           if livello-abil = 2 
              move ZERO to e-cancella
              move BitmapDeleteDisabled to BitmapNumDelete
           end-if.
      * FINE

           modify tool-cancella, enabled = e-cancella.
           modify tool-salva,    enabled = e-salva.
       
      * ISACCO (SERVE SEMPRE PER LA MODIFICA PRCEDENTE E PER RIPRISTINARE
      * LE BITMAP DISABILITATE. - 10/11/2003
      *     perform FORM1-DISPLAY.
           display tool-nuovo, tool-salva, tool-cancella, tool-modifica.
      * FINE.

           perform GRID-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ANTEPRIMA
BEGIN-TEXT
           set anteprima       to true.
      *     perform paragrafo-stampa.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA
BEGIN-TEXT
      *--->modifica (isacco)
           if mod = 0 exit paragraph end-if.
      *--->fine.                 

           display message box MSG-cancellare-il-record-corrente
                   title = titolo
                   type mb-yes-no
                   default mb-no
                   giving scelta.

           if scelta = mb-yes

              inquire form1-gd-1, cursor-y  in riga
              perform VALORE-RIGA

              delete tcaudisb record invalid continue end-delete

              modify  form1-gd-1, record-to-delete = riga 
              inquire form1-gd-1, last-row in tot-righe
              if riga > tot-righe 
                 move tot-righe to riga 
              end-if
              modify form1-gd-1, cursor-y = riga, cursor-x = 1
              move riga to event-data-2
              perform COLORE-RIGA
              display message MSG-cancellazione-avvenuta-con-successo
                      title = titolo
           end-if.


END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           inquire form1-gd-1, cursor-x in colonna.
           evaluate colonna
           when 3
                perform VALORE-RIGA
                move tdc-caus-carico to tca-codice
                set  tca-fornitore   to true
                move "tcaumag-forn-mov-qta"  to como-file
                call   "zoom-gt"  using como-file, tca-rec
                                 giving stato-zoom

                if stato-zoom = 0
                   move tca-codice to col-carico tdc-caus-carico
                   modify form1-gd-1(riga, 3),
                                     cell-data = col-carico
                   perform SET-SALVATO
                end-if
           when 4
                perform VALORE-RIGA
                move tdc-caus-scarico to tca-codice
                set  tca-fornitore    to true
                move "tcaumag-forn"   to como-file
                call   "zoom-gt"   using como-file, tca-rec
                                  giving stato-zoom

                if stato-zoom = 0
                   move tca-codice to col-scarico tdc-caus-scarico
                   modify form1-gd-1(riga, 4),
                                     cell-data = col-scarico
                   perform SET-SALVATO
                end-if
           end-evaluate. 
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = COLORE-RIGA
BEGIN-TEXT
      *     if event-data-2 < 2 
      *        move 2 to event-data-2 
           if riga < 2 
              move 2 to riga
           end-if.

           modify form1-gd-1, start-x = 1, x     = 4,
                                  start-y = riga|event-data-2,
                                        y = riga|event-data-2,
                                  region-color 257,
                                  cursor-color 481.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           if mod = 0 exit paragraph end-if.
      
           inquire form1-gd-1, cursor-y in riga.
      
           evaluate colonna
           when 1
                if nuovo
                   if tdc-codice = spaces
                      set errori to true
                      move 1     to colonna
                      display message box MSG-Codice-obbligatorio
                              title = tit-err
                              icon mb-warning-icon
                   else  
                      read tcaudisb
                           not invalid
                               set errori to true
                               display message MGS-codice-gia-inserito
                                       title = tit-err
                                       icon mb-warning-icon
                      end-read
                   end-if
                end-if
           when 2
                if tdc-descrizione = spaces
                   set errori to true
                   move 2 to colonna
                   display message box MSG-Descrizione-mancante
                           title = tit-err
                           icon mb-warning-icon
                end-if
           when 3
                move tdc-caus-carico to tca-codice
      
                read tcaumag
                     invalid
                     set errori to true
                     display message "Causale di carico non valida"
                               title tit-err
                                icon mb-warning-icon
                 not invalid
                     if tca-cliente
                        set errori to true
                        display message
                        "Causale di carico non valida "
                        "in quanto di tipo cliente"
                                  title tit-err
                                   icon mb-warning-icon
                     else
                        if tca-no-movim
                           set errori to true
                           display message
                           "Causale di carico non valida in "
                           "quanto non movimenta il magazzino"
                                     title tit-err
                                      icon mb-warning-icon
                        else
                           if tca-no-movim-giac-periodo
                              set errori to true
                              display message
                              "Causale di carico non valida in "
                              "quanto genera variazione di valore"
                                        title tit-err
                                         icon mb-warning-icon
                           end-if
                        end-if
                     end-if
                end-read
           when 4
                move tdc-caus-scarico to tca-codice
      
                read tcaumag
                     invalid
                     set errori to true
                     display message "Causale di scarico non valida"
                               title tit-err
                                icon mb-warning-icon
                 not invalid
                     if tca-cliente
                        set errori to true
                        display message
                        "Causale di scarico non valida "
                        "in quanto di tipo cliente"
                                  title tit-err
                                   icon mb-warning-icon
                     else
                        if tca-no-movim
                           set errori to true
                           display message
                           "Causale di scarico non valida in "
                           "quanto non movimenta il magazzino"
                                     title tit-err
                                      icon mb-warning-icon
                        else
                           if tca-no-movim-giac-periodo
                              set errori to true
                              display message
                              "Causale di scarico non valida in "
                              "quanto genera variazione di valore"
                                        title tit-err
                                         icon mb-warning-icon
                           end-if
                        end-if
                     end-if
                end-read
           end-evaluate.
      
           if errori
              modify form1-gd-1, cursor-y = riga, cursor-x = colonna
              move riga    to event-data-2
              move colonna to event-data-1
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = GRID-HELP
BEGIN-TEXT
      *    se non sono in modifica disabilito sempre lo zoom
           if mod = 0
              move BitmapZoomDisabled to BitmapNumZoom
              move 0 to e-cerca
              modify tool-cerca, bitmap-number = BitmapNumZoom,
                                 enabled       = e-cerca
           else
              evaluate colonna|event-data-1
              when 1
              when 2
                   move BitmapZoomDisabled to BitmapNumZoom
                   move 0 to e-cerca
              when 3
              when 4
                   move BitmapZoomEnabled to BitmapNumZoom
                   move 1 to e-cerca
              when other
                   move BitmapZoomDisabled to BitmapNumZoom
                   move 0 to e-cerca
              end-evaluate
           
              modify tool-cerca, bitmap-number = BitmapNumZoom,
                                 enabled       = e-cerca
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT
BEGIN-TEXT
           set vecchio  to true.
           set tutto-ok to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INTESTAZIONE
BEGIN-TEXT
           modify form1-gd-1(1, 1), cell-data = "Codice".
           modify form1-gd-1(1, 2), cell-data = "Descrizione".
           modify form1-gd-1(1, 3), cell-data = "Causale di Carico".
           modify form1-gd-1(1, 4), cell-data = "Causale di Scarico".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = LOAD-RECORD
BEGIN-TEXT
           modify form1-gd-1, mass-update = 1.
           move low-value to tdc-codice.
           start tcaudisb key is >= tdc-chiave
                  invalid continue
              not invalid
                  perform varying riga from 2 by 1
                          until 1 = 2
                     read tcaudisb next
                          at end exit perform
                      not at end
                          move tdc-codice           to col-codice
                          move tdc-descrizione      to col-des
                          move tdc-caus-carico      to col-carico
                          move tdc-caus-scarico     to col-scarico
                          modify form1-gd-1, record-to-add = rec-grid
                     end-read                                           
                  end-perform
           end-start.
           modify form1-gd-1, mass-update = 0.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           inquire tool-modifica, value in mod.

      *--->modifica (isacco)
           if nuovo 
              move 1 to mod 
           else
              |specifico per i pgm. con grid-control
              if mod = 1
                 close tcaudisb
                 open i-o tcaudisb allowing readers
              else
                 set tutto-ok to true
                 if NoSalvato
                    move 1 to mod
                    perform SALV-MOD
                 end-if   
                 if tutto-ok
                    move 0 to mod
                    close tcaudisb
                    open input tcaudisb

      *             | specifico x le tabelle.
                    | fa in modo che resto sulla cella in cui ero
                    if ricarica
                       set ricarica to false
                       modify form1-gd-1, reset-grid = 1
                       perform INTESTAZIONE
                       perform LOAD-RECORD
                    end-if

                 else
                    move 1 to mod
                 end-if
              end-if
           end-if.

           perform ABILITAZIONI.

           modify tool-modifica, value mod.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           set tutto-ok to true.

           if nuovo
              perform valore-riga
              if tdc-codice not = space
                 perform SALVA
              else
                 set errori to true
              end-if
           end-if.

           if tutto-ok
              set nuovo to true
              inquire form1-gd-1,  last-row in tot-righe
              modify  form1-gd-1,  insert-rows = 1
              modify  form1-gd-1,  cursor-x    = 1 
                                   cursor-y    = tot-righe + 1
              move 1 to mod
              modify  tool-modifica,   value = mod

              add 1 to tot-righe giving riga

              perform ABILITAZIONI

              initialize old-tdc-rec replacing numeric data by zeroes
                                          alphanumeric data by spaces
              perform COLORE-RIGA
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".   
           copy "cerca-testo.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           if nuovo
              perform VALORE-RIGA
              if tdc-rec not = old-tdc-rec
                 set NoSalvato to true
              else
                 set SiSalvato to true
              end-if
           end-if.

           set tutto-ok to true.
           if NoSalvato
              display message box MSG-Salvare-le-modifiche, 
                             title = titolo
                             type = mb-yes-no-cancel
                             giving scelta

              evaluate scelta
              when mb-yes    
                   perform SALVA
              when mb-cancel 
                   set errori    to true
              when mb-no     
                   set tutto-ok  to true
                   set SiSalvato to true
                   set ricarica  to true
              end-evaluate

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if sisalvato or mod = 0 exit paragraph end-if.
      
           inquire form1-gd-1, cursor-x in colonna, 
                               cursor-y in riga.
      
           perform VALORE-RIGA.
           perform varying colonna from 1 by 1 
                     until colonna > 4
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if tutto-ok
              perform VALORIZZA-DATI-COMUNI
              write tdc-rec invalid rewrite tdc-rec end-write
              set sisalvato to true
              set vecchio   to true
           else
              perform ABILITAZIONI
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SET-SALVATO
BEGIN-TEXT
           if tdc-codice       not = old-tdc-codice      or
              tdc-descrizione  not = old-tdc-descrizione or
              tdc-caus-carico  not = old-tdc-caus-carico or
              tdc-caus-scarico not = old-tdc-caus-scarico
              set NoSalvato to true
           else
              set SiSalvato to true
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           inquire form1-gd-1, cursor-x = colonna, cursor-y = riga.
      
           if event-data-2 not = riga
              perform VALORE-RIGA
              if tdc-codice = spaces
                 modify form1-gd-1, record-to-delete riga
                 set vecchio to true
              else  
                 perform SALVA
                 if errori
                    move riga    to event-data-2
                    move colonna to event-data-1 | (isacco)
                    set event-action to event-action-fail
                 end-if
              end-if
           else
              if colonna not = event-data-1
                 perform VALORE-RIGA
                 perform CONTROLLO
                 if errori
                    set event-action to event-action-fail
                 end-if
              end-if
           end-if.
      
      *    valorizzo l'old della riga su cui sono andato
           move    event-data-2 to riga.
           move    event-data-1 to colonna.
           perform VALORE-OLD-RIGA.
      
      * colorazione riga in grid
           perform COLORE-RIGA.
      
           perform GRID-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA
BEGIN-TEXT
           set stampa       to true.
      *     perform paragrafo-stampa.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-RIGA
BEGIN-TEXT
           inquire form1-gd-1(riga, 1), cell-data in tdc-codice.
           inquire form1-gd-1(riga, 2), cell-data in tdc-descrizione. 
           inquire form1-gd-1(riga, 3), cell-data in tdc-caus-carico. 
           inquire form1-gd-1(riga, 4), cell-data in tdc-caus-scarico.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-OLD-RIGA
BEGIN-TEXT
           if vecchio
              perform VALORE-RIGA
              move tdc-codice        to old-tdc-codice
              move tdc-descrizione   to old-tdc-descrizione
              move tdc-caus-carico   to old-tdc-caus-carico
              move tdc-caus-scarico  to old-tdc-caus-scarico
           else
              initialize old-tdc-rec replacing numeric data by zeroes
                                          alphanumeric data by spaces
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      *    se non ho valorizzato i campi di creazione li valorizzo
           if tdc-data-creazione = 0
              accept tdc-data-creazione from century-date
           end-if.
           if tdc-ora-creazione = 0
              accept tdc-ora-creazione from time
           end-if.
           if tdc-utente-creazione = space
              move user-codi to tdc-utente-creazione
           end-if.

           accept tdc-data-ultima-modifica from century-date.
           accept tdc-ora-ultima-modifica  from time.
           move user-codi to tdc-utente-ultima-modifica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = NUOVO-LinkTo
BEGIN-TEXT
           inquire tool-nuovo, enabled in e-nuovo.
           if e-nuovo = 1
              perform NUOVO
           end-if.


END-TEXT
END-DATA
