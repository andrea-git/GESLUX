BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-Gd-1-Ev-Msg-Finish-Entry
BEGIN-TEXT
           set tutto-ok to true.
           perform X-Y.
           perform VALORE-RIGA.

           evaluate colonna
           when 78-col-art
                if como-articolo = 0
                   if col-des    not = spaces or
                      como-perce not = 0
                      display message "Cancellare la riga?"
                                title titolo
                                 type mb-yes-no
                              default mb-no
                               giving scelta
                      if scelta = mb-no
                         set errori to true
                      else
                         subtract 1 from riga giving idx
                         perform COMPATTA-OCCURS

                         perform VALORE-RIGA

                         modify form1-gd-1, record-to-delete = riga
                         modify form1-gd-1, insert-rows = 1
                      end-if
                   end-if
                else
                   perform CONTROLLO-RIGA
                end-if
           when 78-col-qta
                modify form1-gd-1(riga, 78-col-qta), cell-data col-qta
                perform CONTROLLO-RIGA
           when 78-col-perce
                modify form1-gd-1(riga, 78-col-perce), 
                                                  cell-data col-perce
                perform CONTROLLO-RIGA
           end-evaluate.

           if errori
              move 78-ID-form1-gd-1 to store-id
              set event-action to event-action-fail
           else
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if.

END-TEXT
