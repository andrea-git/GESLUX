END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-disattiva-LinkTo
BEGIN-TEXT
           inquire ef-pass, value in password.

           if password = spaces
              move 100 to control-id
              move   4 to accept-control
           else
              if password not = SavePassword
                 display message box "Password errata!"
                         title = tit-err
                         icon 2
                 move 100 to control-id
                 move   4 to accept-control
                 modify pb-conferma, bitmap-number = 1
                 modify pb-esci,     bitmap-number = 1
              else
                 set Passwd-StatusOk to true
                 move 27 to key-status
              end-if
           end-if.
END-TEXT
