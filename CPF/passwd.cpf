BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = ScreenPass
   ACTOBJ            = ScreenPass
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           accept path-txt from environment "PATH_TXT".

           if path-txt = spaces
              move "pass" to path-txt
           end-if.

           set tutto-ok to true.
           open input pass.
           if tutto-ok
           
              |1.  "lxboss" generica
              |2.  "lubex69" stati
              |3.  fido cliente
              |4.  modifica bolla
              |5.  cambio stato ordini fornitori
              |6.  evasiona manuale master
              |7.  stato cliente
              |8.  gestione sessioni
              |9.  sostituzione articoi da gclienti
              |10. cambio codice articolo collegato
              |11. conferma cliente bloccato da bozze note credito
              |12. conferma vettore "su autorizzazione"
              |13. modifica bozze non LBX 
              |14. modifica dati bolla
              |15. modifica scorta su articoli
              |16. modifica stato articoli
              |17. modifica tipologia clienti
              |18. azzeramento ordini fornitori
              perform Passwd-password times
                 read pass next at end continue end-read
              end-perform

              move pass-rec to SavePassword
           end-if.
           
           set Passwd-StatusOk to false.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = passwd
   ACTOBJ            = passwd
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       PASS-ERR SECTION.
           use after error procedure on pass.
           set RecLocked to false.
           evaluate status-pass
           when "35"
                display message "File [PASS] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [PASS] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[PASS] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           end-evaluate.
END-TEXT
END-DATA
