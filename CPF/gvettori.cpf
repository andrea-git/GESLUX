BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO-ARROT
BEGIN-TEXT
           set tutto-ok to true.
           perform X-Y-ARROT.
      
           if event-data-2 not = riga
      
              perform VALORE-RIGA-ARROT
      
              perform varying colonna from 1 by 1
                        until colonna > 2
                 perform CONTROLLO-RIGA-ARROT
                 if errori exit perform end-if
              end-perform
      
              if tutto-ok
                 evaluate true
                 when event-data-2 < 2 
                      move 2            to riga
                      move event-data-1 to colonna
      
                 when event-data-2 > tot-righe
                      add  1                to tot-righe giving riga
                      set event-action      to event-action-fail
                      move 1                to colonna
                      perform PUNTA-CELLA-ARROT
                 when other
                      move event-data-2 to riga
                      move event-data-1 to colonna
                 end-evaluate
                 perform VALORE-RIGA-ARROT
              else
                 set event-action      to event-action-fail
                 perform PUNTA-CELLA-ARROT
              end-if
           else
              move event-data-1 to colonna
           end-if.
      
           perform COLORE-RIGA-ARROT.
           perform HELP-GRID-ARROT.
      
      ***---
       COLORE-RIGA-ARROT.
           modify gd-arrot, start-x = 1,
                                  x = 3,
                            start-y = riga,
                                  y = riga,
                       region-color = 257,
                       cursor-color = 481.
END-TEXT
END-DATA
