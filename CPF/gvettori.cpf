BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd-arrot-Ev-Msg-Begin-Entry
BEGIN-TEXT
           if mod = 0
              set event-action to event-action-fail
           else
              perform X-Y-ARROT
              evaluate colonna
              when 1 perform SET-VIRGOLA
              when 2 
              when 3 if arrot-da-num = 0 and
                        arrot-a-num  = 0
                        set event-action to event-action-fail
                        move 1 to colonna
                        perform PUNTA-CELLA-ARROT
                     else
                        perform SET-VIRGOLA
                     end-if
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gd-arrot-Ev-Msg-Finish-Entry
BEGIN-TEXT
           set tutto-ok to true.
           perform X-Y-ARROT.
           perform VALORE-RIGA-ARROT.
      
           evaluate colonna
           when 1
      *****          if arrot-da-num = 0
      *****             if arrot-a-num not = 0 or valore-num not = 0
      *****                display message "Cancellare la riga?"
      *****                          title titolo
      *****                           type mb-yes-no
      *****                         default mb-no
      *****                         giving scelta
      *****                if scelta = mb-no
      *****                   set errori to true
      *****                else
      *****                   modify gd-arrot, record-to-delete = riga
      *****                   modify gd-arrot, insertion-index  = 11,
      *****                                    insert-rows = 1
      *****                end-if
      *****             end-if
      *****          else
                   perform CONTROLLO-RIGA-ARROT
      *****          end-if
           when 2
           when 3
                perform CONTROLLO-RIGA-ARROT
           end-evaluate.
      
           if errori
              move 78-ID-gd-arrot to store-id
              set event-action to event-action-fail
           else
              perform RESET-VIRGOLA
           end-if.
END-TEXT
END-DATA
