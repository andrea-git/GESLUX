BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterBufToFld
BEGIN-TEXT
           move ef-codice-buf to vet-codice convert.
                                  
           move 0 to idx.
           perform 20 times
              add  1  to idx
              move 0  to vet-qli-da-peso(idx)
              move 0  to vet-qli-a-peso(idx)
           end-perform.

           perform X-Y-PESO.
           perform varying riga from 2 by 1
                     until riga > tot-righe
              perform VALORE-RIGA-PESO
              move col-da-peso to vet-qli-da-peso(riga - 1)
              move col-a-peso  to vet-qli-a-peso(riga - 1)
           end-perform.
                                  
           move 0 to idx.
           perform 10 times
              add  1  to idx
              move 0  to vet-qli-da-arrot(idx)
              move 0  to vet-qli-a-arrot(idx) 
              move 0  to vet-valore-arrot(idx)
           end-perform.

           perform X-Y-ARROT.
           perform varying riga from 2 by 1
                     until riga > tot-righe
              perform VALORE-RIGA-ARROT
              move col-da-arrot to vet-qli-da-arrot(riga - 1)
              move col-a-arrot  to vet-qli-a-arrot(riga - 1)
              move col-valore   to vet-valore-arrot(riga - 1)
           end-perform.
END-TEXT
END-DATA
