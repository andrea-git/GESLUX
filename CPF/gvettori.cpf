BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-del-riga-LinkTo
BEGIN-TEXT
           perform X-Y-PESO.
           perform VALORE-RIGA-PESO.

           if peso-da-num not = 0 or peso-a-num not = 0
              display message "Cancellare la riga?"
                        title titolo
                         type mb-yes-no
                      default mb-no
                       giving scelta
              if scelta = mb-yes
                 set FromDeleteRigaPeso to true
                 perform MESSAGE-DELETE-TARIFFA
                 if scelta = mb-yes
                    if rec-deleted > 0
                       move 2 to riga
                       move 1 to colonna
                       perform PUNTA-CELLA-PESO
                       modify gd-peso, record-to-delete = riga
                       modify gd-peso, insertion-index  = 21,
                                       insert-rows = 1
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-del-riga-2-LinkTo
BEGIN-TEXT
           perform X-Y-ARROT.
           perform VALORE-RIGA-ARROT.

           set FromDeleteRigaArrot to true.
           if arrot-a-num  not = 0 or valore-num not = 0 or
              arrot-da-num not = 0
              display message "Cancellare la riga?"
                        title titolo
                         type mb-yes-no
                       default mb-no
                       giving scelta
              if scelta = mb-yes
                 modify gd-arrot, record-to-delete = riga
                 modify gd-arrot, insertion-index  = 11,
                                  insert-rows = 1
              end-if
           end-if.
END-TEXT
END-DATA
