BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-INTERVALLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-cod-da
                inquire ef-cod-da, value in ef-cod-da-buf
                if ef-cod-da-buf not = 0
                   move ef-cod-da-buf to vet-codice
                   read tvettori no lock 
                        invalid 
                        set errori to true
                        display message "Vettore NON valido"
                                  title tit-err
                   end-read
                end-if

           when 78-ID-ef-cod-a
                inquire ef-cod-da, value in ef-cod-da-buf
                inquire ef-cod-a,  value in ef-cod-a-buf
                evaluate ef-cod-a-buf
                when 0
                     move 99999 to ef-cod-a-buf
                     display ef-cod-a
                when 99999
                     continue
                when other
                     move ef-cod-a-buf to vet-codice
                     read tvettori no lock 
                          invalid
                          set errori to true
                          display message "Vettore NON valido"
                                    title tit-err
                                     icon 2
                      not invalid
                          if ef-cod-a-buf < ef-cod-da-buf
                             display message "Intervallo vettore errato"
                                       title tit-err
                                        icon 2
                             set errori to true
                             move 78-ID-ef-cod-da to control-id
                          end-if
                     end-read
                end-evaluate
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move 4  to accept-control
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-PAGE-1
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.
           set tutto-ok to true.
           perform  varying control-id from 78-ID-ef-des by 1
                      until control-id    > 78-ID-rb-clides
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-PAGE-2
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.
           set tutto-ok to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true. 
           if not ControllaCampi
              set ControllaCampi to true
              exit paragraph
           end-if.
      
           evaluate CONTROL-ID
           when 78-ID-ef-codice
                perform RIEMPI-CHIAVE
      
                if ef-codice-buf not = spaces
                   inspect ef-codice-buf replacing trailing SPACES by low-values
                   initialize CountChar
                   inspect ef-codice-buf 
                      tallying CountChar for characters before low-value
                   inspect ef-codice-buf replacing trailing low-values by SPACES
      
                   | SE E' NUMERICO LEGGO IL RECORD NORMALMENTE
                   if ef-codice-buf(1:CountChar) is numeric
                      perform SELEZIONA-NUMERICO
                   else
                      | HO DIGITATO UNA SIGLA ALFANUMERICA QUINDI 
                      | APRO LO ZOOM (CASE SENSITIVE)
                      perform SELEZIONA-ALFA
                   end-if
                else
                   set errori to true
                   display message box MSG-Codice-obbligatorio
                           title tit-err
                           icon  mb-warning-icon
                end-if
      
           when 78-ID-ef-des
                inquire ef-des value in vet-descrizione
                if vet-descrizione = spaces
                   set errori to true
                   display message box "Descrizione obbligatoria"
                           title tit-err
                           icon  mb-warning-icon
                end-if
                move vet-descrizione to lab-des-buf
                display lab-des
      
           when 78-ID-ef-indirizzo
                inquire ef-indirizzo, value in vet-indirizzo
                if vet-indirizzo = spaces
                   set errori to true
                   display message box "Indirizzo obbligatorio"
                           title tit-err
                           icon  mb-warning-icon
                end-if             
      
           when 78-ID-ef-sigla
                inquire ef-sigla, value in vet-sigla
                if vet-sigla = spaces
                   set errori to true
                   display message box "Sigla obbligatoria"
                           title tit-err
                           icon  mb-warning-icon
                end-if

           when 78-ID-ef-piva
                inquire ef-piva,   value in ef-piva-buf
                if ef-piva-buf   not = spaces
                   set  cf-partita-iva to true
LUBEXX             move ef-piva-buf    to cf-piva
                   perform CHECK-PIVA
                   if errori
                      move 78-ID-ef-piva to control-id 
                      display message MSG-Partita-IVA-errata
                                title tit-err
                                 icon 2
                   end-if
                end-if


           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-RIGA-ARROT
BEGIN-TEXT
           set tutto-ok to true.
      
           evaluate colonna
           when 2
                if arrot-a-num < arrot-da-num
                   set errori to true
                   display message "Range scaglione errato"
                             title tit-err
                              icon 2
                   move 1 to colonna
                end-if
           when 3
                if valore-num not = 0   and
                   valore-num not = 0,1 and
                   valore-num not = 0,2 and
                   valore-num not = 0,5 and
                   valore-num not = 1
                   set errori to true
                   display message "Valori permessi:"
                   x"0d0a""0    = Nessuno"               
                   x"0d0a""0,1 = Ai 10 Kg. superiori"
                   x"0d0a""0,2 = Ai 20 Kg. superiori"
                   x"0d0a""0,5 = Ai 50 Kg. superiori"
                   x"0d0a""1    = Al quintale superiore"
                             title tit-err
                              icon 2
                   move 3 to colonna
                end-if
           end-evaluate.
                   
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-RIGA-PESO
BEGIN-TEXT
           set tutto-ok to true.
      
           evaluate colonna
           when 2
                if peso-a-num < peso-da-num
                   set errori to true
                   display message "Range scaglione errato"
                             title tit-err
                              icon 2
                   move 1 to colonna
                end-if
           end-evaluate.
                   
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if nuovo move 0 to mod end-if.

           if mod = 1
              read tvettori lock invalid 
                 set errori to true 
              end-read
           else
              read tvettori no lock invalid 
                 set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0      to mod
                    move 1      to mod-k
                    move 78-ID-ef-codice to CONTROL-ID
                    move 4         to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1  set StatusModifica to true
                 else        set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG
                 perform VALORIZZA-OLD
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-codice to CONTROL-ID
                       move 4       to ACCEPT-CONTROL
                       display message MSG-Record-inesistente
                                 title tit-err
                                  icon 2
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = DELETE-TARIFFE
BEGIN-TEXT
           move 0 to rec-deleted
           set  tutto-ok   to true.
           move low-value  to tfv-rec.
           move vet-codice to tfv-codice.

           start tarifvet key is >= tfv-chiave
              invalid 
                 continue
              not invalid
                 perform until 1 = 2
                    set RecLocked to false
                    read tarifvet next no lock 
                       at end 
                          exit perform 
                    end-read
           
                    if tfv-codice not = vet-codice 
                       exit perform 
                    end-if
              
                    delete tarifvet record 
                       invalid 
                          continue 
                    end-delete
                    if errori
                       move mb-cancel to scelta
                       exit perform
                    end-if

                    if not RecLocked
                       add 1 to rec-deleted
                    end-if
                 end-perform
           end-start.
           if rec-deleted > 1
              move rec-deleted to riga-ed
              display message 
              "Cancellazione tariffario eseguita su n. rec: ", riga-ed
                        title titolo
           end-if.
           close  tarifvet.
           unlock tarifvet all records.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = HELP-GRID-ARROT
BEGIN-TEXT
           move 0 to StatusHelp.
      
           evaluate colonna
           when 1   move "Digitare i quintali di inizio scaglione"
                      to totem-hint-text
           when 2   move "Digitare i quintali di fine scaglione"
                      to totem-hint-text
           when 3   move "Digitare il valore dell'arrotondamento"
                      to totem-hint-text
           end-evaluate.
      
           modify Form1-St-1-Handle, panel-index = 1,
                                     panel-text  = totem-hint-text.
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = HELP-GRID-PESO
BEGIN-TEXT
           move 0 to StatusHelp.
      
           evaluate colonna
           when 1   move "Digitare i quintali di inizio scaglione"
                      to totem-hint-text
           when 2   move "Digitare i quintali di fine scaglione"
                      to totem-hint-text
           end-evaluate.
      
           modify Form1-St-1-Handle, panel-index = 1,
                                     panel-text  = totem-hint-text.
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT
BEGIN-TEXT
           move 0 to StatusHelp.
           move 1 to mod-k.
           set StatusVisua    to true. 
           set ControllaCampi to true.

           move 1 to Form1-RADIO-1-BUF.
           display   rb-regione.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-vet-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces.

      * SPECIFICO PGM "gvettori"
           Set old-vet-regione        to true.
      * FINE
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MESSAGE-DELETE-TARIFFA
BEGIN-TEXT
           move mb-yes to scelta.

           if vecchio |Il nuovo vettore NON pu avere tariffari associati
              inquire gd-peso, last-row in tot-righe

              if FromDeleteRigaPeso
                 set FromDeleteRigaPeso to false
                 set trovato            to true
              else
                 set trovato to false
                 perform varying store-riga from 2 by 1 
                           until store-riga > tot-righe
                    subtract 1 from store-riga giving idx
                    inquire gd-peso(store-riga, 1) cell-data in peso-da-num
                    inquire gd-peso(store-riga, 2) cell-data in peso-a-num
                    if peso-da-num not = vet-qli-da-peso(idx) or
                       peso-a-num  not = vet-qli-a-peso(idx)
                       set trovato to true
                       exit perform
                    end-if
                 end-perform
              end-if

              if trovato
                 set trovato to false
                 inquire ef-codice, value in vet-codice
                 move low-value  to tfv-rec
                 move vet-codice to tfv-codice
                 start tarifvet key is >= tfv-chiave
                       invalid continue
                   not invalid
                       read tarifvet next
                       if tfv-codice = vet-codice
                          set trovato to true
                       end-if
                 end-start
              end-if
              if trovato
                 move lab-des-buf to como-des
                 inspect como-des replacing trailing spaces by low-value
                 initialize messaggio
                 string "Modificati gli scalari. "         delimited size
                        "Cancelli le tariffe del vettore " delimited size
                        como-des                           delimited low-value
                        " ?"                               delimited size
                        into messaggio
                 end-string
                 display message messaggio
                           title titolo
                            type 3
                          giving scelta
                 if scelta = mb-yes
                    close tarifvet
                    open i-o tarifvet
                    if tutto-ok perform DELETE-TARIFFE
                    else        move mb-cancel to scelta
                    end-if
                    open input tarifvet
                 end-if
              end-if
           end-if. 
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if CONTROL-ID < 78-ID-ef-des
                       move 78-ID-ef-des to CONTROL-ID
                    end-if
                    move 0 to mod-k
                    move 1 to mod
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1    to mod
                 perform SALV-MOD            
                 move 0    to mod
                 move 1    to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    move 78-ID-ef-codice to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock tvettori all records
                 end-if
              end-if
                        
              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           perform SALV-MOD.

           if tutto-ok  
              move 78-ID-ef-codice to CONTROL-ID       
              move 4         to ACCEPT-CONTROL   
              move 1         to mod mod-k
              modify tool-modifica,  value = mod
              perform CANCELLA-COLORE                  
              perform VALORIZZA-NUOVO
              perform INIT-OLD-REC       
              set StatusIns to true
              perform STATUS-BAR-MSG  
              set nuovo to true       
              unlock tvettori all records
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ORDINAMENTO
BEGIN-TEXT
           inquire tool-ord, value in OrderBy.
      
           if OrderBy = 0 perform DataSet1-CHANGETO-KEY1
           else           perform DataSet1-CHANGETO-KEY2
           end-if.
      
           perform CURRENT-RECORD.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok    
 
              perform CHECK-INSERIMENTO
              unlock tvettori all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tvettori-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tvettori-lock-flag
              end-if
                                  
              perform CANCELLA-COLORE
              perform FORM1-FIRST

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tvettori-lock-flag
                 move 1 to mod-k
                 perform FORM1-FIRST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".
           copy "status.cpy".
           copy "calcola-colore-trasparente.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRECEDENTE
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tvettori all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tvettori-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tvettori-lock-flag
              end-if
                    
              perform CANCELLA-COLORE
              perform FORM1-PREVIOUS

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tvettori-lock-flag
                 move 1 to mod-k
                 perform FORM1-PREVIOUS
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PUNTA-CELLA-PESO
BEGIN-TEXT
           move 78-ID-gd-peso      to control-id store-id.
           move 4                  to accept-control.
           modify gd-peso,   cursor-y riga, 
                             cursor-x colonna.
           perform COLORE-RIGA-PESO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PUNTA-CELLA-ARROT
BEGIN-TEXT
           move 78-ID-gd-arrot      to control-id store-id.
           move 4                   to accept-control.
           modify gd-arrot,   cursor-y riga, 
                              cursor-x colonna.
           perform COLORE-RIGA-ARROT.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RESETTA-GRID-ARROT
BEGIN-TEXT
           modify gd-arrot, reset-grid = 1.
           perform GD-ARROT-CONTENT.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RESETTA-GRID-PESO
BEGIN-TEXT
           modify gd-peso, reset-grid = 1.
           perform GD-PESO-CONTENT.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RESET-VIRGOLA
BEGIN-TEXT
           set environment "KEYSTROKE" to "DATA=44 44".
           set environment "KEYSTROKE" to "DATA=46 46".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           inquire ef-codice, value in ef-codice-buf.
           move ef-codice-buf       to vet-codice convert.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-GRID-ARROT
BEGIN-TEXT
           move 0 to idx.
           perform RESETTA-GRID-ARROT.
           perform 10 times
              add   1 to idx
              move vet-qli-da-arrot(idx)  to col-da-arrot
              move vet-qli-a-arrot(idx)   to col-a-arrot
              move vet-valore-arrot(idx)  to col-valore
              modify gd-arrot(idx + 1, 1), cell-data = col-da-arrot
              modify gd-arrot(idx + 1, 2), cell-data = col-a-arrot 
              modify gd-arrot(idx + 1, 3), cell-data = col-valore
           end-perform.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-GRID-PESO
BEGIN-TEXT
           move 0 to idx.
           perform RESETTA-GRID-PESO.
           perform 20 times
              add   1 to idx
              move vet-qli-da-peso(idx) to col-da-peso
              move vet-qli-a-peso(idx)  to col-a-peso
              modify gd-peso(idx + 1, 1), cell-data = col-da-peso
              modify gd-peso(idx + 1, 2), cell-data = col-a-peso
           end-perform.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.
      
           set tutto-ok to true.
      
           perform varying CONTROL-ID from 78-ID-ef-codice by 1
                     until CONTROL-ID > 78-ID-ef-url
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if vecchio
              evaluate form1-radio-1-buf
              when 1 |REGIONE
                   if old-vet-regione continue
                   else 
                      perform MESSAGE-DELETE-TARIFFA
                      if scelta = mb-cancel
                         set errori to true
                      end-if
                   end-if
              when 2 |PROVINCIA
                   if old-vet-prov continue
                   else 
                      perform MESSAGE-DELETE-TARIFFA
                      if scelta = mb-cancel
                         set errori to true
                      end-if
                   end-if
              when 3 |CLIENTE
                   if old-vet-cliente continue
                   else 
                      perform MESSAGE-DELETE-TARIFFA
                      if scelta = mb-cancel
                         set errori to true
                      end-if
                   end-if
              when 4 |CLIENTE/DESTINO
                   if old-vet-clides continue
                   else 
                      perform MESSAGE-DELETE-TARIFFA
                      if scelta = mb-cancel
                         set errori to true
                      end-if
                   end-if
              end-evaluate
           end-if.
      
           if tutto-ok
              perform X-Y-PESO
              perform varying riga from 2 by 1 
                        until riga > tot-righe
                 perform VALORE-RIGA-PESO
                 perform varying colonna from 1 by 1
                           until colonna > 2
                    perform CONTROLLO-RIGA-PESO
                    if errori 
                       move 78-ID-gd-peso to store-id
                       exit perform 
                    end-if
                 end-perform
                 if errori exit perform end-if
              end-perform
           end-if.     
      
           if tutto-ok
              perform X-Y-ARROT
              perform varying riga from 2 by 1 
                        until riga > tot-righe
                 perform VALORE-RIGA-ARROT
                 perform varying colonna from 1 by 1
                           until colonna > 2
                    perform CONTROLLO-RIGA-ARROT
                    if errori 
                       move 78-ID-gd-arrot to store-id
                       exit perform 
                    end-if
                 end-perform
                 if errori exit perform end-if
              end-perform
           end-if.

           if tutto-ok
              perform FORM1-BUF-TO-FLD
              if vet-abruzzo        = 0 or
                 vet-basilicata     = 0 or
                 vet-calabria       = 0 or
                 vet-campania       = 0 or
                 vet-emilia-romagna = 0 or
                 vet-friuli         = 0 or
                 vet-lazio          = 0 or
                 vet-liguria        = 0 or
                 vet-lombardia      = 0 or
                 vet-marche         = 0 or
                 vet-molise         = 0 or
                 vet-piemonte       = 0 or
                 vet-puglia         = 0 or
                 vet-sardegna       = 0 or
                 vet-sicilia        = 0 or
                 vet-toscana        = 0 or
                 vet-trentino       = 0 or
                 vet-umbria         = 0 or
                 vet-valle          = 0 or
                 vet-veneto         = 0
                 set errori to true
                 display message "Valorizzare i giorni per tutte le regioni"
                          title tit-err
                           icon 2
           end-if.
      
           if errori
              perform ABILITAZIONI
              evaluate store-id 
              when 78-ID-gd-arrot
                   perform PUNTA-CELLA-ARROT
              when 78-ID-gd-peso
                   perform PUNTA-CELLA-PESO
              when other
                   move store-id to CONTROL-ID
                   move 4        to ACCEPT-CONTROL
              end-evaluate
           else                       
              perform FORM1-BUF-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              perform CANCELLA-COLORE 
                                     
              accept como-data from century-date
              accept como-ora  from time
          
              write vet-rec invalid rewrite vet-rec end-write
              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD.
      
           if SiSalvato and mod = 1
              move 0 to idx
              perform 20 times
                 add 1 to idx

                 if old-vet-qli-da-peso(idx) not = vet-qli-da-peso(idx)
                    set NoSalvato to true
                    move 78-ID-gd-peso to store-id
                    move 1 to colonna
                    add  1 to idx giving riga
                    exit perform
                 end-if

                 if old-vet-qli-a-peso(idx) not = vet-qli-a-peso(idx)
                    set NoSalvato to true
                    move 78-ID-gd-peso to store-id
                    add  1 to idx giving riga
                    move 2 to colonna
                    exit perform
                 end-if

              end-perform
           end-if.                     
      
           if SiSalvato and mod = 1
              move 0 to idx
              perform 10 times
                 add 1 to idx

                 if old-vet-qli-da-arrot(idx) not = vet-qli-da-arrot(idx)
                    set NoSalvato to true
                    move 78-ID-gd-arrot to store-id
                    move 1 to colonna
                    add  1 to idx giving riga
                    exit perform
                 end-if

                 if old-vet-qli-a-arrot(idx) not = vet-qli-a-arrot(idx)
                    set NoSalvato to true
                    move 78-ID-gd-arrot to store-id
                    add  1 to idx giving riga
                    move 2 to colonna
                    exit perform
                 end-if

                 if old-vet-valore-arrot(idx) not = vet-valore-arrot(idx)
                    set NoSalvato to true
                    move 78-ID-gd-arrot to store-id
                    add  1 to idx giving riga
                    move 3 to colonna
                    exit perform
                 end-if

              end-perform
           end-if.
      
           if NoSalvato
              display message MSG-Salvare-le-modifiche
                        title titolo
                         type mb-yes-no-cancel 
                       giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no
                   continue
              when other
                   perform CANCELLA-COLORE
                   set errori to true
                   inquire Screen1-Ta-1, value in pagina
                   evaluate pagina
                   when 1
                        if store-id > 78-ID-rb-clides
                           move 666 to store-id
                        end-if
                   when 2
                        if store-id < 78-ID-gd-peso
                           move 666 to store-id
                        end-if
                   end-evaluate
                   if store-id = 78-ID-gd-peso or
                      store-id = 78-ID-gd-arrot
                      perform PUNTA-CELLA-PESO
                   else
                      move store-id to CONTROL-ID       
                      move 4        to ACCEPT-CONTROL   
                   end-if
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA
BEGIN-TEXT
           perform RIEMPI-CHIAVE.

           move "tvettori"  to    como-file.
           call "zoom-gt" using como-file, vet-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = 0
              if old-vet-chiave not = vet-chiave or
               ( vet-codice = 0 and mod = 0 )
                 move vet-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to vet-chiave
                    set ReadSecca to true
                    modify ef-codice, value vet-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-des to CONTROL-ID       
                    move 4            to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA-ALFA
BEGIN-TEXT
      * LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      * ANCORA GLI ZERI DAVANTI CHE ELIMINO
           inspect ef-codice-buf replacing leading ZERO by SPACES.
           move ef-codice-buf to vet-descrizione.
      
      * POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      * ALTERNATA
           call "C$JUSTIFY" using vet-descrizione, "L".
      
           start tvettori key >= vet-descrizione
                 invalid  continue
             not invalid  read tvettori next 
           end-start.
      
           move "tvettori-alfa" to como-file.
           call "zoom-gt" using como-file, vet-rec
                          giving stato-zoom.
           cancel "zoom-gt".

           if stato-zoom = 0
              move vet-codice     to codice-ed
              move codice-ed      to ef-codice-buf   
              call "C$JUSTIFY" using ef-codice-buf, "L"
              display ef-codice
      
              if old-vet-descrizione not = vet-descrizione
                 move vet-descrizione to save-key-1
                 perform SALV-MOD
                 if tutto-ok
                    move save-key-1 to vet-descrizione
                    set ReadSecca   to true
                    modify ef-codice, value vet-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-des to CONTROL-ID       
                    move 4            to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA-NUMERICO
BEGIN-TEXT
