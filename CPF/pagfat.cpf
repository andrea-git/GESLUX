BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = pagfat
   ACTOBJ            = pagfat
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       TORDINI-ERR SECTION.
           use after error procedure on tordini.
           evaluate status-tordini
           when "35"
                display message "File [TORDINI] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [TORDINI] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[TORDINI] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "93"
           when "99"
                set RecLocked to true
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-pag
                move "tcodpag"   to como-file
                move "PA"        to tblpa-codice1
                inquire ef-pag,  value in tblpa-codice2
                call "zoom-gt"   using como-file, record-tblpa
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move tblpa-codice2      to  ef-pag-buf 
                   move tblpa-descrizione1 to lab-pag-buf
                   display ef-pag lab-pag
                end-if
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-anno
                inquire ef-anno, value in anno
                if anno = 0
                   set errori to true
                end-if

           when 78-ID-ef-num
                inquire ef-anno, value in anno
                inquire ef-num,  value in numero
                move anno    to tor-anno-fattura
                move numero  to tor-num-fattura
                read tordini no lock key k-fattura
                     invalid 
                     set errori to true
                     display message "Fattura non caricata"
                               title tit-err
                                icon 2
                end-read

           when 78-ID-ef-pag
                move "PA" to tblpa-codice1
                inquire ef-pag, value in tblpa-codice2
                move spaces to lab-pag-buf
                if tblpa-codice2 = spaces
                   set errori to true
                   display message "Pagamento non valido"
                             title tit-err
                              icon 2
                else
                   read tcodpag no lock
                        invalid
                        set errori to true
                        display message "Pagamento non valido"
                                  title tit-err
                                   icon 2
                    not invalid
                        move tblpa-descrizione1 to lab-pag-buf
                   end-read
                end-if
                display ef-pag lab-pag

           end-evaluate.

           if errori
              move 4 to accept-control
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "color-custom.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = READ-RECORD-LOCK
BEGIN-TEXT
           set RecLocked to false.
           initialize geslock-linkage.
           move "tordini" to geslock-nome-file.

           set tutto-ok to true.
           read tordini with lock invalid continue end-read.

           if RecLocked
              move 1 to geslock-v-riprova
              move 0 to geslock-v-ignora
              move 1 to geslock-v-termina
              call   "geslock" using geslock-linkage
              cancel "geslock"
              evaluate true
              when riprova perform READ-RECORD-LOCK
              when ignora  
              when termina set errori to true
                           unlock tordini all records
              end-evaluate
           end-if.
END-TEXT
END-DATA
