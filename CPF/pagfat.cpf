BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = PB-ESEGUI-LinkTo
BEGIN-TEXT
           set tutto-ok to true.
           perform varying control-id from 78-ID-ef-anno by 1 
                     until control-id > 78-ID-ef-pag
              perform CONTROLLO
              if errori
                 exit perform
              end-if
           end-perform.

           perform CANCELLA-COLORE.
           if tutto-ok
              display message "Cambiare il codice pagamento " tor-cod-pagamento
                       x"0d0a""con quello indicato?"
                        type mb-yes-no
                       title titolo
                      giving scelta
              if scelta = mb-yes
                 perform READ-RECORD-LOCK
                 if tutto-ok
