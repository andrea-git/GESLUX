BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ef-cod-a-AfterProcedure
BEGIN-TEXT
      *<<** Customized_Default, SP-G, Form2-Custom1-1 **>>
           perform CONTROLLO.
      *<<** Customized_Default, SP-G, scr-stampa-Custom1-2 **>>

END-TEXT
