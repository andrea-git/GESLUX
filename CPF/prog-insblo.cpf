END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-sib-LinkTo
BEGIN-TEXT
           perform varying control-id from 78-ID-ef-cod-da by 1
                     until control-id > 78-ID-ef-cod-a
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if tutto-ok
              call   "W$MOUSE"   using set-mouse-shape, wait-pointer
              modify lab, visible true
              move low-value     to prg-chiave
              move ef-cod-da-buf to prg-cod-articolo
              start progmag key >= prg-chiave
                    invalid continue
                not invalid                
                    move 0 to art-codice
                    perform until 1 = 2
                       read progmag next at end exit perform end-read
                       if prg-cod-articolo > ef-cod-a-buf
                          exit perform
                       end-if
                       if prg-tipo-imballo = "C04"
                          set prg-bloccato to true
                          rewrite prg-rec
                       end-if
                       if prg-cod-articolo not = art-codice
                          move prg-cod-articolo to art-codice
                          read articoli no lock
                               invalid continue
                           not invalid
                               initialize prg-rec replacing numeric data by zeroes
                                                       alphanumeric data by spaces
                               move art-codice       to prg-cod-articolo
                               move "VIS"            to prg-tipo-imballo
                               move "LBX"            to prg-cod-magazzino
                               add art-peso-utf to art-peso-non-utf giving prg-peso
                               move art-peso-utf     to prg-peso-utf
                               move art-peso-non-utf to prg-peso-non-utf
                               set prg-attivo to true                     
                               accept prg-data-creazione from century-date
                               accept prg-ora-creazione  from time
                               move "AUTO" to prg-utente-creazione
                               write prg-rec           
                                     invalid display message "PROGRESSIVO GIA PRESENTE:" prg-chiave
                               end-write
                               move "EXD"            to prg-cod-magazzino
                               write prg-rec           
                                     invalid display message "PROGRESSIVO GIA PRESENTE:" prg-chiave
                               end-write
                          end-read
                       end-if
                    end-perform
              end-start                    

              modify lab, visible false
              call   "W$MOUSE"   using set-mouse-shape, arrow-pointer
              display message "FINE!"
                        title titolo
                         icon 2

              perform CANCELLA-COLORE
              move 100 to control-id
              move   4 to accept-control
              modify pb-sib, bitmap-number = 2
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-noa-BeforeProcedure
BEGIN-TEXT
           modify pb-noa, bitmap-number = 2.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-noa-AfterProcedure
BEGIN-TEXT
           modify pb-noa, bitmap-number = 1.
END-TEXT
END-DATA

