BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true. 
      
           evaluate CONTROL-ID
           when 78-ID-ef-cod-a
                inquire ef-cod-da, value in ef-cod-da-buf
                inquire ef-cod-a,  value in ef-cod-a-buf
                evaluate ef-cod-a-buf
                when 0
                     move 999999 to ef-cod-a-buf
                     display ef-cod-a
                when 999999
                     continue
                when other   
                     if ef-cod-a-buf < ef-cod-da-buf
                        display message "Intervallo vettore errato"
                                  title tit-err
                                   icon 2
                        set errori to true
                        move 78-ID-ef-cod-da to control-id
                     end-if
                end-evaluate
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
