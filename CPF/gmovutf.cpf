BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-INSERIMENTO
BEGIN-TEXT
      * MODIFICA LA STATUS-BAR
      * CONTROLLO SE DA INSERIMENTO SCORRO. ALLORA DEVO SEGNALARE CHE
      * SONO TORNATO IN MODIFICA.
           inquire tool-modifica, value in mod.

           evaluate mod
           when 1
                set StatusModifica to true
           when ZERO
                set StatusVisua to true   
           end-evaluate.
      
           perform STATUS-BAR-MSG.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

      * Paragrafo per la struttura dei controlli sulla screen Form1
           evaluate control-id
           |78-ID-ef-anno  l'ID del control ef-anno
           when 78-ID-ef-anno
                inquire ef-anno, value in ef-anno-buf
                move ef-anno-buf to mov-anno
                if mov-anno = 0
                   set errori to true
                   display message "Inserimento anno mancante"
                             title tit-err
                              icon 2
                end-if

           |78-ID-ef-num  l'ID del control ef-num
           when 78-ID-ef-num
                inquire ef-num, value in ef-num-buf
                move ef-num-buf to mov-num-reg
                if mov-num-reg = 0
                   set errori to true
                   display message "Inserimento num. registro mancante"
                             title tit-err
                              icon 2
                else
                   if nuovo
                      move high-value to mov-prog-reg
                      start movutf key is <= mov-chiave
                            invalid
                            move 1 to mov-prog-reg ef-prog-buf
                        not invalid
                            move mov-chiave to save-key
                            read movutf previous
                            if mov-num-reg = save-mov-num-reg
                               add 1 to mov-prog-reg
                               move mov-prog-reg to ef-prog-buf
                            else
                               move 1 to mov-prog-reg ef-prog-buf
                            end-if
                            move save-mov-num-reg to mov-num-reg
                            move save-mov-anno    to mov-anno
                      end-start
                      initialize mov-dati 
                                 replacing numeric data by zeroes
                                      alphanumeric data by spaces
                   end-if
                end-if
                display ef-prog

           |78-ID-ef-prog  l'ID del control ef-prog
           when 78-ID-ef-prog
                inquire ef-prog, value in ef-prog-buf
                move ef-prog-buf to mov-prog-reg
                if mov-prog-reg = 0
                   set errori to true
                   display message "Inserimento progressivo mancante"
                             title tit-err
                              icon 2
                else
                   perform RIEMPI-CHIAVE
                   read movutf no lock
                        invalid 
                        if vecchio
                           display message MSG-record-inesistente
                                     title tit-err
                                      icon 2
                           perform CLEAR-SCREEN
                           set errori to true
                           move 78-ID-ef-anno to control-id
                        end-if
                    not invalid
                        if old-mov-chiave not = mov-chiave
                           perform CURRENT-RECORD 
                           set errori to true  
                           move 78-ID-ef-anno to control-id
                        end-if
                   end-read
                end-if

           |78-ID-ef-data  l'ID del control ef-data
           when 78-ID-ef-data
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                perform DATE-FORMAT
                move como-data to ef-data-buf
                display ef-data

           |78-ID-ef-kg  l'ID del control ef-kg
           when 78-ID-ef-kg
                inquire ef-kg, value in ef-kg-buf

           |78-ID-ef-codice  l'ID del control ef-codice
           when 78-ID-ef-codice
                if form1-radio-2-buf = 1 set cli-tipo-C to true
                else                     set cli-tipo-F to true
                end-if
                inquire ef-codice, value in cli-codice
                read clienti no lock
                     invalid
                     move spaces to lab-cli-buf
                     set errori to true
                     display message "Codice NON valido"
                               title tit-err
                                icon 2
                 not invalid
                     move cli-ragsoc-1 to lab-cli-buf
                end-read
                display lab-cli

           |78-ID-ef-numdoc  l'ID del control ef-numdoc
           when 78-ID-ef-numdoc
                inquire ef-numdoc, value in ef-numdoc-buf
                move ef-numdoc-buf to mov-num-doc
                if mov-num-doc = 0
                   set errori to true
                   display message "Inserimento numero documento mancante"
                             title tit-err
                              icon 2
                end-if

           end-evaluate.

           if errori
              move control-id to store-id
              perform CANCELLA-COLORE
              move 4        to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if nuovo move 0 to mod end-if.

           if mod = 1
              read movutf lock invalid 
                 set errori to true 
              end-read
           else
              read movutf no lock invalid 
                 set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0 to mod
                    move 1 to mod-k
                    move 1 to CONTROL-ID
                    move 4 to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else                         
                    set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-id-ef-anno to CONTROL-ID
                       move 4             to ACCEPT-CONTROL
                       display message MSG-Record-inesistente
                                 title tit-err
                                  icon mb-warning-icon
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
