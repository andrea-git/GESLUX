BEGIN-TEXT

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-mov-rec  replacing numeric data by zeroes
                                        alphanumeric data by spaces.

           move tge-anno    to old-mov-anno.
           set  old-mov-col-a   to true.
           set  old-mov-cliente to true.
           set  old-mov-entrata to true.
END-TEXT
