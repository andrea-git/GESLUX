BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA
BEGIN-TEXT
           if mod = 0 exit paragraph  end-if.
      *    
           perform RIEMPI-CHIAVE.
           display message MSG-Cancellare-il-record-corrente
                     title titolo
                      type mb-yes-no 
                   default mb-no
                    giving scelta

           if scelta = mb-yes
              delete movutf record invalid continue end-delete
   
              move 0 to mod
              move 1 to mod-k
              perform FORM1-CLEAR

              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC
                            
              display message  MSG-Cancellazione-avvenuta-con-successo
                      title titolo                         

              unlock movutf all records

              perform PRIMO
              move 1 to CONTROL-ID     
              move 4 to ACCEPT-CONTROL 
              set Statusvisua to true
              perform STATUS-BAR-MSG
              modify TOOL-MODIFICA value = mod
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate CONTROL-ID
           when 78-id-ef-codice
                if form1-radio-2-buf = 1 set cli-tipo-C to true
                else                     set cli-tipo-F to true
                end-if
                inquire  ef-codice value in cli-codice
                move "clienti-CF-all"   to Como-File
                call   "zoom-gt" using como-file, cli-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   modify ef-codice, value cli-codice
                   move cli-ragsoc-1 to lab-cli-buf
                   display lab-cli
                end-if
                            
           end-evaluate. 
      

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CLEAR-SCREEN
BEGIN-TEXT
           move mov-chiave to old-mov-chiave.

           initialize mov-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.

           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.

           display form1.     
  
           unlock movutf all records.
END-TEXT
