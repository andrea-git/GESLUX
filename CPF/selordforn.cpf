BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CAMBIA-FONT-RIGA
BEGIN-TEXT
           modify gd-ordini(riga), ROW-FONT = small-font.

           modify gd-ordini(riga, 1),
                  bitmap        = elemento-bmp,
                  bitmap-number = 1,
                  bitmap-width  = 16.


           move event-data-2 to riga.
           modify gd-ordini(riga), ROW-FONT = font-evidenzia-griglia..

      *    cambio la label legenda

           inquire gd-ordini(riga, 1), HIDDEN-DATA = tipo-record
           evaluate true
           when r-inseriti
                move 78-colore-inseriti        to col-stato-ordine
                move 78-tit-inseriti           to tit-stato-ordine
           when r-accettati
                move 78-colore-accettati       to col-stato-ordine
                move 78-tit-accettati          to tit-stato-ordine
           when r-inviati
                move 78-colore-inviati         to col-stato-ordine
                move 78-tit-inviati            to tit-stato-ordine
           when r-in-lavorazioni
                move 78-colore-in-lavorazioni  to col-stato-ordine
                move 78-tit-in-lavorazioni     to tit-stato-ordine
           when r-chiusi
                move 78-colore-chiusi          to col-stato-ordine
                move 78-tit-chiusi             to tit-stato-ordine
           end-evaluate


           modify gd-ordini, (riga, 1),
                  bitmap        = elemento-bmp,
                  bitmap-number = 2,
                  bitmap-width  = 16.


           display form2.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CARICA-ORDINI
BEGIN-TEXT
           move anno-to      to tof-anno.
           move ordine-from  to tof-numero.

           evaluate true

           when inseriti
                set tof-inserito   to true
                start tordforn key is >= tof-k-stato
                   invalid 
                      continue
                   not invalid 
                      perform SCORRI-ORDINI
                end-start

      *****     when accettati
      *****          set tof-accettato  to true
      *****          start tordforn key is >= k-stato
      *****             invalid 
      *****                continue
      *****             not invalid 
      *****                perform SCORRI-ORDINI
      *****          end-start
           when inviati
                set tof-inviato    to true
                start tordforn key is >= tof-k-stato
                   invalid 
                      continue
                   not invalid 
                      perform SCORRI-ORDINI
                end-start
           when in-lavorazioni
                set tof-in-lavorazione   to true
                start tordforn key is >= tof-k-stato
                   invalid 
                      continue
                   not invalid 
                      perform SCORRI-ORDINI
                end-start
           when chiusi
                set tof-chiuso        to true
                start tordforn key is >= tof-k-stato
                   invalid 
                      continue
                   not invalid 
                      perform SCORRI-ORDINI
                end-start
           when tutti
                start tordforn key is >= tof-k-stato
                   invalid 
                      continue
                   not invalid 
                      perform SCORRI-ORDINI
                end-start
           end-evaluate
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-forn-from
                inquire ef-forn-from, value in cli-codice
                set cli-tipo-f to true
                move spaces to cli-ragsoc-1
                move "clienti-alfa-all" to como-file
                call "zoom-gt"  using  como-file, cli-rec
                               giving  stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move cli-codice to forn-from
                   display ef-forn-from
                   move cli-ragsoc-1 to lab-forn-buf
                   display lab-forn
                end-if
           when 78-ID-ef-dest-forn-from
                inquire ef-forn-from, value in desf-codice
                inquire ef-dest-forn-from, value in desf-prog
                move spaces to desf-ragsoc-1
                move "destinif-forn" to como-file
                call "zoom-gt"  using  como-file, desf-rec
                               giving  stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move desf-prog to dest-forn-from
                   display ef-dest-forn-from
                   move desf-ragsoc-1 to lab-dest-forn-buf
                   display lab-dest-forn
                end-if
           when 78-ID-ef-cliente-from
                inquire ef-cliente-from, value in cli-codice
                set cli-tipo-c to true
                move spaces to cli-ragsoc-1
                move "clienti-alfa-all" to como-file
                call "zoom-gt"  using  como-file, cli-rec
                               giving  stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move cli-codice to cliente-from
                   display ef-cliente-from
                   move cli-ragsoc-1 to lab-cli-buf
                   display lab-cli
                end-if
           when 78-ID-ef-dest-cli-from
                inquire ef-cliente-from, value in des-codice
                inquire ef-dest-cli-from, value in des-prog
                move spaces to des-ragsoc-1
                move "clienti-des" to como-file
                call "zoom-gt"  using  como-file, des-rec
                               giving  stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move des-prog to dest-cli-from
                   display ef-dest-cli-from
                   move des-ragsoc-1 to lab-dest-cli-buf
                   display lab-dest-cli
                end-if
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHIAMA-MANUTENZIONE
BEGIN-TEXT
           set  RicaricaGrid       to false.
           move como-anno          to tof-anno.
           move como-numero-ordine to tof-numero.
           move tof-chiave         to LinkChiave.
           move "selordforn"       to LinkPgm.

           modify form2-Handle, visible = zero.
           call   "gordfornvar" using lk-blockpgm
                                      user-codi
                                      livello-abil
                                      gordfornvar-linkage
           cancel "gordfornvar"
           modify form2-Handle, visible = 1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID

           when 78-ID-ef-anno
                 inquire ef-anno, value in anno-to
                 if anno-to = ZERO
                    set errori  to true
                    display message "Inserimento anno mancante"
                            title tit-err
                            icon  mb-warning-icon
                    move 78-ID-ef-anno   to store-id
                 end-if

           when 78-ID-ef-ordine-from
                if tipo-elab = 1
                   if ordine-from = zero
                      if controllo-finale
                         set errori to true                                                

                         move 78-ID-ef-ordine-from to CONTROL-ID
                         display message "Il numero d'ordine  obbligatorio!"
                                 title tit-err
                                 icon  MB-WARNING-ICON
                      end-if
                   else
                      move anno-to       to tof-anno
                      move ordine-from   to tof-numero
                      read tordforn
                         invalid 
                            set errori to true
                            move 78-ID-ef-ordine-from to CONTROL-ID
                            display message "Ordine inesistente!"
                                    title tit-err
                                    icon  MB-WARNING-ICON
                      end-read
                   end-if
                end-if
           when 78-ID-ef-ordine-to
                if ordine-to = ZERO
                   move all "9" to ordine-to
                   modify EF-ORDINE-TO, VALUE ordine-to
                end-if
                if ordine-to < ordine-from
                   set errori to true
                   move 78-ID-ef-ordine-to to CONTROL-ID
                   display message MSG-intervallo-num-ord-err
                      title tit-err
                      icon  MB-WARNING-ICON
                end-if
           when 78-ID-ef-data-from
                if data-from not = zero
                   move data-from to como-data
                   perform DATE-FORMAT
                   move como-data to data-from
                end-if
           when 78-ID-ef-data-to
                if data-to = zero
                   move 99999999 to data-to
                end-if
                if data-to not = 99999999
                   move data-to to como-data
                   perform DATE-FORMAT
                   move como-data to data-to
                end-if

                move data-from(1:2) to data-from-AAAAMMGG(7:2)
                move data-from(3:2) to data-from-AAAAMMGG(5:2)
                move data-from(5:4) to data-from-AAAAMMGG(1:4)
                move data-to(1:2)   to data-to-AAAAMMGG(7:2)
                move data-to(3:2)   to data-to-AAAAMMGG(5:2)
                move data-to(5:4)   to data-to-AAAAMMGG(1:4)

                if data-from-AAAAMMGG = 0
                   move anno-to to data-from-AAAAMMGG(1:4)
                   move "01"    to data-from-AAAAMMGG(5:2)
                   move "01"    to data-from-AAAAMMGG(7:2)
                end-if

                if data-to-AAAAMMGG < data-from-AAAAMMGG
                   set errori to true
                   move 78-ID-ef-data-to to CONTROL-ID
                   display message MSG-intervallo-date-err
                      title tit-err
                      icon  MB-WARNING-ICON
                end-if

           when 78-ID-ef-forn-from
                if forn-from = 0
                   move "TUTTI I FORNITORI" to cli-ragsoc-1
                   move all "9" to forn-to
                   move zero to mod-dest-forn
                else
                   move forn-from to cli-codice
                   set cli-tipo-F to true
                   read clienti no lock
                      invalid
                         set errori to true
                         display message "Fornitore NON valido"
                                 title tit-err
                                  icon 2
                         move spaces to cli-ragsoc-1
                         move zero to mod-dest-forn
                      not invalid
                         move 1    to mod-dest-forn
                   end-read
                   move forn-from to forn-to
                end-if
                move cli-ragsoc-1 to lab-forn-buf
                display lab-forn
                display ef-dest-forn-from
           when 78-ID-ef-cliente-from
                if cliente-from = 0
                   move "TUTTI I CLIENTI" to cli-ragsoc-1
                   move all "9" to cliente-to
                   move zero to mod-dest-cli
                else
                   move cliente-from to cli-codice
                   set cli-tipo-c to true
                   read clienti no lock
                      invalid
                         set errori to true
                         display message "Cliente NON valido"
                                 title tit-err
                                  icon 2
                         move spaces to cli-ragsoc-1
                         move zero to mod-dest-cli
                      not invalid
                         move 1    to mod-dest-cli
                   end-read
                   move cliente-from to cliente-to
                end-if
                move cli-ragsoc-1 to lab-cli-buf
                display lab-cli
                display EF-DEST-CLI-FROM
           when 78-ID-ef-dest-forn-from
                if mod-dest-forn = 1
                   if dest-forn-from = 0
                      move "TUTTI I DESTINI" to desf-ragsoc-1
                   else
                      move forn-from  to desf-codice
                      move dest-forn-from     to desf-prog
                      read destinif no lock
                         invalid
                            set errori to true
                            display message "Destino NON valido"
                                    title tit-err
                                     icon 2
                            move spaces to desf-ragsoc-1
                      end-read
                   end-if
                   move desf-ragsoc-1 to lab-dest-forn-buf
                   display lab-dest-forn
                end-if
           when 78-ID-ef-dest-cli-from
                if mod-dest-cli = 1
                   if dest-cli-from = 0
                      move "TUTTI I DESTINI" to des-ragsoc-1
                   else
                      move cliente-from     to des-codice
                      move dest-cli-from   to des-prog
                      read destini no lock
                         invalid
                            set errori to true
                            display message "Destino NON valido"
                                    title tit-err
                                     icon 2
                            move spaces to des-ragsoc-1
                      end-read
                   end-if
                   move des-ragsoc-1 to lab-dest-cli-buf
                   display lab-dest-cli
                end-if
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if.

           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = METTI-IN-GRIGLIA
BEGIN-TEXT
           move tof-anno     to col-anno.
           move tof-numero   to col-numero.

           move tof-data-ordine(1:4)  to col-data(7:4)
           move tof-data-ordine(5:2)  to col-data(4:2)
           move tof-data-ordine(7:2)  to col-data(1:2)

           move "/"                   to col-data(3:1)
                                         col-data(6:1).

           move tof-cod-forn          to Col-fornitore
                                         cli-codice
                                         desf-codice.

           set cli-tipo-f to true.
           read clienti   key cli-chiave
              invalid
                 move "FORNITORE NON TROVATO" to desf-ragsoc-1
           end-read
           
           move cli-ragsoc-1          to  Col-rag-soc-forn.

           move tof-destino           to col-dest
                                         desf-prog
           read destinif
              invalid
                 move "DESTINO NON TROVATO" to desf-ragsoc-1
           end-read
           move desf-ragsoc-1         to  Col-rag-soc-dest.


           evaluate true
           when tof-inserito
                set r-inseriti                 to true
                move 78-colore-inseriti        to col-stato-ordine
      *****     when tof-accettato
      *****          set r-accettati                to true
      *****          move 78-colore-accettati       to col-stato-ordine
           when tof-inviato
                set r-inviati                  to true
                move 78-colore-inviati         to col-stato-ordine
           when tof-in-lavorazione
                set r-in-lavorazioni           to true
                move 78-colore-in-lavorazioni  to col-stato-ordine
           when tof-chiuso
                set r-chiusi                   to true
                move 78-colore-chiusi          to col-stato-ordine
           end-evaluate



           modify gd-ordini, RECORD-TO-ADD Gd-ordini-Record.

           inquire gd-ordini, LAST-ROW = riga.

           modify gd-ordini(riga), ROW-COLOR = col-stato-ordine.

           modify gd-ordini(riga, 1) HIDDEN-DATA = tipo-record.
           set trovato to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ORDINE-DA-GRIGLIA
BEGIN-TEXT
           inquire gd-ordini(riga), RECORD-DATA = Gd-ordini-Record.

           move col-anno            to como-anno.
           move Col-numero          to como-numero-ordine.
           move riga                to como-riga.

           perform CHIAMA-MANUTENZIONE.

           if RicaricaGrid
              move riga to save-riga
              perform RIEMPI-GRID
              move save-riga to riga
           end-if.

           modify gd-ordini, cursor-y = riga.
           perform CAMBIA-FONT-RIGA.

           move como-riga to riga, EVENT-DATA-2.
           perform SPOSTAMENTO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "utydata.cpy".
           copy "color-custom.cpy".
           copy "calcola-colore-trasparente.cpy".
           copy "riempi-combo-stato-ordforn.cpy".

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-GRID
BEGIN-TEXT
           modify gd-ordini, mass-update = 1.
           modify gd-ordini, reset-grid = 1.
           perform GD-ORDINI-CONTENT.
           perform CARICA-ORDINI.
           modify gd-ordini, mass-update = 0.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SCORRI-ORDINI
BEGIN-TEXT
           set trovato to false.

           modify gd-ordini, mass-update 1.

           perform until 1 = 2
              read tordforn next
                 at end
                    exit perform
              end-read

              evaluate true
              when inseriti
                   if not tof-inserito
                      exit perform
                   end-if
      *****        when accettati
      *****             if not tof-accettato
      *****                exit perform
      *****             end-if
              when inviati
                   if not tof-inviato
                      exit perform
                   end-if
              when in-lavorazioni
                   if not tof-in-lavorazione
                      exit perform
                   end-if
              when chiusi
                   if not tof-chiuso
                      exit perform
                   end-if
              end-evaluate

              |Ordine per stato
              if tutti
                 continue
              else
                 if tof-anno > anno-to
                    exit perform
                 end-if

                 if tof-numero > ordine-TO
                    exit perform 
                 end-if
              end-if

              perform VALIDA-ORDINE
              
              if record-ok 
                 perform METTI-IN-GRIGLIA 
              end-if
           end-perform.

           modify gd-ordini, mass-update 0.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           inquire gd-ordini, last-row in tot-righe.
           if event-data-2 < 2 move 2 to event-data-2 end-if.
           if event-data-2 > tot-righe move tot-righe to event-data-2 end-if.
           if riga not = event-data-2 
              perform CAMBIA-FONT-RIGA
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALIDA-ORDINE
BEGIN-TEXT
           set record-ok to true.

           if tof-numero < ordine-FROM or
              tof-numero > ordine-TO
              set record-ok to false
           end-if.

           if tof-data-ordine < data-from-AAAAMMGG or tof-data-ordine > data-to-AAAAMMGG
              set record-ok to false
           end-if.

           if tof-cod-forn < forn-from or 
              tof-cod-forn > forn-to
              set record-ok to false
           end-if.

           if tof-cliente < cliente-from or 
              tof-cliente > cliente-to
              set record-ok to false
           end-if.

           if forn-from not = zero and dest-forn-from not = zero
              if tof-destino not = dest-forn-from
                 set record-ok to false
              end-if
           end-if.

           if cliente-from not = zero and dest-cli-from not = zero
              if tof-destino-c not = dest-cli-from
                 set record-ok to false
              end-if
           end-if.

           if tof-anno not = anno-to
              set record-ok to false
           end-if.
END-TEXT
END-DATA
