BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Grid-Rbutton-Up
BEGIN-TEXT
           if mod = 1
              inquire form1-gd-1, cursor-y in riga, cursor-x in colonna
              evaluate colonna
              when 3   
              when 4
              when 5
              when 6 inquire form1-gd-1(riga,colonna), cell-data col-x
                     if col-x = "N" move "S" to col-x
                     else           move "N" to col-x
                     end-if
                     modify form1-gd-1(riga, colonna), cell-data col-x
                     perform VALORE-RIGA
                     if nom-rec not = old-nom-rec
                        set NoSalvato to true                            
                     else 
                        set SiSalvato to true
                     end-if
              when 12 inquire form1-gd-1(riga,colonna), cell-data col-oper
                     evaluate col-oper
                     when ">"   move "<"  to col-oper
                     when "<"   move "="  to col-oper
                     when "="   move "<=" to col-oper
                     when "<="  move ">=" to col-oper
                     when other move ">"  to col-oper
                     end-evaluate
                     modify form1-gd-1(riga, colonna), cell-data col-oper
                     perform VALORE-RIGA
                     if nom-rec not = old-nom-rec
                        set NoSalvato to true                            
                     else 
                        set SiSalvato to true
                     end-if
              end-evaluate
           end-if.
END-TEXT
END-DATA

