BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           set listino-auto  to false
           if link-fornitore not = zero
              move link-fornitore  to ef-forn-buf
                                      cli-codice
              modify ef-forn, VALUE ef-forn-buf

              set cli-tipo-f  to true
              read clienti no lock
                 invalid
                    move "Fornitore non valido"  to cli-ragsoc-1
              end-read
              move cli-ragsoc-1 to lab-forn-buf
              display lab-forn
              perform CONTROLLA-LISTINO-AUTO
           end-if

           if listino-auto
              set scelto-promo   to true
              perform Form1-Exit
           else
              move "<<HELP>> 0 = TUTTI GLI ARTICOLI" to lab-art-buf
              display lab-art

              move "<<HELP>> 0 = TUTTI I DESTINI" to lab-dest-buf
              display lab-dest

              accept start-data from century-date
              move start-data(7:2) to como-data(1:2)
              move start-data(5:2) to como-data(3:2)
              move start-data(1:4) to como-data(5:4)
              move como-data to ef-ini-buf
              display ef-ini

              move 99999999  to como-data
              move como-data to ef-fine-buf
              display ef-fine

              perform CARICA-GRID
              modify form1-Handle, VISIBLE 1
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeExit
BEGIN-TEXT
           if not scelto-promo 
              move 0 to link-codice 
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = AfterEndAccept
BEGIN-TEXT
           if scelto-promo
              perform PB-OK-LINKTO
           end-if
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CARICA-GRID
BEGIN-TEXT
           set tutto-ok to true.
           modify gd1, mass-update = 1.
           modify gd1, reset-grid  = 1.
           perform GD1-CONTENT.
           move 2 to riga.
           inquire ef-forn   value save-forn.
           inquire ef-dest   value save-dest.
           inquire ef-ini    value save-ini.
           inquire ef-fine   value save-fine.
           inquire ef-art    value save-articolo.

           move save-ini  to como-data.
           perform DATE-TO-FILE.
           move como-data to start-data.

           move save-fine  to como-data.
           perform DATE-TO-FILE.
           move como-data to como-data-to.

           move save-forn    to tlis-fornitore
           move save-dest    to tlis-destino
      *     move start-data   to tlis-ini-val
           move low-value    to tlis-ini-val
           move low-value    to tlis-fine-val
           start tlistini key >= tlis-chiave-ricerca
              invalid 
                 continue
              not invalid
                 perform until 1 = 2
                    read tlistini next 
                       at end 
                          exit perform 
                    end-read
                    if save-forn not = tlis-fornitore
                       exit perform 
                    end-if
                    set rec-ok  to true

                    if save-dest not = zero
                       if save-dest not = tlis-destino
                          exit perform 
                       end-if

                       if tlis-ini-val > como-data-to 
                          exit perform 
                       end-if
                    end-if

                    perform CONTROLLO-DATE

                    if rec-ok
                       if save-articolo not = 0
                          move tlis-chiave     to rlis-codice
                          move save-articolo   to rlis-articolo
                          read rlistini no lock
                             invalid 
                                set rec-ok  to false
                          end-read
                       end-if
                    end-if
                    if rec-ok
                       perform RECORD-OK
                    end-if
                 end-perform 
           end-start.

           modify gd1, mass-update = 0.

      ***---
       RECORD-OK.
           move tlis-fornitore  to desf-codice
           move tlis-destino    to desf-prog
           read destinif
              invalid
                 initialize desf-ragsoc-1
           end-read
           move desf-ragsoc-1   to col-dest
           move tlis-descrizione to col-nome.

           move tlis-ini-val to como-data.
           perform DATE-TO-SCREEN.
           move como-data to col-ini-val.

           move tlis-fine-val to como-data.
           perform DATE-TO-SCREEN.
           move como-data to col-fine-val.  

           move tlis-codice to hid-codice.

           modify gd1(riga, 1), hidden-data hid-codice.
           modify gd1(riga),    record-to-add rec-grid.
           add 1 to riga.


      ***---
       CONTROLLO-DATE.
           set sovrapposizione  to false
           if start-data >= tlis-ini-val and 
              como-data-to <= tlis-fine-val 
              set sovrapposizione   to true
           end-if

           if start-data >= tlis-ini-val and 
              como-data-to <= tlis-fine-val 
              set sovrapposizione   to true
           end-if

      *    controllo che il listino salvato che stò controllando non sia 
      *    compreso  nelle date che sto testando 
           if start-data <= tlis-ini-val and 
              como-data-to >= tlis-fine-val 
              set sovrapposizione   to true
           end-if

           if start-data <= tlis-ini-val and 
              como-data-to >= tlis-ini-val
              set sovrapposizione   to true
           end-if


           if start-data <= tlis-fine-val and 
              como-data-to >= tlis-fine-val
              set sovrapposizione   to true
           end-if


           if not sovrapposizione
              set rec-ok  to false
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-forn
                inquire ef-forn, value in cli-codice
                set cli-tipo-f  to true
                move   "clienti-all"     to como-file
                call   "zoom-gt"   using como-file, cli-rec
                                  giving stato-zoom
                cancel "zoom-gt"
      
                if stato-zoom = 0
                   move cli-codice    to ef-forn-buf
                   move cli-ragsoc-1  to lab-forn-buf
                   display ef-forn lab-forn
                   if cli-codice not = save-forn
                      perform CARICA-GRID
                   end-if
                end-if

           when 78-ID-ef-dest
                inquire ef-forn,      value in desf-codice
                inquire ef-dest       value desf-prog
                move "destinif-forn"  to como-file
                call "zoom-gt"   using como-file, desf-rec
                                giving stato-zoom
                end-call
                cancel "zoom-gt"
                if stato-zoom = 0
                   move desf-prog    to ef-dest-buf
                   move desf-ragsoc-1 to lab-dest-buf
                   display ef-dest lab-dest
                end-if

           when 78-ID-ef-art
                inquire ef-art, value in art-codice
                move "articoli"     to Como-File
                call   "zoom-gt" using como-file, art-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0

                   if art-codice not = save-articolo
                      perform CARICA-GRID
                   end-if

                   move art-codice      to ef-art-buf
                   move art-descrizione to lab-art-buf
                   display ef-art lab-art
                end-if

           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true. 

           evaluate control-id
           when 78-ID-ef-forn
                inquire ef-forn, value in cli-codice
                set cli-tipo-f  to true
                read clienti no lock
                   invalid
                      move spaces to cli-ragsoc-1
                      set errori to true
                      display message "Fornitore non valido"
                              title tit-err
                              icon 2
                end-read
                move cli-ragsoc-1 to lab-forn-buf
                display lab-forn
           when 78-ID-ef-dest
                inquire ef-forn value in desf-codice
                inquire ef-dest value in desf-prog
                if desf-prog not = zero
                   read destinif no lock
                      invalid
                         move spaces to desf-ragsoc-1
                         set errori to true
                         display message "Destino non valido"
                                 title tit-err
                                 icon 2
                   end-read
                   move desf-ragsoc-1 to lab-dest-buf
                else
                   move "TUTTI I DESTINI"   to lab-dest-buf
                end-if
                display lab-dest
           when 78-ID-ef-ini
                inquire ef-ini, value in como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-ini-buf
                   display ef-ini
                end-if

           when 78-ID-ef-fine
                inquire ef-fine, value in como-data
                if como-data = 0
                   move 99999999 to como-data
                   perform DATE-TO-SCREEN
                   move como-data to ef-fine-buf
                   display ef-fine
                else
                   if como-data not = 99999999
                      perform DATE-FORMAT
                      move como-data to ef-fine-buf                
                      display ef-fine
                   end-if
                end-if
                perform DATE-TO-FILE
                move como-data to como-data-to
                inquire ef-ini, value in como-data
                perform DATE-TO-FILE
                if como-data-to < como-data
                   set errori to true
                   display message "Range Date validità errato"
                             title tit-err
                              icon 2
                   move 78-ID-ef-ini to control-id
                end-if

           when 78-ID-ef-art
                inquire ef-art, value in art-codice
                if art-codice not = 0
                   read articoli no lock
                      invalid
                         move spaces to art-descrizione
                         set errori to true
                         display message "Articolo non valido"
                                 title tit-err
                                 icon 2
                   end-read
                   move art-descrizione to lab-art-buf
                else
                   move "TUTTI GLI ARTICOLI" to lab-art-buf
                end-if
                display lab-art
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move control-id to store-id
              move 4          to accept-control
           end-if.
END-TEXT
END-DATA
