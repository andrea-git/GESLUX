BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = tscorte
   ACTOBJ            = tscorte
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       tscorte-ERR SECTION.
           use after error procedure on tscorte.
           set RecLocked to false.
           evaluate status-tscorte
           when "35"
                display message "File [tscorte] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [tscorte] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[tscorte] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99"
           when "93"
                set RecLocked to true
                set errori    to true
                move 1 to mod-k
                move 0 to mod
                modify tool-modifica, value = mod
                display form1
                set StatusVisua to true
                perform STATUS-BAR-MSG
                move "00" to status-tscorte
                if ReadSecca
                   read tscorte       no lock 
                        invalid      continue
                        not invalid  perform FORM1-IUD-DISPLAY
                   end-read
                end-if
                display message box MSG-Record-gia-in-uso
                        title = tit-err
                        icon is mb-error-icon
           when "23"
                move 1 to mod-k
                move 0 to mod
                modify tool-modifica, value = mod
                set StatusVisua to true
                perform STATUS-BAR-MSG 
                if nuovo              
                   initialize sco-dati replacing numeric data by zeroes
                                            alphanumeric data by spaces
                   perform FORM1-FLD-TO-BUF
                   display form1-tb-1
                   set vecchio to true
                end-if      
                perform ABILITAZIONI
                display form1
                |RISETTO IL VALORE A "23" PERCHE DENTRO AL PAR. CLEAR-SCREEN
                |CON L'OPERAZIONE UNLOCK VIENE RISETTATO A ZERO COSICCHÈ 
                |QUANDO TORNA DENTRO AI PAR. DI SCORRIMENTO DI TOTEM SI 
                |TROVA IN UNA SITUAZIONE CORRETTA E PROSEGUE CON LA READ
                |USCENDO CON UN MESSAGGIO DI ERRORE DI TIPO "46"
                move "23" to status-tscorte
           end-evaluate.
END-TEXT
END-DATA
