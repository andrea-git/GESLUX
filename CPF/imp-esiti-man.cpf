BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = imp-esiti-man
   ACTOBJ            = imp-esiti-man
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       tvettori-ERR SECTION.
           use after error procedure on tvettori.
           set RecLocked to false.
           set tutto-ok  to true.
           evaluate status-tvettori 
           when "39"
                set errori to true
                display message "File [tvettori] mismatch size!"
                          title titolo
                           icon 3
           when "98"
                set errori to true
                display message "[tvettori] Indexed file corrupt!"
                          title titolo
                           icon 3
           when "35"
                display message box        "Impossibile procedere."
                  x"0d0a""File delle testate [tvettori] inesistente"
                        title = titolo
                        icon 2
                set errori to true
           when "93"
           when "99" set RecLocked to true
           end-evaluate.  

      ***---
       vettel-ERR SECTION.
           use after error procedure on vettel.
           set RecLocked to false.
           set tutto-ok  to true.
           evaluate status-vettel
           when "39"
                set errori to true
                display message "File [vettel] mismatch size!"
                          title titolo
                           icon 3
           when "98"
                set errori to true
                display message "[vettel] Indexed file corrupt!"
                          title titolo
                           icon 3
           when "35"
                display message box        "Impossibile procedere."
                  x"0d0a""File delle testate [vettel] inesistente"
                        title = titolo
                        icon 2
                set errori to true
           when "93"
           when "99" set RecLocked to true
           end-evaluate.
END-TEXT
END-DATA
