BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           move "tvettori"   to como-file.
      
           inquire ef-vet, value in vet-codice.
      
           call "zoom-gt" using como-file, vet-rec
                          giving stato-zoom
           cancel "zoom-gt"
           if stato-zoom = 0
              move vet-codice      to ef-vet-buf
              move vet-descrizione to lbl-vet-buf
              modify ef-vet, VALUE ef-vet-buf
              modify lbl-vet, TITLE lbl-vet-buf
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
      *    

           when 78-ID-ef-vet
                inquire ef-vet, VALUE vet-codice
                if vet-codice = zero
                   move "0 = TUTTI I VETTORI" to lbl-vet-buf
                else
                   read tvettori
                      invalid
                         set errori to true
                         display message "Vettore Inesisitente"
                                  title tit-err
                        move space to lbl-vet-buf
                      not invalid
                        move vet-codice        to vtt-codice
                        move vet-descrizione   to lbl-vet-buf
                        read vettel
                          invalid
                             move space  to vtt-nome-flusso-imp
                        end-read
                        if vtt-nome-flusso-imp = space
                           set errori to true
                           display message "Import non configurato"
                                     title tit-err
                                      icon 2
                        end-if
                   end-read
                   modify lbl-vet, TITLE lbl-vet-buf
                end-if
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move control-id to store-id        
              move 4          to accept-control  
           end-if.

END-TEXT
END-DATA
