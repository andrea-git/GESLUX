BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeAccept
BEGIN-TEXT
           initialize titolo.
           if geslock-nome-file = spaces or geslock-contatore
              move "ATTENZIONE!!!" to titolo
           else                   
              inspect geslock-nome-file replacing trailing spaces by low-value
              string "Gestione Lock on [" delimited by size
                     geslock-nome-file    delimited by low-value
                     "]"                  delimited by size
                     into titolo
              end-string
           end-if.
                  
           initialize lab-messaggio-buf.
           if geslock-messaggio not = spaces
              move geslock-messaggio to lab-messaggio-buf
           else
              inspect geslock-nome-file 
                      replacing trailing spaces by low-value
              string "File: ["          delimited by size
                     geslock-nome-file  delimited by low-value
                     "]"                delimited by size
                     x"0d0a"            delimited by size
                     "Record bloccato." delimited by size
                     into lab-messaggio-buf
              end-string
           end-if.

           move geslock-v-riprova to v-riprova.
           move geslock-v-ignora  to v-ignora.
           move geslock-v-termina to v-termina.
           move geslock-v-annulla to v-annulla.
           move geslock-v-salva   to v-salva.

           display form1.

           modify form-geslock-handle, title = titolo.

           evaluate true
           when geslock-righe
                modify lab-messaggio, color = 5, font = Impact26B-Occidentale
                modify Screen1-La-1,  visible false
                modify Screen1-Bt-1,  bitmap-number = 2
           when geslock-omom
                modify lab-messaggio, color = 5, font = Impact18B-Occidentale
                modify Screen1-La-1,  visible false
                modify Screen1-Bt-1,  bitmap-number = 2
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
   ENTRYPOINT        = BeforeExit
BEGIN-TEXT
           evaluate control-id
           when 1001 perform PB-RIPROVA-LINKTO
           when 1002 perform PB-IGNORA-LINKTO
           when 1003 perform PB-TERMINA-LINKTO
           when 1004 perform PB-ANNULLA-LINKTO
           when 1005 perform PB-SALVA-LINKTO
           end-evaluate.
END-TEXT
END-DATA
