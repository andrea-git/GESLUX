BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = geslock-Ev-Before-Program
BEGIN-TEXT
      * Impact26B-Occidentale
           initialize wfont-data Impact26B-Occidentale.
           move 26                 to wfont-size.
           move "Impact"           to wfont-name.
           set wfcharset-dont-care to true.
           set wfont-bold          to true.
           set wfont-italic        to false.
           set wfont-underline     to false.
           set wfont-strikeout     to false.
           set wfont-fixed-pitch   to false.
           move 0 to wfont-char-set.
           call "W$FONT" using wfont-get-font, 
                     Impact26B-Occidentale, wfont-data.

      * Impact16B-Occidentale
           initialize wfont-data Impact18B-Occidentale.
           move 18                 to wfont-size.
           move "Impact"           to wfont-name.
           set wfcharset-dont-care to true.
           set wfont-bold          to true.
           set wfont-italic        to false.
           set wfont-underline     to false.
           set wfont-strikeout     to false.
           set wfont-fixed-pitch   to false.
           move 0 to wfont-char-set.
           call "W$FONT" using wfont-get-font, 
                     Impact18B-Occidentale, wfont-data.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = geslock-Ev-After-Program
BEGIN-TEXT
           destroy Impact26B-Occidentale.
END-TEXT
END-DATA

