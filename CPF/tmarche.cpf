END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA
BEGIN-TEXT
           if mod = ZERO
              exit paragraph 
           end-if.
      *
           display message box MSg-Cancellare-il-record-corrente
                   title   titolo
                   type    mb-yes-no 
                   default mb-no
                   giving  scelta

           if scelta = mb-yes
              perform RIEMPI-CHIAVE
              delete tmarche record 
                 invalid 
                    continue 
              end-delete     
              move zero to mod
              move 1    to mod-k
              perform FORM1-CLEAR
              set vecchio to true
              perform CANCELLA-COLORE
              perform INIT-OLD-REC

              
              display message box MSG-Cancellazione-avvenuta-con-successo
                      title titolo                         

              unlock tmarche all records

              perform PRIMO 
              move 78-ID-ef-codice to  CONTROL-ID     
              move 4         to  ACCEPT-CONTROL 

              set Statusvisua to true
              perform STATUS-BAR-MSG
              modify TOOL-MODIFICA value = mod
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-cod-da
                inquire ef-cod-da, value in mar-codice
                move  "tmarche"       to como-file
                call   "zoom-gt"   using como-file, mar-rec
                cancel "zoom-gt"
                if stato-zoom = 0
                   move mar-codice to ef-cod-da-buf
                   display ef-cod-da
                end-if

           when 78-ID-ef-cod-a 
                inquire ef-cod-a, value in mar-codice
                move  "tmarche"      to como-file
                call   "zoom-gt"  using como-file, mar-rec
                cancel "zoom-gt"
                if stato-zoom = 0
                   move mar-codice to ef-cod-a-buf
                   display ef-cod-a
                end-if
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CLEAR-SCREEN
BEGIN-TEXT
           move mar-chiave to old-mar-chiave.

           initialize mar-dati replacing numeric data by zeroes
                                   alphanumeric data by spaces.
                         
           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.
                                          
           display form1. 
  
           unlock tmarche all records.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-INSERIMENTO
BEGIN-TEXT
      * MODIFICA LA STATUS-BAR
      * CONTROLLO SE DA INSERIMENTO SCORRO. ALLORA DEVO SEGNALARE CHE
      * SONO TORNATO IN MODIFICA.
           inquire tool-modifica, value in mod.

           evaluate mod
           when 1
                set StatusModifica to true
           when ZERO
                set StatusVisua to true   
           end-evaluate.
      
           perform STATUS-BAR-MSG.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-INTERVALLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-cod-da
                inquire ef-cod-da, value in ef-cod-da-buf
                if ef-cod-da-buf not = 0
                   move ef-cod-da-buf to mar-codice
                   read tmarche no lock 
                        invalid 
                        set errori to true
                        display message "Marca NON valida"
                                  title tit-err
                   end-read
                end-if

           when 78-ID-ef-cod-a
                inquire ef-cod-da, value in ef-cod-da-buf
                inquire ef-cod-a,  value in ef-cod-a-buf
                evaluate ef-cod-a-buf
                when 0
                     move 9999 to ef-cod-a-buf
                     display ef-cod-a
                when 9999
                     continue
                when other
                     move ef-cod-a-buf to mar-codice
                     read tmarche no lock 
                          invalid
                          set errori to true
                          display message "Marca NON valida"
                                    title tit-err
                                     icon 2
                      not invalid
                          if ef-cod-a-buf < ef-cod-da-buf
                             display message "Intervallo marca errato"
                                       title tit-err
                                        icon 2
                             set errori to true
                             move 78-ID-ef-cod-da to control-id
                          end-if
                     end-read
                end-evaluate
           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move 4  to accept-control
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-ID-ef-codice
                perform RIEMPI-CHIAVE

                inspect ef-codice-buf replacing trailing SPACES by low-values
                initialize CountChar
                inspect ef-codice-buf 
                   tallying CountChar for characters before low-value
                inspect ef-codice-buf replacing trailing low-values by SPACES

                | SE E' NUMERICO LEGGO IL RECORD NORMALMENTE
                if ef-codice-buf(1:CountChar) is numeric
                   perform SELEZIONA-NUMERICO
                else
                   | HO DIGITATO UNA SIGLA ALFANUMERICA QUINDI 
                   | APRO LO ZOOM (CASE SENSITIVE)
                   perform SELEZIONA-ALFA
                end-if

           when 78-ID-ef-descrizione
                inquire ef-descrizione value in mar-descrizione
                if mar-descrizione = SPACES
                   set errori to true
                   display message box "Descrizione obbligatoria"
                           title tit-err
                           type  mb-ok
                           icon  mb-warning-icon
                end-if     
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if mod = 1
              read tmarche    lock invalid 
                 set errori to true 
              end-read
           else
              read tmarche no lock invalid 
                 set errori to true 
              end-read
           end-if.                    
           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move ZERO      to mod
                    move 1         to mod-k
                    move 78-ID-ef-codice to CONTROL-ID
                    move 4               to ACCEPT-CONTROL
                 end-if   
                 perform FORM1-IUD-DISPLAY
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else                         
                    set StatusVisua    to true
                 end-if
                 perform STATUS-BAR-MSG
                 perform VALORIZZA-OLD
              else
                 move ZERO to mod
                 move 1    to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-codice to CONTROL-ID
                       move 4               to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT
BEGIN-TEXT
           move ZERO to StatusHelp.
           move 1    to mod-k.
           set StatusVisua to true.     
           set errore-des  to false.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-mar-rec replacing numeric data by zeroes
                                       alphanumeric data by spaces. 


           set old-mar-no-imposta-consumo to true.
           set old-mar-no-cou             to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
      *  SE L'UTENTE E' ABILITATO PUO' MODIFICARE UN RECORD
              if mod = 1
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if CONTROL-ID < 78-ID-ef-descrizione
                       move 78-ID-ef-descrizione to CONTROL-ID
                    end-if
                    move ZERO to mod-k
                    move 1    to mod
                    set StatusModifica to true
                    perform STATUS-BAR-MSG
                 end-if
              else
                 move 1    to mod
                 perform SALV-MOD            
                 move ZERO to mod
                 move 1    to mod-k
                 if errori
                    move 1    to mod
                    move ZERO to mod-k
                 else
                    move 78-ID-ef-codice to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    perform STATUS-BAR-MSG 
                    unlock tmarche all records
                    if KeyboardSaved
                       set KeyboardReleased to true
                       set environment "KEYSTROKE" to "DATA=44 44"
                       set environment "KEYSTROKE" to "DATA=46 46"
                    end-if
                 end-if
              end-if
                        
              display form1
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to ACCEPT-CONTROL
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           perform SALV-MOD.

           if tutto-ok  
              move 78-ID-ef-codice to CONTROL-ID       
              move 4               to ACCEPT-CONTROL   
              move 1               to mod mod-k
              modify tool-modifica,  value = mod
              perform CANCELLA-COLORE                  
              perform VALORIZZA-NUOVO
              perform INIT-OLD-REC       
              set StatusIns to true
              perform STATUS-BAR-MSG  
              set nuovo to true       
              unlock tmarche all records
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ORDINAMENTO
BEGIN-TEXT
           inquire tool-ord, value in OrderBy.
      
           if OrderBy = zero
              perform DataSet1-CHANGETO-KEY1
           else
              perform DataSet1-CHANGETO-KEY2
           end-if.
      
           perform Form1-Curr.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           inquire ef-codice, value in ef-codice-buf.
           move ef-codice-buf       to mar-codice convert.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".
           copy "status.cpy".
           copy "calcola-colore-trasparente.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRECEDENTE
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tmarche all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tmarche-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tmarche-lock-flag
              end-if
                    
              perform CANCELLA-COLORE
              perform FORM1-PREVIOUS

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tmarche-lock-flag
                 move 1 to mod-k
                 perform FORM1-PREVIOUS
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok    
 
              perform CHECK-INSERIMENTO
              unlock tmarche all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tmarche-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tmarche-lock-flag
              end-if
                                  
              perform CANCELLA-COLORE
              perform FORM1-FIRST

              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tmarche-lock-flag
                 move 1 to mod-k
                 perform FORM1-FIRST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = ZERO 
              exit paragraph 
           end-if.

           set tutto-ok to true.

           perform varying CONTROL-ID from 78-ID-ef-codice by 1
                     until CONTROL-ID > 78-ID-chk-cou
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if errori
              perform ABILITAZIONI
              move store-id to CONTROL-ID       
              move 4        to ACCEPT-CONTROL   
           else                       
              perform FORM1-BUF-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              perform cancella-colore 

              write mar-rec 
                 invalid 
                    rewrite mar-rec
              end-write

              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform Form1-CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel 
                            giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no  
                   continue
              when other                
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA
BEGIN-TEXT
           perform RIEMPI-CHIAVE.

           move "tmarche" to    como-file.
           call "zoom-gt" using como-file, mar-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = ZERO
              if old-mar-chiave not = mar-chiave
                 move mar-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to mar-chiave
                    set ReadSecca to true
                    modify ef-codice, value mar-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-codice to CONTROL-ID       
                    move 4               to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA-ALFA
BEGIN-TEXT
      * LA SECONDA CHIAVE E' ALFANUMERICA E LA VALUE VARIABLE CONTIENE
      * ANCORA GLI ZERI DAVANTI CHE ELIMINO
           inspect ef-codice-buf replacing leading ZERO by SPACES.
           move ef-codice-buf to mar-descrizione.

      * POI GIUSTIFICO A SINISTRA E MI POSIZIONO SULLO ZOOM PER CHIAVE
      * ALTERNATA
           call "C$JUSTIFY" using mar-descrizione, "L".

           start tmarche key >= mar-descrizione
              invalid 
                 continue
              not invalid
                 read tmarche next 
           end-start.

           move "tmarche-alfa" to como-file.
           call "zoom-gt" using   como-file, mar-rec
                          giving  stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = ZERO
              move mar-codice    to codice-ed
              move codice-ed     to ef-codice-buf   
              call "C$JUSTIFY" using ef-codice-buf, "L"
              display ef-codice

              if old-mar-descrizione not = mar-descrizione
                 move mar-descrizione to save-key-1
                 perform SALV-MOD
                 if tutto-ok
                    move save-key-1 to mar-descrizione
                    set ReadSecca  to true
                    modify ef-codice, value mar-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-codice to CONTROL-ID       
                    move 4               to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA-NUMERICO
BEGIN-TEXT
           if mar-codice not > ZERO
              set errori to true
              display message box MSG-Codice-obbligatorio
                      title tit-err
                      type  mb-ok
                      icon  mb-warning-icon
           else          
              read tmarche no lock
                 invalid
                    if vecchio
                       display message box MSG-record-inesistente
                               title = tit-err
                               icon mb-warning-icon
                       perform CLEAR-SCREEN
                       set errori to true
                    end-if
                 not invalid      
                    if old-mar-chiave not = mar-chiave
                       perform CURRENT-RECORD 
                       set errori to true
                    end-if
              end-read
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SUCCESSIVO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tmarche all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tmarche-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tmarche-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tmarche-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA
BEGIN-TEXT
           continue.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ULTIMO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tmarche all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tmarche-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tmarche-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tmarche-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move mar-rec to old-mar-rec.
           set vecchio to true.

           move ZERO to StatusHelp
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-NUOVO
BEGIN-TEXT
           move high-value to mar-codice.

           start tmarche key <= mar-codice
                 invalid move 1 to mar-codice
             not invalid
                 read tmarche previous with no lock
                    at end
                       move 1 to mar-codice
                    not at end
                       add  1 to mar-codice
                 end-read
           end-start.

           move "00"    to status-tmarche.

           initialize mar-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                                       
           perform FORM1-IUD-DISPLAY.

           if OrderBy = 1                             
              move high-value to mar-descrizione
              start tmarche key is <= mar-descrizione 
                    invalid continue 
                not invalid
                    read tmarche previous no lock 
                       at end continue 
                    end-read
              end-start
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      * SE NON HO VALORIZZATO I CAMPI DI CREAZIONE LI VALORIZZO
           if mar-data-creazione   = zero
              accept mar-data-creazione from century-date
           end-if.
           if mar-ora-creazione    = zero
              accept mar-ora-creazione  from time
           end-if.
           if mar-utente-creazione = space
              move user-codi to mar-utente-creazione
           end-if.

           accept mar-data-ultima-modifica from century-date.
           accept mar-ora-ultima-modifica  from time.
           move user-codi    to mar-utente-ultima-modifica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-codice to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock tmarche all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              set environment "KEYSTROKE" to "DATA=44 44"
              set environment "KEYSTROKE" to "DATA=46 46"
           end-if.
END-TEXT
