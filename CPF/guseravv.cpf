BEGIN-TEXT
           modify gd1(riga, 1),
                  bitmap        = elemento-bmp,
                  bitmap-number = 1,
                  bitmap-width  = 16.
      
           move event-data-2 to riga.
      
           if riga > 1
              modify gd1(riga, 1),
                     bitmap        = elemento-bmp,
                     bitmap-number = 2,
                     bitmap-width  = 16 
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA
BEGIN-TEXT
           if mod = 0        exit paragraph end-if.
           perform X-Y.
      
           display message box "Cancellare il messaggio?"
                    type mb-yes-no
                    default mb-yes
                    title titolo
                    giving scelta

      *
           if scelta = mb-yes
              perform RIEMPI-CHIAVE
              set tutto-ok to true
              perform X-Y
              if tot-righe <= 2
                 |COSI' MI FA ANCHE IL RIPOSIZIONAMENTO
                 |SUL PRIMO RECORD
                 set DeleteRec to true
              end-if
                   
              perform VALORE-RIGA
              inquire gd1(riga, 2), cell-data in col-tipo
              move col-tipo to uav-tipologia of useravv
              delete useravv record 
                 invalid 
                    continue 
              end-delete
              modify gd1,  record-to-delete = riga
              move 0 to riga-nuova
              |Ho cancellato l'ultima riga
              if riga = tot-righe
                 subtract 1 from riga
                 subtract 1 from tot-righe
              end-if
              move riga   to event-data-2
              move 2      to colonna
              perform PUNTA-CELLA

              perform VALORE-RIGA
              perform VALORE-OLD-RIGA
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           evaluate CONTROL-ID
           when 78-ID-ef-codice
                inquire ef-codice, value in user-cod
                move "user"      to Como-File
                call   "zoom-gt" using como-file, user-rec
                                giving stato-zoom
                cancel "zoom-gt"
                if stato-zoom = 0
                   move user-cod   to ef-codice-buf
                   move user-name  to lab-des-buf
                   display ef-codice lab-des
                end-if
               
           when 78-ID-gd1
                perform X-Y
                perform VALORE-RIGA
                evaluate colonna
                when 2
                     if col-tipo = space
                        move col-tipo    to tav-codice
                        move "ttipoavv"  to Como-File
                        call   "zoom-gt" using como-file, tav-rec
                                        giving stato-zoom
                        cancel "zoom-gt"
                        if stato-zoom = 0
                           move tav-codice to col-tipo
                           modify gd1(riga, 2),cell-data col-tipo
                           move tav-messaggio  to col-messaggio
                           modify gd1(riga, 3),cell-data col-messaggio

                           perform CERCA-DOPPIONE
                           if errori
                              move space to col-tipo
                                            col-messaggio
                              modify gd1(riga, 2),cell-data col-tipo
                              modify gd1(riga, 3),cell-data 
                                                           col-messaggio
                           else
                              perform SALVA
                           end-if
                        end-if     
                     end-if
           end-evaluate. 
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-INSERIMENTO
BEGIN-TEXT
      * modifica la status-bar
