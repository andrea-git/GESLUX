BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok  to true.
           set RecLocked to false.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod

      * Adriano - Gestione lock sul file inizio
                 close useravv
                 open i-o useravv allowing readers
      * Adriano fine

           else 
              if mod = 1
      * Adriano - Gestione lock sul file inizio
                 close useravv
                 open i-o useravv allowing readers
      * Adriano fine
                 set YesMessage to true
                 set ReadSecca  to true
                 perform CURRENT-RECORD
                 
                 if errori
                    move 0 to mod
                    move 1 to mod-k
                    set StatusVisua to true
                 else
                    if not RecLocked
                       move 0 to mod-k
                       move 1 to mod
                       set StatusModifica to true
                    else
                       move 0 to mod
                       move 1 to mod-k
                       set StatusVisua to true
                    end-if
                    move 2 to riga
                    move 1 to colonna
                 end-if
              else
                 move 1 to mod-k
                 move 1 to mod |devo per un attimo simulare 
                               |ancora lo stato di modifica
                               |altrimenti il CONTROLLO-OLD
                               |non viene eseguito
                 perform SALV-MOD
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    unlock useravv all records
                    move 0 to mod
                    set YesMessage to true
                    set ReadSecca  to true
      * Adriano - Gestione lock sul file inizio
                    close useravv
                    open input useravv
      * Adriano - fine
                    perform CURRENT-RECORD
                 end-if
              end-if
                        
              display form1             

              perform CANCELLA-COLORE

              move 78-ID-ef-codice to control-id
              move 4               to accept-control

           end-if.
           modify tool-modifica,  value mod.

           if vecchio
              if mod = 1 set StatusModifica to true
              else       set StatusVisua    to true
              end-if
           end-if.

           perform STATUS-BAR-MSG.
           perform ABILITAZIONI.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           evaluate control-id
           when 78-ID-ef-codice
                perform X-Y
                perform SALV-MOD
                if tutto-ok
                   initialize save-chiave
                   perform CANCELLA-COLORE
                   move spaces to ef-codice-buf
                   move spaces to lab-des-buf
                   display ef-codice lab-des
                   move 78-ID-ef-codice to control-id
                   move 4 to accept-control
                   move 1 to mod mod-k
                   modify tool-modifica,  value = mod
                   move 0      to lab-des-buf
                   perform CLEAR-SCREEN
                   set StatusIns to true
                   perform STATUS-BAR-MSG
                   set nuovo to true
                   unlock useravv all records
                   |IN QUESTO PGM. LE RIGHE SCENDONO DA TVETTORI
                   |modify gd1, insert-rows = 1
                   perform ABILITAZIONI
                end-if

           when 78-ID-gd1
                if rec-grid not = old-rec-grid
                   perform SALVA
                else
                   set tutto-ok to true
                end-if
                if tutto-ok
                   if riga-nuova = 0
                      set nuovo to true
                      move 1 to mod
                      move 0 to mod-k
                      display ef-codice
                      modify tool-modifica,  value = mod
                      move 2 to colonna
                      set StatusModifica to true
                      perform STATUS-BAR-MSG
                      perform ABILITAZIONI
                      move 1 to riga-nuova
                      modify gd1, insert-rows = 1
                      perform X-Y
                      add 1 to tot-righe giving event-data-2
                      modify gd1(riga, 1),
                             bitmap        = elemento-bmp,
                             bitmap-number = 1,
                             bitmap-width  = 16
                      move event-data-2  to riga
                      move 2  to colonna
                      perform PUNTA-CELLA
                      initialize old-rec-grid rec-grid
                                 replacing numeric data by zeroes
                                      alphanumeric data by spaces
                   end-if
                end-if
           end-evaluate.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".   
           copy "status.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRECEDENTE
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock useravv all records 

              perform RIEMPI-CHIAVE
              move low-value to uav-tipologia of useravv

              start useravv key is < uav-chiave of useravv
                 invalid  
                    set errori to true
                 not invalid 
                    read useravv previous no lock
                       at end   
                          set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move uav-chiave of useravv to save-chiave
                 move low-value   to uav-tipologia of useravv
                 set ReadSecca    to false
                 perform CURRENT-RECORD
              end-if

              if errori or not trovato
                 display message "File [USERAVV]"
                          x"0d0a""Raggiunto inizio file"
                           title titolo
              end-if

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRIMO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock useravv all records 

              move low-value to uav-chiave of useravv
              start useravv key is >= uav-chiave of useravv
                    invalid  set errori to true
                not invalid 
                    read useravv next no lock
                         at end   set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move uav-chiave of useravv to save-chiave
                 move low-value   to uav-tipologia of useravv
                 set  ReadSecca   to false
                 perform CURRENT-RECORD
              end-if

              if errori
                 perform CLEAR-SCREEN
                 display message "File [useravv]"
                          x"0d0a""Archivio vuoto"
                           title titolo
                 perform CANCELLA-COLORE
                 move 78-ID-ef-codice to control-id
                 move 4               to accept-control
              end-if

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PUNTA-CELLA
BEGIN-TEXT
           move 78-ID-gd1          to control-id store-id.
           move 4                  to accept-control.
           modify gd1,       cursor-y riga, 
                             cursor-x colonna.
           perform COLORE-RIGA.
           perform CAMBIA-FONT-RIGA.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RESET-GRIGLIA
BEGIN-TEXT
           modify gd1, reset-grid = 1.
           perform GD1-CONTENT.
           initialize rec-grid.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           inquire ef-codice value in uav-utente of useravv.
           inquire gd1(riga, 2),    cell-data uav-tipologia of useravv.


END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel
                            giving scelta
       
              evaluate scelta
              when mb-yes
                   perform TOOL-SALVA-LINKTO
              when mb-no
                   continue
              when other
                   perform CANCELLA-COLORE
                   set errori    to true
                   if control-id= 78-id-gd1
                      perform PUNTA-CELLA
                   end-if
                   move store-id to control-id
                   move 4 to accept-control
              end-evaluate

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           set tutto-ok to true.

           if mod = 0 
              exit paragraph 
           end-if.

           perform VALORE-RIGA.

           if tutto-ok
              |perform varying control-id from 78-ID-ef-codice by 1
              perform varying control-id from 78-ID-gd1 by 1
                        until control-id > 78-ID-gd1
                 perform CONTROLLO

                 if errori
                    exit perform
                 end-if
           end-if.

           if errori
              if control-id = 78-ID-gd1
                 perform PUNTA-CELLA
              end-if
              move store-id to control-id
              move 4 to accept-control
           else
              perform RIEMPI-CHIAVE
              perform GRID-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              write uav-rec  of useravv
                 invalid 
                    rewrite uav-rec  of useravv
              end-write
              perform VALORIZZA-OLD
              |per il salvataggio singolo allo spostarsi di riga
              set SiSalvato to true
              set vecchio   to true
              move 0        to riga-nuova
              |Arriva valorizzato soltanto quando premo F3 da toolbar
      *        if StoSalvando
      *           perform TORNA-IN-VISUA
      *        end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           move   "user"  to como-file.
      
           call   "zoom-gt"  using como-file, user-rec
                            giving stato-zoom.
           cancel "zoom-gt".
      
           if stato-zoom = 0
              if old-uav-utente not = user-cod
      *           move uav-chiave of useravv to save-chiave
                 perform SALV-MOD
                 if tutto-ok
      *              move save-chiave  to uav-chiave of useravv
                    move user-cod  to uav-utente of useravv
                    move uav-utente of useravv to user-cod
                    read user no lock
                       invalid 
                          move spaces to user-name
                    end-read
                    move uav-utente of useravv to ef-codice-buf
                    move user-name to lab-des-buf
                    display lab-des ef-codice
                    set ReadSecca to true
                    |Cos se sono in mod = 1 mi lock la prima riga
                    modify gd1, cursor-y = 2
                    perform CANCELLA-COLORE
                    perform CURRENT-RECORD
                    move 78-ID-ef-codice to control-id
                    move 4 to accept-control
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA-NUMERICO
BEGIN-TEXT
           perform RIEMPI-CHIAVE.

      *    guardo se ho almeno 1 messaggio
         

           move uav-utente of useravv to save-codice
           move low-value  to uav-tipologia of useravv

           set trovato   to false.

           move uav-utente of useravv to user-cod.
           start useravv key >= uav-chiave of useravv
              invalid 
                 continue
              not invalid
                 read useravv next no lock
                    at end
                       continue
                    not at end
                       if uav-utente of useravv = save-codice
                          set trovato to true 
                       end-if
                 end-read
           end-start.

           if not trovato
              if vecchio
                 display message MSG-record-inesistente
                         title tit-err
                         icon 2
                 perform CLEAR-SCREEN
                 set errori to true
              else
                 perform X-Y
                 if tot-righe = 1 and riga-nuova = 0
                    move 1 to riga-nuova
                    modify gd1, insert-rows = 1
                    move 2 to colonna
                    move 2 to riga
                    perform PUNTA-CELLA
                    move 2 to event-data-2
                    move 0            to riga
                    perform CAMBIA-FONT-RIGA
                    perform GRID-HELP
                 end-if
              end-if
           else           
              inquire gd1, last-row in tot-righe
              if nuovo and tot-righe > 1
                 perform CURRENT-RECORD
              else
                 if old-uav-utente not = uav-utente of useravv
                    set ReadSecca to true
                    perform CURRENT-RECORD 
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           set tutto-ok to true.
           perform X-Y.

           if event-data-2 not = riga
              perform VALORE-RIGA

              if riga-nuova = 1 
                 |Ho inserito una riga ma  ancora vuota
                 if riga > tot-righe
                    move 0 to riga-nuova
                    modify gd1, record-to-delete = riga
                    move event-data-2 to riga
                    perform VALORE-OLD-RIGA
                    move 2 to colonna
                    perform PUNTA-CELLA
                 end-if
              end-if

              if tutto-ok
                 if vecchio
                    if rec-grid not = old-rec-grid
                       perform SALVA
                       if errori
                          perform PUNTA-CELLA
                          set event-action to event-action-fail
                       end-if
                    end-if
                    move rec-grid  to old-rec-grid
      *              if mod = 1
      *                 perform LOCK-RECORD
      *              end-if
                 end-if
              end-if
           else
              if colonna not = event-data-1 and riga-nuova = 1
                 perform CONTROLLO-RIGA
                 if errori
                    perform PUNTA-CELLA
                    set event-action to event-action-fail
                 end-if
              end-if
           end-if.

           if tutto-ok
              perform CAMBIA-FONT-RIGA
      *       valorizzo l'old della riga su cui sono andato
              move event-data-2 to riga
              move event-data-1 to colonna
              perform VALORE-OLD-RIGA
              perform INQUIRE-GRID
      *       colorazione riga in grid
              perform COLORE-RIGA
           end-if.

           perform GRID-HELP.
           set tutto-ok to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA
BEGIN-TEXT
           set stampa       to true.
      *     perform paragrafo-stampa.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SUCCESSIVO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock useravv all records 

              perform RIEMPI-CHIAVE
              move high-value to uav-tipologia of useravv

              start useravv key is > uav-chiave of useravv
                 invalid  
                    set errori to true
                 not invalid 
                    read useravv next no lock
                       at end   
                          set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move uav-chiave of useravv to save-chiave
                 move low-value   to uav-tipologia of useravv
                 set ReadSecca    to false
                 perform CURRENT-RECORD
              end-if

              if errori or not trovato
                 display message "File [useravv]"
                          x"0d0a""Raggiunta fine file"
                           title titolo
              end-if

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           set StatusVisua to true.
           display form1.
           modify tool-modifica, value = mod.
           perform CANCELLA-COLORE.
           move 78-ID-ef-codice to control-id.
           move 4               to accept-control.
           unlock useravv  all records.
           perform STATUS-BAR-MSG.
           perform ABILITAZIONI.
           unlock useravv all records.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ULTIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok                 
              perform CHECK-INSERIMENTO
              unlock useravv all records 

              move high-value to uav-chiave of useravv
              start useravv key is < uav-chiave of useravv
                 invalid  
                    set errori to true
                 not invalid 
                    read useravv previous no lock
                       at end   
                          set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move uav-chiave of useravv to save-chiave
                 move low-value   to uav-tipologia of useravv
                 set  ReadSecca   to false
                 perform CURRENT-RECORD
              end-if

              if errori
                 perform CLEAR-SCREEN
                 display message "File [useravv]"
                          x"0d0a""Archivio vuoto"
                           title titolo
                 perform CANCELLA-COLORE
                 move 78-ID-ef-codice to control-id
                 move 4               to accept-control
              end-if

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-OLD-RIGA
BEGIN-TEXT
           inquire gd1(riga, 2),    cell-data old-col-tipo.
           inquire gd1(riga, 3),    cell-data old-col-messaggio.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-RIGA
BEGIN-TEXT
           perform X-Y.
           perform INQUIRE-GRID.

      ***---
       INQUIRE-GRID.
           inquire gd1(riga, 2),    cell-data col-tipo.
           inquire gd1(riga, 3),   cell-data col-messaggio.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      *    se non ho valorizzato i campi di creazione li valorizzo
           if uav-data-creazione of useravv = 0
              accept uav-data-creazione  of useravv from century-date
           end-if.
           if uav-ora-creazione of useravv = 0
              accept uav-ora-creazione of useravv from time
           end-if.
           if uav-utente-creazione of useravv = space
              move user-codi to uav-utente-creazione of useravv
           end-if.

           accept uav-data-ultima-modifica of useravv from century-date.
           accept uav-ora-ultima-modifica  of useravv from time.
           move user-codi to uav-utente-ultima-modifica of useravv.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move uav-rec of useravv to old-uav-rec.
           move rec-grid  to old-rec-grid
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = X-Y
BEGIN-TEXT
           inquire gd1, cursor-y in riga, 
                        cursor-x in colonna
                        last-row in tot-righe.
END-TEXT
END-DATA
