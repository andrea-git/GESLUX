BEGIN-TEXT
           if riga > 1
              modify gd1, start-y = riga,         y = riga,
                          start-x = 2, x = 78-MaxCol,
                          region-color 257,
                          cursor-color 481
           else
              modify gd1, cursor-color 257
           end-if.
