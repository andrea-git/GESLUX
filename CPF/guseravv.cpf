BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-codice
                inquire ef-codice, value in ef-codice-buf
                evaluate ef-codice-buf
                when spaces
                     move spaces to lab-des-buf
                     set errori to true
                     display message "Utente obbligatorio!"
                             title tit-err
                             icon 2
                when 78-user-tutti
                     move 78-user-tutti-descr  to lab-des-buf
                                                  user-name
                when other
                     move ef-codice-buf to user-cod
                     read user no lock
                        invalid
                           display message "Utente NON valido"
                                   title tit-err
                                    icon 2
                           set errori to true
                           move spaces     to lab-des-buf
                        not invalid
                           move user-name   to lab-des-buf
                     end-read
                end-evaluate
                display lab-des
                if tutto-ok
                   set ReadSecca to true
                   perform SELEZIONA-NUMERICO
                end-if

           when 78-ID-gd1
                perform varying colonna from 2 by 1 
                          until colonna > 78-MaxCol
                   perform CONTROLLO-RIGA
                   if errori
                      exit perform 
                   end-if
                end-perform

           end-evaluate.  

      ****************
           if errori
              if control-id = 78-ID-gd1
                 |Per sicurezza
                 modify ef-codice, color = colore-or
                 |||
                 move riga to event-data-2
                 perform PUNTA-CELLA
              end-if
              move 4 to accept-control
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-OLD
BEGIN-TEXT
           set SiSalvato to true.
           if mod = 0 exit paragraph end-if.
           perform X-Y.
      
           if nuovo
              if tot-righe >= 2
                 set NoSalvato to true
                 |Punta alla prima cella modificabile
                 move 2 to colonna
                 move 2 to riga
              end-if
           else
              perform VALORE-RIGA
              evaluate true
              when col-tipo not = old-col-tipo
                   move 2    to colonna
                   set  NoSalvato        to true
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-RIGA
BEGIN-TEXT
           set tutto-ok to true.
           |IN QUESTO PGM. LA GRID E' COMPLETAMENTE LIBERA!!!
           evaluate colonna
           when 2
                move col-tipo to tav-codice
                read ttipoavv no lock
                   invalid
                      set errori to true
                      display message "Tipologia non valida"
                              title tit-err
                              icon 2
                      move space   to tav-messaggio
                end-read
                move tav-messaggio to col-messaggio
                modify gd1(riga, 3), CELL-DATA col-messaggio
                move zero to riga-nuova
           end-evaluate.


      ***---
       CERCA-DOPPIONE.
           inquire ef-codice, VALUE uav-utente of useravv1
           inquire gd1(riga, 2), cell-data uav-tipologia of useravv1
           read useravv1
              invalid
                 continue
              not invalid
                 set errori to true
                 display message "Messaggio gi inserito"
                         title tit-err
                         icon 2
           end-read.          



END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform X-Y.
           move riga to store-riga.
           if riga = 0
              move 2 to store-riga
           end-if.
           if riga > tot-righe
              move store-riga to riga
           end-if.

           unlock useravv all records.
           if ReadSecca
              perform RIEMPI-CHIAVE
              move uav-utente of useravv to save-codice
              move low-value  to uav-tipologia of useravv
           end-if.

           set tutto-ok  to true.
           set trovato   to false.

           move uav-utente of useravv to user-cod.

           if user-cod = 78-user-tutti
              move 78-user-tutti-descr   to user-name
           else
              read user no lock
                 invalid 
                    continue
              end-read
           end-if.

           start useravv key >= uav-chiave of useravv
              invalid 
                 set errori to true
           end-start.

           if tutto-ok
              modify gd1, mass-update = 1
              perform RESET-GRIGLIA
              move 2 to riga

              perform until 1 = 2
                 read useravv next no lock
                      at end
                      exit perform
                 end-read

                 if uav-utente of useravv not = save-codice
                    exit perform
                 end-if

                 set trovato to true 

                 if nuovo
                    set vecchio to true
                    move 1 to mod-k
                    move 0 to mod
                 end-if
                 
                 move uav-tipologia of useravv to col-tipo
                                                  tav-codice
                 read ttipoavv
                    invalid
                       initialize tav-messaggio
                 end-read
                 move tav-messaggio   to col-messaggio

                 modify gd1(riga, 2),    cell-data = col-tipo
                 modify gd1(riga, 3),   cell-data = col-messaggio
                 add 1 to riga
              end-perform

              modify gd1, mass-update = 0
      *        move save-codice  to ef-codice-buf
      *        modify ef-codice  value ef-codice-buf
           end-if.

           if trovato
              move save-codice to old-uav-utente
                                  uav-utente of useravv
              set tutto-ok to true
              perform FORM1-FLD-TO-BUF
              display ef-codice lab-des
              if mod = 1
                 move store-riga   to riga event-data-2
                 move 2            to colonna
                 perform LOCK-RECORD
                 perform PUNTA-CELLA
                 perform VALORE-OLD-RIGA
                 set  StatusModifica to true
              else
                 perform CANCELLA-COLORE
                 move 78-ID-ef-codice to control-id
                 move 4               to accept-control
                 set  StatusVisua     to true
              end-if
           else
              set errori to true
              unlock useravv all records
              if vecchio
                 if ReadSecca
                    display message MSG-record-inesistente
                              title tit-err
                               icon 2
                    perform CLEAR-SCREEN
                    initialize save-chiave
                 end-if
              end-if
           end-if.

           perform ABILITAZIONI.
           set ReadSecca to false.

           display form1.
           perform STATUS-BAR-MSG.
END-TEXT
END-DATA
BEGIN-DATA
