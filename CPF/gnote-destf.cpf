BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-NUOVOa-LinkTo
BEGIN-TEXT
           move riga-note to riga-old-note

           initialize rec-note


           accept como-data-creazione   from century-date
           accept como-ora-creazione    from time
           move user-codi                   to como-utente-creazione

           inquire gd-note, LAST-ROW = tot-righe-note

           inquire gd-note(tot-righe-note, 2),    
                                         CELL-DATA = tmp-nfod-prog

           add 1 to tmp-nfod-prog
           move tmp-nfod-prog to col-prog-note


           add 1    to tot-righe-note
           move tot-righe-note  to riga-note

           modify gd-note(riga-note), RECORD-DATA rec-note

           modify gd-note(riga-note, 1), 
                 HIDDEN-DATA = como-dati-comuni

           perform COLORA-RIGA

           modify gd-note, CURSOR-X = 3, cursor-y = riga-note

           move riga-note to event-data-2
           


           move riga-old-note to riga-note           

           perform SPOSTAMENTO-NOTE      
     
           set NoteCambiate   to true.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-CANCELLAa-LinkTo
BEGIN-TEXT
      *     if mod-campi = 0 exit paragraph end-if.
           if mod = 0
              exit paragraph
           end-if.

           perform X-Y-NOTE.

           if tot-righe-note > 1 or
              ( riga-note > 1 and riga-note < tot-righe-note)

              display message box "Eliminare la nota selezionata?"
                      title = titolo
                      type mb-yes-no
                      default mb-no
                      giving scelta

              if scelta = mb-yes
                 perform X-Y-NOTE
                 modify gd-note, record-to-delete = riga-note
                 perform RICOLORA-NOTE

                 modify gd-note, cursor-y = riga-note, cursor-x = 1
                                                       cursor-y = 2
                 move 2   to event-data-2
                 perform SPOSTAMENTO-NOTE
                 set NoteCambiate   to true
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-SALVAa-LinkTo
BEGIN-TEXT
           INQUIRE TOOL-SALVA, ENABLED IN E-SALVA
           IF E-SALVA = 1
              PERFORM SALVA-note
           END-IF           
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gnote-ordforn-Ev-Before-Program
BEGIN-TEXT
           move gnd-path  to path-tmp-nforn-dest
           move gnd-mod   to mod

           copy resource "..\resource\elemento.bmp".
           call "W$BITMAP" using wbitmap-load, "elemento.bmp"
                          giving elemento-bmp.
      * Verdana10B-Occidentale
           INITIALIZE WFONT-DATA, font-evidenzia-griglia
           MOVE 10               TO WFONT-SIZE
           MOVE "Arial"          TO WFONT-NAME
           SET WFONT-BOLD        TO true
           SET WFONT-ITALIC      TO true
           SET WFONT-UNDERLINE   TO FALSE
           SET WFONT-STRIKEOUT   TO FALSE
           SET WFONT-FIXED-PITCH TO FALSE
           MOVE 0                TO WFONT-CHAR-SET
           CALL "W$FONT" USING WFONT-GET-FONT, 
                     font-evidenzia-griglia, WFONT-DATA


           perform ABILITA-TOOLBAR.       
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gnote-ordforn-Ev-After-Program
BEGIN-TEXT
           CALL "w$bitmap" USING WBITMAP-DESTROY, elemento-bmp.
END-TEXT
END-DATA

