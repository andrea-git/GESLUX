BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 0
   PARENT            = param-g2
   ACTOBJ            = param-g2
   ENTRYPOINT        = BeforeDeclarative
BEGIN-TEXT
       TCONVANNO-ERR SECTION.
           use after error procedure on tconvanno.
           evaluate status-tconvanno 
           when "35"
                display message "File [TCONVANNO] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [TCONVANNO] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[TCONVANNO] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "93"             
                move 0 to mod
                modify tool-modifica, value = mod  
                set StatusVisua to true
                open input tconvanno
                display message box  MSG-Gia-in-uso-su-altro-terminale
                            x"0d0a", MSG-SOLA-VISUA
                        title = tit-err
                        icon is mb-error-icon      
           end-evaluate.
           
       G2-ERR SECTION.
           use after error procedure on G2.
           set RecLocked to false.
           evaluate status-G2
           when "35"
                display message "File [G2] not found!"
                           title titolo
                            icon 3
                set errori to true
           when "39"
                display message "File [G2] Mismatch size!"
                           title titolo
                            icon 3
                set errori to true
           when "98"
                display message "[G2] Indexed file corrupt!"
                           title titolo
                            icon 3
                set errori to true
           when "99"
           when "93"
                set RecLocked to true
                set errori    to true
                move 0 to mod
                modify tool-modifica, value = mod
                perform DISPLAY-SCREEN
                set StatusVisua to true
                move "00" to status-G2
                if ReadSecca
                   read G2          no  lock
                        invalid     continue
                        not invalid perform FORM1-FLD-TO-BUF
                                    perform DISPLAY-SCREEN
                   end-read
                end-if
                display message box MSG-Record-gia-in-uso
                          title tit-err
                           icon 2
           when "35"
                set errori to true
                open output G2
                perform WRITE-REC-DEFAULT
           end-evaluate.              
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 1
   NODESTYLE         = 2
   PARENT            = Form1
   ACTOBJ            = Form1
   TEXTMODE          = 0
