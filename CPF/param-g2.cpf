BEGIN-TEXT
           set anteprima       to true.
      *     perform paragrafo-stampa.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.

           if control-id = 78-ID-form1-gd-1               

              display message MSG-cancellare-il-record-corrente
                        title titolo
                         type mb-yes-no
                      default mb-no
                       giving scelta

              if scelta = mb-yes

                 inquire form1-gd-1, cursor-y  in riga
                 perform VALORE-RIGA

                 delete tconvanno record invalid continue end-delete

                 modify  form1-gd-1, record-to-delete = riga 
                 inquire form1-gd-1, last-row in tot-righe
              
                 if riga > tot-righe 
                    move tot-righe to riga 
                 end-if
              
                 modify form1-gd-1, cursor-y = riga, cursor-x = 1
                 move riga to event-data-2
                 perform COLORE-RIGA

                 display message MSG-cancellazione-avvenuta-con-successo
                           title titolo
              end-if

           else
      *
              display message "Cancellare parametri G2?"
                        title titolo
                         type mb-yes-no 
                      default mb-no
                       giving scelta

              if scelta = mb-yes
                 perform RIEMPI-CHIAVE
                 delete G2 record invalid continue end-delete     
                 move 0 to mod
                 move 1 to mod-k
                 perform FORM1-CLEAR
                 set vecchio to true
                 perform CANCELLA-COLORE
                 perform INIT-OLD-REC
              
                 display message MSG-Cancellazione-avvenuta-con-successo
                           title titolo                         

                 unlock G2 all records

                 move 78-ID-ef-codice to CONTROL-ID
                 move 4               to ACCEPT-CONTROL 

                 set Statusvisua to true
                 modify TOOL-MODIFICA value = mod
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA
BEGIN-TEXT
           continue.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CLEAR-SCREEN
BEGIN-TEXT
           move G2-codice to old-G2-codice.

           initialize G2-dati replacing numeric data by zeroes
                                   alphanumeric data by spaces.
                         
           perform FORM1-FLD-TO-BUF. 

           perform INIT-OLD-REC.
                                          
           perform DISPLAY-SCREEN.
  
           unlock G2 all records.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = COLORE-RIGA
BEGIN-TEXT
           if riga < 2 
              move 2 to riga 
           end-if.

           modify form1-gd-1, start-x = 1, x     = 2,
                                  start-y = riga,
                                        y = riga,
                                  region-color 257,
                                  cursor-color 481.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
           when 78-ID-ef-codice
                inquire ef-codice, value in G2-codice-login
                if G2-codice-login = spaces
                   display message "Decodifica Ditta obbligatoria"
                             title tit-err
                              icon 2
                   set errori to true
                end-if

           when 78-ID-ef-fat
                inquire ef-fat value in G2-cod-fatture
                if G2-cod-fatture = spaces
                   set errori to true
                   display message box 
                           "Decodifica fatture obbligatoria"
                            title tit-err
                             icon 2
                end-if

           when 78-ID-ef-nc
                inquire ef-nc value in G2-cod-nc
                if G2-cod-nc = spaces
                   set errori to true
                   display message
                           "Decodifica Note Credito obbligatoria"
                             title tit-err
                              icon 2
                end-if
     
           end-evaluate.
      
           if errori
              perform CANCELLA-COLORE
              move control-id to store-id
              move 4          to accept-control
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-GRID
BEGIN-TEXT
           set tutto-ok to true.

           inquire form1-gd-1, cursor-y in riga.

           perform VALORE-RIGA.

           evaluate colonna
           when 1
                if nuovo
                   if cnv-anno-geslux = 0
                      set errori to true
                      move 1     to colonna
                      display message "Anno obbligatorio"
                                title tit-err
                                 icon mb-warning-icon
                   else  
                      read tconvanno
                           not invalid
                               set errori to true
                               display message MGS-codice-gia-inserito
                                       title = tit-err
                                       icon mb-warning-icon
                      end-read
                   end-if
                end-if
           when 2
                if cnv-anno-G2 = spaces
                   set errori to true
                   move 2 to colonna
                   display message "Anno obbligatorio"
                             title tit-err
                              icon mb-warning-icon
                end-if
           end-evaluate.

           if errori
              modify form1-gd-1, cursor-y = riga, cursor-x = colonna
              move riga    to event-data-2
              move colonna to event-data-1
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           perform RIEMPI-CHIAVE.
           set tutto-ok  to true.
           set ReadSecca to true.
           if mod = 1
              read G2    lock invalid set errori to true end-read
           else
              read G2 no lock invalid set errori to true end-read
           end-if.

           set ReadSecca to false.

           if RecLocked
              set RecLocked to false
           else
              if tutto-ok    
                 if nuovo
                    move 0         to mod
                    move 1         to mod-k
                    move 78-ID-ef-codice to CONTROL-ID
                    move 4               to ACCEPT-CONTROL
                 end-if
                 perform FORM1-FLD-TO-BUF
                 perform DISPLAY-SCREEN
                 set vecchio to true
                 if mod = 1
                    set StatusModifica to true
                 else
                    set StatusVisua    to true
                 end-if
                 perform VALORIZZA-OLD
              else
                 move 0 to mod
                 move 1 to mod-k
                 if vecchio
                    perform CLEAR-SCREEN
                    if YesMessage
                       move 78-ID-ef-codice to CONTROL-ID
                       move 4               to ACCEPT-CONTROL
                       display message box MSG-Record-inesistente
                                 title tit-err
                                  icon 2
                    end-if
                 end-if
              end-if
           end-if.
END-TEXT
END-DATA
