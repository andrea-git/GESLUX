                                      alphanumeric data by spaces.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INTESTAZIONE
BEGIN-TEXT
           modify form1-gd-1(1, 1), cell-data = "Anno GESLUX".
           modify form1-gd-1(1, 2), cell-data = "Anno GESTIONALE 2".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = LOAD-RECORD
BEGIN-TEXT
           modify form1-gd-1, mass-update = 1.

           move low-value to cnv-anno-geslux.
           start tconvanno key is >= cnv-chiave
                  invalid continue
              not invalid
                  perform varying riga from 2 by 1
                          until 1 = 2
                     read tconvanno next
                          at end exit perform
                      not at end
                          move cnv-anno-geslux to col-anno-geslux
                          move cnv-anno-g2     to col-anno-g2
                          modify form1-gd-1, record-to-add = rec-grid
                     end-read                                           
                  end-perform
           end-start.

           modify form1-gd-1, mass-update = ZERO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           inquire tool-modifica, value in mod.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else
              |SPECIFICO PER I PGM. CON GRID-CONTROL
              if mod = 1

                 set YesMessage to true
                 perform CURRENT-RECORD
                 if tutto-ok
                    if control-id < 78-ID-ef-codice
                       move 78-ID-ef-codice to control-id
                    end-if
                    move 0 to mod-k
                    move 1 to mod
                    set StatusModifica to true
                 end-if

                 close    tconvanno
                 open i-o tconvanno allowing readers
              else
                 move 1 to mod
                 perform SALV-MOD            
                 move 0 to mod
                 move 1 to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    move 78-ID-ef-codice to CONTROL-ID
                    set NoMessage to true
                    perform CURRENT-RECORD
                    set StatusVisua to true
                    unlock G2 all records
                    move 0     to mod
                    close      tconvanno
                    open input tconvanno
                 end-if
              end-if
                        
              perform DISPLAY-SCREEN
              modify tool-modifica,  value mod
              perform CANCELLA-COLORE 

              move 4 to accept-control

           end-if.

           perform ABILITAZIONI.

           modify tool-modifica, value mod.

           if mod = 1
              move 1 to BitmapNumDefault
           else
              move 3 to BitmapNumDefault
           end-if.
           modify pb-def, bitmap-number = BitmapNumDefault.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           set tutto-ok to true.

           if control-id = 78-ID-form1-gd-1
              
              if nuovo
                 perform VALORE-RIGA
                 if cnv-anno-geslux not = 0
                    perform SALVA-RIGA
                 else
                    set errori to true
                 end-if
              end-if

              if tutto-ok
                 set nuovo to true
                 inquire form1-gd-1,  last-row   in tot-righe
                 modify  form1-gd-1,  insert-rows = 1
                 modify  form1-gd-1,  cursor-x    = 1 
                                      cursor-y    = tot-righe + 1
                 move 1 to mod
                 modify  tool-modifica,   value = mod

                 add 1 to tot-righe giving riga

                 perform ABILITAZIONI
             
                 initialize old-cnv-rec replacing numeric data by zeroes
                                             alphanumeric data by spaces

                 add 1 to tot-righe giving event-data-2
                 perform COLORE-RIGA
              end-if
           else
              perform SALV-MOD

              if tutto-ok  
                 move 78-ID-ef-codice to CONTROL-ID       
                 move 4               to ACCEPT-CONTROL   
                 move 1               to mod mod-k
                 modify tool-modifica,  value = mod
                 perform CANCELLA-COLORE                  
                 perform VALORIZZA-NUOVO
                 perform INIT-OLD-REC       
                 set StatusIns to true
                 set nuovo to true       
                 unlock G2 all records
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".   
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           move spaces to G2-codice.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           if nuovo
              perform VALORE-RIGA
              if cnv-rec not = old-cnv-rec
                 set NoSalvatoGrid to true
              else
                 set SiSalvatoGrid to true
              end-if
           end-if.

           set tutto-ok to true.

           set tutto-ok to true.
           if NoSalvatoGrid
              display message box MSG-Salvare-le-modifiche, 
                             title = titolo
                             type = mb-yes-no-cancel
                             giving scelta

              evaluate scelta
              when mb-yes    perform SALVA
              when mb-cancel set errori        to true
              when mb-no     set tutto-ok      to true
                             set SiSalvatoGrid to true
              end-evaluate
           else
              perform FORM1-CONTROLLO-OLD

              if NoSalvato
                 display message MSG-Salvare-le-modifiche
                           title titolo
                            type mb-yes-no-cancel 
                          giving scelta       
       
                 evaluate scelta
                 when mb-yes 
                      perform SALVA
                 when mb-no  
                      continue
                 when other                
                      perform CANCELLA-COLORE
                      set errori to true
                      move store-id to CONTROL-ID       
                      move 4        to ACCEPT-CONTROL   
                 end-evaluate
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.

           set tutto-ok to true.

           perform varying CONTROL-ID from 78-ID-ef-codice by 1
                     until CONTROL-ID > 78-ID-ef-nc
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if tutto-ok
              inquire form1-gd-1, cursor-x in colonna, 
                                  cursor-y in riga


              perform varying colonna from 1 by 1 
                        until colonna > 2
                 perform CONTROLLO-GRID
              
                 if errori 
                    exit perform 
                 end-if
              end-perform
           end-if.

           if errori
              perform ABILITAZIONI
              move store-id to control-id
              move 4        to accept-control
           else                       
              perform VALORIZZA-DATI-COMUNI
              write cnv-rec invalid rewrite cnv-rec end-write
              set SiSalvatoGrid to true
              set vecchio       to true

              perform FORM1-BUF-TO-FLD
              perform CANCELLA-COLORE 

              if nuovo
                 accept G2-data-creazione from century-date
                 accept G2-ora-creazione  from time
                 move   user-codi         to G2-utente-creazione
              else
                 accept G2-data-ultima-modifica from century-date
                 accept G2-ora-ultima-modifica  from time
                 move   user-codi          to G2-utente-ultima-modifica
              end-if

              write G2-rec invalid rewrite G2-rec end-write
       
              set vecchio to true       
              perform TORNA-IN-VISUA            
           end-if.   
                 
           perform DISPLAY-SCREEN.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA-RIGA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.

           set tutto-ok to true.
           inquire form1-gd-1, cursor-x in colonna, 
                               cursor-y in riga.

           perform varying colonna from 1 by 1 
                     until colonna > 2
              perform CONTROLLO-GRID
           
              if errori 
                 exit perform 
              end-if
           end-perform.

           if errori
              perform ABILITAZIONI
              move store-id to control-id
              move 4        to accept-control
           else                       
              perform VALORIZZA-DATI-COMUNI
              write cnv-rec invalid rewrite cnv-rec end-write
              set SiSalvatoGrid to true
              set vecchio       to true
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           inquire form1-gd-1, cursor-x = colonna, cursor-y = riga.

           if event-data-2 not = riga
              perform VALORE-RIGA
              if cnv-anno-geslux = 0
                 modify form1-gd-1, record-to-delete riga
                 set vecchio to true
              else  
                 perform SALVA-RIGA
                 if errori
                    move riga    to event-data-2
                    move colonna to event-data-1 | (ISACCO)
                    set event-action to event-action-fail
                 end-if
              end-if
           else
              if colonna not = event-data-1
                 perform CONTROLLO-GRID
                 if errori
                    set event-action to event-action-fail
                 end-if
              end-if
           end-if.

      * VALORIZZO L'OLD DELLA RIGA SU CUI SONO ANDATO
           move event-data-2 to riga.
           move event-data-1 to colonna.
           perform VALORE-OLD-RIGA.

      * COLORAZIONE RIGA IN GRID
           perform COLORE-RIGA.

           perform GRID-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA
BEGIN-TEXT
           set stampa       to true.
      *     perform paragrafo-stampa.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-codice to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           unlock G2 all records.
           move 3 to BitmapNumDefault.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-RIGA
BEGIN-TEXT
           inquire form1-gd-1(riga, 1), cell-data cnv-anno-geslux.
           inquire form1-gd-1(riga, 2), cell-data cnv-anno-g2.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-OLD-RIGA
BEGIN-TEXT
           if vecchio
              inquire form1-gd-1(riga, 1) cell-data old-cnv-anno-geslux
              move old-cnv-anno-geslux    to cnv-anno-geslux
              read tconvanno              no lock
              move cnv-rec                to old-cnv-rec
           else
              initialize old-cnv-rec replacing numeric data by zeroes
                                          alphanumeric data by spaces
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      *    se non ho valorizzato i campi di creazione li valorizzo
           if cnv-data-creazione   = 0
              accept cnv-data-creazione from century-date
           end-if.
           if cnv-ora-creazione    = 0
              accept cnv-ora-creazione  from time
           end-if.
           if cnv-utente-creazione = space
              move user-codi to cnv-utente-creazione
           end-if.

           accept cnv-data-ultima-modifica from century-date.
           accept cnv-ora-ultima-modifica  from time.
           move user-codi    to cnv-utente-ultima-modifica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-NUOVO
BEGIN-TEXT
