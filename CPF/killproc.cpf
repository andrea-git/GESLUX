BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CARICA-TMP-CPU
BEGIN-TEXT
           initialize path-tmp-cpu
           string  path-kill    delimited low-value
                   "tmp_wmic"   delimited size
                   "_"          delimited size
                   como-data    delimited size
                   "_"          delimited size
                   como-ora     delimited size
                   into path-tmp-cpu
           end-string.
           open output tmp-cpu
           close tmp-cpu
           open i-o tmp-cpu


           move como-path-wmic  to wstampa
           open input lineseq
           perform until 1 = 2
              move spaces to line-riga
              read lineseq next 
                 at end 
                    exit perform 
              end-read
              perform RIPULISCI-REC
              perform VAL-TMP
           end-perform
           close       lineseq.
           delete file lineseq.

      ***---
       RIPULISCI-REC.
           move zero   to idx
           initialize como-stringa
           perform varying cont from 1 by 1 until cont > 200
              evaluate line-riga(cont:1)
              when x"FF"
              when x"FE"
              when x"00"
                   continue
              when x"0D"
                   exit perform
              when other
                   add 1 to idx
                   move line-riga(cont:1)   to como-stringa(idx:1)
                   if idx = 100
                      exit perform
                   end-if
              end-evaluate
           end-perform.

      ***---
       VAL-TMP.
           initialize rec-wmic.

           unstring como-stringa delimited by ","
                    into rw-nodo
                         rw-pid
                         rw-nome
                         rw-cpu

           move rw-pid    to cpu-pid convert
           move rw-nome   to cpu-immagine
           move rw-cpu    to cpu-perce convert

           if cpu-pid not = zero
              write cpu-rec
                 invalid
                    rewrite cpu-rec
              end-write
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CANCELLA-PID-ORFANI
BEGIN-TEXT
