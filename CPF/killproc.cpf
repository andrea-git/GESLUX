           cancel "gpgmexe".

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CARICA-GRID-PGM
BEGIN-TEXT
           initialize lbl-utente-buf
                      lbl-postazione-buf
                      lbl-nome-buf

           inquire grid-pid(riga), record-data rec-grid-pid

           modify grid-pgm, mass-update = 1
           modify grid-pgm, RESET-GRID 1.
           perform GRID-PGM-CONTENT

           move col-pid   to pid-num
                             pge-pid
           move low-value to pge-pgm
           start pgmexe key not < pge-chiave
                 invalid continue
             not invalid
                 perform until 1 = 2
                    read pgmexe next no lock
                         at end exit perform
                    end-read
                    if pid-num not = pge-pid
                       exit perform
                    end-if

                    perform METTI-IN-GRIGLIA-PGM
                 end-perform
           end-start.

           modify grid-pgm, MASS-UPDATE = zero.

           modify lbl-utente       title lbl-utente-buf    
           modify lbl-postazione   title lbl-postazione-buf
           modify lbl-nome         title lbl-nome-buf.


      ***---
       METTI-IN-GRIGLIA-PGM.
           initialize rec-grid-pgm.

           move pge-utente      to lbl-utente-buf    
           move pge-nome-utente to lbl-postazione-buf
           move pge-postazione  to lbl-nome-buf      

           move pge-pgm         to col-pgm-pgm

           string pge-data(7:2) delimited by size
                  "/"           delimited by size
                  pge-data(5:2) delimited by size
                  "/"           delimited by size
                  pge-data(1:4) delimited by size
                  into col-pgm-data
           string pge-ora(1:2)  delimited by size
                  ":"           delimited by size
                  pge-ora(3:2)  delimited by size
                  into col-pgm-ora

           modify grid-pgm, RECORD-TO-ADD rec-grid-pgm.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLA-SESSIONE-CORRENTE
BEGIN-TEXT
           inquire grid-pid(riga, 2), CELL-DATA col-pid
           move col-pid   to pid-num
           if pid-num = pid-sessione
              set sessione-corrente   to true
           else
              set sessione-corrente   to false
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = Form1-Pb-2-BeforeProcedure
BEGIN-TEXT
           modify Form1-Pb-2, bitmap-number 2.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = Form1-Pb-2-AfterProcedure
BEGIN-TEXT
           modify Form1-Pb-2, bitmap-number 1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = PB-ESEGUI-BeforeProcedure
BEGIN-TEXT
           modify PB-ESEGUI, bitmap-number 2.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = PB-ESEGUI-AfterProcedure
BEGIN-TEXT
           modify PB-ESEGUI, bitmap-number 1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = PB-ESEGUI-LinkTo
BEGIN-TEXT
           move 8 to Passwd-password.
           call   "passwd" using Passwd-linkage.
           cancel "passwd".

           if not Passwd-StatusOk
LUBEXX        exit paragraph
LUBEXX     end-if.

           set tutto-ok to true.
           set trovato  to false.

           if tot-sel = 0
              display message "Nessun processo selezionato!"
                        title titolo
           else
              if tot-sel = 1
                 display message "Terminare il processo selezionato?"
                           title titolo
                            icon 2
                            type mb-yes-no
                          giving scelta
              else
                 display message "Terminare i processi selezionati?"
                           title titolo
                            icon 2
                            type mb-yes-no
                          giving scelta
              end-if
              if scelta = mb-yes
                 inquire grid-pid, last-row in tot-righe
                 perform varying riga from 2 by 1
                           until riga > tot-righe
                    inquire grid-pid(riga, 2), hidden-data in HiddenSel
                    if YesSel
                       inquire grid-pid(riga, 2), cell-data in col-pid
                       move col-pid to killpid-pid-pgm
                       call "killpid" using killpid-linkage
                       cancel "Killpid"
      *                 move col-pid to como-pid
      *                 call "C$JUSTIFY" using como-pid, "L"
      *                 inspect como-pid replacing trailing spaces by low-value
      *                 initialize comando
      *                 string path-kill    delimited low-value
      *                        "pskill -t " delimited low-value
      *                        como-pid     delimited low-value
      *                        into comando
      *                 end-string
      *                 move 0 to status-call
      *                 call "C$SYSTEM" using comando, 97
      *                                giving status-call
      *                 if status-call = 0
      *                    subtract 1 from tot-sel
      *                 end-if
                    end-if
                 end-perform
                 perform CANCELLA-PID-ORFANI
                 perform RIEMPI-GRID
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = aggmese-Ev-Before-Program
BEGIN-TEXT
      *<<** Customized_Default, SP-G, Screen1-blockpgm-2 **>>           
LUBEXX*****           set AccessConfirmed to true.
LUBEXX*****           call   "passwd" using password.
LUBEXX*****           cancel "passwd".

LUBEXX*****           if AccessDenied
LUBEXX*****              perform AGGMESE-EV-AFTER-PROGRAM
LUBEXX*****              goback
LUBEXX*****           end-if.              

      * Verdana12BI-Occidentale
           INITIALIZE WFONT-DATA, font-evidenzia-griglia
           MOVE 12               TO WFONT-SIZE
           MOVE "Verdana"        TO WFONT-NAME
           SET WFONT-BOLD        TO true
           SET WFONT-ITALIC      TO true
           SET WFONT-UNDERLINE   TO FALSE
           SET WFONT-STRIKEOUT   TO FALSE
           SET WFONT-FIXED-PITCH TO FALSE
           MOVE 0                TO WFONT-CHAR-SET
           CALL "W$FONT" USING WFONT-GET-FONT, 
                     font-evidenzia-griglia, WFONT-DATA.

           copy resource "elemento.bmp".
           call "W$BITMAP" using wbitmap-load, "elemento.bmp"
                          giving elemento-bmp.

           accept pid-sessione-x  from environment "PID_SESSIONE"
           move pid-sessione-x  to pid-sessione convert

END-TEXT
END-DATA
