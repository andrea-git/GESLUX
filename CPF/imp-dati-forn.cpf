END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = Screen4-Pb-1-LinkTo
BEGIN-TEXT
           initialize opensave-data.
      *     accept opnsav-default-dir from environment "PATH_CSV_LISTINI"
                                    
           move "CSV files (*.csv)|*.csv|All files (*.*)|*.*" 
               to opnsav-filters.
           move 1 to opnsav-default-filter.
           move "Selezione Listino" to opnsav-title.
           call "C$OPENSAVEBOX" using opensave-save-box, opensave-data
                               giving opensave-status.

           if opensave-status = 1
              move opnsav-filename to ef-path-buf
              modify ef-path, value = ef-path-buf
           end-if.
END-TEXT
END-DATA

