   BASETYPE          = 2
   PARAGRAPH         = pb-ok-BeforeProcedure
BEGIN-TEXT
           modify pb-ok, bitmap-number 2.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-ok-AfterProcedure
BEGIN-TEXT
           modify pb-ok, bitmap-number 1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = pb-ok-LinkTo
BEGIN-TEXT
           set controllo-finale to true
           perform  varying CONTROL-ID from 78-ID-ef-path by 1
                      until CONTROL-ID > 78-ID-ef-path
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.
           set controllo-finale to false

           if tutto-ok
              perform IMPORTAZIONE
              perform CANCELLA-COLORE
              move 78-ID-ef-path  to control-id
              move 4              to accept-control
           end-if.

           modify pb-ok,        bitmap-number = 1.
           modify pb-annulla,   bitmap-number = 1.
END-TEXT
END-DATA
