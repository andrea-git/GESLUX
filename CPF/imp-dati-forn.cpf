BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO
BEGIN-TEXT
           set tutto-ok to true.

           evaluate CONTROL-ID
      *    

           when 78-id-ef-path
                if controllo-finale
                   inquire ef-path, VALUE ef-path-buf
                   if ef-path-buf = space
                      set errori to true
                      display message "File CSV obbligatorio"
                               title tit-err
                   end-if
                end-if 

           end-evaluate.

           if errori
              perform CANCELLA-COLORE
              move control-id to store-id        
              move 4          to accept-control  
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "color-custom.cpy".
           copy "utydata.cpy".

      ***--- DUMMY ---> NON TOCCARE!!!!
       STATUS-HELP.
           continue.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = IMPORTAZIONE
BEGIN-TEXT
           call "W$MOUSE" using set-mouse-shape, wait-pointer.

           inquire ef-path, VALUE idf-path
           move form1-Handle to idf-Handle

           call   "imp-dati-forn-p" using imp-dati-forn-p-linkage.
           cancel "imp-dati-forn-p".
           call "W$MOUSE" using set-mouse-shape, arrow-pointer.

           move 27 to key-status.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ginqui-Ev-Before-Program
BEGIN-TEXT
           set controllo-finale to false.
END-TEXT
