BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = gsordforn-Ev-After-Init
BEGIN-TEXT
           set gsordforn-cambiati  to false
      *     move gsordforn-path to path-tmp-sordforn
           move gsordforn-mod  to mod

           if mod = 1
              move zero   to v-mod
           else
              move 1      to v-mod
           end-if

           move space  to como-articolo
           if gsordforn-sof-prog = zero
              move zero   to v-righe
           else
              |move 1      to v-righe
              move gsordforn-articolo to art-codice
                                         cod-ed
              read articoli no lock
                 invalid
                    move space  to art-descrizione
              end-read
              string cod-ed  delimited by size
                     " - "   delimited by size
                     art-descrizione  delimited by size
                     into como-articolo
              call "C$JUSTIFY" using como-articolo, "L"
              move gsordforn-qta      to qta-ord
              move gsordforn-qta-eva  to qta-eva
           end-if

           perform ABILITA-TOOLBAR.       
END-TEXT
