       ABILITA-TOOLBAR.
           if mod = zero
              move zero to e-salva
              move BitmapSaveDisabled   to BitmapNumSave
           else
              evaluate livello-abil
              when 1 
                    move zero to e-salva
                    move BitmapSaveDisabled   to BitmapNumSave
              when 2 
                   move 1    to e-salva
                   move BitmapSaveEnabled   to BitmapNumSave
              when 3 
                   move 1    to e-salva
                   move BitmapSaveEnabled   to BitmapNumSave
              end-evaluate
           end-if.

           modify tool-salva,    enabled = e-salva.

      ***---
       CONTROLLO.
           if mod   = 0 and
              mod-k = 0 
              exit paragraph 
           end-if.
           set tutto-ok to true.

           evaluate control-id
           when 78-ID-ef-data
                inquire ef-data, value in ef-data-buf
                move ef-data-buf to como-data
                if como-data not = 0
                   perform DATE-FORMAT
                   move como-data to ef-data-buf
                   display ef-data
                end-if
           when 78-ID-ef-qta
                if v-righe = 1
                   inquire ef-qta, value in como-qta
      *****             if como-qta = 0
      *****                set errori to true
      *****                display message "Quantità obbligatoria"
      *****                          title tit-err
      *****                           icon 2
      *****             end-if
                end-if
           end-evaluate

           if errori
              perform CANCELLA-COLORE
              move CONTROL-ID to store-id
              move 4          to ACCEPT-CONTROL
           end-if.


END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           set tutto-ok  to true.
      ***     read tmp-sordforn no lock invalid 
      ***        set errori to true 
      ***     end-read

           move gsordforn-sof-note       to como-note
           move gsordforn-sof-data-arr   to como-data-arr
           move gsordforn-sof-qta        to como-qta

           perform VALORIZZA-OLD

           perform FORM-SOLLECITI-FLD-TO-BUF
           display form-solleciti
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform FORM-SOLLECITI-CONTROLLO-OLD.  

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel
                            giving scelta

              evaluate scelta
              when mb-yes
                   perform SALVA
              when mb-no
                   continue
              when other
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to control-id
                   move 4        to accept-control
              end-evaluate
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = ZERO 
              exit paragraph 
           end-if.

           set tutto-ok to true.

           set gsordforn-forza-testata to false.
           if gsordforn-sof-prog = 0
              display message "Forzare la data su tutte le righe?"
                        title titolo
                         type mb-yes-no
                       giving scelta
              if scelta = mb-yes
                 set gsordforn-forza-testata to true
              end-if
           end-if.

           perform varying CONTROL-ID from 78-ID-ef-data by 1
                     until CONTROL-ID > 78-ID-ef-note
              perform CONTROLLO
              if errori 
                 exit perform 
              end-if
           end-perform.

           if errori
      *        perform ABILITAZIONI
              move store-id to CONTROL-ID
              move 4        to ACCEPT-CONTROL
           else                       
              perform FORM-SOLLECITI-BUF-TO-FLD
              perform CANCELLA-COLORE 
                                     
              perform VALORIZZA-OLD

              move como-note          to gsordforn-sof-note    
              move como-data-arr      to gsordforn-sof-data-arr
              move como-qta           to gsordforn-sof-qta     
              set gsordforn-cambiati  to true
           end-if.   
                 
           display form-solleciti

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move como-note       to old-como-note    
           move como-data-arr   to old-como-data-arr
           move como-qta        to old-como-qta     

END-TEXT
END-DATA
