BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = MODIFICA
BEGIN-TEXT
           move 5 to key-status.
           inquire tool-modifica, value in mod.
           set tutto-ok to true.

           if nuovo
              move 1 to mod                  
              modify tool-modifica, value mod
           else         
              set ReadSecca to true
      *  se l'utente e' abilitato puo' modificare un record
              if mod = 1
      * Adriano - Gestione lock sul file inizio
                 close tesconsvet
                 open i-o tesconsvet allowing readers
      * Adriano fine
                 set YesMessage to true
                 perform CURRENT-RECORD
                 if not trovato 
                    set errori to true 
                 end-if
                 if RecLocked   
                    set errori to true 
                 end-if
                 
                 if errori 
                    move 0 to mod 
                    move 1 to mod-k
                    set StatusVisua to true
                 else
                    move 0 to mod-k
                    move 1 to mod
                    set StatusModifica to true
                    move 2 to riga MyEventData2
                    move 1 to colonna
                    modify gd-cve, cursor-y = riga, 
                                       cursor-x = colonna
                    move 1   to StatusHelp
                    perform STATUS-HELP

                    perform SPOSTAMENTO
                 end-if   
              else
                 move 1 to mod
                 perform SALV-MOD            
                 move 0 to mod
                 move 1 to mod-k
                 if errori
                    move 1 to mod
                    move 0 to mod-k
                 else
                    if nuovo
                       if totale-righe not > 1
                          perform PRIMO
                       end-if
                    else
      * Adriano - Gestione lock sul file inizio
                       close tesconsvet
                       open input tesconsvet
      * Adriano - fine
                       move 0 to MyEventData2
                       set NoMessage to true
                       perform CURRENT-RECORD
                       set StatusVisua to true
                       unlock tesconsvet all records
                    end-if
                 end-if
              end-if                   

              perform DISPLAY-CAMPI-CHIAVE
                        
              display form1             
              modify control-handle, color = colore-or

              move 78-id-ef-vet to control-id
              move 4 to accept-control

              set ReadSecca to false

           end-if.                      
           modify tool-modifica,  value mod.
           perform STATUS-BAR-MSG.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = NUOVO
BEGIN-TEXT
           if control-id not = 78-id-gd-cve
              perform SALV-MOD
              if tutto-ok
                 move 78-id-ef-vet   to control-id
                 move 4 to accept-control
                 move 1 to mod mod-k
                 modify tool-modifica,  value = mod
                 modify control-handle, color = colore-or
                 perform CLEAR-SCREEN
                 perform INIT-OLD-REC
                 set StatusIns to true
                 perform STATUS-BAR-MSG  
                 set nuovo to true
                 move 0 to RowCounter
                 unlock tesconsvet all records
              end-if
           else   
              if riga-nuova = 0    
                 perform INSERT-RIGA
              else
                 perform VALORE-RIGA
                 if col-esito not = space
                    perform INSERT-RIGA
                 end-if
              end-if
           end-if.

      ***---
       INSERT-RIGA.
           perform X-Y.
           if vecchio
              move 0 to mod-k       
           end-if.   
           set nuovo to true.
           move 1 to mod.
           modify tool-modifica,  value = mod.
           perform ABILITAZIONI.
           perform VALORE-RIGA.
      *     if tev-codice = spaces
      *        perform DEL-RIGA 
      *     end-if.
           add 1 to totale-righe giving riga.
           modify gd-cve, insert-rows = 1.
           move riga to MyEventData2. 
           move    1 to riga-nuova.
           move    1 to colonna.
           move    3 to control-id.
           move    4 to accept-control.
           modify gd-cve, cursor-y = riga, cursor-x = colonna.
           perform COLORE.
           set StatusModifica to true.
           perform STATUS-BAR-MSG.
           move 1   to StatusHelp.
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PARAGRAFO-COPY
BEGIN-TEXT
           copy "abilita-toolbar.cpy".
           copy "color-custom.cpy".   
           copy "status.cpy".
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform CONTROLLO-OLD.

           if NoSalvato
              display message box MSG-Salvare-le-modifiche
                            title titolo
                            type mb-yes-no-cancel
                            giving scelta

              evaluate scelta
              when mb-yes
                   perform SALVA
              when mb-no
                   continue
              when other
                   modify control-handle, color = colore-or
                   set errori to true
                   move store-id to control-id
                   move 4 to accept-control
                   if control-id= 78-id-gd-cve
                      move store-riga to riga MyEventData2
                      modify gd-cve, cursor-y = riga, cursor-x = 2
                      perform COLORE
                   end-if
              end-evaluate

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if sisalvato or mod = 0 
              exit paragraph 
           end-if.

           inquire gd-cve, cursor-x in colonna, 
                               cursor-y in riga.


           if mod-k = 1
              perform varying control-id from 78-ID-ef-vet by 1
                     until control-id > 78-id-gd-cve
                 perform CONTROLLO
                 if errori 
                    exit perform 
                 end-if
              end-perform
           end-if.

           if errori
              move store-id to control-id
              move 4 to accept-control
           else        
              move 78-id-gd-cve   to control-id
              perform X-Y
              if totale-righe = 1
                 set errori to true
              else
                 move riga to store-riga
                 perform RIEMPI-CHIAVE       
                 accept como-data from century-date
                 accept como-ora  from time
                 modify gd-cve, mass-update = 1
                 perform varying riga from 2 by 1 
                           until riga > totale-righe
   
                    modify gd-cve, cursor-y = riga
   
                    perform VALORE-RIGA
   
                    if mod = 1 
                       if nuovo or (riga - 1) > RowCounter
                          move 2 to colonna
                          modify gd-cve, cursor-x = colonna
                          perform CONTROLLO

                          move 3 to colonna
                          modify gd-cve, cursor-x = colonna
                          perform CONTROLLO

                          if not trovato 
                             set errori to true 
                          end-if
                       end-if
                    end-if
   
                    if tutto-ok
                       perform VALORIZZA-DATI-COMUNI
                       write tev-rec 
                          invalid 
                             rewrite tev-rec
                       end-write
                       set sisalvato to true
                       set vecchio   to true
                    else
                       move 3 to control-id
                       move 4 to accept-control
                       modify gd-cve, cursor-y = riga, cursor-x = colonna
                       move riga to MyEventData2
                       perform COLORE
                       exit perform
                    end-if
                 end-perform                
                 modify gd-cve, mass-update = 0
                         
              end-if
           end-if.                    
           perform ABILITAZIONI.             
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SPOSTAMENTO
BEGIN-TEXT
           inquire gd-cve, cursor-x = colonna, cursor-y = riga.

           if event-data-2 not = riga
              perform VALORE-RIGA
      *        if tev-codice = spaces or zero
      *           modify gd-cve, record-to-delete riga
      *           set vecchio to true
      *        else  
                 perform SALVA
                 if errori
                    move riga    to event-data-2
                    move colonna to event-data-1 | (isacco)
                    set event-action to event-action-fail
                 end-if
      *        end-if
           else
              if colonna not = event-data-1
                 perform CONTROLLO
                 if errori
                    set event-action to event-action-fail
                 end-if
              end-if
           end-if.

      *    valorizzo l'old della riga su cui sono andato
           move event-data-2 to riga.
           move event-data-1 to colonna.
           perform VALORE-OLD-RIGA.

      * colorazione riga in grid
           perform COLORE-RIGA.

           perform GRID-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = STAMPA
BEGIN-TEXT
           set stampa       to true.
      *     perform paragrafo-stampa.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-RIGA
BEGIN-TEXT
           perform X-Y.
           inquire gd-cve(riga), RECORD-DATA rec-grid
           move col-esito    to tev-codice
           move col-escons   to tev-esito
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORE-OLD-RIGA
BEGIN-TEXT
           if vecchio
              inquire gd-cve(riga, 1) cell-data old-tev-codice
              move old-tev-codice  to tev-codice
              read tesconsvet no lock
              move tev-rec         to old-tev-rec
           else
              initialize old-tev-rec replacing numeric data by zeroes
                                     alphanumeric data by spaces
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      *    se non ho valorizzato i campi di creazione li valorizzo
           if tev-data-creazione = zero
              accept tev-data-creazione from century-date
           end-if.
           if tev-ora-creazione = zero
              accept tev-ora-creazione from time
           end-if.
           if tev-utente-creazione = space
              move user-codi to tev-utente-creazione
           end-if.

           accept tev-data-ultima-modifica from century-date.
           accept tev-ora-ultima-modifica from time.
           move user-codi to tev-utente-ultima-modifica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok
              perform CHECK-INSERIMENTO
              unlock tesconsvet all records
              modify control-handle, color = colore-or 
              move low-value to tev-chiave

              set ReadSecca to false
              move 0 to MyEventData2
              perform CURRENT-RECORD

              if errori
                 display message box "File [tesconsvet]"
                              x"0d0a""Archivio vuoto"
                         title = titolo
              end-if       

              perform DISPLAY-CAMPI-CHIAVE       
     
              move 1 to control-id
              move 4 to accept-control 
              perform VALORIZZA-OLD
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if                
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = PRECEDENTE
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok     
              perform CHECK-INSERIMENTO
              unlock tesconsvet all records 
              modify control-handle, color = colore-or 
              perform RIEMPI-CHIAVE
              move low-value to tev-codice

              start tesconsvet key is < tev-chiave
                 invalid 
                    set errori to true
                 not invalid 
                    read tesconsvet previous no lock
                       at end 
                          set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move low-value to tev-codice
                 set ReadSecca to false
                 move 0 to MyEventData2
                 perform CURRENT-RECORD
              end-if

              if errori or not trovato
                 display message box "File [tesconsvet]"
                              x"0d0a""Raggiunto inizio file"
                         title = titolo
              end-if                  

              perform DISPLAY-CAMPI-CHIAVE       
     
              move 78-id-ef-vet   to control-id
              move 4            to accept-control 
              perform VALORIZZA-OLD 
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if                 
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SUCCESSIVO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok            
              perform CHECK-INSERIMENTO
              unlock tesconsvet all records 
              modify control-handle, color = colore-or 
              perform RIEMPI-CHIAVE
              move high-value to tev-codice
                                         
              set ReadSecca to false
              move 0 to MyEventData2
              perform CURRENT-RECORD

              if errori or not trovato
                 display message box "File [tesconsvet]"
                              x"0d0a""Raggiunta fine file"
                         title = titolo
              end-if                  

              perform DISPLAY-CAMPI-CHIAVE  
     
              move 1 to control-id
              move 4 to accept-control 
              perform VALORIZZA-OLD
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if                
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ULTIMO
BEGIN-TEXT
           perform SALV-MOD.           
           if tutto-ok                 
              perform CHECK-INSERIMENTO
              unlock tesconsvet all records 
              modify control-handle, color = colore-or 
              move high-value to tev-chiave
              start tesconsvet key is < tev-chiave
                 invalid 
                    set errori to true
                 not invalid 
                    read tesconsvet previous no lock
                       at end 
                          set errori to true 
                    end-read
              end-start

              if tutto-ok
                 move low-value to tev-codice
                 set ReadSecca to false
                 move 0 to MyEventData2
                 perform CURRENT-RECORD
              end-if

              if errori
                 display message box "File [tcauven]"
                              x"0d0a""Archivio vuoto"
                         title = titolo
              end-if         

              perform DISPLAY-CAMPI-CHIAVE
     
              move 78-id-ef-vet   to control-id
              move 4            to accept-control


              perform VALORIZZA-OLD
              if mod = 1
                 move 2 to riga
                 move 1 to colonna
                 modify gd-cve, cursor-x = colonna, cursor-y = riga
                 move riga to MyEventData2
              end-if               

           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CERCA-TESTO
BEGIN-TEXT
           perform SCREEN-SEARCH-OPEN-ROUTINE.

           if ricerca
              move 0 to ResultSearch

              inquire gd-cve, cursor-y in riga

              inquire gd-cve, search-options in grid-search-options
              
              |direzione di ricerca
              set GRID-SEARCH-FORWARDS to true
              set GRID-SEARCH-WRAP     to true

              |Maiuscolo/minuscolo
              if MatchCase = 0
                 set GRID-SEARCH-IGNORE-CASE to true
              else                                                        
                 set GRID-SEARCH-IGNORE-CASE to false
              end-if

              |trova la parola nella cella
              set GRID-SEARCH-MATCH-ANY to true

              |ricerca nelle sole celle visible 
              |ignorando gli eventuali hidden-data
              set GRID-SEARCH-VISIBLE to true

              |non considerare quello su cui sono posizionato
              set GRID-SEARCH-SKIP-CURRENT   to true

              |si posiziona sulla cella trovata
              set GRID-SEARCH-MOVES-CURSOR   to true 

              |num colonna sulla quale fare la ricerca
              move tipo-ricerca to GRID-SEARCH-COLUMN 

              modify gd-cve, search-options GRID-SEARCH-OPTIONS

              modify gd-cve(riga, tipo-ricerca), 
                     search-text TextToSearch |testo da ricercare
                          giving ResultSearch |1, 2 = trovato 0 = non trovato

              if ResultSearch = GRDSRCH-NOT-FOUND
                 display message box MSG-Testo-non-trovato
                         title = titolo
              else
                 inquire gd-cve, cursor-y in riga
                 move riga to event-data-2
                 perform SPOSTAMENTO
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = X-Y
BEGIN-TEXT
           inquire gd-cve, cursor-y in riga, cursor-x in colonna
                            last-row in totale-righe
                            num-rows in NumRows.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-CHIAVE
BEGIN-TEXT
           inquire ef-vet, value in tev-vet.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = COLORE
BEGIN-TEXT
           modify gd-cve, start-y = MyEventData2, y = MyEventData2,
                          start-x = 1,            x = 3,
                          region-color 257,
                          cursor-color 481.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = INIT-OLD-REC
BEGIN-TEXT
           initialize old-tev-rec replacing numeric data by zeroes
                                      alphabetic data by spaces.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CLEAR-SCREEN
BEGIN-TEXT
           move tev-vet to old-tev-vet.

           initialize tev-rec replacing numeric data by zeroes
                              alphabetic data by spaces.
                        
           move old-tev-vet to tev-vet.
           
           move spaces to des-vet.
           display lbl-des-vet.

           perform RESET-GRIGLIA.

           perform INIT-OLD-REC. 
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RESET-GRIGLIA
BEGIN-TEXT
           modify gd-cve, reset-grid = 1.
           perform INTESTAZIONE.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CURRENT-RECORD
BEGIN-TEXT
           move -1 to Memo-vet.
           if ReadSecca
              perform RIEMPI-CHIAVE
              move tev-vet    to Memo-vet
              move low-value to tev-codice
           end-if.
           set tutto-ok  to true.
           set trovato   to false. 
           
           start tesconsvet key not less tev-chiave
              invalid 
                 set errori to true 
           end-start.

           if nuovo
              move tev-vet  to Memo-vet
           end-if.

           if tutto-ok
              modify gd-cve, mass-update = 1
              perform RESET-GRIGLIA
              move 0 to RowCounter
              move 2 to riga
              
              perform until 1 = 2
                 read tesconsvet next no lock 
                    at end 
                       exit perform 
                 end-read

                 if Memo-vet not > -1
                    move tev-vet    to  Memo-vet
                 end-if                  

                 if tev-vet    not = Memo-vet
                    move Memo-vet to tev-vet
                    exit perform
                 end-if

                 set trovato to true 

                 if nuovo
                    move 1 to mod
                    move 0 to mod-k
                    move 78-id-gd-cve   to control-id
                    move 4               to accept-control
                 end-if  

                 set vecchio to true

                 perform METTI-IN-GRIGLIA
                 add 1 to riga
                 add 1 to RowCounter
                 if RowCounter = 1
                    move spaces      to des-vet
                    move Memo-vet to ef-vet-buf
                    display ef-vet lbl-des-vet 
                 end-if
              end-perform          
             
              modify gd-cve, mass-update = 0

           end-if.

           if trovato   
              perform VALORIZZA-OLD
              if mod = 1
                 set  StatusModifica to true
                 perform LOCK-RECORD        
              else
                 set StatusVisua    to true
              end-if
              perform STATUS-BAR-MSG
           end-if.

           if not trovato and ReadSecca
              if vecchio
                 perform CLEAR-SCREEN
                 if YesMessage          
                    display message box "Record inesistente!"
                            title = tit-err
                            icon mb-warning-icon
                 end-if
                 move 0 to mod
                 move 78-id-ef-vet   to control-id
                 move 4 to accept-control
              end-if
           end-if.             

           if MyEventData2 = 0
              move 2 to riga MyEventData2
           end-if.
           set ReadSecca to false.
           perform ABILITAZIONI.

      ***---
       METTI-IN-GRIGLIA.
           move tev-codice  to col-esito
           move tev-esito   to col-escons
                               tec-codice
           read tescons
              invalid
                 move space to tec-descrizione
           end-read
           move tec-descrizione   to col-descr
           modify gd-cve(riga) record-data = rec-grid

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = DISPLAY-CAMPI-CHIAVE
BEGIN-TEXT
           move tev-vet  to ef-vet-buf.
           display ef-vet.
           
           move 78-id-ef-vet   to control-id.
           perform CONTROLLO.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = DEL-RIGA
BEGIN-TEXT
           modify gd-cve, record-to-delete = riga.
           move 0 to riga-nuova.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CHECK-INSERIMENTO
BEGIN-TEXT
      * modifica la status-bar
      * controllo se da inserimento scorro. allora devo segnalare che
      * sono tornato in modifica.
           inquire tool-modifica, value in mod.

           if mod = 1 then 
              set StatusModifica to true
           end-if.

           if mod = 0 then
              set StatusVisua to true   
           end-if.                
      
           perform STATUS-BAR-MSG.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move tev-rec to old-tev-rec.
           set vecchio to true.
           move 0 to riga-nuova.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = LOCK-RECORD
BEGIN-TEXT
           set RecLocked  to false.
           move Memo-vet  to tev-vet.
           read tesconsvet with lock 
              invalid 
                 continue 
           end-read.
           if RecLocked      
              set StatusVisua to true
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = CONTROLLO-OLD
BEGIN-TEXT
           set SiSalvato to true.
           if mod = 0 exit paragraph end-if.
           perform X-Y.
           perform varying riga from 2 by 1
                     until riga > totale-righe
              inquire gd-cve(riga, 2), cell-data in col-escons
              perform VALORE-OLD-RIGA
              if col-escons not = old-tev-esito
                 move riga to store-riga
                 set NoSalvato        to true
                 move 78-id-gd-cve   to store-id
                 exit perform
              end-if
           end-perform.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-GRIGLIA-cve
BEGIN-TEXT
      *     if old-cve-vet   not = cve-vet
              set ReadSecca to true
              perform CURRENT-RECORD
              |set YesZoom to false
              move zero to StatusHelp
              if not trovato and nuovo
                 perform X-Y
                 if totale-righe = 1
                    move 1               to mod             
                    modify gd-cve, enabled = mod
                    initialize rec-grid
                    modify gd-cve, insert-rows = 1
                    move 1               to riga-nuova
                    move 78-id-gd-cve   to control-id
                    move 2               to MyEventData2
                    perform COLORE      
                    move 2               to riga
                    move 1               to colonna
                    move 1               to StatusHelp
                 end-if
              end-if
              perform DISPLAY-CAMPI-CHIAVE
      *     end-if.
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = COLORE-RIGA
BEGIN-TEXT
           if riga < 2 
              move 2 to riga 
           end-if.

           modify gd-cve, start-x = 1, x     = 3,
                                  start-y = riga,
                                        y = riga,
                                  region-color 257,
                                  cursor-color 481.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = NUOVO-LinkTo
BEGIN-TEXT
           inquire tool-nuovo, enabled in e-nuovo.
           if e-nuovo = 1
              perform NUOVO
           end-if.


END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = CANCELLA-LinkTo
BEGIN-TEXT
           inquire tool-cancella, enabled in e-cancella.
           if e-cancella = 1
              perform CANCELLA
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-SALVA-LinkTo
BEGIN-TEXT
           inquire tool-salva, enabled in e-salva.
           if e-salva = 1
              perform salva
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = ANTEPRIMA-LinkTo
BEGIN-TEXT
           inquire tool-anteprima, enabled in e-anteprima.
           if e-anteprima = 1
              perform ANTEPRIMA
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = TOOL-MODIFICA-LinkTo
BEGIN-TEXT
           inquire tool-modifica, enabled in e-modifica
           if e-modifica = 1
              perform MODIFICA
           end-if.

END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = STAMPA-LinkTo
BEGIN-TEXT
           inquire tool-stampa, enabled in e-stampa
           if e-stampa = 1
              perform STAMPA
           end-if.

END-TEXT
END-DATA
