BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Finish-Entry
BEGIN-TEXT
           inquire gd-cve, cursor-y in riga.
           perform VALORE-RIGA.

           perform  CONTROLLO.

           if errori 
              set event-action to event-action-fail 
              perform VALORE-RIGA
           end-if.

           if tev-rec not = old-tev-rec
              set NoSalvato to true                            
           else 
              set SiSalvato to true
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = form1-gd-1-Ev-Msg-Begin-Entry
BEGIN-TEXT
           inquire gd-cve, cursor-x = colonna,
                                   cursor-y in riga.
                         
           perform VALORE-RIGA.

           evaluate colonna
           when 1
                |if tev-codice not = spaces
                if not nuovo
                   set event-action to event-action-fail
                end-if
           when 2
                if mod = zero              
                   set event-action to event-action-fail
                end-if
           when 3
      *          if mod = zero
                   set event-action to event-action-fail
      *          else
      *             if tev-codice = spaces
      *                set event-action to event-action-fail
      *                modify gd-cve, cursor-y = riga, cursor-x = 1
      *             end-if   
      *          end-if
           end-evaluate.
END-TEXT
END-DATA
