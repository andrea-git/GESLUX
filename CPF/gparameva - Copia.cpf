BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-GRID-ARROT
BEGIN-TEXT
      *****     move 0 to idx.
      *****     perform RESETTA-GRID-ARROT.
      *****     perform 10 times
      *****        add   1 to idx
      *****        move vet-qli-da-arrot(idx)  to col-da-arrot
      *****        move vet-qli-a-arrot(idx)   to col-a-arrot
      *****        move vet-valore-arrot(idx)  to col-valore
      *****        modify gd-arrot(idx + 1, 1), cell-data = col-da-arrot
      *****        modify gd-arrot(idx + 1, 2), cell-data = col-a-arrot 
      *****        modify gd-arrot(idx + 1, 3), cell-data = col-valore
      *****     end-perform.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = RIEMPI-GRID-PESO
BEGIN-TEXT
      *****     move 0 to idx.
      *****     perform RESETTA-GRID-PESO.
      *****     perform 20 times
      *****        add   1 to idx
      *****        move vet-qli-da-peso(idx) to col-da-peso
      *****        move vet-qli-a-peso(idx)  to col-a-peso
      *****        modify gd-peso(idx + 1, 1), cell-data = col-da-peso
      *****        modify gd-peso(idx + 1, 2), cell-data = col-a-peso
      *****     end-perform.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALVA
BEGIN-TEXT
           if mod = 0 exit paragraph end-if.
      
           set tutto-ok to true.
      
           perform varying CONTROL-ID from 78-ID-ef-codice by 1
                     until CONTROL-ID > 78-ID-ef-blocco
              perform CONTROLLO
              if errori exit perform end-if
           end-perform.

           if vecchio
      *****        perform CONTROLLA-PRESENZA-SCORTE
           end-if.
      
           if errori
              perform ABILITAZIONI
              move store-id to CONTROL-ID
              move 4        to ACCEPT-CONTROL
           else                       
              perform FORM1-BUF-TO-FLD
              perform VALORIZZA-DATI-COMUNI
              perform CANCELLA-COLORE 
                                     
              accept como-data from century-date
              accept como-ora  from time
          
              write tpa-rec invalid rewrite tpa-rec end-write
              set vecchio to true       
              perform TORNA-IN-VISUA
           end-if.   
                 
           display form1.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SALV-MOD
BEGIN-TEXT
           set tutto-ok to true.
           perform FORM1-CONTROLLO-OLD.
      
           if NoSalvato
              display message MSG-Salvare-le-modifiche
                        title titolo
                         type mb-yes-no-cancel 
                       giving scelta       
       
              evaluate scelta
              when mb-yes 
                   perform SALVA
              when mb-no
                   continue
              when other
                   perform CANCELLA-COLORE
                   set errori to true
                   move store-id to CONTROL-ID       
                   move 4        to ACCEPT-CONTROL   
              end-evaluate
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SELEZIONA
BEGIN-TEXT
           perform RIEMPI-CHIAVE.

           move "tparameva"   to como-file.
           call "zoom-gt"  using como-file, tpa-rec
                          giving stato-zoom.
           cancel "zoom-gt".
           if stato-zoom = 0
              if old-tpa-chiave not = tpa-chiave or
               ( tpa-codice = 0 and mod = 0 )
                 move tpa-chiave to save-key
                 perform SALV-MOD
                 if tutto-ok
                    move save-key to tpa-chiave
                    set ReadSecca to true
                    modify ef-codice, value tpa-codice
                    perform CURRENT-RECORD
                    perform CANCELLA-COLORE
                    move 78-ID-ef-des to CONTROL-ID       
                    move 4            to ACCEPT-CONTROL   
                 end-if                                  
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = SUCCESSIVO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tparameva all records

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tparameva-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tparameva-lock-flag
              end-if
                                     
              perform CANCELLA-COLORE
              perform FORM1-NEXT
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tparameva-lock-flag
                 move 1 to mod-k
                 perform FORM1-NEXT
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = TORNA-IN-VISUA
BEGIN-TEXT
           move 0 to mod.
           move 1 to mod-k.
           move 78-ID-ef-codice to CONTROL-ID.
           set NoMessage to true.
           perform ABILITAZIONI.
           set StatusVisua to true.
           perform STATUS-BAR-MSG.
           unlock tparameva all records.
                        
           modify tool-modifica,  value mod.
           perform CANCELLA-COLORE.
           move 4 to ACCEPT-CONTROL.

           if KeyboardSaved
              set KeyboardReleased to true
              perform RESET-VIRGOLA
           end-if. 
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = ULTIMO
BEGIN-TEXT
           perform SALV-MOD.
           if tutto-ok

              perform CHECK-INSERIMENTO
              unlock tparameva all records 

              if mod = 1
                 move ZERO to mod-k
                 set dataset1-tparameva-lock to true
              else              
                 move 1    to mod-k     
                 initialize dataset1-tparameva-lock-flag
              end-if
                               
              perform CANCELLA-COLORE
              perform FORM1-LAST 
 
              if RecLocked
                 set RecLocked to false             
                 initialize dataset1-tparameva-lock-flag
                 move 1 to mod-k
                 perform FORM1-LAST
              end-if
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-OLD
BEGIN-TEXT
           move tpa-rec to old-tpa-rec.
           set vecchio to true.

           move 0 to StatusHelp
           perform STATUS-HELP.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-NUOVO
BEGIN-TEXT
           move high-value to vet-codice.

           start tparameva key <= tpa-codice
                 invalid move 1 to tpa-codice
             not invalid
                 read tparameva previous with no lock
                      at end    move 1 to tpa-codice
                  not at end    add  1 to tpa-codice
                 end-read
           end-start.

           move "00"    to status-tparameva.

           initialize tpa-dati replacing numeric data by zeroes
                                    alphanumeric data by spaces.
                                       
           perform FORM1-IUD-DISPLAY.

           if OrderBy = 1                             
              move high-value to tpa-descrizione
              start tparameva key <= tpa-descrizione
                    invalid continue 
                not invalid
                    read tparameva previous no lock 
                       at end continue 
                    end-read
              end-start
           end-if.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 3
   PARAGRAPH         = VALORIZZA-DATI-COMUNI
BEGIN-TEXT
      * SE NON HO VALORIZZATO I CAMPI DI CREAZIONE LI VALORIZZO
           if tpa-data-creazione   = 0
              accept tpa-data-creazione from century-date
           end-if.
           if tpa-ora-creazione    = 0
              accept tpa-ora-creazione  from time
           end-if.
           if tpa-utente-creazione = space
              move user-codi to tpa-utente-creazione
           end-if.

           accept tpa-data-modifica from century-date.
           accept tpa-ora-modifica  from time.
           move user-codi    to tpa-utente-modifica.
END-TEXT
END-DATA
BEGIN-DATA
   BASETYPE          = 2
   PARAGRAPH         = NUOVO-LinkTo
BEGIN-TEXT
           INQUIRE TOOL-NUOVO, ENABLED IN E-NUOVO.

           IF E-NUOVO = 1
